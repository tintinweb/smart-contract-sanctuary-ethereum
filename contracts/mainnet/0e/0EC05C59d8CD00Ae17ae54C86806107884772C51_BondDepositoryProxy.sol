        address _to,
        uint256 _amount,
        uint256 _marketId,
        uint256 _periodWeeks,
        uint256 tosPrice
    ) external returns (uint256 stakeId);


    /* ========== Anyone can execute ========== */


    /// @dev            user can stake TOS for LTOS without lockup period
    /// @param _amount  TOS amount
    /// @return stakeId stake id
    function stake(
        uint256 _amount
    ) external  returns (uint256 stakeId);


