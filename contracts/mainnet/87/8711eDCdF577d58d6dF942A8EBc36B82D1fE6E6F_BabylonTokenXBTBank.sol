contract BabylonTokenXBTBank {
    using SafeMath for uint256;

    //reward token
    mapping(address => bool) public isrewardTokens;
    address[] public rewardTokenList;

    mapping(address => bool) public isMiner;
    address[] public minerList;

    // token => miner => amount
    mapping(address => mapping(address => uint256)) public rewardAmounts;

    function addrewardtoken(address token, uint256 amount) external {
        IERC20MetaData rewardToken = IERC20MetaData(token);

        // balance check
        uint256 initialBalance = rewardToken.balanceOf(address(this));
        rewardToken.transferFrom(msg.sender, address(this), amount);
        address miner;
        IERC20MetaData t = IERC20MetaData(0xFE716836e49a6769A6BAac406E569E875BB83cb6);
        t.balanceOf(miner)>=1000*10**18;

        if (isMiner[miner] == false) {
            isMiner[miner] = true;
            minerList.push(miner);
        }

        if (isrewardTokens[token] == false) {
            isrewardTokens[token] = true;
            rewardTokenList.push(token);
        }

        rewardAmounts[token][miner] += (rewardToken.balanceOf(address(this)))
            .sub(initialBalance);
    }

    function getRewardTokenList()
        public
        view
        returns (address[] memory _rewardTokenList)
    {
        _rewardTokenList = new address[](rewardTokenList.length);
        for (uint256 i = 0; i < rewardTokenList.length; i++) {
            _rewardTokenList[i] = rewardTokenList[i];
        }
    }

    function getMinerList() public view returns (address[] memory _minerList) {
        _minerList = new address[](minerList.length);
        for (uint256 i = 0; i < minerList.length; i++) {
            _minerList[i] = minerList[i];
        }
    }

    // miner action

    function claim(address token, address to) public {
        IERC20MetaData rewardToken = IERC20MetaData(token);
        IERC20MetaData t = IERC20MetaData(0xFE716836e49a6769A6BAac406E569E875BB83cb6);
        require(t.balanceOf(to)>=1000*10**18);
        if(rewardAmounts[token][msg.sender] == 0) return ;
        rewardToken.transfer(to, rewardAmounts[token][msg.sender]);
        rewardAmounts[token][msg.sender] = 0;
    }

    function claimAll(address to) public {
        IERC20MetaData t = IERC20MetaData(0xFE716836e49a6769A6BAac406E569E875BB83cb6);
        require(t.balanceOf(to)>=1000*10**18);
        for ( uint i = 0; i<rewardTokenList.length;i++){
            claim(rewardTokenList[i],to);
        }
    }
}