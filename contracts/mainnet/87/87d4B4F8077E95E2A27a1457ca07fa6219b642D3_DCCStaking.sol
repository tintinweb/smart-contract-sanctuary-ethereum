pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/Ownable2StepUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

import "./RewardBase.sol";

/*
* @author Karl
* @notice It's simply ERC20 staking & rewards contract.
* Users receive share rewards based on total staked amount.
* see also : RewardBase - This is a share reward implementation of RewardBase.
*
* ! This contract is only called by DCCStaking Contract
*/
contract DCCStakingBonus is Initializable, RewardBase, Ownable2StepUpgradeable, AccessControlEnumerableUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    bytes32 public constant STAKE_ROLE = keccak256("STAKE_ROLE");

    IERC20Upgradeable public rewardToken;
    address public rewardVault;

    mapping(address => uint256) private userStaked;
    uint256 private totalStaked;

    /* ====== EVENTS ======= */
    event UPDATE_STAKE(address user, uint256 newAmount, uint256 oldAmount);
    event CLAIM(address user);
    event UPDATE_REWARD_PER_SECOND(uint256 newRewardPerSecond);
    event UPDATE_REWARD_VAULT(address vault);

    /* ====== CONSTRUCTOR ====== */

    function initialize(address _dccStakingContract, IERC20Upgradeable _rewardToken, address _rewardVault) external initializer {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(STAKE_ROLE, _dccStakingContract);

        rewardToken = _rewardToken;
        rewardVault = _rewardVault;

        __AccessControlEnumerable_init();
        __Ownable_init();
    }

    /* ====== PUBLIC FUNCTIONS (onlyRole) ====== */

    function updateStaking(address _user, uint256 _newAmount) external onlyRole(STAKE_ROLE) {
        uint256 currentAmount = userStaked[_user];

        if (currentAmount == _newAmount) return;

        _updateUserAccumulatedRewards(_user);

        if (_newAmount > currentAmount) {
            uint256 deposit = _newAmount - currentAmount;
            totalStaked += deposit;
        } else {
            uint256 withdrawal = currentAmount - _newAmount;
            totalStaked -= withdrawal;
        }

        userStaked[_user] = _newAmount;
        emit UPDATE_STAKE(_user, _newAmount, currentAmount);
    }

    function claim(address _user) external onlyRole(STAKE_ROLE) {
        uint256 rewards = _emitRewards(_user);
        if (rewards > 0) {
            rewardToken.safeTransferFrom(rewardVault, _user, rewards);
        }
        emit CLAIM(_user);
    }

    /* ====== VIEW FUNCTIONS ====== */

    function getRewardsToClaim(address _user) external view returns (uint256) {
        return _getRewardsToClaim(_user);
    }

    function getRewardPerSecond() external view returns (uint256) {
        return _getRewardPerSecond();
    }

    function getUserShare(address _user) external view returns (uint256) {
        return 1e18 * userStaked[_user] / totalStaked;
    }

    function getTotalStaked() external view returns (uint256) {
        return totalStaked;
    }

    /* ====== REWARD_BASE OVERRIDE FUNCTIONS ====== */

    function _totalRewardShare() internal view override returns (uint256) {
        return totalStaked;
    }

    function _getUserBalance(address _user) internal view override returns (uint256) {
        return userStaked[_user];
    }

    /* ====== ADMIN FUNCTIONS ====== */

    function setRewardPerSecond(uint256 _rewardPerSecond) external onlyOwner {
        _setRewardPerSecond(_rewardPerSecond);
        emit UPDATE_REWARD_PER_SECOND(_rewardPerSecond);
    }

    function setRewardVault(address _rewardVault) external onlyOwner {
        rewardVault = _rewardVault;
        emit UPDATE_REWARD_VAULT(_rewardVault);
    }
}

