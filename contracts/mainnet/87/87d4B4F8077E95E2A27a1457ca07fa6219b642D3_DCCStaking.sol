pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/Ownable2StepUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

import "../interfaces/ITraitBits.sol";
import "./interfaces/IDCCStaking.sol";
import "./interfaces/ITraitCache.sol";
import "./interfaces/IERC721Staking.sol";

import "./DCCMileage.sol";
import "./DCCStakingBonus.sol";


/*
* @author Karl
* @notice it is the main contract of DCC staking product. there are 3 main features in this contract.
*
* 1. NFT staking (supports multiple nft contracts) (see IERC721Staking.sol, ERC721StakingFixedReward.sol)
* 2. Supports WNCG promotion rewards (see - DCCStakingBonus.sol)
* 3. Additional rewards for completion of collection.
*/
contract DCCStaking is Initializable, IDCCStaking, AccessControlEnumerableUpgradeable, Ownable2StepUpgradeable, PausableUpgradeable, ReentrancyGuardUpgradeable {
    uint256 public constant PARAM_LIMIT = 20;

    /* ====== Staking Variables ======
    *
    * It is managed by setStakingContract
    * nftContracts are NFT Contract
    * stakeContracts are NFT Staking implementation of IERC721Staking
    */
    address[] public nftContracts;
    mapping(address => IERC721Staking) public stakeContracts;

    /* ====== Bonus Staking Variables ======
    *
    * This is an optional feature.
    * bonusStaking - this value does not affect any other behavior of this contract.
    *                Additional reward is granted only if the bonusStaking exists.
    * bonusActivationCollectionId - Condition under which bonus staking can be activated.
    * bonusWeight - Since it can support multiple NFT staking, additional reward weight should be determined.
    */
    DCCStakingBonus public bonusStaking;
    uint256 public bonusActivationCollectionId;
    mapping(address => uint256) private bonusWeight;

    /* ====== Collection & Rewards Variables ======
    *
    * Additional rewards are given when users complete the collection.
    *
    * Collection
    *   slotContracts - ERC721 contracts that can be staked in slots.
    *   slots - Slots and necessary conditions for each slot are saved.
    *   rewardPerSecond - The amount of reward per second to be given if completed of the collection. It is a fixed reward.
    */
    struct Collection {
        string name;
        address[] slotContracts;
        uint64[][] slots;
        uint256 rewardEndsAt;
        uint256 rewardPerSecond;
        bool canStake;
    }
    mapping(uint256 => Collection) private collections;
    uint256 public collectionCount;

    struct UserCollected {
        bool fullCollected;
        uint16 count;
        uint16[14] slots;
    }
    // user => collectionIndex => UserCollected
    mapping(address => mapping(uint256 => UserCollected)) private userCollected;
    // contract => tokenId => CollectionIndex
    mapping(address => mapping(uint256 => uint256)) private nftOfCollection;

    mapping(address => uint256) private lastUpdatedAt;
    mapping(address => uint256) private accumulatedRewards;
    DCCMileage public rewardToken;

    // see : DCCTraitCache.sol
    mapping(address => ITraitCache) public traitCache;

    /* ====== EVENTS ======= */
    event STAKE(address user, address contractAddress, uint256[] tokenIds);
    event STAKE_IN_COLLECTION(address user, address contractAddress, uint256 collectionIndex, uint256[] slotsWithTokenIds);
    event UNSTAKE(address user, address contractAddress, uint256[] tokenIds);
    event CLAIM(address user);
    event SET_STAKING_CONTRACT(address nftContract, address stakeContract);

    /* ====== CONSTRUCTOR ====== */

    function initialize(DCCMileage _rewardToken) external initializer {
        rewardToken = _rewardToken;
        bonusActivationCollectionId = 1;

        __AccessControlEnumerable_init();
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();
    }

    /* ====== PUBLIC FUNCTIONS ====== */

    function stake(
        address _contract,
        uint256[] memory _tokenIds
    ) external nonReentrant whenNotPaused {
        if (_tokenIds.length == 0) revert NoTokenID();
        if (_tokenIds.length > PARAM_LIMIT) revert ExceedMaxTokenLimit();
        if (address(stakeContracts[_contract]) == address(0)) revert UnsupportedToken();

        stakeContracts[_contract].stake(_msgSender(), _tokenIds);

        _syncBonusStaking(_msgSender());

        emit STAKE(_msgSender(), _contract, _tokenIds);
    }

    function stakeInCollection(
        address _contract,
        uint256 _collectionIndex,
        uint256[] memory _slotsWithTokenId
    ) external nonReentrant whenNotPaused {
        if (address(stakeContracts[_contract]) == address(0)) revert UnsupportedToken();
        if (!canStakeInCollectionNow(_contract, _collectionIndex, _slotsWithTokenId)) revert CanNotStakeInSlot();

        stakeContracts[_contract].stake(_msgSender(), _filterTokenIds(_slotsWithTokenId));

        _stakeInCollection(_contract, _collectionIndex, _slotsWithTokenId);
        _syncBonusStaking(_msgSender());

        emit STAKE_IN_COLLECTION(_msgSender(), _contract, _collectionIndex, _slotsWithTokenId);
    }

    function unstake(
        address _contract,
        uint256[] memory _tokenIds,
        uint256[] memory _tokenIndexes
    ) public nonReentrant whenNotPaused {
        if (_tokenIds.length == 0) revert NoTokenID();
        if (_tokenIds.length > PARAM_LIMIT) revert ExceedMaxTokenLimit();
        if (address(stakeContracts[_contract]) == address(0)) revert UnsupportedToken();

        stakeContracts[_contract].unstake(_msgSender(), _tokenIds, _tokenIndexes);

        _unstakeInCollection(_contract, _tokenIds);
        _syncBonusStaking(_msgSender());

        if (countStaking(_msgSender()) == 0) {
            _claim();
        }

        emit UNSTAKE(_msgSender(), _contract, _tokenIds);
    }

    function claim() external nonReentrant whenNotPaused {
        _claim();
    }

    /* ====== VIEW FUNCTIONS ====== */

    function getRewardsToClaim(address _user) external view returns (uint256) {
        uint256 stakingRewards = 0;
        uint256 collectionRewards = getCollectionRewards(_user);

        for (uint i = 0; i < nftContracts.length; i++) {
            stakingRewards += stakeContracts[nftContracts[i]].getRewardsToClaim(_user);
        }

        return accumulatedRewards[_user] + collectionRewards + stakingRewards;
    }

    function getCollectionRewards(address _user) public view returns (uint256) {
        uint256 collectionRewards = 0;
        for (uint collectionIndex = 1; collectionIndex <= collectionCount; collectionIndex++) {
            if (userCollected[_user][collectionIndex].fullCollected) {
                Collection storage collection = collections[collectionIndex];
                uint endsAt = collection.rewardEndsAt < block.timestamp ? collection.rewardEndsAt : block.timestamp;
                if (endsAt > lastUpdatedAt[_user]) {
                    collectionRewards += collection.rewardPerSecond * (endsAt - lastUpdatedAt[_user]);
                }
            }
        }
        return collectionRewards;
    }

    function countStaking(address _user) public view returns (uint256) {
        uint256 count;
        for (uint i = 0; i < nftContracts.length; i++) {
            count += stakeContracts[nftContracts[i]].countStaking(_user);
        }
        return count;
    }

    function getFullCollected(address _user) external view returns (uint256[] memory) {
        uint256 count;
        for (uint i = 1; i <= collectionCount; i++) {
            if (userCollected[_user][i].fullCollected) {
                count++;
            }
        }
        uint256 index;
        uint256[] memory result = new uint256[](count);
        for (uint i = 1; i <= collectionCount; i++) {
            if (userCollected[_user][i].fullCollected) {
                result[index++] = i;
            }
        }
        return result;
    }

    function getCollection(uint256 _index) external view returns (Collection memory) {
        return collections[_index];
    }

    function getStaking(address _contract, address _user) external view returns (uint256[] memory) {
        return stakeContracts[_contract].getStaking(_user);
    }

    function getStakingInCollection(address _user, uint256 _index) external view returns (uint16[] memory){
        uint16[] memory staking = new uint16[](collections[_index].slots.length);
        for (uint s = 0; s < collections[_index].slots.length; s++) {
            staking[s] = userCollected[_user][_index].slots[s];
        }
        return staking;
    }

    function canStakeInCollectionNow(
        address _contract,
        uint256 _collectionIndex,
        uint256[] memory _slotsWithTokenId
    ) public view returns (bool) {
        if (_collectionIndex == 0 || _collectionIndex > collectionCount) revert InvalidCollectionIndex();
        Collection storage collection = collections[_collectionIndex];
        if (_slotsWithTokenId.length != collection.slots.length) revert MismatchSlots();

        if (!collection.canStake) revert CollectionNotAvailable();
        if (collection.rewardEndsAt < block.timestamp) revert CollectionNotAvailable();

        for (uint slotIndex = 0; slotIndex < _slotsWithTokenId.length; slotIndex++) {
            if (_slotsWithTokenId[slotIndex] > 0) {
                if (userCollected[_msgSender()][_collectionIndex].slots[slotIndex] != 0) revert SlotIsFull();
                if (!_isMatchedCollectionSlot(_contract, _collectionIndex, slotIndex, _slotsWithTokenId[slotIndex])) return false;
            }
        }

        return true;
    }

    function isMatchedCollectionSlot(
        address _contract,
        uint256 _collectionIndex,
        uint256 _slotIndex,
        uint256 _tokenId
    ) external view returns (bool) {
        return _isMatchedCollectionSlot(_contract, _collectionIndex, _slotIndex, _tokenId);
    }

    function getBlockTimestamp() external view returns (uint256) {
        return block.timestamp;
    }

    /* ====== INTERNAL FUNCTIONS ====== */

    function _filterTokenIds(uint256[] memory _slotsWithTokenId) internal pure returns (uint256[] memory) {
        uint256 size = 0;
        for (uint i = 0; i < _slotsWithTokenId.length; i++) {
            if (_slotsWithTokenId[i] > 0) size+=1;
        }
        uint256[] memory tokenIds = new uint256[](size);
        uint f = 0;
        for (uint i = 0; i < _slotsWithTokenId.length; i++) {
            if (_slotsWithTokenId[i] > 0) {
                tokenIds[f++] = _slotsWithTokenId[i];
            }
        }
        return tokenIds;
    }

    function _syncBonusStaking(address _user) internal {
        if (address(bonusStaking) != address(0)) {
            uint256 amount = 0;

            if (_isEligibleForBonus(_user)) {
                for (uint i = 0; i < nftContracts.length; i++) {
                    address nftContract = nftContracts[i];
                    amount += stakeContracts[nftContract].countStaking(_user) * bonusWeight[nftContract];
                }
            }

            bonusStaking.updateStaking(_user, amount);
        }
    }

    function _isEligibleForBonus(address _user) internal view returns (bool) {
        return userCollected[_user][bonusActivationCollectionId].fullCollected;
    }

    function _bonusClaim(address _user) internal {
        if (address(bonusStaking) != address(0)) {
            bonusStaking.claim(_user);
        }
    }

    function _updateAccumulatedCollectionRewards(address _user) internal {
        for (uint collectionIndex = 1; collectionIndex <= collectionCount; collectionIndex++) {
            if (userCollected[_user][collectionIndex].fullCollected) {
                Collection storage collection = collections[collectionIndex];
                uint endsAt = collection.rewardEndsAt < block.timestamp ? collection.rewardEndsAt : block.timestamp;
                if (endsAt > lastUpdatedAt[_user]) {
                    accumulatedRewards[_user] += collection.rewardPerSecond * (endsAt - lastUpdatedAt[_user]);
                }
            }
        }
        lastUpdatedAt[_user] = block.timestamp;
    }

    function _stakeInCollection(address _contract, uint256 _collectionIndex, uint256[] memory _slotsWithTokenId) internal {
        _updateAccumulatedCollectionRewards(_msgSender());

        UserCollected storage collected = userCollected[_msgSender()][_collectionIndex];

        uint16 count;
        for (uint slotIndex = 0; slotIndex < _slotsWithTokenId.length; slotIndex++) {
            if (_slotsWithTokenId[slotIndex] > 0) {
                count += 1;

                uint256 tokenId = _slotsWithTokenId[slotIndex];
                collected.slots[slotIndex] = uint16(tokenId);
                nftOfCollection[_contract][tokenId] = _collectionIndex << 32 | slotIndex;
            }
        }
        collected.count += count;

        if (collections[_collectionIndex].slots.length == collected.count) {
            collected.fullCollected = true;
        }
    }

    function _unstakeInCollection(address _contract, uint256[] memory _tokenIds) internal {
        _updateAccumulatedCollectionRewards(_msgSender());

        for (uint i = 0; i < _tokenIds.length; i++) {
            uint256 tokenId = _tokenIds[i];
            if (nftOfCollection[_contract][tokenId] > 0) {
                uint256 collectionIndex = nftOfCollection[_contract][tokenId] >> 32;
                uint256 slotIndex = nftOfCollection[_contract][tokenId] & 0xffffffff;

                UserCollected storage collected = userCollected[_msgSender()][collectionIndex];
                collected.slots[slotIndex] = 0;
                collected.count -= 1;
                if (collected.fullCollected) {
                    collected.fullCollected = false;
                }
                nftOfCollection[_contract][tokenId] = 0;
            }
        }
    }

    function _claim() internal {
        _updateAccumulatedCollectionRewards(_msgSender());

        uint256 rewards = accumulatedRewards[_msgSender()];
        if (rewards > 0) {
            accumulatedRewards[_msgSender()] = 0;
            rewardToken.mint(_msgSender(), rewards);
        }

        for (uint i = 0; i < nftContracts.length; i++) {
            stakeContracts[nftContracts[i]].claim(_msgSender());
        }

        _bonusClaim(_msgSender());
        emit CLAIM(_msgSender());
    }

    function _isMatchedCollectionSlot(
        address _contract,
        uint256 _collectionIndex,
        uint256 _slotIndex,
        uint256 _tokenId
    ) internal view returns (bool) {
        if (_collectionIndex == 0 || _collectionIndex > collectionCount) revert InvalidCollectionIndex();
        Collection storage collection = collections[_collectionIndex];

        if (_contract != collection.slotContracts[_slotIndex]) revert InvalidSlotContract();
        if (_slotIndex >= collection.slots.length) revert InvalidSlotIndex();

        // No condition on slot means any card is possible.
        if (collection.slots[_slotIndex].length == 0) {
            return true;
        }

        uint256 traitBits = _getTraitBits(_contract, _tokenId);

        // staking is possible to the slot if a case that meets the slot condition exists.
        for (uint r = 0; r < collection.slots[_slotIndex].length; r++) {
            uint256 requirement = collection.slots[_slotIndex][r];
            if (_meetRequirement(traitBits, requirement)) {
                return true;
            }
        }

        return false;
    }

    function _meetRequirement(uint256 _traitBits, uint256 _requirement) internal pure returns (bool) {
        if ((_traitBits & _requirement) != _requirement) {
            return false;
        }

        for (uint i = 0; i < 9; i++) {
            uint mask = 0x3f << (i * 6);
            uint requiredAttr = _requirement & mask;
            if (requiredAttr > 0 && requiredAttr != (_traitBits & mask)) {
                return false;
            }
        }

        return true;
    }

    function _getTraitBits(address _contract, uint256 _tokenId) internal view returns (uint256) {
        uint256 traitBits;
        if (address(traitCache[_contract]) > address(0)) {
            traitBits = traitCache[_contract].getTraitBits(_tokenId);
        }
        if (traitBits == 0) traitBits = ITraitBits(_contract).getTraitBitsMemoized(_tokenId);
        if (traitBits == 0) traitBits = ITraitBits(_contract).getTraitBits(_tokenId);
        return traitBits;
    }

    /* ====== ADMIN FUNCTIONS ====== */
    function setStakingContract(IERC721Staking stakeContract) external onlyOwner {
        address nftContract = stakeContract.getStakeToken();
        if (address(stakeContracts[nftContract]) == address(0)) {
            nftContracts.push(nftContract);
        }
        stakeContracts[nftContract] = stakeContract;

        emit SET_STAKING_CONTRACT(nftContract, address(stakeContract));
    }

    function setTraitCache(address _contract, ITraitCache _cache) external onlyOwner {
        traitCache[_contract] = _cache;
    }

    function addCollection(
        string memory _name,
        address[] memory _slotContracts,
        uint64[][] memory _slots,
        uint256 _rewardPerSecond,
        uint256 _rewardEndsAt,
        bool _canStake
    ) external onlyOwner {
        if (_slots.length == 0 || _slots.length > 14 || _slots.length != _slotContracts.length) revert InvalidSlotIndex();
        if (collectionCount > 100) revert ExceedsLimit();

        collectionCount += 1;
        collections[collectionCount].name = _name;
        collections[collectionCount].slotContracts = _slotContracts;
        collections[collectionCount].slots = _slots;
        collections[collectionCount].canStake = _canStake;
        collections[collectionCount].rewardEndsAt = _rewardEndsAt;
        collections[collectionCount].rewardPerSecond = _rewardPerSecond;
    }

    function updateCollection(
        uint256 _collectionIndex,
        string memory _name,
        uint256 _rewardEndsAt,
        bool _canStake
    ) external onlyOwner {
        if (_collectionIndex == 0 || _collectionIndex > collectionCount) revert InvalidCollectionIndex();
        collections[_collectionIndex].name = _name;
        collections[_collectionIndex].canStake = _canStake;
        collections[_collectionIndex].rewardEndsAt = _rewardEndsAt;
    }

    function setBonusContract(DCCStakingBonus _contract) external onlyOwner {
        bonusStaking = _contract;
    }

    function setBonusActivationCollectionId(uint256 _collectionId) external onlyOwner {
        bonusActivationCollectionId = _collectionId;
    }

    function setBonusWeight(address _nftContract, uint256 weight) external onlyOwner {
        bonusWeight[_nftContract] = weight;
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }
}

