// File: SaveUkraine.sol


pragma solidity ^0.8.11;



contract SaveUkraine is ERC1155, Ownable {
    address private constant ukraineEthAddress = 0x165CD37b4C644C2921454429E7F9358d18A45e14;
    uint256 private constant MINT_PRICE = 0.01 ether;

    constructor() ERC1155("https://app.saveukraine-nft.com/metadata.json") {
        _mint(_msgSender(), 0, 1, "");
    }

    function setURI(string memory newuri) external onlyOwner {
        _setURI(newuri);
    }

    function mint(address account)
        external
        payable
    {
        require(MINT_PRICE <= msg.value, "Invalid funds provided.");
        _mint(account, 0, 1, "");
    }

    function withdrawToUkraine() external onlyOwner {
        (bool success, ) = _msgSender().call{value: (address(this).balance / 20)}("");
        require(success, "Owner transfer failed.");
        (success, ) = ukraineEthAddress.call{value: address(this).balance}("");
        require(success, "Ukraine transfer failed.");
    }

}