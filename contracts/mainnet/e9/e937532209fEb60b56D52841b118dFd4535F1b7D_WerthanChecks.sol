    string public baseURI = "ipfs://QmUEjYSsfn3FoyyEpCaKirTKMcCfUTedkCxDLtp34B9EcD/";

    // Protection
    bool public saleStatus = false;
    uint256 public mintLimit = 100;
    uint256 public limitPerTx = 100;
    mapping (address => uint256) public amountMinted;

    constructor (
        address[] memory payees,
        uint256[] memory shares_
    ) ERC721A("Werthan Art Collection - Werthan Bag Pay Roll Checks", "WBPRC") PaymentSplitter(payees, shares_)  {
        levels['platinum'] = true;
        levels['gold'] = true;
        levels['silver'] = true;
        levels['bronze'] = true;

        maxSupplyOfLevel['platinum'] = 100;
        maxSupplyOfLevel['gold'] = 100;
        maxSupplyOfLevel['silver'] = 100;
        maxSupplyOfLevel['bronze'] = 100;

        priceOfLevel['platinum'] = 0.1 ether;
        priceOfLevel['gold'] = 0.05 ether;
        priceOfLevel['silver'] = 0.02 ether;
        priceOfLevel['bronze'] = 0.01 ether;

        priceOfLevelLocked['platinum'] = 0.1 ether;
        priceOfLevelLocked['gold'] = 0.05 ether;
        priceOfLevelLocked['silver'] = 0.02 ether;
        priceOfLevelLocked['bronze'] = 0.01 ether;
    }

    function numberMinted(address owner) public view returns (uint256) {
        return _numberMinted(owner);
    }

    function getOwnershipData(uint256 tokenId) external view returns (TokenOwnership memory) {
        return ownershipOf(tokenId);
    }

    function getMintedAmountOfLevel(string memory level) external view returns (uint256) {
        return mintedAmountOfLevel[level];
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory newBaseURI) external onlyOwner {
        baseURI = newBaseURI;
    }

    function reveal(bool value) external onlyOwner {
        require(revealed != value, "Already set the value");
        revealed = value;
    }

    function setSaleStatus(bool status) public onlyOwner {
        require(saleStatus != status, "Status is already set same value");
        saleStatus = status;
    }

    // Control over mint protection
    function setMintLimit(uint256 newLimit) public onlyOwner {
        mintLimit = newLimit;
    }

    // Control over mint protection
    function setLimitPerTx(uint256 newLimit) public onlyOwner {
        limitPerTx = newLimit;
    }

    function setMaxSupply(uint256 supply) public onlyOwner {
        TOKEN_MAX_SUPPLY = supply;
    }

    function setMaxSupplyOfLevel(string memory level, uint256 supply) public onlyOwner {
        maxSupplyOfLevel[level] = supply;
    }

    function setPriceOfLevel(string memory level, uint256 price) public onlyOwner {
        priceOfLevel[level] = price;
    }

    function setPriceOfLevelLocked(string memory level, uint256 price) public onlyOwner {
        priceOfLevelLocked[level] = price;
    }

    function setLevelBaseURI(string memory level, string memory uri) public onlyOwner {
        levelBaseURI[level] = uri;
    }

    function setLevel(string memory level, bool value) public onlyOwner {
        levels[level] = value;
    }

    function mint(uint256 serialNumber, string memory level, bool locked) external payable {
        require(tx.origin == msg.sender, "The caller is another contract");

        // Check if level exists
        require(levels[level], "Incorrect level.");
        // Check Sale status
        require(saleStatus, "The sale is not active");
        // Check mint limit
        require(amountMinted[msg.sender] + 1 <= mintLimit, "You can't mint more than that for now");
        // Check total supply
        require(totalSupply() + 1 <= TOKEN_MAX_SUPPLY, "No more token left");
        // Check level supply
        require(mintedAmountOfLevel[level] + 1 <= maxSupplyOfLevel[level], "No more token left");
        // Check price
        uint256 tokenPrice = priceOfLevel[level];
        if (locked) {
            tokenPrice = priceOfLevelLocked[level];
        }
        require(msg.value >= tokenPrice, "Wrong price");

        _mint(msg.sender, 1, serialNumber, level, locked, '', true);

        // Increment protection variable to make sure minter never goes over the set limit
        amountMinted[msg.sender] = amountMinted[msg.sender] + 1;
        mintedAmountOfLevel[level] = mintedAmountOfLevel[level] + 1;        
    }

    function airdrop(address account, uint256 serialNumber, string memory level, bool locked) external onlyOwner {
        // Check if level exists
        require(levels[level], "Incorrect level.");
        // Check total supply
        require(totalSupply() + 1 <= TOKEN_MAX_SUPPLY, "No more token left");
        // Check level supply
        require(mintedAmountOfLevel[level] + 1 <= maxSupplyOfLevel[level], "No more token left");
        // Check price
        
        _mint(account, 1, serialNumber, level, locked, '', true);

        // Increment protection variable to make sure minter never goes over the set limit
        mintedAmountOfLevel[level] = mintedAmountOfLevel[level] + 1;
    }

    function withdraw() external onlyOwner {
        require(address(this).balance > 0, "No enough balance");

        (bool success, ) = owner().call{value: address(this).balance}("");

        require(success, "Unable to send eth");
    }
}