            if(
                !(char >= 0x30 && char <= 0x39) && //9-0
                !(char >= 0x61 && char <= 0x7A) //a-z
            )
                return false;
        }
        return true;
    }

    function _checkIfSameStrings(string memory a, string memory b) internal pure returns (bool) {
        if (keccak256(bytes(a)) == keccak256(bytes(b))) {
            return true;
        } else {
            return false;
        }
    }
}

