contract DaoContract is IERC721Receiver {
    using SafeMath for uint256;
    address public feeReceiver1; // business wallet
    address public owner;
    OneEyedSloth public nftAddress;
    bool public enableRefund = false;
    bool public enablePaySplit = false;
    uint256 public maxRefund = 0.2 ether;
    mapping(address => bool) public excludeFromSplit;

    constructor(address _feeReceiver1, address _nftAddress) {
        owner = msg.sender;
        feeReceiver1 = _feeReceiver1;
        nftAddress = OneEyedSloth(_nftAddress);
    }

    modifier onlyToken() {
        require(msg.sender == owner, "Only token contract is allowed to call this function.");
        _;
    }

    receive() external payable {distribute();}

    event ClaimedRefund(address indexed claimer, uint256 tokenId, uint256 refundAmount);
    function claimRefund(uint256 tokenId) public {
        require(enableRefund, "Buyer Protection is not enabled yet.");
        uint256 balance = address(this).balance;
        uint256 bps = balance.div(100000).mul(7);
        uint256 amountRefundable;
        require(bps != 0, "Buyer Protection halted at the moment");        
        require(nftAddress.getApproved(tokenId) == address(this) || nftAddress.isApprovedForAll(msg.sender, address(this)), "not approved to spend the nft id");
        nftAddress.safeTransferFrom(msg.sender, address(this), tokenId);
        if(bps >= maxRefund) {amountRefundable = maxRefund;}
        if(bps < maxRefund) {amountRefundable = bps;}
        payable(msg.sender).transfer(amountRefundable);
        emit ClaimedRefund(msg.sender, tokenId, amountRefundable);
    }

    function claimAllNFTRefund() external {
        require(nftAddress.isApprovedForAll(msg.sender, address(this)), "nfts are not approved to spend");
        uint256[] memory tokenIds = nftAddress.tokensOfOwner(msg.sender);
        for(uint256 i; i < tokenIds.length; i++) {
            claimRefund(tokenIds[i]);
        }
    }

    event Distribution(address indexed receiver1, uint256 fee1);
    function distribute() internal {
        uint256 balance = msg.value;
        if(balance > 0 && !excludeFromSplit[msg.sender]) {
            uint256 fee1 = balance.div(2);
            sendEthToWallets(fee1);
            emit Distribution(feeReceiver1, fee1);
        }
    }

    function sendEthToWallets(uint256 _fee1) internal {
       (bool success1, ) = payable(feeReceiver1).call{value: _fee1}("");
       require(success1, "distribution failed");
    }

    event UpdateFeeReceivers(address indexed _feeReceiver1);
    function updateFeeReceivers(address _feeReceiver1) external onlyToken {
        feeReceiver1 = _feeReceiver1;
        emit UpdateFeeReceivers(_feeReceiver1);
    }

    event EthTransferred(address indexed wallet, uint256 amount);
    function transferEth(address wallet, uint256 amount) external onlyToken {
        (bool success, ) = payable(wallet).call{value: amount}("");
        require(success, "unable to transfer");
        emit EthTransferred(wallet, amount);
    }

    event NftUpdated(address indexed nftAddress_);
    function updateNftAddress(address nftAddress_) external onlyToken {
        nftAddress = OneEyedSloth(nftAddress_);
        emit NftUpdated(nftAddress_);
    }

    event UpdateEnableRefund(bool enableRefund_);
    function updateEnableRefund(bool enableRefund_) external onlyToken {
        enableRefund = enableRefund_;
        emit UpdateEnableRefund(enableRefund_);
    }

    event TransferAllNfts(address indexed to);
    function transferAllNFts(address to) external onlyToken {
        uint256[] memory tokenIds = nftAddress.tokensOfOwner(address(this));
        for(uint256 i; i < tokenIds.length; i++){
            nftAddress.transferFrom(address(this), to, tokenIds[i]);
        }
        emit TransferAllNfts(to);
    }

    event Transfer(address indexed to, uint256 tokenId);
    function transfer(address to, uint256 tokenId) external onlyToken {
        nftAddress.safeTransferFrom(address(this), to, tokenId);
        emit Transfer(to, tokenId);
    }

    event UpdateMaxRefund(uint256 maxRefund_);
    function updateMaxRefund(uint256 maxRefund_) external onlyToken {
        maxRefund = maxRefund_;
        emit UpdateMaxRefund(maxRefund_);
    }

    event UpdateEnablePaySplit(bool enable_);
    function updateEnablePaySplit(bool enable_) external onlyToken {
        require(!enableRefund, "can not enable unless refund disabled.");
        enablePaySplit = enable_;
        emit UpdateEnablePaySplit(enable_);
    }

    event UpdateExcludeFromSplit(address indexed _addr, bool _split);
    function updateExcludeFromSplit(address _addr, bool _split) external onlyToken {
        excludeFromSplit[_addr] = _split;
        emit UpdateExcludeFromSplit(_addr, _split);
    }

    /**
     * Always returns `IERC721Receiver.onERC721Received.selector`.
     */
    function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }
}

contract OneEyedSloth is ERC721Enumerable, Ownable {
    using SafeMath for uint256;
    using Strings for uint256;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdTracker;

    uint256 public constant MAX_SUPPLY = 10000;
    uint256 public PRICE = 0.2 ether;

    string private baseTokenURI;
    string private contractMetaURI;
    uint256 public privateMintBlock;
    uint256 public publicMintBlock;
    uint256 public maxMint = 3;
    uint256 public maxWhitelist = 300;
    uint256 public whitelistCount;
    bool public hideImage = true;
    string private hideImageUrl = "ipfs://bafkreialgrhqkaykm2iaj7qmk4ii4mz5ttedexvuertjxoonxohtx3sozu";

    //whitelist mint
    mapping(address => bool) public whitelists;

    DaoContract public daoContract;

    modifier checkMintTime() {
        require(block.timestamp >= privateMintBlock || msg.sender == owner(), "not the minting time yet");
        if(block.timestamp >= privateMintBlock && block.timestamp < publicMintBlock && msg.sender != owner()){
            require(whitelists[msg.sender], "whitelist mint round");
        }
        _;
    }

    modifier checkMaxMint(uint256 _count) {
        uint256 _maxMint = maxMint;
        if(block.timestamp >= privateMintBlock && block.timestamp < publicMintBlock) {
            _maxMint = 1;
        }
        if(msg.sender != owner()) {
            uint256 balance = balanceOf(msg.sender);
            require(balance.add(_count) <= _maxMint, "balance exceeds maximum mint");
        }
        _;
    }

    constructor() 
    ERC721("One Eyed Sloth", "OES")
    {
        address _feeReceiver1 = 0xAE5A8f8BFD68d42DA78cB0E98eFBFb95BEff3e26;
        privateMintBlock = 1677780000;
        publicMintBlock = 1677866400;
        whitelists[msg.sender] = true;
        daoContract = new DaoContract(_feeReceiver1, address(this));
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenURI;
    }

    function contractURI() external view returns (string memory) {
        return contractMetaURI;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory){
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        if(hideImage) return hideImageUrl;

        string memory baseURI = _baseURI();
        return
            bytes(baseURI).length > 0
                ? string(abi.encodePacked(baseURI, tokenId.toString(), ".json"))
                : "";
    }

    event SetBaseURI(string _baseURI);
    function setBaseURI(string memory _baseTokenURI) public onlyOwner {
        baseTokenURI = _baseTokenURI;
        emit SetBaseURI(_baseTokenURI);
    }

    function mintNFTs(uint256 _count) external payable checkMintTime checkMaxMint(_count){
        uint256 totalMinted = _tokenIdTracker.current();
        require(totalMinted.add(_count) <= MAX_SUPPLY, "OES: Not enough NFTs!");
        if(msg.sender != owner()){
            require(_count > 0 ,"OES: cannot mint specified number of NFTs.");
            require(msg.value >= PRICE.mul(_count),"OES: not enough ether received");
            (bool success, ) = payable(daoContract).call{value: msg.value}("");
            require(success, "transfer failed");
        }
        
        for (uint256 i = 0; i < _count; i++) {
            _mintSingleNFT();
        }
    }

    function _mintSingleNFT() private {
        _tokenIdTracker.increment();
        uint256 newTokenID = _tokenIdTracker.current();
        _safeMint(msg.sender, newTokenID);
    }

    function tokensOfOwner(address _owner) external view returns (uint256[] memory){
        uint256 tokenCount = balanceOf(_owner);
        uint256[] memory tokensId = new uint256[](tokenCount);
        for (uint256 i = 0; i < tokenCount; i++) {
            tokensId[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokensId;
    }

    event ETHClaimed(address claimer, uint256 amount);
    function withdrawEth() external payable onlyOwner {
        uint256 balance = address(this).balance;
        require(balance > 0, "OES: no ether left to withdraw");
        (bool success, ) = payable(msg.sender).call{value: balance}("");
        require(success, "OES: transfer failed.");
        emit ETHClaimed(msg.sender, balance);
    }

    function transfer(uint256 tokenId, address to) external {
        safeTransferFrom(msg.sender, to, tokenId);
    }

    event UpdateMintPrice(uint256 _price);
    function updatePrice(uint256 _newPrice) external onlyOwner {
        PRICE = _newPrice;
        emit UpdateMintPrice(_newPrice);
    }

    function updateWallets(address _feeReceiver1) external onlyOwner {
        daoContract.updateFeeReceivers(_feeReceiver1);
    }

    function updateEnableRefund(bool enableRefund_) external onlyOwner {
        daoContract.updateEnableRefund(enableRefund_);
    }

    function updateNftAddress() external onlyOwner {
        daoContract.updateNftAddress(address(this));
    }

    event UpdatedMintTiming(uint256 newStartTime, uint256 newPublicMintTime);
    function updateMintTiming(uint256 newStartTime, uint256 newPublicMintTime) external onlyOwner {
        privateMintBlock = newStartTime;
        publicMintBlock = newPublicMintTime;
        emit UpdatedMintTiming(newStartTime, newPublicMintTime);
    }

    event UpdateMaxMint(uint256 _maxMint);
    function updateMaxMint(uint256 _maxMint) external onlyOwner {
        maxMint = _maxMint;
        emit UpdateMaxMint(_maxMint);
    }

    event UpdateCollectionURI(string newContractURI);
    function setContractURI(string memory newContractURI) external onlyOwner {
        contractMetaURI = newContractURI;
        emit UpdateCollectionURI(newContractURI);
    }

    event UpdateHideImage(bool _show);
    function updateHideImage(bool _show) external onlyOwner {
        hideImage = _show;
        emit UpdateHideImage(_show);
    }

    event UpdateHideImageURL(string url_);
    function updateHideImageUrl(string memory url_) external onlyOwner {
        hideImageUrl = url_;
        emit UpdateHideImageURL(url_);
    }
    
    //if tokenId is 0 then it will transfer all nft holdings
    function transferDaoNFT(address to, uint256 tokenId) external onlyOwner {
        if(tokenId != 0){
            daoContract.transfer(to, tokenId);
        } else {
            daoContract.transferAllNFts(to);
        }
    }

    function updateMaxRefund(uint256 maxRefund_) external onlyOwner {
        daoContract.updateMaxRefund(maxRefund_);
    }

    function updaterdaoContract(address daoContract_) external onlyOwner {
        daoContract = DaoContract(payable(daoContract_));
    }

    event DaoFundsTransferred(address indexed to, uint256 amount);
    function transferDaoFunds(address to, uint256 amount) external onlyOwner {
        daoContract.transferEth(to, amount);
        emit DaoFundsTransferred(to, amount);
    }

    function updateExcludeFromSplit(address _addr, bool _split) external onlyOwner {
        daoContract.updateExcludeFromSplit(_addr, _split);
    }

    event UpdateWhitelist(address indexed account_, bool whitelist_);
    function updateWhitelist(address account_, bool whitelist_) external onlyOwner {
        whitelists[account_] = whitelist_;
        emit UpdateWhitelist(account_, whitelist_);
    }

    event WhitelistWallet(address indexed wallet);
    function whitelistWallet() external {
        require(whitelistCount < maxWhitelist, "whitelist quota finished");
        require(!whitelists[msg.sender], "already whitelisted");
        whitelistCount += 1;
        whitelists[msg.sender] = true;
        emit WhitelistWallet(msg.sender);
    }

    event UpdatedMaxWhitelist(uint256 _maxWhitelist);
    function updateMaxWhitelist(uint256 _maxWhitelist) external onlyOwner {
        maxWhitelist = _maxWhitelist;
        emit UpdatedMaxWhitelist(_maxWhitelist);
    }

}