pragma solidity =0.8.14;

import "@openzeppelin/contracts-upgradeable/proxy/ClonesUpgradeable.sol";

import "./DAOVesting.sol";

contract Factory {
    address implementation;

    struct Params {
        IERC20 vestingToken;
        uint32 startTime;
        uint16 firstUnlockPercentage;
        uint32 linearVestingOffset;
        uint32 linearVestingPeriod;
        uint32 linearUnlocksCount;
        uint16 feeRateStart;
        uint16 feeRateEnd;
        uint16 feeSplitPercentage;
        address[] feeCollectors;
        string roundName;
    }

    event Deployment(address indexed deployer, Params params);

    constructor() {
        implementation = address(new DAOVesting());
    }

    function deployMultiple(Params[] calldata params) external returns (address[] memory) {
        address[] memory deployedAt = new address[](params.length);
        for (uint i; i < params.length; i++) {
            deployedAt[i] = deploy(params[i]);
        }
        return deployedAt;
    }

    function deploy(Params calldata params) public returns (address) {
        address deployedAt = ClonesUpgradeable.clone(implementation);
        DAOVesting(deployedAt).init(
            params.vestingToken,
            params.startTime,
            params.firstUnlockPercentage,
            params.linearVestingOffset,
            params.linearVestingPeriod,
            params.linearUnlocksCount,
            params.feeRateStart,
            params.feeRateEnd,
            params.feeSplitPercentage,
            params.feeCollectors,
            params.roundName
        );
        DAOVesting(deployedAt).transferOwnership(msg.sender);
        emit Deployment(msg.sender, params);
        return deployedAt;
    }
}

