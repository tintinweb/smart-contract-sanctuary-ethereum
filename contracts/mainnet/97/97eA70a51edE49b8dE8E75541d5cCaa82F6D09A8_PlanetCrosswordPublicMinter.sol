    answers[0x7ef75553a406bd1759f6a22b1d084d3d5e1bce4181066b35a8366e896ba310a9] = true;
    answers[0x4a1df76d5fdca1f3267693f5b684847b77f02f85a5d9767d357ab2aa90634fc9] = true;
