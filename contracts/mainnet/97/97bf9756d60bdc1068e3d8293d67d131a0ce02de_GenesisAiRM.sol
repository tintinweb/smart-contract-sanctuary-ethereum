//****************   MAIN AiRM NFT CONTRACT  ****************************
//Who? A3g1X. When? October 2022

contract GenesisAiRM is ERC721Enumerable, Ownable{

	using Counters for Counters.Counter;
	using Address for address;

	uint public mintFee = 0.05 ether;
   	uint public upgradeFee = 0.001 ether;
    uint16 public xpDivisor = 800;
    bool public isMintEnabled = true;

    string private _baseURIextended;
    string private _contractURI;
  	Counters.Counter private _tokenIdTracker;

	address payable private admin;
   
//main token stats
	struct AiRM_Stats {
        uint birthdate;
        uint dna;
        uint16 xp;
        uint8 class;
        uint8 stat1;
        uint8 stat2;
        uint8 stat3;
        uint8 stat4;
        uint8 stat5;
 	}
	
	AiRM_Stats[] public AiRMs;

//user stats
	struct User {
        uint research1;
        uint research1date;
        uint research2;
        uint research2date;
        uint research3;
        uint research3date;
        uint16 userxp;
	}

    mapping (address => User) public users;

    //freemintlist
    mapping (address => bool) private ethogenholders;

	modifier onlyOwnerOf(uint _tokenID) {
        require(msg.sender == ownerOf(_tokenID));
        _;
	}

	constructor()  ERC721("Krypti-AiRM", "AiRM") {  

        _baseURIextended = "https://krypti.io/tokenmeta/getmetadata.php?tokenid=";
        _contractURI = "https://krypti.io/tokenmeta/contractmetadata";

 		admin = payable(0x78F93C92bc94Eaff33388E1ABda97A4689ce3B87);
 
    //genesis first one
        _mintAiRM(owner());

    //second one
        _mintAiRM(0x5d3bA4C2B2946761957Ed72849b6A1B519641A1E);

        
    //loop through all ethogens and add to freemint mapping
        for (uint i = 0; i<=233; i++){
            if(!isFreeminter(getEthogenOwner(0xf147BB9A5b75C8794136777aF50fb763F2548E49,i))){
                setFreeminter(true,getEthogenOwner(0xf147BB9A5b75C8794136777aF50fb763F2548E49,i));
            }
        }
      
    }

    //getEthogenOwner
    function getEthogenOwner(address _nft, uint256 _tokenId) public view returns (address){
        return Ethogens(_nft).ownerOf(_tokenId);
    }

//set and return URIs
    function setBaseURI(string memory baseURI_) external onlyOwner() {
        _baseURIextended = baseURI_;
    }
    function _baseURI() internal view virtual override returns (string memory) {
        return _baseURIextended;
    }
    function setContractURI(string memory contractURI_) external onlyOwner() {
        _contractURI = contractURI_;
    }
    function contractURI() public view returns (string memory) {
        return _contractURI;
    }

	function setFee(uint _newfee) external onlyOwner {
       mintFee = _newfee;
    }
    function setFeeAdmin(uint _newfee) external {
        require(msg.sender == admin ,"Caller not Admin");
        mintFee = _newfee;
	}
	function setUpgradeFee(uint _newfee) external onlyOwner {
       upgradeFee = _newfee;
    }
    function setUpgradeFeeAdmin(uint _newfee) external {
        require(msg.sender == admin ,"Caller not Admin");
        upgradeFee = _newfee;
	}
    function setxpDivisor(uint16 _newdivisor) external onlyOwner {
       xpDivisor = _newdivisor;
    }
    function setxpDivisorAdmin(uint16 _newdivisor) external {
        require(msg.sender == admin ,"Caller not Admin");
        xpDivisor = _newdivisor;
	}
    //to change admin address if needed
    function setAdmin(address payable _newadmin) external onlyOwner {
            admin = _newadmin;
        }

    //add to freemint ethogen holders
    function setFreeminter(bool _set,address _wallet) private{
        ethogenholders[_wallet]= _set;
    }

    function isFreeminter(address _wallet) public view returns (bool){
        return ethogenholders[_wallet];
    }
    
    //enable or disable mint    
    function setEnableMint(bool _newstate) external onlyOwner {
        isMintEnabled = _newstate;
    }
    function setEnableMintAdmin(bool _newstate) external {
        require(msg.sender == admin ,"Caller not Admin");
        isMintEnabled = _newstate;
	}

//Mint the AiRM
	function _mintAiRM(address _to) private {

        uint newNftTokenId = _tokenIdTracker.current();
   	
        uint randomdna = uint(keccak256(abi.encodePacked(block.number, msg.sender, _tokenIdTracker.current() + 1)));
        
        uint8 rank;
        if (newNftTokenId <= 110){rank = 1;}
        else if (newNftTokenId <= 443){rank = 2;}
        else if (newNftTokenId <= 999){rank = 3;}
        else {rank = 4;}
    
        
        uint8 class = uint8(uint(keccak256(abi.encodePacked(block.number + _tokenIdTracker.current() + 17, msg.sender))) % 6  + 1);

        uint8 stat1 = uint8(uint(keccak256(abi.encodePacked(block.number + _tokenIdTracker.current() + 37, msg.sender))) % 206  + 50);
        uint8 stat2 = uint8(uint(keccak256(abi.encodePacked(block.timestamp + _tokenIdTracker.current() + 42, msg.sender))) % 206  + 50);
        uint8 stat3 = uint8(uint(keccak256(abi.encodePacked(block.number + _tokenIdTracker.current() + 3, msg.sender))) % 206  + 50);
        uint8 stat4 = uint8(uint(keccak256(abi.encodePacked(block.timestamp + _tokenIdTracker.current() + 11, msg.sender))) % 206  + 50);
        uint8 stat5 = uint8(uint(keccak256(abi.encodePacked(block.number + _tokenIdTracker.current() + 19, msg.sender))) % 206  + 50);


       //add to AiRMs array
        AiRMs.push(AiRM_Stats(block.timestamp,randomdna,0,uint8((rank * 10) + class),stat1,stat2,stat3,stat4,stat5 ));
       
        _mint(_to, newNftTokenId);
        _tokenIdTracker.increment();

        if(newNftTokenId == 110 || newNftTokenId == 443 || newNftTokenId == 999 ){
            isMintEnabled = false;
        }

    }
//mint a AiRM from owner
	function mintOwner(address _to) external onlyOwner {
    	 require (_tokenIdTracker.current()<=1776, "Sold Out");
		 _mintAiRM(_to);
	}	
	function mintAdmin(address _to) external {
    	 require(msg.sender == admin ,"Caller not Admin");
         require (_tokenIdTracker.current()<=1776, "Sold Out");
		 _mintAiRM(_to);
	}	

//mint nft
	function mintNFT(uint amount) external payable {
        require(amount > 0 && amount <=3,"Amount 1-3");
		require(msg.value == mintFee * amount ,"Fee mismatch");
        require(_tokenIdTracker.current() + amount -1 <= 1776, "Sold Out");
        require(isMintEnabled, "Mint Paused");
 
        
        for (uint i = 1; i<=amount; i++){
            _mintAiRM(msg.sender);
        }
        
    }

//freemint nft - get 2 if on list
	function freemintNFT() external {
       
		require(isFreeminter(msg.sender),"Not on the list");
        require(_tokenIdTracker.current() + 1 <= 1776, "Sold Out");
        require(isMintEnabled, "Mint Paused");
        
        _mintAiRM(msg.sender);
        _mintAiRM(msg.sender);

        setFreeminter(false,msg.sender);
 
        
    }


//************************* R and D (quasi-staking) ******************************************
//lock token in incubator for upgrade fee
	function researchin1(uint _tokenID) external payable onlyOwnerOf(_tokenID){
		
		require (users[msg.sender].research1 == 0 , " Full");
		require(msg.value == upgradeFee);
		
	    users[msg.sender].research1date = uint(block.timestamp);
        users[msg.sender].research1 = _tokenID;

	}
	function researchin2(uint _tokenID) external payable onlyOwnerOf(_tokenID){
		
		require (users[msg.sender].research2 == 0 , " Full");
		require (users[msg.sender].userxp >= 20000 , " Levelup to unlock");
		require(msg.value == upgradeFee);
		
	    users[msg.sender].research2date = uint(block.timestamp);
        users[msg.sender].research2 = _tokenID;

	}
	function researchin3(uint _tokenID) external payable onlyOwnerOf(_tokenID){
		
		require (users[msg.sender].research3 == 0 , " Full");
		require(msg.value == upgradeFee*3);
		
	    users[msg.sender].research3date = uint(block.timestamp);
        users[msg.sender].research3 = _tokenID;
	}

//take out of r and d
	function researchout(uint8 _whichone) external {
		
		uint tokenID;
		uint16 xpgained;
		
		if(_whichone == 1){
		    if(users[msg.sender].research1 == 0){revert("Empty");}
		    tokenID = users[msg.sender].research1;
		    users[msg.sender].research1 = 0;
            xpgained = uint16((block.timestamp - users[msg.sender].research1date)/xpDivisor); 
		}	
		else if(_whichone == 2){
		    if(users[msg.sender].research2 == 0){revert("Empty");}
		    tokenID =  users[msg.sender].research2;
		    users[msg.sender].research2 = 0;
            xpgained = uint16((block.timestamp - users[msg.sender].research2date)/xpDivisor); 
		}	
		else if(_whichone == 3){
		    if(users[msg.sender].research3 == 0){revert("Empty");}
		    tokenID =  users[msg.sender].research3;
		    users[msg.sender].research3 = 0;
            xpgained = uint16((block.timestamp - users[msg.sender].research3date) * 3 /xpDivisor); 
		}	

        if(ownerOf(tokenID) == msg.sender){
            addxp(tokenID,xpgained);
        }	
    }

//add xp check Max
	function addxp(uint _tokenID,uint16 _xp) private {
        
        if(_xp > 32000){_xp=32000;}
        if(AiRMs[_tokenID].xp + _xp > 32000){
            AiRMs[_tokenID].xp = 32000;
        }
        else{
            AiRMs[_tokenID].xp += _xp;
        }
    }
//add xp check Max
	function useraddxp(uint16 _xp) private {
       
        if(_xp > 32000){_xp=32000;}
         //max xp 32k
        if(users[msg.sender].userxp + _xp > 32000){
            users[msg.sender].userxp = 32000;
        }
        else{
             users[msg.sender].userxp += _xp; 
        }
     }

//distribute balance
	function collectFees() external onlyOwner {
        address payable payme = payable(owner());
        payme.transfer(address(this).balance);
	}
	function collectxtoken(address _token,uint _amount) external onlyOwner {
        IERC20 xtoken;
        xtoken = IERC20(_token);
		xtoken.transfer(owner(), _amount);
	}

}