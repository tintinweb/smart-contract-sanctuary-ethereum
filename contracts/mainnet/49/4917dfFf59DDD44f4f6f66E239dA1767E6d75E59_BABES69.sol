    using Strings for uint256;
    uint256 public constant MAX_SUPPLY = 69;
    uint256 public constant MAX_PUBLIC_MINT = 69;
    uint256 public constant MAX_WHITELIST_MINT = 69;
    uint256 public PUBLIC_SALE_PRICE = 0.00 ether;
    uint256 public WHITELIST_SALE_PRICE = 0.00 ether;

    string private  baseTokenUri;
    string public   placeholderTokenUri;

    bool public isRevealed;
    bool public publicSale;
    bool public whiteListSale;
    bool public pause;
    bool public teamMinted;

    bytes32 private merkleRoot;

    mapping(address => uint256) public totalPublicMint;
    mapping(address => uint256) public totalWhitelistMint;

    constructor() ERC721A("69BABES", "69ER"){

    }

    modifier callerIsUser() {
        require(tx.origin == msg.sender, "69ERS :: Cannot be called by a contract");
        _;
    }

    function mint(uint256 _quantity) external payable callerIsUser{
        require(publicSale, "69ERS :: Not Yet Active.");
        require((totalSupply() + _quantity) <= MAX_SUPPLY, "69ERS :: Beyond Max Supply");
        require((totalPublicMint[msg.sender] +_quantity) <= MAX_PUBLIC_MINT, "69ERS :: Already minted 3 times!");
        require(msg.value >= (PUBLIC_SALE_PRICE * _quantity), "69ERS :: Below ");

        totalPublicMint[msg.sender] += _quantity;
        _safeMint(msg.sender, _quantity);
    }

    function whitelistMint(bytes32[] memory _merkleProof, uint256 _quantity) external payable callerIsUser{
        require(whiteListSale, "69ERS :: Minting is on Pause");
        require((totalSupply() + _quantity) <= MAX_SUPPLY, "69ERS :: Cannot mint beyond max supply");
        require((totalWhitelistMint[msg.sender] + _quantity)  <= MAX_WHITELIST_MINT, "69ERS :: Cannot mint beyond whitelist max mint!");
        require(msg.value >= (WHITELIST_SALE_PRICE * _quantity), "69ERS :: Payment is below the price");
        //create leaf node
        bytes32 sender = keccak256(abi.encodePacked(msg.sender));
        require(MerkleProof.verify(_merkleProof, merkleRoot, sender), "69ERS :: You are not whitelisted");

        totalWhitelistMint[msg.sender] += _quantity;
        _safeMint(msg.sender, _quantity);
    }

    function teamMint() external onlyOwner{
        require(!teamMinted, "69ERS :: Team already minted");
        teamMinted = true;
        _safeMint(msg.sender, 500);
    }

