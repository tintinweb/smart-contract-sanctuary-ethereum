contract CryptoNinjaChildren is
    AccessControl,
    Ownable,
    RevokableDefaultOperatorFilterer,
    ERC721MultiSaleByMerkle,
    ERC2981,
    TokenUriSupplier,
    ERC721AntiScam
{
    using Strings for uint256;
    // ==================================================================
    // Constants
