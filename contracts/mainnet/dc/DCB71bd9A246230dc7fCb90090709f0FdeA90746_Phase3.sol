        require(msg.value == mutationPrice * totalNftsToMint, "INVALID_ETH");
        require(mergeStarted, "Not Started Yet!");

        require(IERC721(CRUMBOS_CONTRACT_ADDRESS).isApprovedForAll(msg.sender, address(this)) , "Approve CRUMBOS Nfts");
