        maxPerTx = t_max_per_tx;
    }
   function sendGiftsToOwner(address _wallet, uint256 _num) external onlyOwner{
            require(totalSupply() + _num < maxToken, "Max Supply Reached.");
            _safeMint(_wallet, _num);
