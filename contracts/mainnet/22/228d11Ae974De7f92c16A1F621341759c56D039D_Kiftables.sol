        uint256 positionToAssume = lastIndex;
        for (uint256 j = 0; j < lastIndex; j++) {
            int128 rangeStart = ranges[j].start;
            int128 rangeEnd = ranges[j].end;
            if (start < rangeStart && positionToAssume == lastIndex) {
                positionToAssume = j;
            }
