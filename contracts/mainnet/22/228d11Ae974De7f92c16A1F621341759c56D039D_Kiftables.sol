    int128 constant intTOKEN_LIMIT = int128(int256(TOKEN_LIMIT));

    // ranges include the start but not the end [start, end)
    function addRange(
        Range[RANGE_LENGTH] memory ranges,
        int128 start,
        int128 end,
        uint256 lastIndex
