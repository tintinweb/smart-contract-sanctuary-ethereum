// File: .deps/contracts/VizTeamVesting.sol



pragma solidity ^0.8.18;







contract VizTeamVesting is ReentrancyGuard, EIP712, Ownable {
    using SafeERC20 for IERC20;

    struct Stake {
        uint256 amount;
        uint256 startTime;
        uint256 endTime;
        uint256 bonus;
        bool claimed;
    }

    IERC20 public token;
    uint256 public presaleCloseTimestamp = 1685545200;
    uint256 public vestingStartTimestamp = 1685545200;
    uint256 public overallPurchaseLimit = 45000000000000000000;

    uint256 public constant MONTH_IN_SECONDS = 2592000;

    mapping(address => uint256) public purchasedTokens;
    mapping(address => uint256) public stakedTokens;
    mapping(address => uint256) public claimedTokens; //paid out tokens
    mapping(address => uint256) public claimedBonusTokens; //paid out bonus tokens (from staking)
    mapping(address => Stake[]) public stakingInfo;

    address public signer = 0x0076a35889Ac98C5055a5b4adba5480eaFb93Abc;
    uint256 public maxTokensToPurchase = 45000000000000000000;
    uint256 public totalPurchased;
    uint256 public totalStaked;
    uint256 public totalClaimed;
    uint256 public totalBonusClaimed;
    uint256 public totalEth;
    bool public paused;
    bool public stakingEnabled = false;

    string private constant SIGNING_DOMAIN = "PresaleReserveT-Voucher";
    
    string private constant SIGNATURE_VERSION = "1";

    uint256[] public stakingPeriods;
    uint256[] public stakingBonuses;

    uint256[] public monthlyVestingPercentages = [0, 0, 0, 0, 0, 1000, 0, 0, 0, 0, 0, 1000, 0, 0, 2000, 0, 0, 2000, 0, 0, 2000, 333, 333, 333, 333, 333, 335];
    uint256[] public vestingDurations = [2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000, 2592000];
    uint256 public initialVestingPercentage = 0;
    uint256 public initialVestingOffsetDays = 10;
    uint256 public emergencyUnstakePenalty = 20;

    event TokensPurchased(address indexed user, uint256 amount);
    event TokensStaked(address indexed user, uint256 amount, uint256 startTime, uint256 endTime, uint256 bonus);
    event TokensClaimed(address indexed user, uint256 amount);
    event BonusTokensClaimed(address indexed user, uint256 amount);
    event EmergencyUnstake(address indexed user, uint256 index, uint256 amount);
    event ERC20Withdrawn(address indexed token, address indexed to, uint256 amount);
    event Withdrawn(address indexed wallet, uint256 amount);

    constructor(IERC20 _token, address teamWallet) EIP712(SIGNING_DOMAIN, SIGNATURE_VERSION) {
        token = _token;

        totalPurchased = maxTokensToPurchase;
        purchasedTokens[teamWallet] = totalPurchased;
    }
    
    struct PresaleReserveVoucher {
        uint256 amount;
        uint256 expires;
        uint256 value;
        address wallet;
        /// @notice the EIP-712 signature of all other fields in the struct. For a voucher to be valid, it must be signed by an account with the signer.
        bytes signature;
    }

    function _hash(PresaleReserveVoucher memory voucher) internal view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(
            keccak256("Struct(uint256 amount,uint256 expires,uint256 value,address wallet)"),
            voucher.amount,
            voucher.expires,
            voucher.value,
            voucher.wallet
        )));
    }

    function _verify(PresaleReserveVoucher memory voucher) internal view returns (address) {
        bytes32 digest = _hash(voucher);
        return ECDSA.recover(digest, voucher.signature);
    }

    function pause() public onlyOwner {
        paused = true;
    }

    function resume() public onlyOwner {
        paused = false;
    }

    function setStakingEnabled(bool _enabled) public onlyOwner {
        stakingEnabled = _enabled;
    }

    function setStakingPeriods(uint256[] memory _periods) public onlyOwner {
        stakingPeriods = _periods;
    }

    function setStakingBonuses(uint256[] memory _bonuses) public onlyOwner {
        stakingBonuses = _bonuses;
    }

    function setSigner(address _signer) public onlyOwner {
        signer = _signer;
    }

    function setMaxTokensToPurchase(uint256 maxValue) external onlyOwner {
        maxTokensToPurchase = maxValue;
    }

    function setPresaleCloseTimestamp(uint256 _timestamp) external onlyOwner {
        presaleCloseTimestamp = _timestamp;
    }

    function setVestingStartTimestamp(uint256 _timestamp) external onlyOwner {
        vestingStartTimestamp = _timestamp;
    }

    function setOverallPurchaseLimit(uint256 _limit) external onlyOwner {
        overallPurchaseLimit = _limit;
    }

    function setEmergencyUnstakePenalty(uint256 _penalty) external onlyOwner {
        emergencyUnstakePenalty = _penalty;
    }

    function getVestingDurations() external view returns (uint256[] memory) {
        return vestingDurations;
    }

    function purchaseTokens(uint256 amount, uint256 value, uint256 expires, address wallet, bytes memory signature) payable external nonReentrant {
        require(!paused, "Paused");
        require(block.timestamp < presaleCloseTimestamp, "Purchase stage is over");
        require(totalPurchased + amount <= overallPurchaseLimit, "Overall token purchase limit exceeding");
        require(purchasedTokens[wallet] + amount <= maxTokensToPurchase, "Cannot reserve more tokens");
        require(amount > 0, "Amount has to be greater than zero");
        require(msg.value >= value, "Eth amount not valid");
        require(block.number <= expires, "Voucher expired");

        PresaleReserveVoucher memory voucher = PresaleReserveVoucher({amount: amount, value: value, expires: expires, wallet: wallet, signature: signature});
        address _signer = _verify(voucher);

        require(_signer == signer, "Signature invalid");

        purchasedTokens[voucher.wallet] += amount;
        totalPurchased += amount;
        totalEth += value;

        emit TokensPurchased(voucher.wallet, amount);
    }

    function claimVestedTokens() external nonReentrant {
        uint256 vestedAmount = calculateVestedAmount(msg.sender);
        require(vestedAmount > 0, "No vested tokens available");

        claimedTokens[msg.sender] += vestedAmount;
        totalClaimed += vestedAmount;

        token.safeTransfer(msg.sender, vestedAmount);
        emit TokensClaimed(msg.sender, vestedAmount);
    }

    function stakeVestedTokens(uint256 stakingPeriodIndex) external nonReentrant {
        require(stakingPeriodIndex < stakingPeriods.length, "Invalid staking period index");

        uint256 stakingPeriod = stakingPeriods[stakingPeriodIndex];
        uint256 bonusPercentage = stakingBonuses[stakingPeriodIndex];

        uint256 vestedAmount = calculateVestedAmount(msg.sender);
        require(vestedAmount > 0, "No vested tokens available");

        uint256 bonusAmount = (vestedAmount * bonusPercentage) / 100;

        uint256 startTime = block.timestamp;
        uint256 endTime = startTime + (stakingPeriod * MONTH_IN_SECONDS);

        stakedTokens[msg.sender] += vestedAmount;
        totalStaked += vestedAmount;

        Stake memory newStake = Stake({
            amount: vestedAmount,
            startTime: startTime,
            endTime: endTime,
            bonus: bonusAmount,
            claimed: false
        });

        stakingInfo[msg.sender].push(newStake);
        emit TokensStaked(msg.sender, vestedAmount, startTime, endTime, bonusAmount);
    }

    function restakeStakedTokens(uint256 index, uint256 stakingPeriodIndex) external nonReentrant {
        require(stakingEnabled, "Staking is not enabled");
        Stake storage stake = stakingInfo[msg.sender][index];
        require(!stake.claimed, "Stake already claimed or re-staked");
        require(block.timestamp >= stake.endTime, "Staking period not ended yet");
        require(stakingPeriodIndex < stakingPeriods.length, "Invalid staking period index");

        uint256 stakingPeriod = stakingPeriods[stakingPeriodIndex];
        uint256 bonusPercentage = stakingBonuses[stakingPeriodIndex];
        
        claimedBonusTokens[msg.sender] += stake.bonus;
        totalBonusClaimed += stake.bonus;

        stakingInfo[msg.sender][index].claimed = true;

        token.safeTransfer(msg.sender, stake.bonus);
        emit BonusTokensClaimed(msg.sender, stake.bonus);

        uint256 bonusAmount = (stake.amount * bonusPercentage) / 100;
        uint256 startTime = block.timestamp;
        uint256 endTime = startTime + (stakingPeriod * MONTH_IN_SECONDS);

        Stake memory newStake = Stake({
            amount: stake.amount,
            startTime: startTime,
            endTime: endTime,
            bonus: bonusAmount,
            claimed: false
        });

        stakingInfo[msg.sender].push(newStake);
        emit TokensStaked(msg.sender, stake.amount, startTime, endTime, bonusAmount);
    }

    function claimStakedTokens(uint256 index) external nonReentrant {
        require(stakingEnabled, "Staking is not enabled");
        Stake storage stake = stakingInfo[msg.sender][index];
        require(!stake.claimed, "Stake already claimed");
        require(block.timestamp >= stake.endTime, "Staking period not ended yet");

        uint256 totalAmount = stake.amount + stake.bonus;
        stakingInfo[msg.sender][index].claimed = true;

        stakedTokens[msg.sender] -= stake.amount;
        claimedTokens[msg.sender] += stake.amount;
        claimedBonusTokens[msg.sender] += stake.bonus;
        totalStaked -= stake.amount;
        totalClaimed += stake.amount;
        totalBonusClaimed += stake.bonus;

        token.safeTransfer(msg.sender, totalAmount);
        emit TokensClaimed(msg.sender, stake.amount);
        emit BonusTokensClaimed(msg.sender, stake.bonus);
    }

    function claimAllStakedTokens() external nonReentrant {
        require(stakingEnabled, "Staking is not enabled");

        Stake[] storage stakes = stakingInfo[msg.sender];

        for (uint256 i = 0; i < stakes.length; i++) {
            if (!stakes[i].claimed && block.timestamp >= stakes[i].endTime) {
                uint256 totalAmount = stakes[i].amount + stakes[i].bonus;
                stakingInfo[msg.sender][i].claimed = true;

                stakedTokens[msg.sender] -= stakes[i].amount;
                claimedTokens[msg.sender] += stakes[i].amount;
                totalStaked -= stakes[i].amount;
                totalClaimed += stakes[i].amount;

                token.safeTransfer(msg.sender, totalAmount);
                emit TokensClaimed(msg.sender, totalAmount);

            }
        }
    }

    function restakeAllStakedTokens(uint256 stakingPeriodIndex) external nonReentrant {
        require(stakingEnabled, "Staking is not enabled");
        
        require(stakingPeriodIndex < stakingPeriods.length, "Invalid staking period index");
        
        uint256 stakingPeriod = stakingPeriods[stakingPeriodIndex];
        uint256 bonusPercentage = stakingBonuses[stakingPeriodIndex];
                
        Stake[] storage stakes = stakingInfo[msg.sender];

        uint256 bonusAccumulated = 0;

        for (uint256 i = 0; i < stakes.length; i++) {
            if (!stakes[i].claimed && block.timestamp >= stakes[i].endTime) {
                bonusAccumulated += stakes[i].bonus;
                
                uint256 bonusAmount = (stakes[i].amount * bonusPercentage) / 100;
                uint256 startTime = block.timestamp;
                uint256 endTime = startTime + (stakingPeriod * MONTH_IN_SECONDS);

                stakingInfo[msg.sender][i].claimed = true;

                Stake memory newStake = Stake({
                    amount: stakes[i].amount,
                    startTime: startTime,
                    endTime: endTime,
                    bonus: bonusAmount,
                    claimed: false
                });

                stakingInfo[msg.sender].push(newStake);
                emit TokensStaked(msg.sender, stakes[i].amount, startTime, endTime, bonusAmount);
                emit BonusTokensClaimed(msg.sender, stakes[i].bonus);
            }
        }

        claimedBonusTokens[msg.sender] += bonusAccumulated;
        totalBonusClaimed += bonusAccumulated;

        token.safeTransfer(msg.sender, bonusAccumulated);
    }

    function emergencyUnstake(uint256 index) external nonReentrant {
        require(stakingEnabled, "Staking is not enabled");
        
        Stake storage stake = stakingInfo[msg.sender][index];
        require(!stake.claimed, "Stake already claimed");
        
        stakedTokens[msg.sender] -= stake.amount;
        totalStaked -= stake.amount;
        claimedTokens[msg.sender] += stake.amount;
        totalClaimed += stake.amount;

        uint256 penalty = (stake.amount * emergencyUnstakePenalty) / 100; // Apply a 20% penalty fee
        uint256 unstakeAmount = stake.amount - penalty;
        stakingInfo[msg.sender][index].claimed = true;

        token.safeTransfer(msg.sender, unstakeAmount);
        emit EmergencyUnstake(msg.sender, index, unstakeAmount);
    }

    function getStakingInfo(address user) external view returns (Stake[] memory) {
        return stakingInfo[user];
    }

    function getStakeBonusProgress(address user, uint256 index) external view returns (uint256) {
        require(stakingEnabled, "Staking is not enabled");

        Stake storage stake = stakingInfo[user][index];
        if (block.timestamp >= stake.endTime) {
            return stake.bonus;
        }

        uint256 elapsedSeconds = block.timestamp - stake.startTime;
        uint256 stakingDuration = stake.endTime - stake.startTime;
        uint256 bonusPercentage = (elapsedSeconds * 100) / stakingDuration;

        return (stake.bonus * bonusPercentage) / 100;
    }

    function setMonthlyVestingPercentages(uint256[] memory newPercentages) external onlyOwner {
        monthlyVestingPercentages = newPercentages;
    }

    function setInitialVestingPercentage(uint256 newPercentage) external onlyOwner {
        initialVestingPercentage = newPercentage;
    }

    function setInitialVestingOffsetDays(uint256 newOffset) external onlyOwner {
        initialVestingOffsetDays = newOffset;
    }

    function setVestingDurations(uint256[] memory newDurations) external onlyOwner {
        vestingDurations = newDurations;
    }

    function calculateLockedTokens(address user) public view returns (uint256) {
        if (block.timestamp <= vestingStartTimestamp || vestingStartTimestamp == 0) return purchasedTokens[user];
        return purchasedTokens[user] - calculateUnlockedAmount(user);
    }

    function calculateUnlockedAmount(address user) public view returns (uint256) {
        if (block.timestamp <= vestingStartTimestamp || vestingStartTimestamp == 0) return 0;

        uint256 elapsedTime = block.timestamp - vestingStartTimestamp;
        uint256 vestedPercentage;

        if (elapsedTime >= initialVestingOffsetDays * (MONTH_IN_SECONDS / 30)) {
            vestedPercentage = initialVestingPercentage * 100;
        }

        if (elapsedTime >= MONTH_IN_SECONDS) {
            uint256 monthsPassed = elapsedTime / MONTH_IN_SECONDS;

            for (uint256 i = 0; i < monthlyVestingPercentages.length; i++) {
                if (monthsPassed > i) {
                    vestedPercentage += monthlyVestingPercentages[i];
                } else {
                    break;
                }
            }
        }

        return (purchasedTokens[user] * vestedPercentage) / 10000;
    }

    function calculateVestedAmount(address user) public view returns (uint256) {
        if (block.timestamp <= vestingStartTimestamp || vestingStartTimestamp == 0) return 0;

        uint256 unlockedAmount = calculateUnlockedAmount(user);

        // staked and already claimed tokens have to be deducted from the vested token amount as the tokens are not available
        unlockedAmount -= stakedTokens[user] + claimedTokens[user];

        return unlockedAmount;
    }

    function withdrawERC20(IERC20 _token, address to, uint256 amount) public onlyOwner {
        uint256 erc20balance = _token.balanceOf(address(this));
        require(amount <= erc20balance, "Balance is too low");
    
        // If amount is set to zero, the entire token balance is withdrawn
        if (amount == 0) {
            amount = erc20balance;
        }

        _token.transfer(to, amount);

        emit ERC20Withdrawn(address(_token), to, amount);
    }

    function withdraw() public onlyOwner {
        address payable receiver = payable(msg.sender);

        uint256 _balance = address(this).balance;
        require(_balance > 0, "Balance must be higher than zero");

        receiver.transfer(_balance);

        emit Withdrawn(msg.sender, _balance);
    }
}