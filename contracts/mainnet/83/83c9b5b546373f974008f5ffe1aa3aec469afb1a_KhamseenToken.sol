    address payable public owner;
    bool internal locked;

    event tokensMinted(uint256 _nbTokens);

    function decimals() public view override returns (uint8) {
        return 16;
    }

    // CONSTRUCTOR

    constructor() ERC20("Khamseen Token", "KHMS") ERC20Capped(khamseenCap) {
        /*
        why the 2 following lines:
        https://forum.openzeppelin.com/t/erc20capped-immutable-variables-cannot-be-read-during-contract-creation-time/6174/5
        */
        require(initialSupply <= khamseenCap);
        ERC20._mint(msg.sender, initialSupply);
        owner = payable(msg.sender);
    }

    // MODIFIERS

    modifier capNotReached {
        require(capReached == false);
        _;
    }

    modifier noReentrant {
        require(!locked, "No re-entrancy");
        locked = true;
        _;
        locked = false;
    }

    //MINTING

    function mintMinerReward() external capNotReached noReentrant {
        uint256 reward = computeMinerReward(msg.sender);
        _mint(msg.sender, reward);
        streaks[msg.sender] += 1;
        emit tokensMinted(reward);
    }

    function mintMinerRewardForAddress(address _address) external capNotReached noReentrant {
        uint256 reward = computeMinerReward(_address);
        _mint(_address, reward);
        streaks[_address] += 1;
        emit tokensMinted(reward);
    }

    function computeMinerReward(address _miner) internal returns (uint256) {
        uint256 reward = (10 ** (decimals()-((totalSupply()/10**(decimals()+7))+2))) * Math.max(1, ((balanceOf(_miner)/10**decimals())+streaks[_miner])/2);
        if (totalSupply() + reward > cap()) {
            reward = cap() - totalSupply();
            capReached = true;
        }
        return reward;
    }

    function getMyRewardEstimation() public view returns (uint256) {
        return Math.min(
            (10 ** (decimals()-((totalSupply()/10**(decimals()+7))+2))) * Math.max(1, ((balanceOf(msg.sender)/10**decimals())+streaks[msg.sender])/2),
            cap() - totalSupply()
        );
    }

   // Function to receive Ether. msg.data must be empty
    receive() external payable {
        revert();
    }

    // Fallback function is called when msg.data is not empty
    fallback() external payable {
        revert();
    }

}