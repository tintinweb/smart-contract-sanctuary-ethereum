        __ERC1155Supply_init();
        __EIP712_init(SIGNING_DOMAIN, SIGNATURE_VERSION);
        __Ownable_init();
    }

    /**
     * @dev Updates the address of the WETH contract used for depositing and
     * harvesting user WETH rewards.
     */
    function setWethSmartContractAddress(address newWethSmartContractAddress) external {
        require(
            newWethSmartContractAddress != address(0),
            'New WETH contract address must not equal 0x'
        );

        wethSmartContractAddress = newWethSmartContractAddress;
    }

    /**
     * @dev Stores a new WETH reward that is credited to all of the tokens.
     * This method is only callable by the contract owner.
     */
    function depositWethAirdropRewards(uint256 wethRewardPerToken) external onlyOwner {
        require(wethRewardPerToken > 0, 'Reward per token must be greater than 0');

        // solium-disable whitespace
        FeeReward memory feeReward = FeeReward({
            wethRewardPerToken: wethRewardPerToken,
            blockNumber: block.number
        });
        // solium-enable whitespace
        airdropFeeRewards.push(feeReward);
    }

    /**
     * @dev Unlocks all of the tokens that a user has available to unlock.
     */
    function unlockTokens(AirdropTokenRewardVoucher calldata voucher) external {
        // Voucher must be signed by the contract owner.
        address extractedOwnerAddress = _verify(voucher);
        require(extractedOwnerAddress == owner(), 'Voucher signer must be owner of smart contract');

        if (
            airdropUserInfo[voucher.userPublicAddress].sharesUserHasUnlocked <
            voucher.totalAirdropTokensReceived
        ) {
            // Unlock new tokens and add the rewards the user is eligible to
            // receive.
            uint256 tokensToUnlock = voucher.totalAirdropTokensReceived -
                airdropUserInfo[voucher.userPublicAddress].sharesUserHasUnlocked;
            _mint(
                voucher.userPublicAddress,
                AIRDROP_TOKEN,
                tokensToUnlock,
                /* data= */
                ''
            );
            airdropUserInfo[voucher.userPublicAddress].sharesUserHasUnlocked += tokensToUnlock;
            uint256 wethRewardPerToken = 0;
            for (uint256 i = 0; i < airdropFeeRewards.length; i++) {
                if (airdropFeeRewards[i].blockNumber < block.number) {
                    wethRewardPerToken += airdropFeeRewards[i].wethRewardPerToken;
                }
            }
            // solium-disable operator-whitespace
            airdropUserInfo[voucher.userPublicAddress].totalWethRewards +=
                wethRewardPerToken *
                tokensToUnlock;
            // solium-enable operator-whitespace
        }
    }

    /**
     * @dev Withdraws all of a user's WETH rewards to their wallet.
     */
    function harvestAirdropRewards() external {
        refreshUserAirdropRewards(msg.sender);
        if (airdropUserInfo[msg.sender].totalWethRewards > 0) {
            require(
                wethSmartContractAddress != address(0),
                'WETH contract address must not equal 0x'
            );
            require(
                IWETH9(payable(wethSmartContractAddress)).balanceOf(address(this)) >=
                    airdropUserInfo[msg.sender].totalWethRewards,
                'Insufficient WETH balance to harvest user rewards'
            );

            uint256 wethToWithdraw = airdropUserInfo[msg.sender].totalWethRewards;
            airdropUserInfo[msg.sender].totalWethRewards = 0;

            IWETH9(payable(wethSmartContractAddress)).transferFrom(
                address(this),
                msg.sender,
                wethToWithdraw
            );
        }
    }

    /**
     * @dev Returns the number of airdrop tokens that a user has unlocked.
