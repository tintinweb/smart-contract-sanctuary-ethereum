        uint256 rewards = airdropUserInfo[user].totalWethRewards;
        rewards += getPendingUserWethRewardsForUnlockedTokens(user);

        uint256 lockedTokens = numberOfAirdropTokensReceived -
            airdropUserInfo[user].sharesUserHasUnlocked;
        if (lockedTokens > 0) {
            uint256 wethRewardPerToken = 0;
            for (uint256 i = 0; i < airdropFeeRewards.length; i++) {
                if (airdropFeeRewards[i].blockNumber < block.number) {
                    wethRewardPerToken += airdropFeeRewards[i].wethRewardPerToken;
