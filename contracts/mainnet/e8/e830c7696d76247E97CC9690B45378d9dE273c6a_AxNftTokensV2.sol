    string private constant SIGNING_DOMAIN = 'AX NFT Tokens';
    string private constant SIGNATURE_VERSION = '2';
    uint256 private constant AIRDROP_TOKEN = 0;

    address public wethSmartContractAddress;
    FeeReward[] airdropFeeRewards;
    mapping(address => UserInfo) airdropUserInfo;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

