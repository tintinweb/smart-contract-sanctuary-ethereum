            try IERC721Receiver(to).onERC721Received(_msgSender(), from, tokenId, _data) returns (bytes4 retval) {
                return retval == IERC721Receiver(to).onERC721Received.selector;
            } catch (bytes memory reason) {
                if (reason.length == 0) {
                    revert TransferToNonERC721ReceiverImplementer();
                } else {
                    assembly {
                        revert(add(32, reason), mload(reason))
                    }
                }
            }
        } else {
            return true;
        }
    }

    /**
     * @dev Hook that is called before a set of serially-ordered token ids are about to be transferred. This includes minting.
     * And also called before burning one token.
     *
     * startTokenId - the first token id to be transferred
     * quantity - the amount to be transferred
     *
     * Calling conditions:
     *
     * - When `from` and `to` are both non-zero, `from`'s `tokenId` will be
     * transferred to `to`.
     * - When `from` is zero, `tokenId` will be minted for `to`.
     * - When `to` is zero, `tokenId` will be burned by `from`.
     * - `from` and `to` are never both zero.
     */
    function _beforeTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity
    ) internal virtual {}

    /**
     * @dev Hook that is called after a set of serially-ordered token ids have been transferred. This includes
     * minting.
     * And also called after one token has been burned.
     *
     * startTokenId - the first token id to be transferred
     * quantity - the amount to be transferred
     *
     * Calling conditions:
     *
     * - When `from` and `to` are both non-zero, `from`'s `tokenId` has been
     * transferred to `to`.
     * - When `from` is zero, `tokenId` has been minted for `to`.
     * - When `to` is zero, `tokenId` has been burned by `from`.
     * - `from` and `to` are never both zero.
     */
    function _afterTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity
    ) internal virtual {}
}

contract BoredMemesNFT is ERC721A, Ownable {
    using Strings for uint256;
    using Address for address payable;
    
    string private BASE_URI;
    
    uint256 public SALE_STEP = 0; // 0 => NONE, 1 => START

    uint256 public SILVER_PACK = 1;
    uint256 public GOLD_PACK = 3;
    uint256 public PREMIUM_PACK = 5;

    uint256 public SILVER_PACK_PRICE = 0.016 ether;
    uint256 public GOLD_PACK_PRICE = 0.028 ether;
    uint256 public PREMIUM_PACK_PRICE = 0.053 ether;

    address public DISTRIBUTE_ADDRESS1 = 0x6b390BECbdB3043c8c3834C30ce8fe9b62bE9487;
    address public DISTRIBUTE_ADDRESS2 = 0xf68343018471A942381d2ede39616a0b0FaB9a75;
    address public STAKING_ADDRESS = 0xE0C1aB853b3a3c21Cc3C50BA6494c7AfCC8eCea7;

    uint256 public DISTRIBUTE_PERCENT1 = 50;
    uint256 public DISTRIBUTE_PERCENT2 = 25;
    uint256 public STAKING_PERCENT = 25;

    mapping(uint256=>uint256) public MAP_REQUEST;
    

    constructor() ERC721A("Bored Memes", "BoredM") {}

    function setBaseURI(string memory _newBaseURI) external onlyOwner {
        BASE_URI = _newBaseURI;
    }

    function setSaleStep(uint256 _saleStep) external onlyOwner {
        SALE_STEP = _saleStep;
    }

    function setPacks(uint256 _silverPack, uint256 _goldPack, uint256 _premiumPack) external onlyOwner {
        SILVER_PACK = _silverPack;
        GOLD_PACK = _goldPack;
        PREMIUM_PACK = _premiumPack;
    }

    function setDistributeAddress(address _disAddr1, address _disAddr2, address _stakingAddr) external onlyOwner {
        DISTRIBUTE_ADDRESS1 = _disAddr1;
        DISTRIBUTE_ADDRESS2 = _disAddr2;
        STAKING_ADDRESS = _stakingAddr;
    }

    function setDistributePercent(uint256 _disPercent1, uint256 _disPercent2, uint256 _stakingPercent) external onlyOwner {
        DISTRIBUTE_PERCENT1 = _disPercent1;
        DISTRIBUTE_PERCENT2 = _disPercent2;
        STAKING_PERCENT = _stakingPercent;
    }
    
    function numberMinted(address _owner) public view returns (uint256) {
        return _numberMinted(_owner);
    }

    function mintSilverPack(uint256 _requestId) external payable {
        require(SALE_STEP == 1, "Public Sale is not opened");

        require(msg.value >= SILVER_PACK_PRICE, "ETH amount is not sufficient");

        for (uint256 i = _currentIndex; i < _currentIndex + SILVER_PACK; i++) {
            MAP_REQUEST[i] = _requestId;
        }
        payable(DISTRIBUTE_ADDRESS1).transfer(msg.value * DISTRIBUTE_PERCENT1 / 100);
        payable(DISTRIBUTE_ADDRESS2).transfer(msg.value * DISTRIBUTE_PERCENT2 / 100);
        payable(STAKING_ADDRESS).sendValue(msg.value * STAKING_PERCENT / 100);
        _mintLoop(msg.sender, SILVER_PACK);
    }

    function mintGoldPack(uint256 _requestId) external payable {
        require(SALE_STEP == 1, "Public Sale is not opened");

        require(msg.value >= GOLD_PACK_PRICE, "ETH amount is not sufficient");

        for (uint256 i = _currentIndex; i < _currentIndex + GOLD_PACK; i++) {
            MAP_REQUEST[i] = _requestId;
        }
        payable(DISTRIBUTE_ADDRESS1).transfer(msg.value * DISTRIBUTE_PERCENT1 / 100);
        payable(DISTRIBUTE_ADDRESS2).transfer(msg.value * DISTRIBUTE_PERCENT2 / 100);
        payable(STAKING_ADDRESS).sendValue(msg.value * STAKING_PERCENT / 100);
        _mintLoop(msg.sender, GOLD_PACK);
    }

    function mintPremiumPack(uint256 _requestId) external payable {
        require(SALE_STEP == 1, "Public Sale is not opened");

        require(msg.value >= PREMIUM_PACK_PRICE, "ETH amount is not sufficient");

        for (uint256 i = _currentIndex; i < _currentIndex + PREMIUM_PACK; i++) {
            MAP_REQUEST[i] = _requestId;
        }
        payable(DISTRIBUTE_ADDRESS1).transfer(msg.value * DISTRIBUTE_PERCENT1 / 100);
        payable(DISTRIBUTE_ADDRESS2).transfer(msg.value * DISTRIBUTE_PERCENT2 / 100);
        payable(STAKING_ADDRESS).sendValue(msg.value * STAKING_PERCENT / 100);
        _mintLoop(msg.sender, PREMIUM_PACK);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return BASE_URI;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0 ? string(abi.encodePacked(currentBaseURI, tokenId.toString())) : "";
    }


    function _mintLoop(address _receiver, uint256 _mintAmount) internal {
        _safeMint(_receiver, _mintAmount);
    }

    function getOwnershipData(uint256 tokenId) external view returns (TokenOwnership memory) {
        return ownershipOf(tokenId);
    }

    function withdraw() external onlyOwner {
        uint256 curBalance = address(this).balance;
        payable(msg.sender).transfer(curBalance);
    }
}