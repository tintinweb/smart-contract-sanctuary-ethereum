abstract contract MouheiKolSeller is OwnerPausable, ReentrancyGuard {
    using Address for address payable;
    using Monotonic for Monotonic.Increaser;
    using Strings for uint256;

    struct SellerConfig {
        uint256 totalInventory;
        bool lockTotalInventory;
    }

    constructor(SellerConfig memory config, address payable _beneficiary) {
        setSellerConfig(config);
        setBeneficiary(_beneficiary);
    }

    SellerConfig public sellerConfig;

    function setSellerConfig(SellerConfig memory config) public onlyOwner {
        require(
            config.totalInventory >= _totalSold.current(),
            "Seller: inventory < already sold"
        );

        if (sellerConfig.lockTotalInventory) {
            config.lockTotalInventory = true;
            config.totalInventory = sellerConfig.totalInventory;
        }

        sellerConfig = config;
    }

    address payable public beneficiary;

    function setBeneficiary(address payable _beneficiary) public onlyOwner {
        beneficiary = _beneficiary;
    }

    function _handlePurchase(
        address to,
        uint256 tokenId,
        uint256 cardLevel
    ) internal virtual;

    Monotonic.Increaser private _totalSold;

    function totalSold() public view returns (uint256) {
        return _totalSold.current();
    }

    event Refund(address indexed buyer, uint256 amount);

    event Revenue(address indexed beneficiary, uint256 tokenId, uint256 amount);

    function _purchase(
        address to,
        uint256 costMetadata,
        uint256 tokenId,
        uint256 cardLevel
    ) internal nonReentrant whenNotPaused {
        SellerConfig memory config = sellerConfig;

        uint256 n = 1;

        uint256 maxAvailable;
        uint256 sold;

        maxAvailable = config.totalInventory;
        sold = _totalSold.current();

        n = Math.min(n, maxAvailable - sold);
        require(n > 0, "Seller: Sold out");

        uint256 _cost = costMetadata;
        if (msg.value < _cost) {
            revert(
                string(
                    abi.encodePacked(
                        "Seller: Costs ",
                        (_cost / 1e9).toString(),
                        " GWei"
                    )
                )
            );
        }

        _handlePurchase(to, tokenId, cardLevel);
        _totalSold.add(n);
        assert(_totalSold.current() <= config.totalInventory);

        if (_cost > 0) {
            uint256 revenuePartTwoEst = _cost / 2;
            uint256 revenuePartTwoReal = 0;

            uint256 k1c = kol1Users.length();

            if (cardLevel == 1 && k1c != 0) {
                for (uint256 i = 0; i < k1c; i++) {
                    address payable k1a = payable(kol1Users.at(i));
                    k1a.sendValue(revenuePartTwoEst / k1c);
                    emit Revenue(
                        kol1Users.at(i),
                        tokenId,
                        revenuePartTwoEst / k1c
                    );
                }
                revenuePartTwoReal = (revenuePartTwoEst / k1c) * k1c;
            }

            uint256 k2c = kol2Users.length();

            if (cardLevel == 2 && ((k1c + k2c) != 0)) {
                for (uint256 i = 0; i < k1c; i++) {
                    address payable k1a = payable(kol1Users.at(i));
                    k1a.sendValue((revenuePartTwoEst / (k1c + k2c)));
                    emit Revenue(
                        kol1Users.at(i),
                        tokenId,
                        (revenuePartTwoEst / (k1c + k2c))
                    );
                }

                for (uint256 j = 0; j < k2c; j++) {
                    address payable k2a = payable(kol2Users.at(j));
                    k2a.sendValue((revenuePartTwoEst / (k1c + k2c)));
                    emit Revenue(
                        kol2Users.at(j),
                        tokenId,
                        (revenuePartTwoEst / (k1c + k2c))
                    );
                }

                revenuePartTwoReal =
                    (revenuePartTwoEst / (k1c + k2c)) *
                    (k1c + k2c);
            }

            uint256 k3c = kol3Users.length();
            if (cardLevel == 3 && ((k1c + k2c + k3c) != 0)) {
                for (uint256 i = 0; i < k1c; i++) {
                    address payable k1a = payable(kol1Users.at(i));
                    k1a.sendValue((revenuePartTwoEst / (k1c + k2c + k3c)));
                    emit Revenue(
                        kol1Users.at(i),
                        tokenId,
                        (revenuePartTwoEst / (k1c + k2c + k3c))
                    );
                }

                for (uint256 j = 0; j < k2c; j++) {
                    address payable k2a = payable(kol2Users.at(j));
                    k2a.sendValue((revenuePartTwoEst / (k1c + k2c + k3c)));
                    emit Revenue(
                        kol2Users.at(j),
                        tokenId,
                        (revenuePartTwoEst / (k1c + k2c + k3c))
                    );
                }

                for (uint256 k = 0; k < k3c; k++) {
                    address payable k3a = payable(kol3Users.at(k));
                    k3a.sendValue((revenuePartTwoEst / (k1c + k2c + k3c)));
                    emit Revenue(
                        kol3Users.at(k),
                        tokenId,
                        (revenuePartTwoEst / (k1c + k2c + k3c))
                    );
                }

                revenuePartTwoReal =
                    (revenuePartTwoEst / (k1c + k2c + k3c)) *
                    (k1c + k2c + k3c);
            }

            uint256 revenuePartOne = _cost - revenuePartTwoReal;

            beneficiary.sendValue(revenuePartOne);
            emit Revenue(beneficiary, tokenId, revenuePartOne);
        }

        if (msg.value > _cost) {
            address payable reimburse = payable(_msgSender());
            uint256 refund = msg.value - _cost;

            (bool success, bytes memory returnData) = reimburse.call{
                value: refund
            }("");

            require(success, string(returnData));

            emit Refund(reimburse, refund);
        }
    }

    using EnumerableSet for EnumerableSet.AddressSet;

    EnumerableSet.AddressSet internal kol1Users;
    EnumerableSet.AddressSet internal kol2Users;
    EnumerableSet.AddressSet internal kol3Users;

    function addKol1User(address userAddress) external onlyOwner {
        kol1Users.add(userAddress);
    }

    function removeKol1User(address userAddress) external onlyOwner {
        kol1Users.remove(userAddress);
    }

    function addKol2User(address userAddress) external onlyOwner {
        kol2Users.add(userAddress);
    }

    function removeKol2User(address userAddress) external onlyOwner {
        kol2Users.remove(userAddress);
    }

    function addKol3User(address userAddress) external onlyOwner {
        kol3Users.add(userAddress);
    }

    function removeKol3User(address userAddress) external onlyOwner {
        kol3Users.remove(userAddress);
    }
}

