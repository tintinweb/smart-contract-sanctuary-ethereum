contract MouheiConnoisseurPass is
    ERC721Common,
    BaseTokenURI,
    MouheiKolSeller,
    SignerManager
{
    using EnumerableSet for EnumerableSet.AddressSet;
    using Monotonic for Monotonic.Increaser;
    using SignatureChecker for EnumerableSet.AddressSet;

    constructor(address payable beneficiary)
        ERC721Common("MouheiConnoisseurPass", "MCP")
        BaseTokenURI("")
        MouheiKolSeller(
            MouheiKolSeller.SellerConfig({
                totalInventory: 1000,
                lockTotalInventory: false
            }),
            beneficiary
        )
    {}

    mapping(uint256 => uint256) private _cardLevels;

    function cardLevelOf(uint256 tokenId)
        public
        view
        virtual
        returns (uint256)
    {
        uint256 cardLevel = _cardLevels[tokenId];
        return cardLevel;
    }

    function _handlePurchase(
        address to,
        uint256 tokenId,
        uint256 cardLevel
    ) internal override {
        _safeMint(to, tokenId, "");
        _cardLevels[tokenId] = cardLevel;
    }

    mapping(bytes32 => bool) public usedMessages;

    function mint(
        address to,
        uint256 price,
        uint256 tokenId,
        uint256 cardLevel,
        bytes calldata sig
    ) external payable {
        signers.requireValidSignature(
            signaturePayload(to, price, tokenId, cardLevel),
            sig,
            usedMessages
        );
        _purchase(to, price, tokenId, cardLevel);
    }

    function alreadyMinted(
        address to,
        uint256 price,
        uint256 tokenId,
        uint256 cardLevel
    ) external view returns (bool) {
        return
            usedMessages[
                SignatureChecker.generateMessage(
                    signaturePayload(to, price, tokenId, cardLevel)
                )
            ];
    }

    function signaturePayload(
        address to,
        uint256 price,
        uint256 tokenId,
        uint256 cardLevel
    ) internal pure returns (bytes memory) {
        return abi.encodePacked(to, price, tokenId, cardLevel);
    }

    function _baseURI()
        internal
        view
        override(BaseTokenURI, ERC721)
        returns (string memory)
    {
        return BaseTokenURI._baseURI();
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Common)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}