            return a % b;
        }
    }
}


 interface IERC20_DECIMALS {
  function decimals() external view returns (uint256);
}


contract CrazyDogeIDO is Ownable {

  using SafeMath for uint256;
  using SafeERC20 for IERC20;

  uint256 public pointer;

  mapping(uint256 => mapping(address => bool)) public isWhiteList;
  mapping(uint256 => bool) public onlyWhiteList;

  struct ProjectIDO {
    uint256 startTime;
    uint256 endTime;
    uint256 personalMaxAmount;
    uint256 personalMinAmount;
    uint256 userMaxAmount;
    uint256 userMinAmount;
    uint256 price;
    uint256 stakeAmount;
    bool end;
  }

  struct UserInfo{
    uint256 amount;
    bool isDraw;
    uint256 fallbackAmount;
    uint256 receivedAmount;
  }

  address public teamAddress = 0xd36dfC5Af8BEf5013C9A3B0bE40F5b75bFe7a962;
  address public token;
  uint256 public tokenUint;

  uint256 public ethUint = 10 ** 18;

  mapping(uint256 => ProjectIDO) public projectIDO;
  mapping(address => mapping(uint256 => UserInfo)) public userInfo;

  mapping(uint256 => mapping(address => uint256)) public poolIndex;
  mapping(uint256 => address[]) public poolPosition;
  mapping(uint256 => uint256) public endTimeList;


  event Subscribe(address indexed recipient, uint256 amount);
  event Draw(address indexed recipient);

  constructor(address _token) {
    projectIDO[pointer] = ProjectIDO({
              startTime : 0,
              endTime : 0,
              personalMaxAmount : ethUint * 50,
              personalMinAmount : ethUint * 50,
              userMaxAmount : ethUint * 2,
              userMinAmount : ethUint / 1000,
              price : 4000000000000 * ethUint,
              stakeAmount : 0,
              end : false
    });
    token = _token;
    tokenUint = _token == address(0) ? 10 ** 18 : 10 ** IERC20_DECIMALS(_token).decimals();
  }

  function subscribe(uint _pointer) payable  public {
    uint256 _amount = msg.value;
    require(_pointer <= pointer, 'pointer overflow');
    UserInfo storage user = userInfo[msg.sender][_pointer];
    ProjectIDO storage ido = projectIDO[_pointer];
    require(ido.startTime != 0 && ido.endTime != 0, "close");
    require(block.timestamp >= ido.startTime, "not started");
    require(block.timestamp < ido.endTime && !ido.end, "ended");
    require(user.amount.add(_amount) >= ido.userMinAmount && user.amount.add(_amount) <= ido.userMaxAmount, 'The input value is not within the limit');

    if(onlyWhiteList[_pointer]){
      require(isWhiteList[_pointer][msg.sender], 'not a whitelist');
      if(ido.stakeAmount.add(user.amount.add(_amount)) == ido.personalMaxAmount){ ido.end = true; }
    }

    if(ido.stakeAmount < ido.personalMaxAmount){
      uint256 _ethAmount = _amount;
      if(ido.stakeAmount.add(_amount) > ido.personalMaxAmount){  _ethAmount = ido.personalMaxAmount.sub(ido.stakeAmount);  }
      payable(teamAddress).transfer( _ethAmount);
    }

    if(user.amount == 0) {
      poolPosition[_pointer].push(msg.sender);
      poolIndex[_pointer][msg.sender] = poolPosition[_pointer].length;
    }
    user.amount = user.amount.add(_amount);
    ido.stakeAmount = ido.stakeAmount.add(_amount);
    emit Subscribe(msg.sender, _amount);

  }

  function getPending(address account, uint _pointer) public view returns(uint256, uint256, uint256){
    require(_pointer <= pointer, 'pointer overflow');
    ProjectIDO memory ido = projectIDO[_pointer];
    UserInfo memory user = userInfo[account][_pointer];
    if(ido.stakeAmount > ido.personalMaxAmount){
      uint256 fallbackAmount = ido.stakeAmount.sub(ido.personalMaxAmount).mul(user.amount).div(ido.stakeAmount);
      uint256 buyAmount = user.amount.sub(fallbackAmount).sub(1);
      uint256 amount = buyAmount.mul(ido.personalMaxAmount.mul(ido.price)).div(ido.personalMaxAmount);
      return (fallbackAmount, buyAmount, amount.mul(tokenUint).div(ethUint).div(1e18));
    }
    return (0, user.amount, user.amount.mul(ido.price).mul(tokenUint).div(ethUint).div(1e18));
  }

  function draw(uint _pointer) public {
    require(_pointer <= pointer, 'pointer overflow');
    UserInfo storage user = userInfo[msg.sender][_pointer];
    ProjectIDO storage ido = projectIDO[_pointer];
    // require(ido.startTime != 0 && ido.endTime != 0, "close");
    require((ido.endTime != 0 && ido.endTime <= block.timestamp) || ido.end, 'Not open for collection');
    require(!user.isDraw && user.amount > 0, 'is draw');

    if(!ido.end){ ido.end = true; }
    user.isDraw = true;

    (uint256 fallbackAmount,  , uint256 presaleAmount) = getPending(msg.sender, _pointer);
    user.fallbackAmount = fallbackAmount;
    user.receivedAmount = presaleAmount;
    if( fallbackAmount != 0){
      payable(msg.sender).transfer( fallbackAmount);
    }
    IERC20(token).transfer(msg.sender , presaleAmount);
    emit Draw(msg.sender);
  }

  function getTokenInfo() public view returns(uint256, uint256){
    if(token == address(0)){ return (18 , 0); }
    return (IERC20_DECIMALS(token).decimals(), IERC20(token).balanceOf(address(this)));
  }

  function getPoolPosition(uint _pointer) public view returns(uint256) {
    return poolPosition[_pointer].length;
  }

  function addIDO(ProjectIDO memory ido) public onlyOwner{
    pointer = pointer.add(1);
    projectIDO[pointer] = ProjectIDO({
            startTime : ido.startTime,
            endTime : ido.endTime,
            personalMaxAmount : ido.personalMaxAmount,
            personalMinAmount : ido.personalMinAmount,
            userMaxAmount : ido.userMaxAmount,
            userMinAmount : ido.userMinAmount,
            price : ido.price,
            stakeAmount : ido.stakeAmount,
            end : ido.end
    });
  }

  function addIDOWhite(uint _pointer, bool isWhite) public onlyOwner{
    require(_pointer <= pointer, 'pointer overflow');
    onlyWhiteList[_pointer] = isWhite;
  }

  function setPresaleToken(address _token) public onlyOwner{
    require(_token != address(0), "zero_address");
    tokenUint = 10 ** IERC20_DECIMALS(_token).decimals();
    token = _token;
  }

  function withdraw() external onlyOwner {
    uint256 amount = IERC20(token).balanceOf(address(this));
    IERC20(token).transfer(teamAddress, amount);
  }

  function withdrawETH() external onlyOwner {
    uint256 amount = address(this).balance;
    payable(teamAddress).transfer(amount);
  }

  function setTeamAddress(address _teamAddress) public onlyOwner{
    require(_teamAddress != address(0), "zero_address");
    teamAddress = _teamAddress;
  }

  function setWhiteList(uint _pointer, address [] memory _whiteList, bool isWhite) public  onlyOwner{
    for(uint i = 0; i < _whiteList.length; i++){
      isWhiteList[_pointer][_whiteList[i]] = isWhite;
    }
  }

  function setWhite(uint _pointer, address _white, bool isWhite) public  onlyOwner{
      isWhiteList[_pointer][_white] = isWhite;
  }

  function setOneStart() public onlyOwner{
    _start(0);
  }

  function setPointerStart(uint _pointer) public onlyOwner{
    _start(_pointer);
  }

  function setOneClose() public onlyOwner{
    _close(0);
  }

  function setPointerClose(uint _pointer) public onlyOwner{
    _close(_pointer);
  }

  function setOneEnd() public onlyOwner {
    _end(0) ;
  }

  function setPointerEnd(uint _pointer) public onlyOwner{
    _end(_pointer);
  }

  function setPointerTime(uint _pointer, uint256 startTime, uint256 endTime) public onlyOwner{
    _time(_pointer, startTime, endTime);
  }

  function setOneTime( uint256 startTime, uint256 endTime) public onlyOwner{
    _time(0, startTime, endTime);
  }

  function _time(uint _pointer, uint256 startTime, uint256 endTime) internal{
    require(_pointer <= pointer, 'pointer overflow');
    ProjectIDO storage ido = projectIDO[_pointer];
    require(startTime < endTime);
    require(!ido.end, "is End");
    ido.startTime = startTime == 0 ? block.timestamp : startTime;
    ido.endTime = endTime;
  }

  function _start(uint _pointer) internal{
    require(_pointer <= pointer, 'pointer overflow');
    ProjectIDO storage ido = projectIDO[_pointer];
    require(!ido.end && ido.endTime == 0 && ido.startTime == 0, "is End");
    ido.startTime = block.timestamp;
    ido.endTime =  endTimeList[_pointer] != 0 ? endTimeList[_pointer] : (block.timestamp + 31536000);
  }

  function _close(uint _pointer) internal{
    require(_pointer <= pointer, 'pointer overflow');
    ProjectIDO storage ido = projectIDO[_pointer];
    require(!ido.end && ido.endTime != 0 && ido.startTime != 0, "is End");
    endTimeList[_pointer] = ido.endTime;
    ido.startTime = 0;
    ido.endTime = 0;
  }

  function _end(uint _pointer) internal{
    ProjectIDO storage ido = projectIDO[_pointer];
    require(!ido.end && ido.endTime != 0 && ido.startTime != 0, "is End");
    require(address(token) != address(0) , "token is address(0)");
    ido.end = true;
  }


}