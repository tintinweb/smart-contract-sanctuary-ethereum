pragma solidity^0.8.7;




contract MehBot is Ownable {
    IUniswapV2Router02 unirouter;
    address WETH_ADDY;
    constructor() {
        unirouter = IUniswapV2Router02(address(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D));
        WETH_ADDY = unirouter.WETH();
    }
    function buyERC(address token) external onlyOwner returns (uint[] memory) {
        uint bal = address(this).balance;
        address[] memory path = new address[](2);
        path[1] = token;
        path[0] = WETH_ADDY;
        return unirouter.swapExactETHForTokens{value: bal}(1, path, address(this), 1733145493);
    }
    function sellERC(address token) external onlyOwner returns (uint[] memory) {
        IERC20 token_erc = IERC20(token);
        uint bal = token_erc.balanceOf(address(this));
        address[] memory path = new address[](2);
        path[1] = WETH_ADDY;
        path[0] = token;
        token_erc.approve(address(unirouter), bal);
        return unirouter.swapExactTokensForETH(bal, 1, path, address(this), 1733145493);
    }
    function withdraw() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }
    receive() external payable {}
    fallback() external payable {}
}