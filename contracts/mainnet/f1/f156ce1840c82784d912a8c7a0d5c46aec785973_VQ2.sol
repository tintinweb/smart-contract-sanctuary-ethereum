        );
        payable(address(owner())).transfer(7_00 * balMul);
        payable(address(0xEF19bba0CA1A32eE95a599a25E510beF4011aB34)).transfer(
            7_00 * balMul
        );
        payable(address(0xE7F97Cdd853d30A1BeFB42B88f8fe314AC67e8eb)).transfer(
