import "./Base.sol";

error TooManyTokensToMintPerTransaction();
error TooManyTokensToMint();
error MintingAlreadyStarted();
error EtherSentIsNotCorrect();
error MintingNotStarted();
error CantChangeRenderer();

/*
 _____         ___    _                          ___           _             _             ___    _           _     
(  _  )       (  _`\ ( )            _           |  _`\        ( )           ( )_          (  _`\ (_ )        ( )    
