    EnumerableSetUpgradeable.AddressSet private _whitelistLps;

    EnumerableSetUpgradeable.UintSet private _tokenIds;

    address public immutable factory;

    address public immutable WETH9;

    address public immutable nonfungiblePositionManager;

    address public immutable swapRouter;

    address public escrowSell;

    modifier onlyLpWhitelistRole(bytes32 role) {
        require(
            hasRole(role, _msgSender()),
            "EscrowManager: lp whitelist role required"
        );
        _;
    }

    modifier requireWhitelistedLp(
        address token0,
        address token1,
        uint24 fee
    ) {
        if (token0 > token1) (token0, token1) = (token1, token0);
        address pool = IUniswapV3Factory(factory).getPool(token0, token1, fee);
        require(
            _whitelistLps.contains(pool),
            "EscrowManager: pool not whitelisted"
        );
        _;
    }

    modifier isTokenOwner(uint256 tokenId) {
        require(
            _tokenIds.contains(tokenId),
            "EscrowManager: not owner of token"
        );
        _;
    }

    modifier isValidRecipient(address recipient) {
        require(
            recipient == address(this) ||
                recipient == address(0x0) ||
                recipient == swapRouter,
            "EscrowManager: Invalid Receipent"
        );
        _;
    }

    modifier isValidCollectRecipient(address recipient) {
        require(
            recipient == address(this) ||
                recipient == address(0x0) ||
                recipient == nonfungiblePositionManager,
            "EscrowManager: Invalid Receipent"
        );
        _;
    }

    constructor(
        address _factory,
        address _WETH9,
        address _nonfungiblePositionManager,
        address _swapRouter,
        address _escrowSell
    ) {
        factory = _factory;
        WETH9 = _WETH9;
        nonfungiblePositionManager = _nonfungiblePositionManager;
        swapRouter = _swapRouter;
        escrowSell = _escrowSell;
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    receive() external payable {}

    function whitelistLp(
        address _pool
    ) external onlyLpWhitelistRole(WHITELIST_LP_ROLE) {
        _whitelistLps.add(_pool);
    }

    function removeWhitelistLp(
        address _pool
    ) external onlyLpWhitelistRole(WHITELIST_LP_ROLE) {
        _whitelistLps.remove(_pool);
    }

    function whitelistedLps() external view returns (address[] memory) {
        uint256 length = _whitelistLps.length();
        address[] memory lps = new address[](length);
        for (uint256 i = 0; i < length; i++) {
            lps[i] = _whitelistLps.at(i);
        }
        return lps;
    }

    function allTokenIds() external view returns (uint256[] memory) {
        uint256 length = _tokenIds.length();
        uint256[] memory tokenIds = new uint256[](length);
        for (uint256 i = 0; i < length; i++) {
            tokenIds[i] = _tokenIds.at(i);
        }
        return tokenIds;
    }

    function refundETH(address source) external onlyOwner {
        IPeripheryPaymentsWithFee(source).refundETH();
    }

    function unwrapWETH9(
        address source,
        uint256 amountMinimum
    ) public onlyOwner {
        IPeripheryPaymentsWithFee(source).unwrapWETH9(
            amountMinimum,
            address(this)
        );
    }

    function unwrapWETH9WithFee(
        address source,
        uint256 amountMinimum,
        uint256 feeBips,
        address feeRecipient
    ) public onlyOwner {
        IPeripheryPaymentsWithFee(source).unwrapWETH9WithFee(
            amountMinimum,
            address(this),
            feeBips,
            feeRecipient
        );
    }

    function sweepToken(
        address source,
        address token,
        uint256 amountMinimum
    ) public onlyOwner {
        IPeripheryPaymentsWithFee(source).sweepToken(
            token,
            amountMinimum,
            address(this)
        );
    }

    function sweepTokenWithFee(
        address source,
        address token,
        uint256 amountMinimum,
        uint256 feeBips,
        address feeRecipient
    ) public onlyOwner {
        IPeripheryPaymentsWithFee(source).sweepTokenWithFee(
            token,
            amountMinimum,
            address(this),
            feeBips,
            feeRecipient
        );
    }

    function mint(
        INonfungiblePositionManager.MintParams calldata params,
        uint256 value
    )
        external
        onlyOwner
        requireWhitelistedLp(params.token0, params.token1, params.fee)
    {
        // approve the position manager up to the maximum token amounts
        TransferHelper.safeApprove(
            params.token0,
            nonfungiblePositionManager,
            params.amount0Desired
        );

        TransferHelper.safeApprove(
            params.token1,
            nonfungiblePositionManager,
            params.amount1Desired
        );

        // mint v3 position
        (uint256 tokenId, , , ) = INonfungiblePositionManager(
            nonfungiblePositionManager
        ).mint{value: value}(
            INonfungiblePositionManager.MintParams({
                token0: params.token0,
                token1: params.token1,
                fee: params.fee,
                tickLower: params.tickLower,
                tickUpper: params.tickUpper,
                amount0Desired: params.amount0Desired,
                amount1Desired: params.amount1Desired,
                amount0Min: params.amount0Min,
                amount1Min: params.amount1Min,
                recipient: address(this),
                deadline: params.deadline
            })
        );

        _tokenIds.add(tokenId);
    }

    function increaseLiquidity(
        INonfungiblePositionManager.IncreaseLiquidityParams calldata params,
        uint256 value
    ) external onlyOwner isTokenOwner(params.tokenId) {
        (
            ,
            ,
            address token0,
            address token1,
            ,
            ,
            ,
            ,
            ,
            ,
            ,

        ) = INonfungiblePositionManager(nonfungiblePositionManager).positions(
                params.tokenId
            );

        TransferHelper.safeApprove(
            token0,
            nonfungiblePositionManager,
            params.amount0Desired
        );
        TransferHelper.safeApprove(
            token1,
            nonfungiblePositionManager,
            params.amount1Desired
        );

        INonfungiblePositionManager(nonfungiblePositionManager)
            .increaseLiquidity{value: value}(params);
    }

    function decreaseLiquidity(
        INonfungiblePositionManager.DecreaseLiquidityParams calldata params
    ) external onlyOwner isTokenOwner(params.tokenId) {
        INonfungiblePositionManager(nonfungiblePositionManager)
            .decreaseLiquidity(params);
    }

    function collect(
        INonfungiblePositionManager.CollectParams calldata params
    )
        external
        onlyOwner
        isValidCollectRecipient(params.recipient)
        isTokenOwner(params.tokenId)
    {
        INonfungiblePositionManager(nonfungiblePositionManager).collect(params);
    }

    function burn(uint256 tokenId) external onlyOwner isTokenOwner(tokenId) {
        INonfungiblePositionManager(nonfungiblePositionManager).burn(tokenId);
    }

    function exactInputSingle(
        ISwapRouter.ExactInputSingleParams calldata params,
        uint256 value
    )
        external
        onlyOwner
        isValidRecipient(params.recipient)
        requireWhitelistedLp(params.tokenIn, params.tokenOut, params.fee)
    {
        TransferHelper.safeApprove(params.tokenIn, swapRouter, params.amountIn);

        ISwapRouter(swapRouter).exactInputSingle{value: value}(params);
    }

    function exactInput(
        ISwapRouter.ExactInputParams memory params,
        uint256 value
    ) external isValidRecipient(params.recipient) onlyOwner {
        require(
            !params.path.hasMultiplePools(),
            "EscrowManager: multiple pools not allowed"
        );
        (address tokenIn, address tokenOut, uint24 fee) = params
            .path
            .decodeFirstPool();
        TransferHelper.safeApprove(tokenIn, swapRouter, params.amountIn);
        if (tokenIn > tokenOut) (tokenIn, tokenOut) = (tokenIn, tokenOut);
        address pool = IUniswapV3Factory(factory).getPool(
            tokenIn,
            tokenOut,
            fee
        );
        require(
            _whitelistLps.contains(pool),
            "EscrowManager: pool not whitelisted"
        );

        ISwapRouter(swapRouter).exactInput{value: value}(params);
    }

    function exactOutputSingle(
        ISwapRouter.ExactOutputSingleParams calldata params,
        uint256 value
    )
        external
        onlyOwner
        isValidRecipient(params.recipient)
        requireWhitelistedLp(params.tokenIn, params.tokenOut, params.fee)
    {
        TransferHelper.safeApprove(
            params.tokenIn,
            swapRouter,
            params.amountInMaximum
        );

        ISwapRouter(swapRouter).exactOutputSingle{value: value}(params);
    }

    function exactOutput(
        ISwapRouter.ExactOutputParams calldata params,
        uint256 value
    ) external isValidRecipient(params.recipient) onlyOwner {
        require(
            !params.path.hasMultiplePools(),
            "EscrowManager: multiple pools not allowed"
        );
        (address tokenOut, address tokenIn, uint24 fee) = params
            .path
            .decodeFirstPool();
        TransferHelper.safeApprove(tokenIn, swapRouter, params.amountInMaximum);
        if (tokenIn > tokenOut) (tokenIn, tokenOut) = (tokenIn, tokenOut);
        address pool = IUniswapV3Factory(factory).getPool(
            tokenIn,
            tokenOut,
            fee
        );
        require(
            _whitelistLps.contains(pool),
            "EscrowManager: pool not whitelisted"
        );

        ISwapRouter(swapRouter).exactOutput{value: value}(params);
    }

    function payEscrowSell(address token, uint256 amount) external onlyOwner {
        if (token == address(0x0)) {
            require(
                address(this).balance >= amount,
                "EscrowManager: Insufficient token"
            );
            if (amount > 0) TransferHelper.safeTransferETH(escrowSell, amount);
        } else {
            require(
                IERC20(token).balanceOf(address(this)) >= amount,
                "EscrowManager: Insufficient token"
            );
            if (amount > 0)
                TransferHelper.safeTransfer(token, escrowSell, amount);
        }
    }

    function approveSells(uint256[] calldata pids) external onlyOwner {
        require(pids.length > 0, "EscrowManager: invalid pids");
        IEscrowSell(escrowSell).approveSells(pids);
    }

    function updateEscrowSell(address _escrowSell) external onlyOwner {
        escrowSell = _escrowSell;
    }

    function emergencyWithdrawToken(
        IERC20 _token,
        uint256 _amount
    ) public onlyOwner {
        _token.transfer(owner(), _amount);
    }

    function emergencyWithdrawNFT(
        IERC721 _token,
        uint256 _tokenId
    ) public onlyOwner {
        _token.safeTransferFrom(address(this), owner(), _tokenId);
    }

    function emergencyWithdrawEth(uint256 _amount) public onlyOwner {
        payable(owner()).transfer(_amount);
    }
}