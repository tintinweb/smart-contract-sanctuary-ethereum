// File: TargetPriceOracle.sol
contract TargetPriceOracle is ChainlinkClient, ConfirmedOwner {

  using SafeMath for uint256;
  using Chainlink for Chainlink.Request;
  uint256 public currentTargetPrice;
  uint256 public baseFloorPrice;
  string[] public apiStrings;
  uint256 public floorPrice;
  address public oracle;
  address public linkToken;
  string public jobId;
  uint256 public ORACLE_PAYMENT;

  constructor() ConfirmedOwner(msg.sender) {
    linkToken = 0x514910771AF9Ca656af840dff83E8264EcF986CA;
    setChainlinkToken(linkToken);
    oracle = 0x188b71C9d27cDeE01B9b0dfF5C1aff62E8D6F434;
    setChainlinkOracle(oracle);
    jobId = "7599d3c8f31e4ce78ad2b790cbcfc673";
    ORACLE_PAYMENT = 1350000000000000000; // 1.35 LINK
    currentTargetPrice = 1e18;
    baseFloorPrice = 140; //sum of all floor prices
  }

  function updateCurrentTargetPrice(uint256 _currentTargetPrice) 
  external 
  onlyOwner
  {
    currentTargetPrice = _currentTargetPrice;
  }

  function updateBaseFloorPrice(uint256 _baseFloorPrice) 
  external 
  onlyOwner
  {
    baseFloorPrice = _baseFloorPrice;
  }

  function updateFloorPrice(uint256 _floorPrice) 
  external 
  onlyOwner
  {
    floorPrice = _floorPrice;
  }

  function updateLink(address _linkToken) 
  external 
  onlyOwner
  {
    linkToken = _linkToken;
    setChainlinkToken(linkToken);
  }
   
  function updateOracle(address _oracle) 
  external 
  onlyOwner
  {
    oracle = _oracle;
    setChainlinkOracle(oracle);
  }

  function updateJobId(string memory _jobId) 
  external 
  onlyOwner
  {
    jobId = _jobId;
  }

  function updateFees(uint256 _fee) 
  external 
  onlyOwner
  {
    ORACLE_PAYMENT = _fee;
  }

  function setApiStrings(string[] memory _apiStrings) 
    external 
    onlyOwner
  {
    require(apiStrings.length == 0, "Data already exists, update the array");
    apiStrings = _apiStrings;
  }

  function allApi() 
    external 
    view 
    returns(string[] memory)
  {
    return apiStrings;
  }

  function insertApiString(string memory _apiStrings) 
    external 
    onlyOwner
  {
    apiStrings.push(_apiStrings);
  }

  function updateApiString(uint256 _index, string memory _apiString) 
    external 
    onlyOwner
  {
    require(_index <= apiStrings.length, "Invalid index");
    apiStrings[_index] = _apiString;
  }

  function LinkBalance() 
    external 
    view 
    returns(uint256)
  {
    return IERC20(linkToken).balanceOf(address(this));
  }

  function withdrawLink(uint256 _amount) 
    external 
    onlyOwner
  {
    IERC20(linkToken).transfer(msg.sender, _amount);
  }

  function requestFloorPrice() 
    external 
    onlyOwner 
    {
        floorPrice = 0;
        for(uint256 i = 0;i < apiStrings.length ;i++){
            Chainlink.Request memory request = buildChainlinkRequest(stringToBytes32(jobId), address(this),  this.fulfill.selector);
            
            // Set the URL to perform the GET request on
            // NOTE: If this oracle gets more than 5 requests from this job at a time, it will not return. 
            request.add("get",apiStrings[i]);
            request.addInt("multiply", 1);
            
        // https://api.opensea.io/api/v1/collection/boredapeyachtclub/stats?format=json
            string[] memory path = new string[](2);
            path[0] = "stats";
            path[1] = "floor_price";
            request.addStringArray("path", path);

            sendChainlinkRequest(request, ORACLE_PAYMENT);
        }    
        // Multiply the result by 10000000000 to remove decimals
        //request.addInt("times", 100);
        
        // Sends the request
        //return sendChainlinkRequestTo(oracle, request, fee);
        
    }

    /**
     * Receive the response in the form of uint256
     */ 
    function fulfill(bytes32 _requestId, uint256 _price) 
      public 
      recordChainlinkFulfillment(_requestId)
    {
        floorPrice = floorPrice + _price;
    }
  
    /**
    * @return AggregatedValue: return the reported values.
    *         valid: Boolean indicating an aggregated value was computed successfully.
    */
    function getData()
        external
        view 
        returns (uint256, bool)
    {
        
        return ((floorPrice.mul(1e18)).div(baseFloorPrice), true);
    }

    function stringToBytes32(string memory source)
        private
        pure
        returns (bytes32 result)
    {
        bytes memory tempEmptyStringTest = bytes(source);
        if (tempEmptyStringTest.length == 0) {
            return 0x0;
        }

        assembly {
            // solhint-disable-line no-inline-assembly
            result := mload(add(source, 32))
        }
    }

}