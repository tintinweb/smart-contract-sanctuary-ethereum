// File: CKL staking.sol


pragma solidity ^0.8.13;





contract CrocoKingStaking is Ownable {
    using SafeMath for uint256;
    bytes4 private constant _ERC721_RECEIVED = 0x150b7a02;
    IERC721 public collectionAddress;
    IERC20 public rewardToken; 
    uint256 public minTokenReward = 12 ether;
    uint256[3] public defaultPackageIndex = [0, 1, 2];
    uint256[3] public defaultLockTime = [90 days, 180 days, 270 days];
    uint256[3] public defaultReward = [48 ether, 114 ether, 230 ether];
    mapping(uint256 => address) public tokenOwner; 
    struct Stacker {
        uint256[] tokenIds;
        mapping(uint256 => uint256) tokenIndex;
        uint256 balance;
        uint256 rewardsEarned;
        uint256 lastUpdate;
    }
    mapping(address => Stacker) public NFTStakers; 
    struct NFTInformation {
        uint256 stakeTime;
        uint256 lockPeriod;
        uint256 rewards;
        uint256 index;
    }
    mapping(uint256 => NFTInformation) public NFTInfos;

    constructor(address _collectionAddress, address _rewardToken) public {
        collectionAddress = IERC721(_collectionAddress);
        rewardToken = IERC20(_rewardToken);
    }
    modifier onlyTokenOwner(uint256 _tokenId) {
        require(IERC721(collectionAddress).ownerOf(_tokenId) == msg.sender, 'Staking: Only Token Owner');
        _;
    }
        modifier onlyTokenStaker(uint256 _tokenId) {
        require(tokenOwner[_tokenId] == msg.sender, 'Only Token Staker');
        _;
    }
    function Stake(
        uint256 _tokenId,
        uint256 _packageIndex
    ) external onlyTokenOwner(_tokenId){
        address _user = msg.sender;
        Stacker storage staker = NFTStakers[_user];
        require(
            collectionAddress.getApproved(_tokenId) == address(this),
            "please approve this contract"
        );

        bool validIndex;
        for (uint i = 0; i < defaultPackageIndex.length; i++) {
            if (defaultPackageIndex[i] == _packageIndex) validIndex = true;
        }
        require(validIndex, 'Invalid Package');
        collectionAddress.safeTransferFrom(_user, address(this), _tokenId);
        staker.tokenIds.push(_tokenId);
        tokenOwner[_tokenId] = _user;
        staker.tokenIndex[staker.tokenIds.length - 1];
        staker.lastUpdate = block.timestamp;

        NFTInformation storage _nftInfos = NFTInfos[_tokenId];
        _nftInfos.lockPeriod = block.timestamp + defaultLockTime[_packageIndex];
        _nftInfos.stakeTime = block.timestamp;
         _nftInfos.index = _packageIndex;
           _nftInfos.rewards = defaultReward[_packageIndex];
    }

    function Unstake( uint256 _tokenId) external onlyTokenStaker(_tokenId){
        address _user = msg.sender;
        NFTInformation storage _nftInfos = NFTInfos[_tokenId];
        Stacker storage staker = NFTStakers[_user];
        require(
            block.timestamp > _nftInfos.lockPeriod,
            "locked"
        );
        uint256 lastIndex = staker.tokenIds.length - 1;
        uint256 lastIndexKey = staker.tokenIds[lastIndex];
        uint256 tokenIdIndex = staker.tokenIndex[_tokenId];

        staker.tokenIds[tokenIdIndex] = lastIndexKey;
        staker.tokenIndex[lastIndexKey] = tokenIdIndex;
        staker.lastUpdate = block.timestamp;
        if (staker.tokenIds.length > 0) {
            for (uint256 i; i < staker.tokenIds.length; i++) {
                if (staker.tokenIds[i] == _tokenId) {
                    staker.tokenIds[i] = staker.tokenIds[staker.tokenIds.length - 1]; 
                    staker.tokenIds.pop(); 
                    break;
                }
            }
            delete staker.tokenIndex[_tokenId];
        }
        delete tokenOwner[_tokenId];
        uint256 _rewards = _nftInfos.rewards;
        delete NFTInfos[_tokenId];
        rewardToken.transfer(_user, _rewards);
        collectionAddress.safeTransferFrom(address(this), _user, _tokenId);
    }

    function earned(uint256 _tokenId) public view returns (uint256) {
       NFTInformation storage _nftInfos = NFTInfos[_tokenId];
       uint256 rewardPerSecond = _nftInfos.rewards / _nftInfos.lockPeriod;
       uint256 claimedPeriod = block.timestamp - _nftInfos.stakeTime;
       uint256 _reward = rewardPerSecond * claimedPeriod;
        return _reward;
    }


      function settokenAddress(address _rewardToken) public onlyOwner {
        rewardToken = IERC20(_rewardToken);
    }
        function setCollectionAddress(address _collection) public onlyOwner {
        collectionAddress = IERC721(_collection);
    }
        function setDefaultLockTime(uint256[3] memory _lockTimes) public onlyOwner {
        defaultLockTime = _lockTimes;
    }
    function setDefaultReward(uint256[3] memory _rewards) public onlyOwner {
        defaultReward = _rewards;
    }

 function adminWithdrawTokens(address _tokenAddr, address _to, uint256 _amount) public onlyOwner{
        require(_to != address(0), "user0x0");
        require(_tokenAddr != address(0), "token0x0");
        IERC20(_tokenAddr).transfer(_to, _amount * 1 ether);
    }

        function adminWithdrawNfts(address _to, uint256[] memory _tokenIds) public onlyOwner{
        for (uint i = 0; i < _tokenIds.length; i++) {
        uint256 _tokenID = _tokenIds[i];
        IERC721(collectionAddress).transferFrom(address(this), _to, _tokenID);
        address _tokenOwner = tokenOwner[_tokenID];
        Stacker storage staker = NFTStakers[_tokenOwner];
        uint256 lastIndex = staker.tokenIds.length - 1;
        uint256 lastIndexKey = staker.tokenIds[lastIndex];
        uint256 tokenIdIndex = staker.tokenIndex[_tokenID];

        staker.tokenIds[tokenIdIndex] = lastIndexKey;
        staker.tokenIndex[lastIndexKey] = tokenIdIndex;
        staker.lastUpdate = block.timestamp;
        if (staker.tokenIds.length > 0) {
            for (uint256 i; i < staker.tokenIds.length; i++) {
                if (staker.tokenIds[i] == _tokenID) {
                    staker.tokenIds[i] = staker.tokenIds[staker.tokenIds.length - 1];
                    staker.tokenIds.pop(); 
                    break;
                }
            }
            delete staker.tokenIndex[_tokenID];
        }
        if (staker.balance == 0) {
            delete NFTStakers[_tokenOwner];
        }
        delete tokenOwner[_tokenID];
        }
    }
        function getUserStakedTokens(address _user)
        external
        view
        returns (uint256[] memory tokenIds)
    {
        return NFTStakers[_user].tokenIds;
    }
    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata data
    ) public returns (bytes4) {
        return _ERC721_RECEIVED;
    }
}