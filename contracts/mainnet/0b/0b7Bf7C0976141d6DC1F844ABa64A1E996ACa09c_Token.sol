            require(amount <= maxTxAmount, "Transfer amount exceeds the maxTxAmount.");
        }
        
        //Calculate amounts
        uint256 EducationAmt = amount.mul(EducationFee).div(100);
        uint256 MarketingAmt = amount.mul(MarketingFee).div(100);
        uint256 BurnAmt = amount.mul(BurnFee).div(100);
        uint256 taxAmt = amount.mul(taxFee).div(100);

        
        if (_isExcludedFromFee[sender] || _isExcludedFromFee[recipient]){
            _transferStandard(sender, recipient, (amount));
        } else {
        require(amount <= maxTxAmount, "Transfer amount exceeds the maxTxAmount.");
            _transferStandard(sender, recipient, (amount.sub(EducationAmt).sub(MarketingAmt).sub(BurnAmt).sub(taxAmt)));
        }
        
        
        //Temporarily remove fees to transfer
        EducationFee = 0;
        MarketingFee = 0;
        BurnFee = 0;
        taxFee = 0;

        //Send transfers
        _transferStandard(sender, EducationWallet, EducationAmt);
        _transferStandard(sender, MarketingWallet, MarketingAmt);
        _transferStandard(sender, BurnWallet, BurnAmt);
        _transferStandard(sender, taxWallet, taxAmt);

        //Restore tax and liquidity fees
        EducationFee = previousEducationFee;
        MarketingFee = previousMarketingFee;
        BurnFee = previousBurnFee;
        taxFee = previoustaxFee;


        if(_isExcludedFromFee[sender] || _isExcludedFromFee[recipient])
            restoreAllFee();
    }

    function _transferStandard(address sender, address recipient, uint256 Amount) private {
        balances[recipient] += Amount;
        balances[sender] -= Amount;
        emit Transfer(sender, recipient, Amount);
    }

    function approve(address spender, uint value) public returns(bool) {
        allowance[msg.sender][spender] = value;
        emit Approval(msg.sender, spender, value);
        return true;
    }

    function removeAllFee() private {
        if(taxFee == 0 && BurnFee == 0) return;
        EducationFee = previousEducationFee;
        MarketingFee = previousMarketingFee;
        BurnFee = previousBurnFee;
        taxFee = previoustaxFee;
        
        EducationFee = 0;
        MarketingFee = 0;
        BurnFee = 0;
        taxFee = 0;
    }
    
    function restoreAllFee() private {
        EducationFee = previousEducationFee;
        MarketingFee = previousMarketingFee;
        BurnFee = previousBurnFee;
        taxFee = previoustaxFee;
    }
    

    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }
    
    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = false;
    }

    function setmaxTxAmount(uint limit) public {
        require(msg.sender == admin, "Only admin is allowed to change the fees");
        maxTxAmount = limit;
    }

    function getmaxTxAmount() public view returns (uint) {
        return maxTxAmount;
    }

    function getTaxFee() public view returns(uint) {
        return taxFee;
    }

    function getBurnFee() public view returns(uint) {
    return BurnFee;
    }
    function getMarketingFee() public view returns(uint) {
    return MarketingFee;
    }
    function getEducationFee() public view returns(uint) {
    return EducationFee;
    }
    function changeBuyFees(uint Education, uint Marketing, uint Burn, uint tax) public {
      require(msg.sender == admin, "Only admin is allowed to change the fees");
        EducationFee = Education;
        MarketingFee = Marketing;
        BurnFee = Burn;
        taxFee = tax;
      totalFee = Education + Marketing + Burn + tax;
    }
    function changeSellFees(uint Education, uint Marketing, uint Burn, uint tax) public {
       require(msg.sender == admin, "Only admin is allowed to change the fees");
        EducationFee = Education;
        MarketingFee = Marketing;
        BurnFee = Burn;
        taxFee = tax;
      totalFee = Education + Marketing + Burn + tax;
    }
}