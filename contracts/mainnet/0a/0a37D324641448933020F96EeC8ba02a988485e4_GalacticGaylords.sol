  function _verifyProof(address _addr, bytes32[] calldata _proof)
    internal
    view
    returns (bool _isValid)
  {
    bytes32 leaf = keccak256(bytes.concat(keccak256(abi.encode(_addr))));
    _isValid = MerkleProof.verify(_proof, rootHash, leaf);
  }

