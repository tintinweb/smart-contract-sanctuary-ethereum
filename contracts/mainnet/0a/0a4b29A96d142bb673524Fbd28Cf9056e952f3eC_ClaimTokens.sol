import "@openzeppelin/contracts/interfaces/IERC721Enumerable.sol";

contract ClaimTokens is Ownable, ReentrancyGuard {
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeERC20 for IERC20;

    struct UserInfo {
        EnumerableSet.AddressSet lockedTokens; // records all token addresses the user has locked
        mapping(address => uint256[]) locksForToken; // map erc20 address to lockId for that token
    }
    struct TokenLock {
        address tokenAddress; // The token address
        address nftContract; // The ERC721 contract that is checked on the lock.
        uint256 sharesDeposited; // the total amount of shares deposited
        uint256 sharesWithdrawn; // amount of shares withdrawn
        uint256 startEmission; // date token emission begins
        uint256 endEmission; // the date the tokens can be withdrawn
        uint256 expiration; // the date the lock expires
        uint256 lockID; // lock id per token lock
        address owner; // the owner who can edit or withdraw the lock
        address condition; // address(0) = no condition, otherwise the condition contract must implement IUnlockCondition
    }

    struct LockParams {
        address nftContract; // the nftcontract on which the tokens are locked to.
        uint256 amount; // amount of tokens to lock
        uint256 startEmission; // 0 if lock type 1, else a unix timestamp
        uint256 endEmission; // the unlock date as a unix timestamp (in seconds)
        uint256 expiration; // the expiration date as a unix timestamp (in seconds)
        address condition; // address(0) = no condition, otherwise the condition must implement IUnlockCondition
    }

    struct nftInfo {
        uint256 sharesDeposited;
        uint256 sharesWithdrawn;
    }

    EnumerableSet.AddressSet private TOKENS; // list of all unique tokens that have a lock
    mapping(uint256 => TokenLock) public LOCKS; // map lockID nonce to the lock
    uint256 public NONCE = 0; // incremental lock nonce counter, this is the unique ID for the next lock
    uint256 public MINIMUM_DEPOSIT = 100; // minimum divisibility per lock at time of locking

    mapping(address => uint256[]) private TOKEN_LOCKS; // map token address to array of lockIDs for that token
    mapping(uint256 => mapping(uint256 => nftInfo)) public NFT_LOCKS; // map lock id to token id to nftInfo
    mapping(address => UserInfo) private USERS;
    mapping(uint256 => mapping(uint256 => bool)) public BLACKLIST; // map lockID to array of tokenIDs that are blacklisted

    mapping(address => uint) public SHARES; // map token to number of shares per token, shares allow rebasing and deflationary tokens to compute correctly

    event onLock(
        uint256 lockID,
        address token,
        address nftContract,
        uint256 amountInTokens,
        uint256 startEmission,
        uint256 endEmission,
        uint256 expiration
    );
    event onWithdraw(address tokenAddr, uint256 tokenAmount);
    event onClaim(address userAddr, uint256 tokemAmount);

    constructor() {}

    /**
     * @notice Creates one or multiple locks for the specified token
     * @param _token the erc20 token address
     * @param _lock_params an array of locks with format: [LockParams[nftContract, amount, startEmission, endEmission, condition]]
     * amount: must be >= 100 units
     * startEmission = 0 : LockType 1
     * startEmission != 0 : LockType 2 (linear scaling lock)
     * use address(0) for no premature unlocking condition
     * Fails if startEmission is not less than EndEmission
     * Fails is amount < 100
     */
    function lock(address _token, LockParams calldata _lock_params)
        external
        nonReentrant
    {
        // Transfer tokens
        uint256 balanceBefore = IERC20(_token).balanceOf(address(this));
        IERC20(_token).safeTransferFrom(
            address(msg.sender),
            address(this),
            _lock_params.amount
        );
        uint256 amountInTokens = IERC20(_token).balanceOf(address(this)) -
            balanceBefore;

        uint256 shares = 0;
        LockParams memory lock_param = _lock_params;
        require(lock_param.startEmission < lock_param.endEmission, "PERIOD");
        require(lock_param.endEmission < 1e10, "TIMESTAMP INVALID"); // prevents errors when timestamp entered in milliseconds
        require(lock_param.amount >= MINIMUM_DEPOSIT, "MIN DEPOSIT");

        if (SHARES[_token] == 0) {
            shares = amountInTokens;
        } else {
            shares = FullMath.mulDiv(
                amountInTokens,
                SHARES[_token],
                balanceBefore == 0 ? 1 : balanceBefore
            );
        }
        require(shares > 0, "SHARES");
        SHARES[_token] += shares;
        balanceBefore += amountInTokens;

        TokenLock memory token_lock;
        token_lock.tokenAddress = _token;
        token_lock.sharesDeposited = shares;
        token_lock.startEmission = lock_param.startEmission;
        token_lock.endEmission = lock_param.endEmission;
        token_lock.expiration = lock_param.expiration;
        token_lock.lockID = NONCE;
        token_lock.nftContract = lock_param.nftContract;
        token_lock.owner = msg.sender;
        if (lock_param.condition != address(0)) {
            // if the condition contract does not implement the interface and return a bool
            // the below line will fail and revert the tx as the conditional contract is invalid
            // IUnlockCondition(lock_param.condition).unlockTokens();
            token_lock.condition = lock_param.condition;
        }

        // record the lock globally
        LOCKS[NONCE] = token_lock;
        TOKENS.add(_token);
        TOKEN_LOCKS[_token].push(NONCE);

        // For each token id, set the shares deposited and shares withdrawn
        uint256 testShares = 0;
        testShares = 777 * 52500000000000000000000 + (1300 - 777) * 17500000000000000000000;

        require(testShares == shares, "SHARES MISMATCH");

        // record the lock for the user
        UserInfo storage user = USERS[msg.sender];
        user.lockedTokens.add(_token);
        user.locksForToken[_token].push(NONCE);

        NONCE++;
        emit onLock(
            token_lock.lockID,
            _token,
            token_lock.nftContract,
            amountInTokens,
            token_lock.startEmission,
            token_lock.endEmission,
            token_lock.expiration
        );
    }

    /**
     * @notice withdraw a specified amount from a lock. _amount is the ideal amount to be withdrawn.
     * however, this amount might be slightly different in rebasing tokens due to the conversion to shares,
     * then back into an amount
     * @param _lockID the lockID of the lock to be withdrawn
     * @param _tokenId the token id of the nft
     * @param _amount amount of tokens to withdraw
     */
    function withdraw(uint256 _lockID, uint256 _tokenId, uint256 _amount) external nonReentrant {
        require(BLACKLIST[_lockID][_tokenId] == false, "BLACKLISTED");
        TokenLock storage nftLock = LOCKS[_lockID];
        nftInfo storage nftShares = NFT_LOCKS[_lockID][_tokenId];
        require(
            msg.sender ==
                IERC721Enumerable(nftLock.nftContract).ownerOf(
                    _tokenId
                ),
            "OWNER"
        );
        // If it's the first time withdrawing, set the shares deposited
        if (nftShares.sharesDeposited == 0) {
            if (_tokenId >= 1 || _tokenId <= 777) {
                nftShares.sharesDeposited = 52500000000000000000000;
            }
            if (_tokenId > 777) {
                nftShares.sharesDeposited = 17500000000000000000000;
            }
        }
        // convert _amount to its representation in shares
        uint256 balance = IERC20(nftLock.tokenAddress).balanceOf(address(this));
        uint256 withdrawableShares = getWithdrawableSharesToken(nftLock.lockID, _tokenId);

        // if amount is 0, withdraw all
        if (_amount == 0) {
            _amount = withdrawableShares;
        }
        uint256 shareDebit = FullMath.mulDiv(
            SHARES[nftLock.tokenAddress],
            _amount,
            balance
        );
        // round _amount up to the nearest whole share if the amount of tokens specified does not translate to
        // at least 1 share.
        if (shareDebit == 0 && _amount > 0) {
            shareDebit++;
        }
        require(shareDebit > 0, "ZERO WITHDRAWL");
        // dust clearance block, as mulDiv rounds down leaving one share stuck, clear all shares for dust amounts
        if (shareDebit + 1 == withdrawableShares) {
            if (
                FullMath.mulDiv(
                    SHARES[nftLock.tokenAddress],
                    balance / SHARES[nftLock.tokenAddress],
                    balance
                ) == 0
            ) {
                shareDebit++;
            }
        }
        require(withdrawableShares >= shareDebit, "AMOUNT");
        nftShares.sharesWithdrawn += shareDebit;
        nftLock.sharesWithdrawn += shareDebit;

        // now convert shares to the actual _amount it represents, this may differ slightly from the
        // _amount supplied in this methods arguments.
        uint256 amountInTokens = FullMath.mulDiv(
            shareDebit,
            balance,
            SHARES[nftLock.tokenAddress]
        );
        SHARES[nftLock.tokenAddress] -= shareDebit;

        IERC20(nftLock.tokenAddress).safeTransfer(msg.sender, amountInTokens);
        emit onWithdraw(nftLock.tokenAddress, amountInTokens);
    }

    // returns withdrawable share amount from the lock, taking into consideration start and end emission
    function getWithdrawableSharesToken(uint256 _lockID, uint256 _tokenId)
        public
        view
        returns (uint256)
    {
        TokenLock storage nftLock = LOCKS[_lockID];
        nftInfo storage nftShares = NFT_LOCKS[_lockID][_tokenId];
        uint8 lockType = nftLock.startEmission == 0 ? 1 : 2;
        uint256 amount = lockType == 1
            ? nftShares.sharesDeposited - nftShares.sharesWithdrawn
            : nftShares.sharesDeposited;
        uint256 withdrawable;
        withdrawable = VestingMathLibrary.getWithdrawableAmount(
            nftLock.startEmission,
            nftLock.endEmission,
            amount,
            block.timestamp,
            nftLock.condition
        );
        if (lockType == 2) {
            withdrawable -= nftShares.sharesWithdrawn;
        }
        return withdrawable;
    }

    // TODO claim all is a security risk? leave for now
    function claimAll(uint256 _lockId) external {
        TokenLock storage nftLock = LOCKS[_lockId];
        uint256 ownerTokenCount = IERC721(nftLock.nftContract).balanceOf(msg.sender);
        require(ownerTokenCount > 0, "NO TOKENS");
        for (uint256 i; i < ownerTokenCount; i++) {
            uint256 tokenId = IERC721Enumerable(nftLock.nftContract)
                .tokenOfOwnerByIndex(msg.sender, i);
            // Withdraw the tokens
            (bool success, bytes memory data) = address(this).delegatecall(
                abi.encodeWithSignature("withdraw(uint256,uint256,uint256)", _lockId, tokenId, 0)
            );
            require(success, "WITHDRAW FAILED");
        }
    }

    function withdrawExpired(uint256 _lockId, uint256 _tokenId) external nonReentrant {
        TokenLock storage nftLock = LOCKS[_lockId];
        nftInfo storage nftShares = NFT_LOCKS[_lockId][_tokenId];
        require(msg.sender == nftLock.owner, "OWNER");
        require(nftLock.expiration < block.timestamp, "NOT EXPIRED");

        // If it's the first time withdrawing, set the shares deposited
        if (nftShares.sharesDeposited == 0) {
            if (_tokenId >= 1 || _tokenId <= 777) {
                nftShares.sharesDeposited = 52500000000000000000000;
            }
            if (_tokenId > 777) {
                nftShares.sharesDeposited = 17500000000000000000000;
            }
        }

        uint256 amount = nftShares.sharesDeposited - nftShares.sharesWithdrawn;

        nftLock.sharesWithdrawn += amount;
        nftShares.sharesWithdrawn = nftShares.sharesDeposited;
        SHARES[nftLock.tokenAddress] -= amount;

        IERC20(nftLock.tokenAddress).safeTransfer(msg.sender, amount);
        emit onWithdraw(nftLock.tokenAddress, amount);
    }

    // TODO have nonrentrant modifier? this fails.
    function withdrawAllExpired(uint256 _lockId) external nonReentrant {
        TokenLock storage nftLock = LOCKS[_lockId];
        
        require(msg.sender == nftLock.owner, "OWNER");
        require(nftLock.expiration < block.timestamp, "NOT EXPIRED");

        // empty the lock
        uint256 amount = nftLock.sharesDeposited - nftLock.sharesWithdrawn;
        nftLock.sharesWithdrawn = nftLock.sharesDeposited;
        SHARES[nftLock.tokenAddress] -= amount;

        IERC20(nftLock.tokenAddress).safeTransfer(msg.sender, amount);
    }

    // Sets blacklist for lock with array of tokenIds
    function setBlacklist(uint256 _lockId, uint256[] memory _tokenIds) external {
        TokenLock storage nftLock = LOCKS[_lockId];
        require(msg.sender == nftLock.owner, "OWNER");
        for (uint256 i; i < _tokenIds.length; i++) {
            BLACKLIST[_lockId][_tokenIds[i]] = true;
        }
    }    

    function getUserLocksForTokenLength(address _user, address _token)
        external
        view
        returns (uint256)
    {
        return USERS[_user].locksForToken[_token].length;
    }

    function getUserLockIDForTokenAtIndex(
        address _user,
        address _token,
        uint256 _index
    ) external view returns (uint256) {
        return USERS[_user].locksForToken[_token][_index];
    }
}

