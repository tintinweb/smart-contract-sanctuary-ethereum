pragma solidity >=0.4.22 <0.9.0;


contract SpinalFluid is Ownable, ERC20 {

	using SafeMath for uint256;


  uint public DRIP = 11 ether; 
  uint256 constant public INITIAL_ISSUANCE = 11 ether;
  uint256 constant public END = 1992988800;  //Sat Feb 26 2033
  uint public OwnCowCost = 0 ether;



	mapping(address => uint256) public lastUpdate;

	IBAM public  BAMContract;

	event DRIPPaid(address indexed user, uint256 DRIP);

	constructor(address _BAM)
	           {
		BAMContract = IBAM(_BAM);
	}


	function min(uint256 a, uint256 b) internal pure returns (uint256) {
		return a < b ? a : b;
	}

	// called when minting many NFTs
	// updated_amount = (balanceOG(user) * base_rate * delta / 86400) + amount * initial rate
	function updateRewardOnMint(address _user, uint256 _amount) external {
		require(msg.sender == address(BAMContract), "Can't call this");
		uint256 time = min(block.timestamp, END);
		uint256 timerUser = lastUpdate[_user];
		if (timerUser > 0)
			lastUpdate[_user] = lastUpdate[_user].add(BAMContract.balanceOf(_user).mul(DRIP.mul((time.sub(timerUser)))).div(86400)
				.add(_amount.mul(INITIAL_ISSUANCE)));
		else 
			lastUpdate[_user] = lastUpdate[_user].add(_amount.mul(INITIAL_ISSUANCE));
		lastUpdate[_user] = time;
	}
	// called on transfers
	function updateDRIP(address _from, address _to, uint256 _tokenId) external {
		require(msg.sender == address(BAMContract));
		if (_tokenId < 10001) {
			uint256 time = min(block.timestamp, END);
			uint256 timerFrom = lastUpdate[_from];
			if (timerFrom > 0)
				lastUpdate[_from] += BAMContract.balanceOf(_from).mul(DRIP.mul((time.sub(timerFrom)))).div(86400);
			if (timerFrom != END)
				lastUpdate[_from] = time;
			if (_to != address(0)) {
				uint256 timerTo = lastUpdate[_to];
				if (timerTo > 0)
					lastUpdate[_to] += BAMContract.balanceOf(_to).mul(DRIP.mul((time.sub(timerTo)))).div(86400);
				if (timerTo != END)
					lastUpdate[_to] = time;
			}
		}
	}

	function getDRIP(address _to) external {
		if (DRIP > 0) {
			lastUpdate[_to] = 0;
			_mint(_to, DRIP);
			emit DRIPPaid(_to, DRIP);
		}
	}

	function burn(address _from, uint256 _amount) external {
		require(msg.sender == address(BAMContract));
		_burn(_from, _amount);
	}

	function getTotalClaimable(address _user) external view returns(uint256) {
		uint256 time = min(block.timestamp, END);
		uint256 pending = BAMContract.balanceOf(_user).mul(DRIP.mul((time.sub(lastUpdate[_user])))).div(86400);
		return lastUpdate[_user] + pending;
	}
}