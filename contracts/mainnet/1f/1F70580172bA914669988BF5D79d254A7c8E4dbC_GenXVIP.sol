pragma solidity ^0.8.17;

import "erc721a/contracts/extensions/ERC721ABurnable.sol";
import "./mason/utils/Administrable.sol";
import "./mason/utils/EIP712Common.sol";
import "./mason/utils/Ownable.sol";
import "operator-filter-registry/src/DefaultOperatorFilterer.sol";
import "@openzeppelin/contracts/utils/Address.sol";

error CrossmintNotActive();
error CrossmintAddressNotSet();
error CrossmintAddressDoesNotMatch();
error CrossmintInsufficientPayment();
error ExceedsMaxPerWallet();
error ExceedsMaxSupply();
error InsufficientPayment();

contract GenXVIP is
  ERC721ABurnable,
  Administrable,
  EIP712Common,
  DefaultOperatorFilterer
{
  uint64 public MAX_PER_WALLET = 4;
  uint64 public MAX_SUPPLY;
  uint256 public PRICE;
  uint256 public WHITELIST_PRICE;

  address private royaltyAddress;
  uint256 private royaltyPercent;

  constructor(
    string memory _tokenName,
    string memory _tokenSymbol,
    string memory _customBaseURI,
    uint256 _price,
    uint64 _tokensForSale,
    address _royaltyAddress,
    uint256 _royaltyPercent
  ) ERC721A(_tokenName, _tokenSymbol) {
    customBaseURI = _customBaseURI;

    MAX_SUPPLY = _tokensForSale;
    PRICE = _price;
    WHITELIST_PRICE = _price;

    royaltyAddress = _royaltyAddress;
    royaltyPercent = _royaltyPercent;
    paymentRecipient = _royaltyAddress;

    _setRoyalties(_royaltyAddress, _royaltyPercent);
  }

  /** MINTING **/
  function mint(uint256 _count) external payable noContracts requireActiveSale {
    if (_totalMinted() + _count > MAX_SUPPLY) revert ExceedsMaxSupply();
    if (msg.value < PRICE * _count) revert InsufficientPayment();
    if (
      _numberMinted(msg.sender) + _count > MAX_PER_WALLET + _getAux(msg.sender)
    ) revert ExceedsMaxPerWallet();

    _mint(msg.sender, _count);
  }

  function whitelistMint(
    uint256 _count,
    bytes calldata _signature
  )
    external
    payable
    requiresWhitelist(_signature)
    requireActiveWhitelist
    noContracts
  {
    if (_totalMinted() + _count > MAX_SUPPLY) revert ExceedsMaxSupply();
    if (msg.value < WHITELIST_PRICE * _count) revert InsufficientPayment();
    if (
      _numberMinted(msg.sender) + _count > MAX_PER_WALLET + _getAux(msg.sender)
    ) revert ExceedsMaxPerWallet();

    _mint(msg.sender, _count);
  }

  function mintTo(
    address _to,
    uint _count
  ) external payable onlyCrossmint requireActiveSale {
    if (_totalMinted() + _count > MAX_SUPPLY) revert ExceedsMaxSupply();
    if (msg.value < PRICE * _count) revert CrossmintInsufficientPayment();

    if (_numberMinted(_to) + _count > MAX_PER_WALLET + _getAux(_to))
      revert ExceedsMaxPerWallet();

    _mint(_to, _count);
  }

  function airdrop(
    uint64 _count,
    address _recipient
  ) external onlyOperatorsAndOwner {
    if (_totalMinted() + _count > MAX_SUPPLY) revert ExceedsMaxSupply();

    _mint(_recipient, _count);
    _setAux(_recipient, _getAux(_recipient) + _count);
  }

  function airdropBatch(
    uint64[] calldata _counts,
    address[] calldata _recipients
  ) external onlyOperatorsAndOwner {
    uint256 sum;
    for (uint256 i; i < _counts.length; ) {
      sum = sum + _counts[i];
      unchecked {
        ++i;
      }
    }

    if (_totalMinted() + sum > MAX_SUPPLY) revert ExceedsMaxSupply();

    for (uint256 i; i < _recipients.length; ) {
      _mint(_recipients[i], _counts[i]);
      _setAux(_recipients[i], _getAux(_recipients[i]) + _counts[i]);
      unchecked {
        ++i;
      }
    }
  }

  /** MINTING LIMITS **/
  function allowedMintCount(address _minter) external view returns (uint256) {
    return MAX_PER_WALLET + _getAux(_minter) - _numberMinted(_minter);
  }

  function allowedWhitelistMintCount(
    address _minter,
    bytes calldata _signature
  ) external view requiresWhitelist(_signature) returns (uint256) {
    return MAX_PER_WALLET + _getAux(_minter) - _numberMinted(_minter);
  }

  /** WHITELIST **/
  function checkWhitelist(
    bytes calldata _signature
  ) external view requiresWhitelist(_signature) returns (bool) {
    return true;
  }

  /** ADMIN **/
  function setMaxPerWallet(uint64 _max) external onlyOperatorsAndOwner {
    MAX_PER_WALLET = _max;
  }

  function setPrice(uint256 _price) external onlyOperatorsAndOwner {
    PRICE = _price;
  }

  function setWhitelistPrice(uint256 _price) external onlyOperatorsAndOwner {
    WHITELIST_PRICE = _price;
  }

  bool public crossmintIsActive;
  address private crossmintAddress = 0xdAb1a1854214684acE522439684a145E62505233;

  function flipCrossmintState() public virtual onlyOperatorsAndOwner {
    crossmintIsActive = !crossmintIsActive;
  }

  function setCrossmintAddress(
    address _crossmintAddress
  ) public virtual onlyOperatorsAndOwner {
    crossmintAddress = _crossmintAddress;
  }

  modifier onlyCrossmint() {
    if (!crossmintIsActive) revert CrossmintNotActive();
    if (crossmintAddress == address(0)) revert CrossmintAddressNotSet();
    if (msg.sender != crossmintAddress) revert CrossmintAddressDoesNotMatch();
    _;
  }

  /** URI HANDLING **/
  string private customBaseURI;
  string private metadataExtension;

  function baseTokenURI() public view returns (string memory) {
    return customBaseURI;
  }

  function getMetadataExtension() public view returns (string memory) {
    return metadataExtension;
  }

  function setBaseURI(
    string memory customBaseURI_
  ) external onlyOperatorsAndOwner {
    customBaseURI = customBaseURI_;
  }

  function setMetadataExtension(
    string memory metadataExtension_
  ) external onlyOperatorsAndOwner {
    metadataExtension = metadataExtension_;
  }

  function tokenURI(
    uint256 tokenId
  ) public view virtual override(ERC721A, IERC721A) returns (string memory) {
    return string.concat(customBaseURI, _toString(tokenId), metadataExtension);
  }

  function _baseURI() internal view virtual override returns (string memory) {
    return customBaseURI;
  }

  function _metadataExtension() internal view virtual returns (string memory) {
    return metadataExtension;
  }

  function _startTokenId() internal view virtual override returns (uint256) {
    return 1;
  }

  /** PAYOUT **/
  function _setRoyalties(address _receiver, uint256 _percentage) internal {
    royaltyAddress = _receiver;
    royaltyPercent = _percentage;
  }

  function royaltyInfo(
    uint256 _tokenId,
    uint256 _salePrice
  ) external view returns (address receiver, uint256 royaltyAmount) {
    receiver = royaltyAddress;
    royaltyAmount = (_salePrice * royaltyPercent) / 10000;
  }

  function setRoyaltyInfo(
    address _royaltyAddress,
    uint256 _percentage
  ) external onlyOperatorsAndOwner {
    _setRoyalties(_royaltyAddress, _percentage);
  }

  function supportsInterface(
    bytes4 interfaceId
  )
    public
    view
    override(ERC721A, IERC721A, AccessControlEnumerable)
    returns (bool)
  {
    return ERC721A.supportsInterface(interfaceId);
  }

  address private paymentRecipient;

  function setPaymentRecipient(
    address _recipient
  ) external onlyOperatorsAndOwner {
    paymentRecipient = _recipient;
  }

  function release() external virtual onlyOperatorsAndOwner {
    uint256 balance = address(this).balance;
    Address.sendValue(payable(paymentRecipient), balance);
  }

  function setApprovalForAll(
    address operator,
    bool approved
  ) public override(ERC721A, IERC721A) onlyAllowedOperatorApproval(operator) {
    super.setApprovalForAll(operator, approved);
  }

  function approve(
    address operator,
    uint256 tokenId
  )
    public
    payable
    override(ERC721A, IERC721A)
    onlyAllowedOperatorApproval(operator)
  {
    super.approve(operator, tokenId);
  }

  function transferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public payable override(ERC721A, IERC721A) onlyAllowedOperator(from) {
    super.transferFrom(from, to, tokenId);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public payable override(ERC721A, IERC721A) onlyAllowedOperator(from) {
    super.safeTransferFrom(from, to, tokenId);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId,
    bytes memory data
  ) public payable override(ERC721A, IERC721A) onlyAllowedOperator(from) {
    super.safeTransferFrom(from, to, tokenId, data);
  }
}

