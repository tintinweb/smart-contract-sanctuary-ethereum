    function withdraw() external {
        require(
            msg.sender == owner() || msg.sender == treasury,
            "Caller is not owner or treasury"
        );
        payable(treasury).transfer(address(this).balance);
    }

    /**
     * @dev Public Mint switch
     */
    function setPublicMintState(bool _state) external onlyOwner {
        isPublicMintEnabled = _state;
    }

    /**
     * @dev Pre Mint switch
     */
    function setPreMintState(bool _state) external onlyOwner {
        isPreMintEnabled = _state;
    }

    /**
     * @dev Sets the base URI for the API that provides the NFT data.
     */
    function setBaseURI(string memory _uri) external onlyOwner {
        baseURI = _uri;
    }

    /**
     * @dev Sets Mint Params.
     */
    function setParams(uint256 _communityMintCount, uint256 _maxPerWallet)
        external
        onlyOwner
    {
        communityMintCount = _communityMintCount;
        maxPerWallet = _maxPerWallet;
    }

    /**
     * @dev Sets mint price.
     */
    function setMintPrice(uint256 _mintPrice) external onlyOwner {
        mintPrice = _mintPrice;
    }

    /**
     * @dev Sets Merkle Root for whitelisting.
     */
    function setWhitelistMerkleRoot(bytes32 newMerkleRoot_) external onlyOwner {
        _whitelistMerkleRoot = newMerkleRoot_;
    }

    /**
     * @dev Gets the count of Mints left for a given account
     */
    function getMintsLeft(address _account) external view returns (uint256) {
        return maxPerWallet - _numberMinted(_account);
    }

    /**
     * @dev Converts addrs to bytes32.
     */
    function toBytes32(address addr) internal pure returns (bytes32) {
        return bytes32(uint256(uint160(addr)));
    }

    /**
     * @dev Gets the count of Mints left for a given account
     */
    function isWhitelisted(bytes32[] memory proof, address _account)
        external
        view
        returns (bool)
    {
        return
            MerkleProofUpgradeable.verify(
                proof,
                _whitelistMerkleRoot,
                toBytes32(_account)
            );
    }

    /**
     * @dev Community mint
     */
    function communityMint(uint256 _count) external onlyOwner {
        require(_count <= communityMintCount, "Exceeds community mint");
        _safeMint(treasury, communityMintCount);
        communityMintCount -= _count;
    }

    /**
     * @dev Public mint
     */
    function publicMint(uint256 _count) external payable nonReentrant {
        require(isPublicMintEnabled, "mint is not active");
        require(_count > 0, "can't mint 0 tokens");
        require(_currentIndex + _count <= maxMintCount, "total mint exceeded");
        require(msg.value >= mintPrice * _count, "not enough ETH");
        require(
            _numberMinted(msg.sender) + _count <= maxPerWallet,
            "can't mint more"
        );
        // Mint to user account
        emit Mint(msg.sender, _count, _currentIndex);
        _safeMint(msg.sender, _count);
    }

    /**
     * @dev Pre mint
     */
    function preMint(bytes32[] memory proof, uint256 _count)
        external
        payable
        nonReentrant
    {
        require(isPreMintEnabled, "mint is not active");
        require(_count > 0, "can't mint 0 tokens");
        require(_currentIndex + _count <= maxMintCount, "total mint exceeded");
        require(msg.value >= mintPrice * _count, "not enough ETH");
        require(
            _numberMinted(msg.sender) + _count <= maxPerWallet,
            "can't mint more"
        );
        // merkle tree list related
        require(_whitelistMerkleRoot != "", "whitelist isn't set");
        require(
            MerkleProofUpgradeable.verify(
                proof,
                _whitelistMerkleRoot,
                toBytes32(msg.sender)
            ),
            "not whitelisted"
        );
        // Mint to user account
        emit Mint(msg.sender, _count, _currentIndex);
        _safeMint(msg.sender, _count);
    }

    /**
     * @dev See {ERC721}.
     */
    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    /**
     * @dev For upgrading
     */
    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyOwner
    {}
}