abstract contract RoyaltySpliterStatic {
   
    struct Receiver {
        address wallet;
        uint16 revenue;
    }

    Receiver[] private receivers;
    
    event RoyaltyPaid(address receiver, uint256 sum);

    constructor() {
        // validateAndSaveReceivers( initialReceivers );
    }

