// SPDX-License-Identifier: MIT
pragma solidity ^0.8.13;

import {ERC20} from "../lib/solmate/src/tokens/ERC20.sol";
import {Ownable} from "../lib/openzeppelin-contracts/contracts/access/Ownable.sol";

/**
 * @title Open Exchange Token (OX)
 * @notice An ERC-20 with minting implemented as described in the OX whitepaper.
 * @author opnxj
 */
contract OpenExchangeToken is ERC20, Ownable {
    uint256 public constant MAX_SUPPLY = 10_000_000_000 ether; // 10 billion

    // Contracts/EOAs that can mint OX to users, as long as below MAX_SUPPLY
    mapping(address => bool) public minters;

    event MinterUpdated(address indexed account, bool isMinter);

    error Error_NotMinter();
    error Error_MaxSupplyExceeded();

    constructor() ERC20("Open Exchange Token", "OX", 18) {}

    modifier onlyMinters() {
        if (!minters[msg.sender]) revert Error_NotMinter();
        _;
    }

    /**
     * @notice Adds/removes minter permissions.
     * @dev Only the contract owner can call this function.
     * @param account The address to add/remove as a minter.
     * @param isMinter A boolean value indicating whether the address should be a minter or not.
     */
    function setMinter(address account, bool isMinter) external onlyOwner {
        minters[account] = isMinter;
        emit MinterUpdated(account, isMinter);
    }

    /**
     * @notice Mints new tokens and sends them to the specified account.
     * @dev Only minters can call this function.
     * @param to The account to receive the minted tokens.
     * @param amount The amount of tokens to mint.
     */
    function mint(address to, uint256 amount) external onlyMinters {
        if (totalSupply + amount > MAX_SUPPLY) revert Error_MaxSupplyExceeded();
        _mint(to, amount);
    }
}

