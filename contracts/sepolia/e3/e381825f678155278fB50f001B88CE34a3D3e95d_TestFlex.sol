pragma solidity ^0.8.13;

import {ERC20} from "../lib/solmate/src/tokens/ERC20.sol";
import {Script} from "../lib/forge-std/src/Script.sol";
import {OpenExchangeToken} from "../src/OpenExchangeToken.sol";
import {VotingEscrow} from "../src/VotingEscrow.sol";
import {FlexMinter} from "../src/FlexMinter.sol";
import {TestMinter} from "../test/util/TestMinter.sol";
import {console2} from "../lib/forge-std/src/console2.sol";

contract TestFlex is ERC20 {
    constructor() ERC20("TestFlex", "TFLEX", 18) {}

    function mint(address to, uint256 value) public {
        _mint(to, value);
    }
}

contract DeployScript is Script {
    OpenExchangeToken public oxToken;
    TestFlex public flexToken;
    TestMinter public testMinter;

    function setUp() public {}

    function _deployContracts() internal {
        // Deploy OpenExchangeToken
        oxToken = new OpenExchangeToken();

        // Deploy VotingEscrow
        VotingEscrow votingEscrow = new VotingEscrow(address(oxToken));
        oxToken.setMinter(address(votingEscrow), true);

        // Deploy TestFlex
        flexToken = new TestFlex();

        // Deploy FlexMinter
        uint256 conversionDeadline = type(uint256).max;
        FlexMinter flexMinter = new FlexMinter(
            address(oxToken),
            address(votingEscrow),
            address(flexToken),
            conversionDeadline
        );
        oxToken.setMinter(address(flexMinter), true);

        // Deploy TestMinter
        testMinter = new TestMinter(address(oxToken));
        oxToken.setMinter(address(testMinter), true);

        // Log all addresses
        console2.log("OpenExchangeToken: %s", address(oxToken));
        console2.log("VotingEscrow: %s", address(votingEscrow));
        console2.log("TestFlex: %s", address(flexToken));
        console2.log("FlexMinter: %s", address(flexMinter));
        console2.log("TestMinter: %s", address(testMinter));
    }

    function _mintTestTokens() internal {
        address[] memory accounts = new address[](2);
        accounts[0] = 0x3E9aa6cB61B853Bc6243D718CC53D85B2fe01fE4;
        accounts[1] = 0xb29B1ddb5448e1BB06c1e32383BE159F77a47c0C;

        uint256 amount = 1_000_000 ether;

        for (uint256 i = 0; i < accounts.length; i++) {
            // Mint FLEX
            flexToken.mint(accounts[i], amount);

            // Mint OX using TestMinter
            testMinter.mint(accounts[i], amount);
        }
    }

    function run() public {
        vm.startBroadcast();

        _deployContracts();
        _mintTestTokens();

        vm.stopBroadcast();
    }
}

