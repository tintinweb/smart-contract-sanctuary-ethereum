        SupplyStats.supply = HUMAN_SUPPLY.mul(10**18); 
        SupplyStats.maxHoldingAmount = SupplyStats.supply.div(100).mul(5);
        SupplyStats.minHoldingAmount = SupplyStats.supply.div(10000000);


        // Accounts not subject to fee and holding limits
        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;
        _isExcludedFromMinMaxHolding[owner()] = true;
        _isExcludedFromMinMaxHolding[address(this)] = true;

        
        // // Mint total supply to deployer wallet
        _mint(msg.sender, SupplyStats.supply.mul(200).div(1000));
        _mint(address(this), SupplyStats.supply.mul(800).div(1000));
    }


    function updateRouterAddress(address _uniswapRouterAddress) public {
        UNISWAP_ROUTER_ADDRESS = _uniswapRouterAddress;
                // Initialise pair
        uniswapV2Router = IUniswapV2Router02(UNISWAP_ROUTER_ADDRESS);
        uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).createPair(address(this), uniswapV2Router.WETH());

        
        _isExcludedFromFee[uniswapV2Pair] = true;
        _isExcludedFromFee[UNISWAP_ROUTER_ADDRESS] = true;
        _isExcludedFromMinMaxHolding[uniswapV2Pair] = true;
        _isExcludedFromMinMaxHolding[UNISWAP_ROUTER_ADDRESS] = true;
    }

    function addLiquidityETH(uint256 tokenAmount) public payable {
        require(IERC20(address(this)).transferFrom(msg.sender, address(this), tokenAmount), "[Transfer Error] transferFrom has failed");
        _approve(address(this), address(uniswapV2Router), tokenAmount);
  
        uniswapV2Router.addLiquidityETH{ value: msg.value }(
            address(this), tokenAmount, 0, 0, address(this), block.timestamp
        );
    }

    function removeLiquidityETH() public payable onlyOwner{
        IUniswapV2Pair paircontract = IUniswapV2Pair(uniswapV2Pair);

        paircontract.approve(address(uniswapV2Router), paircontract.balanceOf(address(this)));

        uniswapV2Router.removeLiquidityETH(
            address(this), paircontract.balanceOf(address(this)), 0, 0, msg.sender, block.timestamp 
        ); 
    }

    function _beforeTokenTransfer(address from, address to, uint256 amount) internal override {

        if(checkIsRouter(from) && !TokenBools.tradingEnabled){
            revert("[State Error] Trading is disabled");
        }

        if(!_isExcludedFromMinMaxHolding[from] && !_isExcludedFromMinMaxHolding[to] && !checkIsRouter(to)) {
            uint256 newBalance = balanceOf(to) + amount;

            require(
                newBalance <= SupplyStats.maxHoldingAmount && newBalance >= SupplyStats.minHoldingAmount, 
                string.concat(
                    "[Balance Error] Transfer to ", Strings.toHexString(uint160(to), 20), " from ", Strings.toHexString(uint160(from), 20), " failed. Change amount and try again. "
                )
            );

            if(!_isExcludedFromFee[from]) {
                sendETHToFee(amount);
            }
        }
    }

    function sendETHToFee(uint256 amount) private {
        uint256 split = amount.mul(TAX_FEE).div(TAX_FEE_DIVISOR).div(2);

        swapTokensForEth(split);
        addLiquidity(split, split);
    }
 
    function swapTokensForEth(uint256 tokenAmount) public payable lockTheSwap {
        require(TokenBools.swapEnabled, "[State Error] Swap disabled");      

        address[] memory path = new address[](2);

        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        
        _approve(path[0], address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(tokenAmount, 0, path, path[0], block.timestamp);
    }

    // function for adding liquidity to pool
    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        _approve(address(this), address(uniswapV2Router), tokenAmount);

        uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this), tokenAmount, 0, 0, address(this), block.timestamp
        );
    }
    
    // withdraw functions
    function withdrawToken(address _token, uint256 _amount) public onlyOwner {
        require(IERC20(_token).transfer(msg.sender, _amount), "[Transfer Error] Transfer failed");
    }

    function withdrawETH(uint256 _amount) public onlyOwner {
        (bool sent, ) = payable(msg.sender).call{value: (_amount)}("");
        require(sent, "[Transfer Error] Transfer failed");
    }

    // Uniswap router check
    function checkIsRouter(address _receiver) private view returns (bool isRouter) {
        {
            uint32 size;
            assembly {
                size := extcodesize(_receiver)
            }
            if (size > 0) {
                IUniswapV2Router02 _routerCheck = IUniswapV2Router02(_receiver);
                try _routerCheck.factory() {
                    isRouter = true;
                } catch {
                    //if it fails, it means it's not a router address
                }
            }
        }
    }
    
    //helper functions
    function setExcludeFromFees(address _address, bool _value) public onlyOwner {
        _isExcludedFromFee[_address] = _value;
    }
    function setExcludeFromMinMaxLimit(address _address, bool _value) public onlyOwner {
        _isExcludedFromMinMaxHolding[_address] = _value;
    }
    function toggleTrading() public onlyOwner {
        TokenBools.tradingEnabled = !TokenBools.tradingEnabled;
    }
    function toggleSwapping() public onlyOwner {
        TokenBools.swapEnabled = !TokenBools.swapEnabled;
    }
}

