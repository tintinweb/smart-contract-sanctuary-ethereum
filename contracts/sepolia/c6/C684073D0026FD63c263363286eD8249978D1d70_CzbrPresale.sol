// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "./CZBR_COIN.sol";

contract CzbrPresale is Ownable {
    struct TokenInfo {
        uint256 totalTokens;
        uint256 tokenPrice;
        address token;
    }

    TokenInfo public TokenData;
    IUniswapV2Router02 public uniswapV2Router;

    constructor(address _token, uint256 _totalTokens, uint256 _tokenPrice, address _uniswapRouter) {
        TokenData.token = payable(_token);
        TokenData.totalTokens = _totalTokens;
        TokenData.tokenPrice = _tokenPrice;
        
        uniswapV2Router = IUniswapV2Router02(_uniswapRouter);
    }

    function buyTokens() public payable {
        uint256 tokensToBuy = msg.value / TokenData.tokenPrice;

        require(msg.value >= TokenData.tokenPrice, "Must send enough ETH to buy tokens");
        require(TokenData.totalTokens >= tokensToBuy, "Not enough tokens left for sale");

        //send tokens to the user and add the ETH to liquidity
        IERC20(TokenData.token).transfer(msg.sender, tokensToBuy * 1 ether);
        addLiquidityETH(msg.value);

        TokenData.totalTokens -= tokensToBuy;
    }

    function addLiquidityETH(uint256 ethAmount) private  {
        IERC20(TokenData.token).approve(address(uniswapV2Router), IERC20(TokenData.token).balanceOf(address(this)));
        
        uniswapV2Router.addLiquidityETH{ value: ethAmount }(
            TokenData.token,
            IERC20(TokenData.token).balanceOf(address(this)),
            0,
            ethAmount, //take all ETH
            TokenData.token, //LP tokens will be minted to token itself
            block.timestamp
        );
    }

    // function to change token price if needed
    function setTokenPrice( uint256 price ) public onlyOwner {
        TokenData.tokenPrice = price;
    }

    // withdraw functions
    function withdrawToken( uint256 _amount) public onlyOwner {
        require(IERC20(TokenData.token).transfer(msg.sender, _amount), "transfer is failed");
    }

    function withdrawETH(uint256 _amount) public onlyOwner {
        (bool sent, ) = payable(msg.sender).call{value: (_amount)}("");
        require(sent, "transfer is failed");
    }
}

