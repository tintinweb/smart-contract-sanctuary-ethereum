pragma solidity ^0.8.18;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

contract KSService is Ownable {
    using Counters for Counters.Counter;
    Counters.Counter private _jobIds;
    Counters.Counter private _jobSold;

    IERC20 public tokenUSDT;

    mapping(uint256 => ListJobInfo) private jobInfo;
    mapping(uint256 => ConfigKS) private ksInfo;

    struct ListJobInfo {
        uint256 jobID;
        uint256 workerReceive;
        uint256 priceBeforeFee;
        uint256 priceAfterFee;
        uint256 typeJob;
        uint256 feeBuyer;
        uint256 feeSeller;
        uint256 estimate;
        uint256 createDate;
        address owner;
        address worker;
        string jobName;
        string timeStar;
        bool ownerConfirm;
        bool ownerComplaint;
        bool adminConfirm;
        bool status;
    }

    struct ConfigKS {
        uint256 configID;
        uint256 rate;
        uint256 feeSystem;
    }

    constructor(IERC20 _tokenUSDT) {
        tokenUSDT = _tokenUSDT;
    }

    function CreateJob(
        address applyAddress,
        string memory jobName,
        uint256 amountAfter,
        uint256 amountBefore,
        string memory timeStar,
        uint256 estimate,
        uint256 typeJob
    ) public returns (string memory) {
        _jobIds.increment();
        uint256 _newJobId = _jobIds.current();
        address _addressKS = address(this);
        address _addressPoster = msg.sender;
        string memory _jobName = jobName;
        uint256 _indexCur = 1;
        uint256 _typeJob = typeJob;
        uint256 _rate = ksInfo[_indexCur].rate;
        uint256 _feeSystem = ksInfo[_indexCur].feeSystem;
        string memory _timeStar = timeStar;
        uint256 _estimate = estimate;
        address _addressApply = applyAddress;
        uint256 _total = amountAfter;
        uint256 _priceBeforeFee = amountBefore;
        uint256 perWorker = 100 - _feeSystem;
        uint256 percents = 100;

        require((_priceBeforeFee + ((_priceBeforeFee * _rate) / 100)) >= _total, "Please input the price correct");

        require(_rate > 0, "Please config fee owner system");
        require(_feeSystem > 0, "Please config fee worker system");
        uint256 allowance = tokenUSDT.allowance(_addressPoster, _addressKS);
        require( allowance >= _total, "Please check the token allowance to this KICKSTAR contract");


        tokenUSDT.transferFrom(_addressPoster, _addressKS, _total);
        
        jobInfo[_newJobId] = ListJobInfo(
            _newJobId,
            (_priceBeforeFee * perWorker) / percents,
            _total - ((_priceBeforeFee * _rate) / 100),
            _total,
            _typeJob,
            _rate,
            _feeSystem,
            _estimate,
            block.timestamp,
            _addressPoster,
            _addressApply,
            _jobName,
            _timeStar,
            false,
            false,
            false,
            true
        );
        return "Post job completed";
    }

    function CreateConfigKS(
        uint256 rate,
        uint256 feeSystem
    ) public onlyOwner returns (string memory) {
        ksInfo[1] = ConfigKS(1, rate, feeSystem);
        return "Config fee KICKSTAR completed";
    }

    function OwnerConfirmJob(uint256 id, bool ownerComplaint) public returns (string memory) {
        uint256 totalItemCount = _jobIds.current();
        uint256 itemCount = 0;
        uint256 currentIndex = 0;

        for (uint256 i = 0; i < totalItemCount; i++) {
            if (jobInfo[i + 1].jobID == id) {
                itemCount++;
            }
        }

        ListJobInfo[] memory items = new ListJobInfo[](itemCount);
        for (uint256 i = 0; i < totalItemCount; i++) {
            uint256 currentId = i + 1;
            if (jobInfo[currentId].jobID == id) {
                ListJobInfo storage currentItem = jobInfo[currentId];
                items[currentIndex] = currentItem;
                currentIndex++;
            }
        }
        require(currentIndex == 1, "Error Contract");
        require(items[0].jobID > 0, "Not Found Job");
        require(items[0].owner == msg.sender, "You not Owner Job");
        require(items[0].status == true, "Job is Closed");
        require(items[0].worker != address(0), "You not Owner Job");

        if(ownerComplaint == true){
            if(items[0].adminConfirm == false){
                jobInfo[id].ownerConfirm = true;
                return "Buyer have to confirm but it have complain";
            }
        }

        tokenUSDT.transfer(items[0].worker, items[0].workerReceive);
        jobInfo[id].ownerConfirm = true;
        jobInfo[id].status = false;

        return "Completed job completed";

    }

    function adminConfirmJob(uint256 id) public onlyOwner returns (string memory) {
        uint256 totalItemCount = _jobIds.current();
        uint256 itemCount = 0;
        uint256 currentIndex = 0;

        for (uint256 i = 0; i < totalItemCount; i++) {
            if (jobInfo[i + 1].jobID == id) {
                itemCount++;
            }
        }

        ListJobInfo[] memory items = new ListJobInfo[](itemCount);
        for (uint256 i = 0; i < totalItemCount; i++) {
            uint256 currentId = i + 1;
            if (jobInfo[currentId].jobID == id) {
                ListJobInfo storage currentItem = jobInfo[currentId];
                items[currentIndex] = currentItem;
                currentIndex++;
            }
        }
        require(currentIndex == 1, "Error Contract");
        require(items[0].jobID > 0, "Not Found Job");
        require(items[0].status == true, "Job is Closed");
        require(items[0].worker != address(0), "You not Owner Job");

        tokenUSDT.transfer(items[0].worker, items[0].workerReceive);
        jobInfo[id].adminConfirm = true;
        if(jobInfo[id].status == true){
            jobInfo[id].status = false;
        }

        return "Completed job completed";

    }

    function ListConfigKS() public view onlyOwner returns (ConfigKS[] memory) {
        uint256 totalItemCount = 1;

        ConfigKS[] memory items = new ConfigKS[](totalItemCount);
        ConfigKS storage currentItem = ksInfo[1];
        items[0] = currentItem;
        return items;
    }

    function FetchAllJobs() public view returns (ListJobInfo[] memory) {
        uint256 totalJobCount = _jobIds.current();

        ListJobInfo[] memory info = new ListJobInfo[](totalJobCount);
        for (uint i = 0; i < totalJobCount; i++) {
            ListJobInfo storage item = jobInfo[i + 1];
            info[i] = item;
        }
        return info;
    }

    function FetchWorkerJobs() public view returns (ListJobInfo[] memory) {
        uint256 totalItemCount = _jobIds.current();
        uint256 itemCount = 0;
        uint256 currentIndex = 0;

        for (uint256 i = 0; i < totalItemCount; i++) {
            if (
                jobInfo[i + 1].status == true &&
                jobInfo[i + 1].worker == msg.sender
            ) {
                itemCount++;
            }
        }

        ListJobInfo[] memory items = new ListJobInfo[](itemCount);
        for (uint256 i = 0; i < totalItemCount; i++) {
            if (jobInfo[i + 1].worker == msg.sender) {
                uint256 currentId = i + 1;
                ListJobInfo storage currentItem = jobInfo[currentId];
                items[currentIndex] = currentItem;
                currentIndex++;
            }
        }
        return items;
    }

    function FetchOwnerJobs() public view returns (ListJobInfo[] memory) {
        uint256 totalItemCount = _jobIds.current();
        uint256 itemCount = 0;
        uint256 currentIndex = 0;

        for (uint256 i = 0; i < totalItemCount; i++) {
            if (
                jobInfo[i + 1].status == true &&
                jobInfo[i + 1].owner == msg.sender
            ) {
                itemCount++;
            }
        }

        ListJobInfo[] memory items = new ListJobInfo[](itemCount);
        for (uint256 i = 0; i < totalItemCount; i++) {
            if (jobInfo[i + 1].owner == msg.sender) {
                uint256 currentId = i + 1;
                ListJobInfo storage currentItem = jobInfo[currentId];
                items[currentIndex] = currentItem;
                currentIndex++;
            }
        }
        return items;
    }

    function FetchCompleteJobs() public view returns (ListJobInfo[] memory) {
        uint256 totalItemCount = _jobIds.current();
        uint256 itemCount = 0;
        uint256 currentIndex = 0;

        for (uint256 i = 0; i < totalItemCount; i++) {
            if (jobInfo[i + 1].status == false) {
                itemCount++;
            }
        }

        ListJobInfo[] memory items = new ListJobInfo[](itemCount);
        for (uint256 i = 0; i < totalItemCount; i++) {
            if (jobInfo[i + 1].status == false) {
                uint256 currentId = i + 1;
                ListJobInfo storage currentItem = jobInfo[currentId];
                items[currentIndex] = currentItem;
                currentIndex++;
            }
        }
        return items;
    }
}

