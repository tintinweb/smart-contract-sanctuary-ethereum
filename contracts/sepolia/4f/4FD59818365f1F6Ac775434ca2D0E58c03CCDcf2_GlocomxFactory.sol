pragma solidity ^0.8.2;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

contract GlocomxNFT is ERC721, AccessControl {
    using Counters for Counters.Counter;
    Counters.Counter private tokenId;
    using SafeERC20 for IERC20;

    event UpdateUser(
        uint256 indexed tokenId,
        address indexed user,
        uint256 start,
        uint256 expires
    );
    event RewardClaimed(uint256 indexed tokenId, uint256 rewardAmount);

    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");
    struct TenentInfo {
        address user; // address of user role
        uint256 start; // rent start time
        uint256 expires; // unix timestamp, user expires
    }
    mapping(uint256 => TenentInfo) internal _tenants;

    struct NFTData {
        uint256 category;
        uint256 rentPrice;
    }
    mapping(uint256 => NFTData) private nftRentInfo;
    mapping(address => mapping(uint256 => uint256)) public rentalRewards;
    mapping(uint256 => uint256) public lastRewardClaimedTime;

    // category => price
    mapping(uint256 => uint256) private rentCategoryPrice;
    mapping(uint256 => uint256) private mintPrice;

    IERC20 public token;
    address public owner;
    address public platformAddress;
    bool public ownerModifyRentStatus;
    bool public feeModification;

    // rental reward fees
    uint256 public platformFee;
    uint256 public ownerFee;
    uint256 constant denominator = 10000;

    constructor(
        string memory name_,
        string memory symbol_,
        address _owner,
        address _platform,
        uint16[2] memory fees,
        IERC20 _token,
        uint256[] memory _categories,
        uint256[] memory _rentPrices,
        uint256[] memory _mintPrices,
        bool _ownerModifyRentStatus,
        bool _feeModification
    ) ERC721(name_, symbol_) {
        require(
            _categories.length == _rentPrices.length,
            "Invalid Length: _rentPrices"
        );
        require(
            _categories.length == _mintPrices.length,
            "Invalid Length: _mintPrices"
        );

        _grantRole(DEFAULT_ADMIN_ROLE, _platform);
        _grantRole(OWNER_ROLE, _owner);
        owner = _owner;
        platformAddress = _platform;
        tokenId.increment();
        token = _token;

        platformFee = fees[0];
        ownerFee = fees[1];
        for (uint256 indx = 0; indx < _categories.length; indx++) {
            rentCategoryPrice[_categories[indx]] = _rentPrices[indx];
            mintPrice[_categories[indx]] = _mintPrices[indx];
        }
        ownerModifyRentStatus = _ownerModifyRentStatus;
        feeModification = _feeModification;
    }

    modifier setRentPrice(uint256 _tokenId) {
        require(feeModification, "only owner can set fees");
        require(
            hasRole(OWNER_ROLE, msg.sender) || msg.sender == ownerOf(_tokenId),
            "not authorized"
        );
        _;
    }

    // hotel owner can set the data
    function setNftRent(
        uint256 _tokenId,
        uint256 _rentPrice
    ) external setRentPrice(_tokenId) {
        nftRentInfo[_tokenId].rentPrice = _rentPrice;
    }

    function setCategoryRent(
        uint256 _cat,
        uint256 _price
    ) external onlyRole(OWNER_ROLE) {
        rentCategoryPrice[_cat] = _price;
    }

    function getRentPrice(uint256 _tokenId) public view returns (uint256) {
        uint256 rentPrice;
        if (feeModification) {
            rentPrice = nftRentInfo[_tokenId].rentPrice;
        } else {
            uint256 _cat = nftRentInfo[_tokenId].category;
            rentPrice = rentCategoryPrice[_cat];
        }
        return rentPrice;
    }

    function mint(address _user, uint256 _category) external {
        uint256 id = tokenId.current();
        nftRentInfo[id].category = _category;
        nftRentInfo[id].rentPrice = rentCategoryPrice[_category];
        uint256 mintFee = mintPrice[_category];
        SafeERC20.safeTransferFrom(token, _user, owner, mintFee);
        _mint(_user, id);
        tokenId.increment();
    }

    function setUser(
        address _user,
        uint256 _tokenId,
        uint256 _duration,
        uint256 _price,
        bytes memory _signature
    ) public {
        require(userOf(_tokenId) == address(0), "Tenant already exists");

        if (ownerModifyRentStatus) {
            require(
                verify(_user, _tokenId, _duration, _price, _signature),
                "Invalid Signature"
            );
        }

        uint256 totalRentPrice = getRentPrice(_tokenId) * _duration;
        require(totalRentPrice > 0, "zero rent price");
        SafeERC20.safeTransferFrom(token, _user, address(this), totalRentPrice);
        TenentInfo storage info = _tenants[_tokenId];
        info.user = _user;
        info.start = block.timestamp;
        info.expires = info.start + _duration;

        emit UpdateUser(_tokenId, _user, block.timestamp, _duration);
    }

    // claim pending rewards
    function claimRent(uint256 _tokenId) public {
        require(
            lastRewardClaimedTime[_tokenId] <= _tenants[_tokenId].expires,
            "all claimed"
        );
        (uint256 currentRentPrice, uint256 timeNow) = calculateRewards(
            _tokenId
        );
        require(currentRentPrice > 0, "zero reward");
        lastRewardClaimedTime[_tokenId] = timeNow;
        (uint256 platformFeeAmount, uint256 ownerFeeAmount) = calculateFees(
            currentRentPrice
        );
        if (platformFeeAmount > 0) {
            SafeERC20.safeTransfer(token, platformAddress, platformFeeAmount);
        }

        if (ownerFeeAmount > 0) {
            SafeERC20.safeTransfer(token, owner, ownerFeeAmount);
        }

        uint256 nftHolderShare = currentRentPrice -
            platformFeeAmount -
            ownerFeeAmount;
        address currentOwner = ownerOf(_tokenId);
        SafeERC20.safeTransfer(token, currentOwner, nftHolderShare);
        emit RewardClaimed(_tokenId, currentRentPrice);
    }

    function calculateRewards(
        uint256 _tokenId
    ) public view returns (uint256, uint256) {
        uint256 lastClaimTime;
        if (lastRewardClaimedTime[_tokenId] == 0) {
            lastClaimTime = _tenants[_tokenId].start;
        } else {
            lastClaimTime = lastRewardClaimedTime[_tokenId];
        }
        uint256 currentTime;
        if (block.timestamp < _tenants[_tokenId].expires) {
            currentTime = block.timestamp;
        } else {
            currentTime = _tenants[_tokenId].expires;
        }
        uint256 rentDurationPassed = currentTime - lastClaimTime;
        return (getRentPrice(_tokenId) * rentDurationPassed, currentTime);
    }

    function calculateFees(
        uint256 amount
    ) private view returns (uint256, uint256) {
        uint256 platformFees = (amount * platformFee) / denominator;
        uint256 ownerFees = (amount * ownerFee) / denominator;

        return (platformFees, ownerFees);
    }

    /// @notice Get the user address of an NFT
    /// @dev The zero address indicates that there is no user or the user is expired
    /// @param _tokenId The NFT to get the user address for
    /// @return The user address for this NFT
    function userOf(uint256 _tokenId) public view returns (address) {
        if (uint256(_tenants[_tokenId].expires) >= block.timestamp) {
            return _tenants[_tokenId].user;
        } else {
            return address(0);
        }
    }

    /// @notice Get the user expires of an NFT
    /// @dev The zero value indicates that there is no user
    /// @param _tokenId The NFT to get the user expires for
    /// @return The user expires for this NFT
    function userExpires(
        uint256 _tokenId
    ) public view virtual returns (uint256) {
        return _tenants[_tokenId].expires;
    }

    function updateFeeSetStatus() external onlyRole(OWNER_ROLE) {
        feeModification = !feeModification;
    }

    // If NFT got transferred, implement a method in which
    // reward distributed to older holder instantly or it
    // should save the time duration for old holder and next time
    // to new holder
    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _tokenId,
        uint256 _batchSize
    ) internal virtual override {
        if (
            _from != _to &&
            _tenants[_tokenId].user != address(0) &&
            userOf(_tokenId) == address(0)
        ) {
            delete _tenants[_tokenId];
            emit UpdateUser(_tokenId, _tenants[_tokenId].user, 0, 0);
        } else if (_from != address(0)) {
            claimRent(_tokenId);
        }
        super._beforeTokenTransfer(_from, _to, _tokenId, _batchSize);
    }

    function getMessageHash(
        address _user,
        uint256 _id,
        uint256 _duration,
        uint256 _price
    ) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(_user, _id, _duration, _price));
    }

    function getEthSignedMessageHash(
        bytes32 _messageHash
    ) public pure returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    "\x19Ethereum Signed Message:\n32",
                    _messageHash
                )
            );
    }

    function verify(
        address _signer,
        uint256 _id,
        uint256 _duration,
        uint256 _price,
        bytes memory signature
    ) public pure returns (bool) {
        bytes32 messageHash = getMessageHash(_signer, _id, _duration, _price);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);
        return recoverSigner(ethSignedMessageHash, signature) == _signer;
    }

    function recoverSigner(
        bytes32 _ethSignedMessageHash,
        bytes memory _signature
    ) public pure returns (address) {
        (bytes32 r, bytes32 s, uint8 v) = splitSignature(_signature);
        return ecrecover(_ethSignedMessageHash, v, r, s);
    }

    function splitSignature(
        bytes memory sig
    ) public pure returns (bytes32 r, bytes32 s, uint8 v) {
        require(sig.length == 65, "invalid signature length");
        assembly {
            r := mload(add(sig, 32))
            s := mload(add(sig, 64))
            v := byte(0, mload(add(sig, 96)))
        }
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view override(ERC721, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}

