pragma solidity ^0.8.10;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/SignatureCheckerUpgradeable.sol";
import "./GlocomxNFT.sol";

contract GlocomxFactory is Initializable,UUPSUpgradeable,OwnableUpgradeable, AccessControlUpgradeable, ReentrancyGuardUpgradeable  {
    using SignatureCheckerUpgradeable for bytes32;
    
    //Events
    event ContractDeployed( address contractAddress, address owner);
    event SetPaymentToken( address account, uint256 paymentIndex, IERC20 paymentToken);
    event SetSignerAddress( address account, address signerAddress);
    event UpdatePlatformAddress(address account, address platformAddress);
    event SetPlatformFee(address account, uint256 newFee);
    event UpdateDeploymentFeeAddress(address account , address newFeeAddress);
    event UpdateDeploymentFee(address account, uint256 newDeploymentFee);

    struct deployPoolParams {
    string name;
    string symbol;
    uint256[] categories;
    uint256[] rentPrices;
    uint256[] mintPrices;
    uint256 sigExpiryTime;
    uint16 paymentIndex;
    uint16 ownerFee;
    bool ownerModifyRentStatus;
    bool feeModification;
    bytes signature;
}

    bytes32 public constant DEPLOYER_ROLE = keccak256("DEPLOYER_ROLE");
    address public platformAddress;
    address public signerAddress;
    address public deploymentFeeAddress;
    mapping(uint16 => IERC20) private paymentToken;
    mapping(bytes => bool) signatureUsed;
    uint16 platformFee;
    uint256 public deploymentFee;

    function initialize(address _platformAddress) initializer public  {
        __Ownable_init();
        __UUPSUpgradeable_init();
      platformAddress = _platformAddress;
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        platformAddress = _platformAddress;
        _grantRole(DEPLOYER_ROLE, _platformAddress);
        deploymentFee = 0.0001 ether;
     }
    
    function _authorizeUpgrade(address newImplementation)
        internal
        onlyOwner
        override
    {}

    function deployPool(
       deployPoolParams memory params
    ) payable external nonReentrant {
        _IsNotZeroAdd(msg.sender);
        _validateInputs(params);
        require(block.timestamp <= params.sigExpiryTime, "Sig expired");
        accessChecker(msg.sender, params.sigExpiryTime, params.signature);
        require(!signatureUsed[params.signature], "sig already used");
        require(msg.value == deploymentFee, "Invalid Deployment Fee");

        (bool success,) = payable(deploymentFeeAddress).call{value: msg.value}("");
        require(success, "Fee not Sent");

        IERC20 payment = getPaymentToken(params.paymentIndex);
        GlocomxNFT newGlocomx = new GlocomxNFT(
            params.name,
            params.symbol,
            msg.sender,
            platformAddress,
            [platformFee, params.ownerFee],
            payment,
            params.categories,
            params.rentPrices,
            params.mintPrices,
            params.ownerModifyRentStatus,
            params.feeModification
        );
        signatureUsed[params.signature] = true;
        emit ContractDeployed(address(newGlocomx), msg.sender);
    }

    // add the signature which consists of user address, appoval time, expiry time
    // if signature is valid, then mark the signature as true, else return the error
    // signature should not be expired
    // add the contract deployment fee which should be payed in native currency (ETH, BNB etc)

    function accessChecker(
        address user,
        uint256 expiryTime,
        bytes memory signature
    ) internal view {
        require(
            SignatureCheckerUpgradeable.isValidSignatureNow(
                signerAddress,
                getEthSignedHash(getHash(user, expiryTime)),
                signature
            ),
            "signature not match"
        );
    }

    function _IsNotZeroAdd(address _add) internal pure {
        require(_add != address(0), "zero add");
    }

    function setPaymentToken(
        uint16 _indx,
        IERC20 _token
    ) external onlyRole(DEPLOYER_ROLE) {
        _IsNotZeroAdd(address(_token));
        paymentToken[_indx] = _token;
        emit SetPaymentToken(msg.sender, _indx, _token);
    }

    function setSignerAddress(address _signer) external onlyRole(DEPLOYER_ROLE) {
        _IsNotZeroAdd(address(_signer));
        signerAddress = _signer;
        emit SetSignerAddress( msg.sender, _signer);
    }

    function getPaymentToken(uint16 _indx) public view returns (IERC20) {
        return paymentToken[_indx];
    }

    function updatePlatformAddress(
        address _newAddress
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _IsNotZeroAdd(_newAddress);
        platformAddress = _newAddress;
        emit UpdatePlatformAddress(msg.sender, _newAddress);
    }

    function setPlatformFee(
        uint16 _newFee
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        platformFee = _newFee;
        emit SetPlatformFee(msg.sender, _newFee);
    }

    function updateDeploymentFeeAddress(address _newFeeAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _IsNotZeroAdd(_newFeeAddress);
        deploymentFeeAddress = _newFeeAddress;
        emit UpdateDeploymentFeeAddress(msg.sender, _newFeeAddress);
    }

    function updateDeploymentFee(uint256 _newFee) external onlyRole(DEFAULT_ADMIN_ROLE) {
        deploymentFee = _newFee;
    }

    
        // take the keccak256 hashed message from the getHash function above and input into this function
    // this function prefixes the hash above with \x19Ethereum signed message:\n32 + hash
    // and produces a new hash signature
    function getEthSignedHash(bytes32 _messageHash)
        public
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encodePacked(
                    "\x19Ethereum Signed Message:\n32",
                    _messageHash
                )
            );
    }

    // use this function to get the hash of any string
    function getHash(
        address user,
        uint256 expiryTime
    ) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(user, expiryTime));
    }

    //private functions 
        function _validateInputs(deployPoolParams memory params) private pure {
        require(bytes(params.name).length > 0, "Invalid name");
        require(bytes(params.symbol).length > 0, "Invalid symbol");
        require(params.categories.length > 0, "No categories specified");
        require(params.rentPrices.length > 0, "No rent prices specified");
        require(params.mintPrices.length > 0, "No mint prices specified");
        require(params.categories.length == params.rentPrices.length, "Mismatched categories and rent prices");
        require(params.rentPrices.length == params.mintPrices.length, "Mismatched rent pricess and mint prices");
    }
}

