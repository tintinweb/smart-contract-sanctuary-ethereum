contract StakingStorage is AddressHelper {
  uint internal _premiumMinUpdateInterval;

  // asset => RewardInfo
  mapping(address => PoolInfo) internal _poolInfos;
  // staker => asset => RewardInfo
  mapping(address => mapping(address => RewardInfo)) internal _shortRewardInfos;

  function _readRewardInfo(
    address staker,
    address asset
  ) internal view returns (RewardInfoResponseItem memory) {
    RewardInfo memory rewardInfo = _shortRewardInfos[staker][asset];
    if (_isEmptyReward(rewardInfo)) return RewardInfoResponseItem(address(0), 0, 0);
    PoolInfo memory poolInfo = _poolInfos[asset];
    decimal memory poolIndex = poolInfo.shortRewardIndex;
    _beforeShareChange(poolIndex, rewardInfo);
    return RewardInfoResponseItem(asset, rewardInfo.bondAmount, rewardInfo.pendingReward);
  }

  // function _readUserRewardInfos(
  //   address staker,
  //   address asset
  // ) internal view returns (RewardInfoResponseItem[] memory) {
  //   mapping(address => RewardInfo) userRewardInfos = _shortRewardInfos[staker];
  //   // TODO: import listed assrt list.
  // }

  function _isEmptyReward(RewardInfo memory rewardInfo) internal pure returns (bool) {
    return
      rewardInfo.index.value == 0 && rewardInfo.bondAmount == 0 && rewardInfo.pendingReward == 0;
  }

  function _increaseBondAmount(address staker, address asset, uint amount) internal {
    PoolInfo memory poolInfo = _poolInfos[asset];
    RewardInfo memory rewardInfo = _shortRewardInfos[staker][asset];

    decimal memory poolIndex = poolInfo.shortRewardIndex;

    _beforeShareChange(poolIndex, rewardInfo);

    poolInfo.totalShortAmount += amount;
    rewardInfo.bondAmount += amount;
    _shortRewardInfos[staker][asset] = rewardInfo;
    _poolInfos[asset] = poolInfo;
  }

  function _decreaseBondAmount(address staker, address asset, uint amount) internal {
    PoolInfo memory poolInfo = _poolInfos[asset];
    RewardInfo memory rewardInfo = _shortRewardInfos[staker][asset];
    require(rewardInfo.bondAmount >= amount, "Cannot unbond more than bond amount");

    decimal memory poolIndex;
    {
      poolIndex = poolInfo.shortRewardIndex;
    }

    _beforeShareChange(poolIndex, rewardInfo);

    // Decrease total short or bond amount
    poolInfo.totalShortAmount = poolInfo.totalShortAmount - amount;

    // Update rewards info
    rewardInfo.bondAmount = rewardInfo.bondAmount - amount;

    if (rewardInfo.pendingReward == 0 && rewardInfo.bondAmount == 0) {
      delete _shortRewardInfos[staker][asset];
    } else {
      _shortRewardInfos[staker][asset] = rewardInfo;
    }
    // Update pool info
    _poolInfos[asset] = poolInfo;
  }

  function _withdrawReward(address staker, address asset) internal returns (uint amount) {
    RewardInfo memory rewardInfo = _shortRewardInfos[staker][asset];
    _beforeShareChange(_poolInfos[asset].shortRewardIndex, rewardInfo);

    amount = rewardInfo.pendingReward;

    rewardInfo.pendingReward = 0;
    if (rewardInfo.bondAmount == 0) {
      delete _shortRewardInfos[staker][asset];
    } else {
      _shortRewardInfos[staker][asset] = rewardInfo;
    }
  }

  // Withdraw reward to pending reward; before changing share
  function _beforeShareChange(decimal memory poolIdx, RewardInfo memory rewardInfo) internal pure {
    uint pendingReward = Decimal.toUint(
      Decimal.mulUint(Decimal.sub(poolIdx, rewardInfo.index), rewardInfo.bondAmount)
    );
    rewardInfo.index = poolIdx;
    rewardInfo.pendingReward += pendingReward;
  }

  // REVIEW:
  function _computeShortRewardWeight(
    decimal memory premiumRate
  ) internal pure returns (decimal memory) {
    // REVIEW:
    if (Decimal.gt(premiumRate, Decimal.permilleToDecimal(70))) {
      return Decimal.permilleToDecimal(800);
    }

    decimal memory two = Decimal.mulUint(Decimal.one(), 2);
    decimal memory sqrtTwo = Decimal.fromRatio(14_142_135_624, 10_000_000_000);

    bool isPlus;
    decimal memory x;
    decimal memory p = Decimal.mulUint(premiumRate, 100);
    if (Decimal.gt(p, two)) {
      isPlus = true;
      x = Decimal.div(Decimal.sub(p, two), sqrtTwo);
    } else {
      isPlus = false;
      x = Decimal.div(Decimal.sub(two, p), sqrtTwo);
    }

    decimal memory shortRewardWeight = Decimal.div(
      Decimal.erfPlusOne(isPlus, x),
      Decimal.fromRatio(5, 2)
    );

    return shortRewardWeight;
  }
}

