pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "../interface/IStaking.sol";
import "./StakingStorage.sol";
import "../interface/IDexManager.sol";
import "../interface/IPriceOracle.sol";
import "../interface/IAddressMap.sol";
import "../library/AddressHandler.sol";
import "../library/Constant.sol";
import "hardhat/console.sol";

contract Staking is IStaking, StakingStorage, AddressHandler, Ownable {
  using SafeERC20 for IERC20;

  constructor(
    address addressMap,
    uint premiumMinUpdateInterval
  ) AddressHandler(IAddressMap(addressMap)) {
    _premiumMinUpdateInterval = premiumMinUpdateInterval;
  }

  function updateInterval(uint premiumMinUpdateInterval) external onlyOwner {
    require(premiumMinUpdateInterval > 0);
    _premiumMinUpdateInterval = premiumMinUpdateInterval;
    emit UpdateInterval(premiumMinUpdateInterval);
  }

  function registerAsset(address asset) external onlyRole(ROLE_FACTORY) {
    PoolInfo memory poolInfo = _poolInfos[asset];
    require(poolInfo.asset == address(0), "Asset was already registered");

    poolInfo.asset = asset;
    _poolInfos[asset] = poolInfo;

    emit RegisterAsset(asset);
  }

  function increaseShortToken(
    address user,
    address asset,
    uint amount
  ) external onlyRole(ROLE_LENDING) {
    _increaseBondAmount(user, asset, amount);

    emit IncreaseShortToken(user, asset, amount);
  }

  function decreaseShortToken(
    address user,
    address asset,
    uint amount
  ) external onlyRole(ROLE_LENDING) {
    _decreaseBondAmount(user, asset, amount);

    emit DecreaseShortToken(user, asset, amount);
  }

  function depositReward(Reward[] memory rewards) external onlyRole(ROLE_FACTORY) {
    uint amount;
    for (uint i = 0; i < rewards.length; i++) {
      Reward memory reward = rewards[i];
      PoolInfo memory poolInfo = _poolInfos[reward.asset];

      uint shortReward = reward.amount;

      if (poolInfo.totalShortAmount == 0) {
        poolInfo.shortPendingReward += shortReward;
      } else {
        shortReward += poolInfo.shortPendingReward;
        decimal memory shortRewardPerBond = Decimal.fromRatio(
          shortReward,
          poolInfo.totalShortAmount
        );
        poolInfo.shortRewardIndex = Decimal.add(poolInfo.shortRewardIndex, shortRewardPerBond);
        poolInfo.shortPendingReward = 0;
      }

      _poolInfos[reward.asset] = poolInfo;
      amount += reward.amount;
    }

    emit DepositReward(amount);
  }

  function withdrawReward(address asset) external {
    uint amount = _withdrawReward(msg.sender, asset);
    IERC20(lookup(TOKEN_ZDXT)).safeTransfer(msg.sender, amount);

    emit WithdrawReward(asset, msg.sender, amount);
  }

  // NOTE: Changes the price premium rate for a specified zAsset, can be done by anyone.
  function adjustPremium(address[] memory assetList) external {
    for (uint i = 0; i < assetList.length; i++) {
      PoolInfo memory poolInfo = _poolInfos[assetList[i]];
      if (poolInfo.asset != address(0)) {
        require(
          block.timestamp >= poolInfo.premiumUpdatedTime + _premiumMinUpdateInterval,
          "Cannot adjust premium before interval passed"
        );

        (decimal memory premiumRate, bool noPriceFeed) = _computePremiumRate(assetList[i]);
        decimal memory shortRewardWeight;
        {
          if (noPriceFeed) {
            shortRewardWeight = Decimal.zero();
          } else {
            shortRewardWeight = _computeShortRewardWeight(premiumRate);
          }
        }

        poolInfo.premiumRate = premiumRate;
        poolInfo.shortRewardWeight = shortRewardWeight;
        poolInfo.premiumUpdatedTime = block.timestamp;

        _poolInfos[assetList[i]] = poolInfo;

        emit AdjustPremium(assetList[i], block.timestamp);
      }
    }
  }

  function _computePremiumRate(address asset) private view returns (decimal memory, bool) {
    address base = lookup(TOKEN_DAI);
    bool isFlipped = asset < base;
    (address token0, address token1) = isFlipped ? (asset, base) : (base, asset);

    (uint price0, ) = IPriceOracle(lookup(ROLE_PRICE_ORACLE)).queryAssetPrice(
      token0,
      DEFAULT_TIME_FRAME
    );
    (uint price1, ) = IPriceOracle(lookup(ROLE_PRICE_ORACLE)).queryAssetPrice(
      token1,
      DEFAULT_TIME_FRAME
    );

    decimal memory oraclePrice = Decimal.fromRatio(price1, price0);
    decimal memory poolPrice = IDexManager(lookup(ROLE_DEX_MANAGER)).queryPoolPrice(asset);

    if (Decimal.isZero(oraclePrice)) {
      return (Decimal.zero(), true);
    } else if (isFlipped && Decimal.gt(oraclePrice, poolPrice)) {
      return (Decimal.div(Decimal.sub(oraclePrice, poolPrice), poolPrice), false);
    } else if (!isFlipped && Decimal.gt(poolPrice, oraclePrice)) {
      return (Decimal.div(Decimal.sub(poolPrice, oraclePrice), oraclePrice), false);
    } else {
      return (Decimal.zero(), false);
    }
  }

  function queryPoolInfo(address asset) external view returns (PoolInfo memory) {
    return _poolInfos[asset];
  }

  function queryRewardInfo(
    address owner,
    address asset
  ) external view returns (RewardInfoResponseItem memory) {
    return _readRewardInfo(owner, asset);
  }
}

