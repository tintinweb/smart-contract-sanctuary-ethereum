import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "./base/Dispatcher.sol";
import "./libraries/SafeTransferLib.sol";

pragma solidity ^0.8.15;

contract Executor is
    Dispatcher,
    ReentrancyGuardUpgradeable,
    AccessControlUpgradeable
{
    using SafeTransferLib for address;
    uint256 public swapFee; // In basis points (e.g., 100 basis points = 1%)
    address payable public swapFeeRecipient;
    address public implementation;
    function initialize(
        ExecutorParameters memory params,
        uint256 _swapFee,
        address payable _swapFeeRecipient
    ) public initializer {
        initializeParameters(params);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        swapFee = _swapFee;
        swapFeeRecipient = _swapFeeRecipient;
    }

    receive() external payable {}

    /**
     * @dev Sets new swap fee percentage in basic points (can only be called by the admin role owner).
     * @param _newSwapFee Fee percentage in basic points.
     */
    function changeSwapFee(
        uint256 _newSwapFee
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_newSwapFee <= 10000, "Executor: Invalid swap fee");
        swapFee = _newSwapFee;
    }

    /**
     * @dev Sets new swap fee recipient (can only be called by the admin role owner).
     * @param _newSwapFeeRecipient Fee recipient address.
     */
    function changeFeeRecipient(
        address payable _newSwapFeeRecipient
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        swapFeeRecipient = _newSwapFeeRecipient;
    }

    /**
     * @dev Withdraws ERC20 tokens from the contract and transfer them to a specified recipient.
     * @param token The address of the ERC20 token contract.
     * @param recipient The address of the recipient who will receive the withdrawn tokens.
     */
    function withdrawERC20Token(
        address token,
        address recipient
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 balanceToken = IERC20(token).balanceOf(address(this));
        require(balanceToken >= 0, "Executor: Insufficient token balance");
        IERC20(token).transfer(recipient, balanceToken);
    }

    /**
     * @dev Withdraws Ether (ETH) from the contract and transfer them to a specified recipient.
     * @param recipient The address of the recipient who will receive the withdrawn tokens.
     */
    function withdrawETHToken(
        address recipient
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint balance = address(this).balance;
        require(balance >= 0, "Executor: Insufficient eth token balance");
        recipient.safeTransferETH(balance);
    }

    /**
     * @dev Executes a series of token swaps using various DeFi protocols.
     * @param commands The list of commands to execute.
     * @param swapData The input data for each command.
     * @param inputToken Token address that will be used for fees.
     * @param inputAmount Whole amount of tokens user will spend on this swap.
     * @param deadline The timestamp after which the transaction should be reverted.
     */
    function executeSwap(
        bytes calldata commands,
        bytes[] calldata swapData,
        address inputToken,
        uint256 inputAmount,
        uint256 deadline
    ) external payable nonReentrant {
        require(
            block.timestamp <= deadline,
            "Executor: Transaction deadline is passed"
        );
        uint256 numCommands = commands.length;
        require(swapData.length == numCommands, "Executor: Length mismatch");
        bool success;
        bool isFeePaid;
        bytes memory result;
        uint256 currentAmount = inputAmount;
        // loop through all given commands, execute them and pass along outputs as defined
        for (uint256 i = 0; i < numCommands; i++) {
            if (commands[i] < 0x06 && !isFeePaid && swapFee != 0) {
                isFeePaid = true;
                uint256 feeAmount = (inputAmount * swapFee) / 10000;
                IERC20(inputToken).transfer(swapFeeRecipient, feeAmount);
            }

            (success, result, currentAmount) = dispatch(
                commands[i],
                swapData[i],
                currentAmount
            );

            if (!success) {
                // Next 5 lines from https://ethereum.stackexchange.com/a/83577
                if (result.length < 68)
                    revert("Executor: Transaction reverted silently");
                assembly {
                    result := add(result, 0x04)
                }
                revert(abi.decode(result, (string)));
            }
        }
    }
}

