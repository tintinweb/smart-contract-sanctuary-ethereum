     * @param inputs The input data for the transferFrom operation, containing the token address, the sender address, the recipient address, and the amount to transfer.
     */
    function transferFrom(
        bytes memory inputs,
        uint256 currentAmount
    ) internal returns (uint256) {
        (address token, address from, address to, uint256 amount) = abi.decode(
            inputs,
            (address, address, address, uint256)
        );
        uint256 transferAmount = currentAmount < amount
            ? currentAmount
            : amount;
        require(from == msg.sender, "Dispatcher: Unauthorized user");
        require(token != Constants.ETH, "Dispatcher: Token is eth");
        IERC20(token).transferFrom(from, to, transferAmount);
        return transferAmount;
    }

    /**
     * @dev Executes a token swap using X528 Limit Protocol.
     * @param inputs The input data for the swap.
     */
    function x528LimitExecute(bytes memory inputs) internal returns (uint256) {
        IX528LimitOffer.SwapOptions memory swapParams = abi.decode(
            inputs,
            (IX528LimitOffer.SwapOptions)
        );
        uint256 returnAmount = x528LimitOffer.swap(swapParams);
        return returnAmount;
    }
}

