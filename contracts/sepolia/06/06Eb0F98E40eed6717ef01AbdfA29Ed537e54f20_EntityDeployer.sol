pragma solidity ^0.8.16;
import { BeaconProxy } from "openzeppelin-contracts/contracts/proxy/beacon/BeaconProxy.sol";
import { ReentrancyGuard } from "openzeppelin-contracts/contracts/security/ReentrancyGuard.sol";
import { IERC20 } from "openzeppelin-contracts/contracts/token/ERC20/IERC20.sol";

import "../lib/Constant.sol";
import "../interfaces/ICommodity.sol";
import "../interfaces/IMiddleware.sol";

library CommodityFeature {
    
    event CollectedCommodityEvent(   
        uint256 indexed entityId,
        uint256 indexed id,
        address indexed user
    );

    event RegisterCommodityEvent (
        uint256 indexed entityId,
        uint256 indexed id,
        string name,
        string symbol,
        address addr,
        address indexed owner
    );

    event DeployedCommodityEvent (
        uint256 indexed entityId,
        uint256 indexed id,
        address indexed commodityAddr
    );

    function _deployCommodityNFT(
        address beacon,
        uint256 entityId, 
        uint256 commodityId,
        Types.RegisterNFTParams memory nftParams,
        mapping(uint256 => mapping(uint256 => Types.Commodity)) storage entityCommodityMapping) private returns(address) {
        bytes memory initData = abi.encodeWithSelector(
            ICommodity.initialize.selector,
            nftParams
        );
        address commodityNFT = address(
            new BeaconProxy{ salt: bytes32(entityId) }(beacon, initData)
        );
        entityCommodityMapping[entityId][commodityId].commodityNFT = commodityNFT;
        emit DeployedCommodityEvent(entityId, commodityId, commodityNFT);
        return commodityNFT;
    }

    function registerCommodity(
        address beacon, 
        Types.RegisterNFTParams memory nftParams,
        bytes calldata data, 
        mapping(uint256 => Types.Entity) storage entitiesById,
        mapping(uint256 => mapping(uint256 => Types.Commodity)) storage entityCommodityMapping) internal returns(uint256, address) {        
        uint256 id = ++entitiesById[nftParams.entityId].totalCommodities;
        entityCommodityMapping[nftParams.entityId][id].name = nftParams.name;
        entityCommodityMapping[nftParams.entityId][id].symbol = nftParams.symbol;
        entityCommodityMapping[nftParams.entityId][id].owner = nftParams.owner;
        entityCommodityMapping[nftParams.entityId][id].middleware = nftParams.nftMw;
        if (nftParams.nftMw != address(0)) {
            // config the metadata
            IMintingMiddleware(nftParams.nftMw).setMwData(nftParams.entityId, id, data);
        }
        address commodity = _deployCommodityNFT(beacon, nftParams.entityId, id, nftParams, entityCommodityMapping);
        return (id, commodity);
    }

    function collect(
                    Types.CollectNFTParams memory params,
                    bytes calldata data, 
                    mapping(uint256 => mapping(uint256 => Types.Commodity)) storage entityCommodityMapping) internal {
        require(entityCommodityMapping[params.entityId][params.nftId].commodityNFT != address(0), "NO_COMMODITY_AVAILABLE");
        address mw = entityCommodityMapping[params.entityId][params.nftId].middleware;
        if (mw != address(0) ) {
            IMintingMiddleware(mw).preProcess{value: msg.value}(params, data);
        }
        ICommodity(entityCommodityMapping[params.entityId][params.nftId].commodityNFT).mint(params.to, params.nftId, params.quantity, "");
        if (mw != address(0) ) {
            IMintingMiddleware(mw).postProcess(params, data);
        }
        emit CollectedCommodityEvent(params.entityId, params.nftId, params.to);
    }
}

