pragma solidity 0.8.16;

import {IEntityDeployer} from "../interfaces/IEntityDeployer.sol";
import {EntityNFT} from "../core/EntityNFT.sol";
import {Types} from  "../lib/Types.sol";

contract EntityDeployer is IEntityDeployer {
    Types.EntityDeployerParams public override entityParams;
    
    function deployEntity(
        bytes32 salt,
        address assetBeacon,
        address commodityBeacon
    ) external returns (address addr) {
        entityParams.assetBeacon = assetBeacon;
        entityParams.commodityBeacon = commodityBeacon;
        addr = address(new EntityNFT{ salt: salt }());
        delete entityParams;   
    }

    function version() external pure returns(uint256) {
        return 6;
    }
}

