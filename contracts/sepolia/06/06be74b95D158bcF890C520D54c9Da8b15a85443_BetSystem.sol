import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {SignatureChecker} from "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";
import {ECDSA} from "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import {AggregatorV3Interface} from "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import {IBetSystem} from "./IBetSystem.sol";

import "hardhat/console.sol";

// TODO
// - Add new event parameters as required
// - struct packing
// - Ensure cannot rejoin an activated bet => activated via either method
// - Ensure withdrawal functionality - no counterparty joined yet
// - Put transfer in an inernal function to save gas
// - Rename to bet system

// 0x867e0ce94d5F949a98d546Fa42a05b840C62E374

contract BetSystem is IBetSystem {
    // ************************************ STATE VARIABLES ************************************

    // Only USDC is bet currency => can use immutable to save gas
    IERC20 public immutable USDC;

    // Assume BTC/USD price aggregator is immutable to save gas
    AggregatorV3Interface public immutable priceAggregator;

    // Incremented counter used to generate bet IDs
    uint256 internal betId;

    // Mapping of bet IDs to Bet structs
    mapping(uint256 => Bet) public bets;

    // ************************************ CONSTRUCTOR ************************************

    constructor(address _USDCAddress, address _priceAggregatorAddress) {
        USDC = IERC20(_USDCAddress);
        priceAggregator = AggregatorV3Interface(_priceAggregatorAddress);
    }

    // ************************************ STATE-CHANGING FUNCTIONS ************************************

    // *** BET CREATION FUNCTIONS ***

    function openBet(
        Side _side,
        uint256 _amount,
        uint256 _timeTillExpiry,
        uint256 _closingTime
    ) external {
        // Transfer USDC from bet initiator to this contract to be held in escrow
        // Will revert if fails so no need to check returned boolean value
        USDC.transferFrom(msg.sender, address(this), _amount);

        // Cache from storage
        uint256 betId_ = betId;

        Bet storage newBet = bets[betId_];

        // Assign creator to specified side of bet
        if (_side == Side.Long) {
            newBet.long = msg.sender;
        } else {
            newBet.short = msg.sender;
        }

        // Save bet details
        newBet.amount = _amount;
        newBet.expiryTime = _timeTillExpiry + block.timestamp;
        newBet.closingTime = _closingTime;

        // Increment bet ID counter
        betId = betId_ + 1;

        emit BetOpened(
            betId_,
            _side,
            msg.sender,
            _amount,
            _timeTillExpiry,
            _closingTime
        );
    }

    // Check if issues with arbitrary betId
    // Could pass in long or short => save storage read if honest
    function joinBet(uint256 _betId) external {
        Bet storage targetBet = bets[_betId];

        // Revert if proposed bet has expired
        if (targetBet.expiryTime <= block.timestamp) {
            revert BetExpired();
        }

        // Caller takes free side of bet
        if (targetBet.long == address(0)) {
            targetBet.long = msg.sender;
        } else if (targetBet.short == address(0)) {
            targetBet.short = msg.sender;
        } else {
            revert BetAlreadyActivated();
        }

        // Transfer USDC from caller to this contract to be held in escrow
        USDC.transferFrom(msg.sender, address(this), targetBet.amount);

        int256 openingPrice = _getCurrentBTCPrice();

        targetBet.openingPrice = openingPrice;

        emit BetActivated(_betId, msg.sender, openingPrice);
    }

    // Create bet instantly via both-party meta-transaction
    function createBet(
        address _long,
        address _short,
        uint256 _amount,
        uint256 _expiryTime,
        uint256 _closingTime,
        bytes calldata _longSignature,
        bytes calldata _shortSignature
    ) external {
        if (_expiryTime <= block.timestamp) {
            revert BetMetaTransactionHasExpired();
        }

        // Signing this hash approves the contract to create the input bet using approved signer funds
        bytes32 betCreationMessageHash = ECDSA.toEthSignedMessageHash(
            keccak256(
                abi.encodePacked(
                    _long,
                    _short,
                    _amount,
                    _expiryTime,
                    _closingTime
                )
            )
        );

        bool validLongSignature = _checkSignature(
            _long,
            betCreationMessageHash,
            _longSignature
        );

        bool validShortSignature = _checkSignature(
            _short,
            betCreationMessageHash,
            _shortSignature
        );

        if (!validLongSignature || !validShortSignature) {
            revert InvalidBetCreationSignature();
        }

        // Escrow bet funds
        USDC.transferFrom(_long, address(this), _amount);
        USDC.transferFrom(_short, address(this), _amount);

        uint256 betId_ = betId;

        Bet storage newBet = bets[betId_];

        // Create bet
        newBet.long = _long;
        newBet.short = _short;
        newBet.amount = _amount;
        newBet.closingTime = _closingTime;
        newBet.openingPrice = _getCurrentBTCPrice();

        // Increment bet ID
        betId = betId_ + 1;

        // Emit event
        // emit BetCreated(betId_, );
    }

    // *** BET RESOLUTION  ***

    /// @notice Resolves a bet, transferring the escrowed funds to the winner.
    /// @dev Let the caller submit the roundId of the round within which the bet's closing time lies.
    /// It can be obtained via chainlink log and can be checked during the resolution.
    function resolveBet(uint256 _betId, uint80 _roundId) external {
        Bet storage targetBet = bets[_betId];

        // Get price data for specified round
        // Works for current round => see Chainlink's OffchainAggregator.sol
        (
            ,
            int256 price,
            uint256 roundStart,
            uint256 updatedAt,

        ) = priceAggregator.getRoundData(_roundId);

        uint256 closing = targetBet.closingTime;

        // Check bet's closing time is within specified round
        if (closing < roundStart) {
            revert BetDidNotExpireInRound();
        } else if (roundStart != updatedAt && closing < updatedAt) {
            revert BetDidNotExpireInRound();
        }

        // Cache from storage
        uint256 amount = targetBet.amount;
        int256 openingPrice = targetBet.openingPrice;

        // Resolve bet by transferring funds to winner
        if (openingPrice < price) {
            _resolve(_betId, targetBet.long, amount);
        } else if (price < openingPrice) {
            _resolve(_betId, targetBet.short, amount);
        } else {
            // Draw as price is equal => transfer funds back to original pre-bet owners
            USDC.transfer(targetBet.long, amount);
            USDC.transfer(targetBet.short, amount);
        }

        USDC.transfer(targetBet.long, amount * 2);

        // Short wins
        USDC.transfer(targetBet.short, amount * 2);

        // Delete bet to prevent repeat resolutions
        delete bets[_betId];
    }

    function _resolve(
        uint256 _betId,
        address _winner,
        uint256 amount
    ) internal {
        // Transfer all escrowed funds to winner
        USDC.transfer(_winner, amount * 2);

        emit BetResolved(_betId, _winner);
    }

    function withdraw(uint256 _betId) external {
        Bet storage targetBet = bets[_betId];

        address long = targetBet.long;
        address short = targetBet.short;
        uint256 amount = targetBet.amount;

        if (short == address(0) || msg.sender == long) {
            USDC.transfer(long, targetBet.amount);
        } else if (long == address(0) || msg.sender == short) {
            USDC.transfer(short, amount);
        } else {
            revert InvalidWithdrawal();
        }

        delete bets[_betId];
    }

    // Get current BTC/USDC price from Chainlink data feed
    // Price returned has 8 decimal places
    function _getCurrentBTCPrice() internal view returns (int256 price) {
        (, price, , , ) = priceAggregator.latestRoundData();
    }

    function _checkSignature(
        address _signer,
        bytes32 _messageHash,
        bytes calldata _signature
    ) internal view returns (bool) {
        return
            SignatureChecker.isValidSignatureNow(
                _signer,
                _messageHash,
                _signature
            );
    }
}

