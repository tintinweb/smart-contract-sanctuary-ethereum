        ERC721("TipMeCryptoNFT", "TMCNFT")
    {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        
        _levelTable[0] = 10;
        _levelTable[1] = 100;
        _levelTable[2] = 1000;
        _levelTable[3] = 10000;
        _levelTable[4] = 100000;
        _levelTable[5] = 1000000;
        _levelTable[6] = 10000000;
    }

    // function _baseURI() internal pure override returns (string memory) {
    //     return "";
    // }

    function hasUserMintedToken(address to) public virtual view returns (bool){
        return  _tokenMintedUsers[to];
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId, uint256 batchSize)
        internal
        whenNotPaused
        override
    {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function levelUpToken(uint256 _tokenId) external onlyRole(MINTER_ROLE) {
        require(_exists(_tokenId), "ERC721: token does not exist");
        uint256 requriedBoostAmount = getRequiredTokenLevelUpBoost(_tokenId);
        require(requriedBoostAmount != 0, "This level not upgradable");

        _tokens[_tokenId].level += 1;
        _tokens[_tokenId].tmcBoost += requriedBoostAmount;

        emit TokenLevelUp(_tokenId, _tokens[_tokenId].level);
    }

    // call this function to mint a NFT
    function mint(address to, string memory uri) public onlyRole(MINTER_ROLE) {
        require(to != address(0), "Address can not be zero");
        require(hasUserMintedToken(to) != true, "User has minted a token");

        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();
        _tokens[tokenId].createTimestamp = block.timestamp;
        _tokens[tokenId].level = 0; // start from level 0
        _tokens[tokenId].tmcBoost = 0;
        _tokenMintedUsers[to] = true;
        _safeMint(to, tokenId);
        // add uri for this token
        // if base url is set, the whole url will become `{base}{uri}`
        _setTokenURI(tokenId, uri);
    }

    function _safeMint(address to, uint256 tokenId) internal override {
        super._safeMint(to, tokenId);
        emit TokenMint(to, tokenId, _tokens[tokenId].level);
    }

    function getRequiredTokenLevelUpBoost(uint256 _tokenId) public view returns (uint256) {
        uint256 currentLevel = _tokens[_tokenId].level;
        require(_levelTable[currentLevel] != 0, "This level not upgradable");
        return _levelTable[currentLevel];
    }

    function getToken(uint256 _tokenId)
        public
        view
        returns (
            uint256 tokenId,
            address tokenOwner,
            uint256 level,
            uint256 tmcBoost,
            uint256 createTimestamp,
            string memory uri
        )
    {
        require(_exists(_tokenId), "ERC721: token does not exist");

        Token memory token = _tokens[_tokenId];
        tokenId = _tokenId;
        tokenOwner = ownerOf(_tokenId);
        level = token.level;
        tmcBoost = token.tmcBoost;
        createTimestamp = token.createTimestamp;
        uri = tokenURI(_tokenId);

        return (
            tokenId, tokenOwner, level, tmcBoost, createTimestamp, uri
        );
    }

    // The following functions are overrides required by Solidity.
    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721URIStorage, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}