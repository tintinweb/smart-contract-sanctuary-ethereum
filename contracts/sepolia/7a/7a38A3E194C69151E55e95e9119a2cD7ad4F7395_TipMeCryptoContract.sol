pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
// import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/ConfirmedOwner.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "./TipMeCryptoNFT.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";

contract TipMeCryptoContract is
    ConfirmedOwner,
    VRFConsumerBaseV2
{
    event TipEth(address indexed from, address indexed to, uint256 amount);
    event TipERC20(
        address indexed from,
        address indexed to,
        address indexed token,
        uint256 amount
    );
    event SetETHCommission(uint _ethCommission);
    event SetERC20Commission(address indexed tokenAddress, uint256 _ethCommission);
    event IncreaseUserBoost(address indexed userAddress, uint256 amount);
    event LevelUpTmcNFT(address indexed userAddress, uint256 tokenId);

    event VrfRequestSent(uint256 requestId, uint32 numWords);
    event VrfRequestFulfilled(
        uint256 requestId,
        uint256[] randomWords
    );

    struct VrfRequestStatus {
        // uint256 paid;   // amount paid in link
        bool exists;
        bool fulfilled; // whether the request has been successfully fulfilled
        uint256[] randomWords;
        address userAddress;
        uint256 amount;
    }

    mapping(uint256 => VrfRequestStatus) public s_requests; /* requestId --> requestStatus */
    mapping(address => uint256) private _userBoosts;
    VRFCoordinatorV2Interface COORDINATOR;
    // Chainlink VRF subscription ID.
    uint64 s_subscriptionId;
    // The gas lane to use, which specifies the maximum gas price to bump to.
    // For a list of available gas lanes on each network,
    // see https://docs.chain.link/docs/vrf/v2/subscription/supported-networks/#configurations
    bytes32 keyHash =
        0x474e34a077df58807dbe9c96d3c009b23b3c6d0cce433e59bbf5b34f823bc56c;

    uint256[] public requestIds; // past requests Id.
    uint256 public lastRequestId;
    // Depends on the number of requested values that you want sent to the
    // fulfillRandomWords() function. Test and adjust
    // this limit based on the network that you select, the size of the request,
    // and the processing of the callback request in the fulfillRandomWords()
    // function.
    uint32 callbackGasLimit = 100000;

    // The default is 3, but you can set this higher.
    uint16 requestConfirmations = 3;

    // For this example, retrieve 1 random values in one request.
    // Cannot exceed VRFV2Wrapper.getConfig().maxNumWords.
    uint32 numWords = 1;
    // LINK Address - hardcoded for Sepolia
    address linkAddress = 0x779877A7B0D9E8603169DdbD7836e478b4624789;
    // VRF contract address WRAPPER - hardcoded for Sepolia
    // address wrapperAddress = 0x8103b0a8a00be2ddc778e6e7eaa21791cd364625;

    address tmcNFTAddress = 0x40b26fc4d8c3fd31447206F1dc2Bd4d0434Bba3e;

    uint256 public ethCommission = 1; // 1% ETH commission
    mapping(address => uint256) public erc20Commissions; // map of ERC20 commissions

    /**
     * HARDCODED FOR SEPOLIA
     * COORDINATOR: 0x8103B0A8A00be2DDC778e6e7eaa21791Cd364625
     */
    constructor(
        uint64 subscriptionId
    )
        VRFConsumerBaseV2(0x8103B0A8A00be2DDC778e6e7eaa21791Cd364625)
        ConfirmedOwner(msg.sender)
    {
        COORDINATOR = VRFCoordinatorV2Interface(
            0x8103B0A8A00be2DDC778e6e7eaa21791Cd364625
        );
        s_subscriptionId = subscriptionId;
    }

    function getEthBalance(address _user) public view returns (uint) {
        return _user.balance;
    }

    function getContractEthBalance() public view returns (uint) {
        return address(this).balance;
    }

    function tipEth(address payable _to) public payable {
        require(_to != address(0), "Invalid recipient");
        require(msg.sender != _to, "Cannot tip yourself");
        require(msg.value > 0, "Invalid amount");

        uint256 ethCommissionAmount = (msg.value / 100) * ethCommission;
        uint256 ethTipAmount = msg.value - ethCommissionAmount;
        payable(owner()).transfer(ethCommissionAmount);
        _to.transfer(ethTipAmount);
        emit TipEth(msg.sender, _to, ethTipAmount);

        // Mint a NFT if the sender is the first time Tip ERC20 token
        bool hasUserMintedNFT = TipMeCryptoNFT(tmcNFTAddress).hasUserMintedToken(msg.sender);

        if (hasUserMintedNFT != true) {
            TipMeCryptoNFT(tmcNFTAddress).mint(msg.sender, "");
        }

        _increaseRandomUserBoost(msg.sender, msg.value);
    }

    function tipErc20(
        address _tokenAddress,
        address payable _to,
        uint256 _amount
    ) public {
        require(_to != address(0), "Invalid recipient address");
        require(_amount > 0, "Invalid tip amount");

        // Create an instance of the ERC20 token contract
        IERC20 token = IERC20(_tokenAddress);
        uint256 commission = (_amount * 1) / 100; // hardcoded for 1%
        uint256 remainingAmount = _amount - commission;

        // Check if the contract has sufficient allowance
        uint256 allowance = token.allowance(msg.sender, address(this));
        require(allowance >= _amount, "Approved allowance is not sufficient");

        // Calculate and deduct the commission
        if (commission > 0) {
            bool commissionSuccess = token.transferFrom(
                msg.sender,
                owner(),
                commission
            );
            require(commissionSuccess, "Commission transfer failed");
        }

        // Transfer the tokens to the recipient
        bool transferSuccess = token.transferFrom(
            msg.sender,
            _to,
            remainingAmount
        );
        require(transferSuccess, "Transfer failed");

        emit TipERC20(msg.sender, _to, _tokenAddress, _amount);

        // Mint a NFT if the sender is the first time Tip ERC20 token
        bool hasUserMintedNFT = TipMeCryptoNFT(tmcNFTAddress).hasUserMintedToken(msg.sender);

        if (hasUserMintedNFT != true) {
            TipMeCryptoNFT(tmcNFTAddress).mint(msg.sender, "");
        }

        _increaseRandomUserBoost(msg.sender, _amount);
    }

    function setEthCommission(uint256 _ethCommission) public onlyOwner {
        ethCommission = _ethCommission;
        emit SetETHCommission(ethCommission);
    }

    function setErc20Commission(address _token, uint256 _commission)
        public
        onlyOwner
    {
        erc20Commissions[_token] = _commission;
        emit SetERC20Commission(_token, ethCommission);
    }

    function withdrawETH() public {
        require(msg.sender == owner(), "Only owner can call this function");
        payable(owner()).transfer(address(this).balance);
    }

    function withdrawToken(address _token) public {
        require(msg.sender == owner(), "Only owner can call this function");
        IERC20 token = IERC20(_token);
        token.transfer(owner(), token.balanceOf(address(this)));
    }

    function getUserBoostBalance(address userAddress) external view returns (uint256) {
        return _userBoosts[userAddress];
    }

    function boostTmcNFT(uint256 _tokenId) external {
        require(_userBoosts[msg.sender] > 0, "user's boost banlance is 0");
        uint256 requriedBoostAmount = TipMeCryptoNFT(tmcNFTAddress).getRequiredTokenLevelUpBoost(_tokenId);
        require(_userBoosts[msg.sender] >= requriedBoostAmount, "user doesn't have enough boost to upgrade token");
    
        TipMeCryptoNFT(tmcNFTAddress).levelUpToken(_tokenId);
        _userBoosts[msg.sender] -= requriedBoostAmount;

        emit LevelUpTmcNFT(msg.sender, _tokenId);
    }

    // Chainlink functions
    // functions to mint NFT and integrate Chainlink VRF service
    function requestRandomWords(address _to, uint256 _amount)
        private
        returns (uint256 requestId)
    {
        requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        s_requests[requestId] = VrfRequestStatus({
            randomWords: new uint256[](1),
            fulfilled: false,
            exists: true,
            userAddress: _to,
            amount: _amount
        });
        requestIds.push(requestId);
        lastRequestId = requestId;
        emit VrfRequestSent(requestId, numWords);
        return requestId;
    }

    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {
        require(s_requests[_requestId].exists, "request not found");
        s_requests[_requestId].fulfilled = true;
        s_requests[_requestId].randomWords = _randomWords;
        emit VrfRequestFulfilled(
            _requestId,
            _randomWords
        );
        uint256 r10Value = (_randomWords[0] % 10) + 1;
        _safeIncreaseUserBoost(s_requests[_requestId].userAddress, r10Value);
    }

    function _increaseRandomUserBoost(address _to, uint256 _amount) private {
        requestRandomWords(_to, _amount);
    }

    function _safeIncreaseUserBoost(address _to, uint256 _randomAmount) private
    {
        // request to increase NFT Boost
        require(_to != address(0), "user address not found");
        // transform the result to a number between 1 and 10 inclusively
        _userBoosts[_to] += _randomAmount;
        emit IncreaseUserBoost(_to, _randomAmount);
    }

    function getRequestStatus(
        uint256 _requestId
    )
        external
        view
        returns (bool fulfilled, uint256[] memory randomWords)
    {
        require(s_requests[_requestId].exists, "request not found");
        VrfRequestStatus memory request = s_requests[_requestId];
        return (request.fulfilled, request.randomWords);
    }

    /**
     * Allow withdraw of Link tokens from the contract
     */
    function withdrawLink() public onlyOwner {
        LinkTokenInterface link = LinkTokenInterface(linkAddress);
        require(
            link.transfer(msg.sender, link.balanceOf(address(this))),
            "Unable to transfer"
        );
    }
}

