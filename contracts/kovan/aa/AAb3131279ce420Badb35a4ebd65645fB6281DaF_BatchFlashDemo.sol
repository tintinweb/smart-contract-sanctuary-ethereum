pragma solidity >=0.7.5;
pragma abicoder v2;

import { FlashLoanReceiverBase } from "./FlashLoanReceiverBase.sol";
import { IUniswapV2Router02, ILendingPool, ILendingPoolAddressesProvider, IERC20 } from "./Interfaces.sol";
import { SafeMath } from "./Libraries.sol";
import "@uniswap/v3-core/contracts/interfaces/callback/IUniswapV3SwapCallback.sol";
import "@uniswap/v3-core/contracts/interfaces/IUniswapV3Pool.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import  {TransferHelper} from "@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol";
import "./Ownable.sol";

/*
* A contract that executes the following logic in a single atomic transaction:
*
*   1. Gets a batch flash loan of Enj, DAI and Kyber
*   2. Deposits all of this flash liquidity onto the Enj V2 lending pool
*   3. Borrows 100 Kyber based on the deposited collateral
*   4. Repays 100 Kyber and unlocks the deposited collateral
*   5. Withdrawls all of the deposited collateral (Enj/DAI/Kyber)
*   6. Repays batch flash loan including the 9bps fee
*
*/
contract BatchFlashDemo is FlashLoanReceiverBase, Ownable {
    //For uniswap
    IUniswapV3Pool pool;
    IUniswapV2Router02 public sushiSwapRouter;

    ISwapRouter public immutable swapRouter;

    address public constant DAI = 0x6B175474E89094C44Da98b954EedeAC495271d0F;
    address public constant WETH9 = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
    address public constant USDC = 0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48;

    uint24 public constant poolFee = 3000;


    ILendingPoolAddressesProvider provider;
    using SafeMath for uint256;
    uint256 flashEnjAmt0;
    uint256 flashDaiAmt1;
    uint256 flashKyberAmt2;
    address lendingPoolAddr;
    
    // kovan reserve asset addresses
    // address kovanEnj = 0x9c3C9283D3e44854697Cd22D3Faa240Cfb032889; //wmatic
    address kovanDai = 0x001B3B4d0F3714Ca98ba10F6042DaEbF0B1B7b6F;  //dai
    address kovanCOMP = 0x61460874a7196d6a22D1eE4922473664b3E95270; //weth
    address kovanWETH = 0xd0A1E359811322d97991E03f863a0C30C2cF029C;
    address kovanLINK = 0xa36085F69e2889c224210F603D836748e7dC0088; //weth
    // address polygonDai = 0x8f3Cf7ad23Cd3CaDbD9735AFf958023239c6A063; //dai
    // address kovanKyber = 0x3C68CE8504087f89c640D02d133646d98e64ddd9; //weth
    
    // intantiate lending pool addresses provider and get lending pool address
    constructor(ILendingPoolAddressesProvider _addressProvider , ISwapRouter _swapRouter,  IUniswapV2Router02  _sushiSwap) FlashLoanReceiverBase(_addressProvider) {
        provider = _addressProvider;
        swapRouter = _swapRouter;
        sushiSwapRouter = _sushiSwap;
        lendingPoolAddr = provider.getLendingPool();
    }


   

    /**
        This function is called after your contract has received the flash loaned amount
     */
    function executeOperation(
        address[] calldata assets,
        uint256[] calldata amounts,
        uint256[] calldata premiums,
        address initiator,
        bytes calldata params
    )
        external
        override
        returns (bool)
    {
        
        // initialise lending pool instance
        ILendingPool lendingPool = ILendingPool(lendingPoolAddr);
        
        // deposits the flashed Enj, DAI and Kyber liquidity onto the lending pool
        flashDeposit(lendingPool);

        uint256 borrowAmt = 100 * 1e18; // to borrow 100 units of x asset
        
        // borrows 'borrowAmt' amount of Kyber using the deposited collateral
        // flashBorrow(lendingPool, kovanKyber, borrowAmt);
        
        // repays the 'borrowAmt' mount of Kyber to unlock the collateral
        // flashRepay(lendingPool, kovanKyber, borrowAmt);
 
        // withdraws the Enj, DAI and Kyber collateral from the lending pool
        flashWithdraw(lendingPool);

        // Approve the LendingPool contract allowance to *pull* the owed amount
        // i.e. Enj V2's way of repaying the flash loan
        for (uint i = 0; i < assets.length; i++) {
            uint amountOwing = amounts[i].add(premiums[i]);
            IERC20(assets[i]).approve(address(_lendingPool), amountOwing);
        }

        return true;
    }

    /*
    * Deposits the flashed Enj, DAI and Kyber liquidity onto the lending pool as collateral
    */
    function flashDeposit(ILendingPool _lendingPool) public {
        
        // approve lending pool
        IERC20(kovanDai).approve(lendingPoolAddr, flashDaiAmt1);
        // IERC20(kovanEnj).approve(lendingPoolAddr, flashEnjAmt0);
        // IERC20(kovanKyber).approve(lendingPoolAddr, flashKyberAmt2);
        
        // deposit the flashed Enj, DAI and Kyber as collateral
        _lendingPool.deposit(kovanDai, flashDaiAmt1, address(this), uint16(0));
        // _lendingPool.deposit(kovanEnj, flashEnjAmt0, address(this), uint16(0));
        // _lendingPool.deposit(kovanKyber, flashKyberAmt2, address(this), uint16(0));
        
    }

    /*
    * Withdraws the Enj, DAI and Kyber collateral from the lending pool
    */
    function flashWithdraw(ILendingPool _lendingPool) public {
        
        // _lendingPool.withdraw(kovanEnj, flashEnjAmt0, address(this));
        _lendingPool.withdraw(kovanDai, flashDaiAmt1, address(this));
        // _lendingPool.withdraw(kovanKyber, flashKyberAmt2, address(this));
        
    }
    
    /*
    * Borrows _borrowAmt amount of _borrowAsset based on the existing deposited collateral
    */
    function flashBorrow(ILendingPool _lendingPool, address _borrowAsset, uint256 _borrowAmt) public {
        
        // borrowing x asset at stable rate, no referral, for yourself
        _lendingPool.borrow(
            _borrowAsset, 
            _borrowAmt, 
            1, 
            uint16(0), 
            address(this)
        );
        
    }

    /*
    * Repays _repayAmt amount of _repayAsset
    */
    function flashRepay(ILendingPool _lendingPool, address _repayAsset, uint256 _repayAmt) public {
        
        // approve the repayment from this contract
        IERC20(_repayAsset).approve(lendingPoolAddr, _repayAmt);
        
        _lendingPool.repay(
            _repayAsset, 
            _repayAmt, 
            1, 
            address(this)
        );
    }

    /*
    * Repays _repayAmt amount of _repayAsset
    */
    function flashSwapBorrowRate(ILendingPool _lendingPool, address _asset, uint256 _rateMode) public {
        
        _lendingPool.swapBorrowRateMode(_asset, _rateMode);
        
    }
    
    

    /*
    * This function is manually called to commence the flash loans sequence
    */
    function executeFlashLoans(uint256 _flashEnjAmt0, uint256 _flashDaiAmt1, uint256 _flashKyberAmt2) public onlyOwner {
        address receiverAddress = address(this);

        // the various assets to be flashed
        address[] memory assets = new address[](1);
        // assets[0] = kovanEnj; 
        assets[0] = kovanDai;
        // assets[2] = kovanKyber;
        
        // the amount to be flashed for each asset
        uint256[] memory amounts = new uint256[](1);
        // amounts[0] = _flashEnjAmt0;
        amounts[0] = _flashDaiAmt1;
        // amounts[2] = _flashKyberAmt2;
        
        // flashEnjAmt0 = _flashEnjAmt0;
        flashDaiAmt1 = _flashDaiAmt1;
        // flashKyberAmt2 = _flashKyberAmt2;

        // 0 = no debt, 1 = stable, 2 = variable
        uint256[] memory modes = new uint256[](1);
        modes[0] = 0;
        // modes[1] = 0;
        // modes[2] = 0;

        address onBehalfOf = address(this);
        bytes memory params = "";
        uint16 referralCode = 0;

        _lendingPool.flashLoan(
            receiverAddress,
            assets,
            amounts,
            modes,
            onBehalfOf,
            params,
            referralCode
        );
    }
    

    /// @notice swapExactInputSingle swaps a fixed amount of DAI for a maximum possible amount of WETH9
    /// using the DAI/WETH9 0.3% pool by calling `exactInputSingle` in the swap router.
    /// @dev The calling address must approve this contract to spend at least `amountIn` worth of its DAI for this function to succeed.
    /// @param amountIn The exact amount of DAI that will be swapped for WETH9.
    /// @return amountOut The amount of WETH9 received.
    function swapExactInputSingle3(uint256 amountIn,  address[] calldata addresses) external returns (uint256 amountOut) {
        // for(uint8 i =0; i< addresses.length; i++){
            TransferHelper.safeApprove(address(addresses[0]), address(swapRouter), amountIn);
        // }
        // for( uint i=0; i+1 < addresses.length; i++){
            
            ISwapRouter.ExactInputParams memory params =
            ISwapRouter.ExactInputParams({
                path: abi.encodePacked(addresses[0], poolFee, addresses[1], poolFee, addresses[2]),
                recipient: msg.sender,
                deadline: block.timestamp,
                amountIn: amountIn,
                amountOutMinimum: 0
            });

            amountOut = swapRouter.exactInput(params);
        // }

        return amountOut;

    }


    /// @notice swapExactInputSingle swaps a fixed amount of DAI for a maximum possible amount of WETH9
    /// using the DAI/WETH9 0.3% pool by calling `exactInputSingle` in the swap router.
    /// @dev The calling address must approve this contract to spend at least `amountIn` worth of its DAI for this function to succeed.
    /// @param amountIn The exact amount of DAI that will be swapped for WETH9.
    /// @return amountOut The amount of WETH9 received.
    function swapExactInputSingle4(uint256 amountIn,  address[] calldata addresses) external returns (uint256 amountOut) {
        // for(uint8 i =0; i< addresses.length; i++){
            TransferHelper.safeApprove(address(addresses[0]), address(swapRouter), amountIn);
        // }
        // for( uint i=0; i+1 < addresses.length; i++){
            
            ISwapRouter.ExactInputParams memory params =
            ISwapRouter.ExactInputParams({
                path: abi.encodePacked(addresses[0], poolFee, addresses[1], poolFee, addresses[2], poolFee, addresses[3]),
                recipient: msg.sender,
                deadline: block.timestamp,
                amountIn: amountIn,
                amountOutMinimum: 0
            });

            amountOut = swapRouter.exactInput(params);
        // }

        return amountOut;

    }

    function swapExactInputSingle5(uint256 amountIn,  address[] calldata addresses) external returns (uint256 amountOut) {
        // for(uint8 i =0; i< addresses.length; i++){
            TransferHelper.safeApprove(address(addresses[0]), address(swapRouter), amountIn);
        // }
        // for( uint i=0; i+1 < addresses.length; i++){
            
            ISwapRouter.ExactInputParams memory params =
            ISwapRouter.ExactInputParams({
                path: abi.encodePacked(addresses[0], poolFee, addresses[1], poolFee, addresses[2], poolFee, addresses[3], poolFee, addresses[4]),
                recipient: msg.sender,
                deadline: block.timestamp,
                amountIn: amountIn,
                amountOutMinimum: 0
            });

            amountOut = swapRouter.exactInput(params);
        // }

        return amountOut;

    }

    function swapExactInputSingle6(uint256 amountIn,  address[] calldata addresses) external returns (uint256 amountOut) {
        // for(uint8 i =0; i< addresses.length; i++){
            TransferHelper.safeApprove(address(addresses[0]), address(swapRouter), amountIn);
        // }
        // for( uint i=0; i+1 < addresses.length; i++){
            
            ISwapRouter.ExactInputParams memory params =
            ISwapRouter.ExactInputParams({
                path: abi.encodePacked(addresses[0], poolFee, addresses[1], poolFee, addresses[2], poolFee, addresses[3], poolFee, addresses[4],poolFee, addresses[5]),
                recipient: msg.sender,
                deadline: block.timestamp,
                amountIn: amountIn,
                amountOutMinimum: 0
            });

            amountOut = swapRouter.exactInput(params);
        // }

        return amountOut;

    }

    function swapSushi(address from, uint amount,  address to) external returns (uint256[] memory finalAmount){
        TransferHelper.safeApprove(address(from), address(sushiSwapRouter), amount);
        uint amountOut = getEstimatedOutTokenforToken(amount,from, to);
        finalAmount = sushiSwapRouter.swapTokensForExactTokens(amountOut,amount, getPathForTokentoToken(from, to), msg.sender, block.timestamp);
        return finalAmount;
    }

    function getEstimatedInTokenforToken(uint amountOut,address from , address to) public view returns (uint ) {
        return sushiSwapRouter.getAmountsIn(amountOut, getPathForTokentoToken(from, to))[0];
    }

    function getEstimatedOutTokenforToken(uint amountIn,address from , address to) public view returns (uint ) {
        return sushiSwapRouter.getAmountsOut(amountIn, getPathForTokentoToken(from, to))[0];
    }

    function getPathForTokentoToken(address from , address to) public view returns (address[] memory) {
        address[] memory path = new address[](2);
        path[0] = from;
        path[1] = to;
        
        return path;
    }


    /*
    * Rugpull all ERC20 tokens from the contract
    */
    function rugPull(address[] calldata addresses) public payable onlyOwner {
        
        // withdraw all ETH
        msg.sender.call{ value: address(this).balance }("");
        
        for (uint8 i = 0; i<addresses.length; i++){
            IERC20(addresses[i]).transfer(msg.sender, IERC20(addresses[i]).balanceOf(address(this)));
        }
        // withdraw all x ERC20 tokens
        // IERC20(kovanEnj).transfer(msg.sender, IERC20(kovanEnj).balanceOf(address(this)));
        // IERC20(kovanDai).transfer(msg.sender, IERC20(kovanDai).balanceOf(address(this)));
        // IERC20(kovanLINK).transfer(msg.sender, IERC20(kovanLINK).balanceOf(address(this)));
        // IERC20(kovanCOMP).transfer(msg.sender, IERC20(kovanCOMP).balanceOf(address(this)));
        // IERC20(kovanWETH).transfer(msg.sender, IERC20(kovanWETH).balanceOf(address(this)));
        
        // IERC20(kovanKyber).transfer(msg.sender, IERC20(kovanKyber).balanceOf(address(this)));
    }
    
}

