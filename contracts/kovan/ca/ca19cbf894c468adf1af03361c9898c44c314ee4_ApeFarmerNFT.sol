        uint256 hashPowerFromUser,
        uint256 hashPowerToUser,
        uint256 tokenId,
        uint256 hashPowerToken
    );

    event LvlChanged(
        address fromUser,
        uint256 hashPowerFromUser,
        uint256 tokenId,
        uint256 newLvl,
        uint256 hashPowerToken
    );

    event SellingChanged(
        address fromUser,
        address toUser,
