        address fromUser,
        uint256 hashPowerFromUser,
        uint256 tokenId,
        uint256 status //1.Ban, 2.Unban
    );

    address public farmer;
    uint256 public totalHashPower;
    address public lvlUpCompute;
    mapping(uint256 => uint256) public farmTypes; //HAMSTER: 1, BUNNY: 2, UNICORN: 3, DINO: 4, DRAGON: 5
    mapping(uint256 => uint256) public farmRarities; //1.Common | 2.Rare | 3.Unique | 4.Epic | 5.Mythic | 6.Legendary
    mapping(uint256 => uint256) public farmBaseHashPowers;
    mapping(uint256 => uint256) public farmUpHashPowers;
    mapping(uint256 => uint256) public farmLvl;
    mapping(uint256 => bool) public isSelling;
    mapping(address => bool) public signers;
    mapping(uint256 => bool) public bans;

    mapping(address => uint256) public hashPowerOfOwner;

    string private constant SIGNING_DOMAIN = "APEMOVE";
    string private constant SIGNATURE_VERSION = "1";

    /**
     * @dev The base URI for all NFT.
     */
    string internal __baseURI;

    modifier onlyAdmin() {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()),
            "ApeFarmerNFT: Must Be Admin Role"
        );
        _;
    }

    constructor() EIP712(SIGNING_DOMAIN, SIGNATURE_VERSION){}

    function initialize(address _lvlUpCompute) external initializer {
        __ERC721_init("APEmove Farmer", "APEFARMER");
        __ERC721Pausable_init();
        __ERC721Enumerable_init();
        __AccessControlEnumerable_init();
        __ReentrancyGuard_init();

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(SIGNER_ROLE, _msgSender());
        __baseURI = "https://api-marketplace.apemove.io/api/v1/ape-farmer/";
        lvlUpCompute = _lvlUpCompute;
    }

    function addSigner(address _signer) external onlyAdmin {
        require(_signer != address(0), "ERR_SIGNER_IS_ZERO_ADDRESS");
        signers[_signer] = true;
    }

    function removeSigner(address _signer) external onlyAdmin {
        require(_signer != address(0), "ERR_SIGNER_IS_ZERO_ADDRESS");
        signers[_signer] = false;
    }

    function setFarmer(address _farmer) external onlyAdmin {
        farmer = _farmer;
    }

    function setLvlUpCompute(address _lvlUpCompute) external onlyAdmin {
        lvlUpCompute = _lvlUpCompute;
    }

    function addTotalHp(uint256 _value) external onlyAdmin {
        totalHashPower += _value;
    }

    function subTotalHp(uint256 _value) external onlyAdmin {
        totalHashPower -= _value;
    }

    function addOwnerHp(address _addr, uint256 _value) external onlyAdmin {
        hashPowerOfOwner[_addr] += _value;
    }

    function subOwnerHp(address _addr, uint256 _value) external onlyAdmin {
        hashPowerOfOwner[_addr] -= _value;
    }

    function ban(uint256[] calldata _tokenIds) external onlyAdmin {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            totalHashPower -= hashPowerOfToken(_tokenIds[i]);
            hashPowerOfOwner[ownerOf(_tokenIds[i])] -= hashPowerOfToken(_tokenIds[i]);
            bans[_tokenIds[i]] = true;
            emit BanChanged (ownerOf(_tokenIds[i]), hashPowerOfOwner[ownerOf(_tokenIds[i])], _tokenIds[i], 1);
        }
    }

    function unban(uint256[] calldata _tokenIds) external onlyAdmin {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            totalHashPower += hashPowerOfToken(_tokenIds[i]);
            hashPowerOfOwner[ownerOf(_tokenIds[i])] += hashPowerOfToken(_tokenIds[i]);
            bans[_tokenIds[i]] = false;
            emit BanChanged (ownerOf(_tokenIds[i]), hashPowerOfOwner[ownerOf(_tokenIds[i])], _tokenIds[i], 2);
        }
    }

    function open(
        uint256 _tokenId,
        uint256 _type,
        uint256 _rarity,
        uint256 _baseHashPower,
        bytes calldata _signature
    ) external whenNotPaused nonReentrant {
        address _sender = _msgSender();
        
        require(!_exists(_tokenId), "ApeFarmerNFT: tokenId is exists");

        _verifySignerSignature(
            keccak256(abi.encodePacked(_sender, _tokenId, _type, _rarity, _baseHashPower)),
            _signature
        );

        _initMetadata(
            _tokenId,
            _type,
            _rarity,
            _baseHashPower
        );

        _safeMint(_sender, _tokenId);       
    }

    function lvlUp(uint256 _targetTokenId, uint256[] calldata _materialIds)
        external
        whenNotPaused
        nonReentrant
    {
        require(lvlUpCompute != address(0), "NOT_FOUND_LVL_UP_COMPUTE");

        address sender = _msgSender();

        require(ownerOf(_targetTokenId) == sender, "TARGET_OWNER_FAIL");

        (
            uint256 _comReq,
            uint256 _rareReq,
            uint256 _uniReq,
            uint256 _epicReq,
            uint256 _powerUp,
            bool _typeReq
        ) = LvlUpCompute(lvlUpCompute).nextLvlUpReq(farmBaseHashPowers[_targetTokenId], farmRarities[_targetTokenId], farmLvl[_targetTokenId]);

        require(
            _materialIds.length == _comReq + _rareReq + _uniReq + _epicReq,
            "INVALID_MATERIAL_AMOUNT"
        );
        
        for (uint256 i = 0; i < _materialIds.length; i++) {
            require(ownerOf(_materialIds[i]) == sender, "MATERIAL_OWNER_FAIL");
            
            if(_typeReq)
                require(farmTypes[_materialIds[i]] == farmTypes[_targetTokenId], "MATERIAL_REQ_TYPE_FAIL");

            _transfer(sender, address(this), _materialIds[i]);

            if (farmRarities[_materialIds[i]] == 1 && _comReq > 0) _comReq--;
            else if (farmRarities[_materialIds[i]] == 2 && _rareReq > 0)
                _rareReq--;
            else if (farmRarities[_materialIds[i]] == 3 && _uniReq > 0)
                _uniReq--;
            else if (farmRarities[_materialIds[i]] == 4 && _epicReq > 0)
                _epicReq--;
        }

        require(
            _comReq + _rareReq + _uniReq + _epicReq == 0,
            "INVALID_MATERIAL_RARITY"
        );

        uint256 _powerUpForLvl = _powerUp - hashPowerOfToken(_targetTokenId);

        farmUpHashPowers[_targetTokenId] += _powerUpForLvl;
        farmLvl[_targetTokenId] += 1;
        hashPowerOfOwner[sender] += _powerUpForLvl;
        totalHashPower += _powerUpForLvl;
        uint256 curHP = hashPowerOfToken(_targetTokenId);
        uint256 curLvl = farmLvl[_targetTokenId];
                
        emit LvlChanged(
            sender,
            hashPowerOfOwner[sender],
            _targetTokenId,
            curLvl,
            curHP
        );
    }

    function setSelling(
        uint256 tokenId
    ) external {
        address sender = _msgSender();
        require(ownerOf(tokenId) == sender, "OWNER_FAIL");
        isSelling[tokenId] = true;

        totalHashPower -= hashPowerOfToken(tokenId);
        hashPowerOfOwner[sender] -= hashPowerOfToken(tokenId);
        emit SellingChanged (sender, address(0), tokenId, hashPowerOfOwner[sender], 0, 1);
    }

