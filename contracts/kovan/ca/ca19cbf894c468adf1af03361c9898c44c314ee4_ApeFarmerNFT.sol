     * - When `from` and `to` are both non-zero, ``from``'s `tokenId` will be
     * transferred to `to`.
     * - When `from` is zero, `tokenId` will be minted for `to`.
     * - When `to` is zero, ``from``'s `tokenId` will be burned.
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    )
        internal
        virtual
        override(
            ERC721Upgradeable,
            ERC721PausableUpgradeable,
            ERC721EnumerableUpgradeable
        )
    {
        ERC721Upgradeable._beforeTokenTransfer(from, to, tokenId);
        ERC721PausableUpgradeable._beforeTokenTransfer(from, to, tokenId);
        ERC721EnumerableUpgradeable._beforeTokenTransfer(from, to, tokenId);

        require(!bans[tokenId], "TOKEN_BAN");

        if (farmer != address(0)) {
            IApeFarmer(farmer).draftClaim(to);

            if (from != address(0)) {
                IApeFarmer(farmer).draftClaim(from);
            }
        }

        if (from != address(0)) {
            //not open (mint) -> transfer
            if(!isSelling[tokenId])
                hashPowerOfOwner[from] -= hashPowerOfToken(tokenId);
        }

        if (to != address(this)) {
            //transfer
            hashPowerOfOwner[to] += hashPowerOfToken(tokenId);
        } else {
            //lvlUp
            totalHashPower -= hashPowerOfToken(tokenId);
        }

        if(isSelling[tokenId])
            totalHashPower += hashPowerOfToken(tokenId);

        isSelling[tokenId] = false;

        emit HashPowerChanged(
            from,
            to,
            hashPowerOfOwner[from],
            hashPowerOfOwner[to],
            tokenId,
            hashPowerOfToken(tokenId)
        );
    }
}