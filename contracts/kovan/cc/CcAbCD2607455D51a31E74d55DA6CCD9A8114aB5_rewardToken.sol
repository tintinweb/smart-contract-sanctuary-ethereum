pragma solidity ^0.8.0;
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@chainlink/contracts/src/v0.8/ChainlinkClient.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./whitelist.sol";
contract rewardToken is ERC20Upgradeable, ChainlinkClient, OwnableUpgradeable,whitelistChecker {
    using Chainlink for Chainlink.Request;

    // uint256 public volume;
   ERC20Upgradeable public token;
    string url;
    address private oracle;
    bytes32 private jobId;
   
    address public signer;
    uint256 private fee;
    mapping (address =>  bytes32[]) public totalRequestsMade;
    //@dev this is used to store the amount received from oracle to the bytes
    mapping (bytes32 => uint) public bytesToData;
    //@dev stores address of the user who made the api requests
    mapping (bytes32 => address) public requestDataToAddress;
    //@dev checks whether a request generated is already used or not
    mapping (bytes32 => bool) public requestChecker;
    // mapping (address=>mapping(string=>mapping(string=>bool))) public isClaimed;
    mapping(address=>mapping(string=>bool)) public isClaimed;
    mapping(address=>mapping(uint=>bool)) public nonceUsed;

    function inititalize(address _tokenAddress) external initializer {
        token=ERC20Upgradeable(_tokenAddress);
        __Ownable_init();
        setPublicChainlinkToken();
        url = "https://sunrisetechs.click/getrewards/?wallet=";
        //https://sunrisetechs.click/getrewards/?wallet=0xeBA41eAa32841629B1d4F64852d0dadf70b0c665&projectid=628f2d0666b4fb6b0f5cb1c8
        oracle = 0xc57B33452b4F7BB189bB5AfaE9cc4aBa1f7a4FD8;
        jobId = "d5270d1c311941d0b08bead21fea7747";
        fee = 0.1 * 10 ** 18; // (Varies by network and job)
    }

    function requestVolumeData(string memory dataUrl,string memory id, game memory getRewards) public returns (bytes32 requestId)
    {
         require (getSigner(getRewards) == signer,'!Signer');
        require (getRewards.userAddress == msg.sender,'!User');
        require (!nonceUsed[getRewards.userAddress][getRewards.timestamp],'Nonce Used Already');
        nonceUsed[getRewards.userAddress][getRewards.timestamp] = true;
        string memory finalUrl = string(abi.encodePacked(url,dataUrl,"&projectid=",id));
        require(!isClaimed[getRewards.userAddress][id],"Already Takens");
        isClaimed[getRewards.userAddress][id]=true;
        Chainlink.Request memory request = buildChainlinkRequest(jobId, address(this), this.fulfill.selector);
        request.add("get", finalUrl);
        request.add("path", "result,0,amount");
        bytes32 byteData = sendChainlinkRequestTo(oracle, request, fee);
        requestDataToAddress[bytes32(byteData)] = msg.sender;
        totalRequestsMade[msg.sender].push(byteData);
        return byteData;
    }

    function fulfill(bytes32 _requestId, uint256 amount) public recordChainlinkFulfillment(_requestId)
    {
        bytesToData[bytes32(_requestId)] = amount;
        address _to = requestDataToAddress[_requestId];
        tokenMinter(_to, amount, _requestId);
    }

    function tokenMinter(address _sender, uint _amount, bytes32 _request) internal {
        require (msg.sender != _sender,'You cannot initiate the transaction');
        require (!requestChecker[_request], 'Request already used');
        requestChecker[_request] = true;

        token.transfer(_sender,_amount);
    }

    function setSigner(address _addr) external onlyOwner{
        signer=_addr;
    }

    function getReward(game memory getRewards) external{
        require (getSigner(getRewards) == signer,'!Signer');
        require (getRewards.userAddress == msg.sender,'!User');
        require (!nonceUsed[getRewards.userAddress][getRewards.timestamp],'Nonce Used Already');
        nonceUsed[getRewards.userAddress][getRewards.timestamp] = true;
        token.transfer(getRewards.userAddress,getRewards.amount);
    }
}

