contract whitelistChecker is EIP712 {

    string private constant SIGNING_DOMAIN = "PlayToEarn";
    string private constant SIGNATURE_VERSION = "1";

    struct game {
        address contractAddress;
        address userAddress;
        uint timestamp;
        uint amount;
        bytes signature;
    }

    constructor() EIP712(SIGNING_DOMAIN, SIGNATURE_VERSION){

    }

    function getSigner(game memory whitelist) public view returns(address){
        return _verify(whitelist);
    }

    /// @notice Returns a hash of the given whitelist, prepared using EIP712 typed data hashing rules.

    function _hash(game memory whitelist) internal view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(
                keccak256("game(address contractAddress,address userAddress,uint256 timestamp,uint256 amount)"),
                whitelist.contractAddress,
                whitelist.userAddress,
                whitelist.timestamp,
                whitelist.amount
            )));
    }

    function _verify(game memory whitelist) internal view returns (address) {
        bytes32 digest = _hash(whitelist);
        return ECDSA.recover(digest, whitelist.signature);
    }

}

