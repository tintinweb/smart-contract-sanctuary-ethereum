/// @title The Moonwalkers NFT Smart Contract.
/// @author Briac.
/// @notice This contract allows users to mint and transfer the Moonwalkers NFTs.
/// @dev The contract inherits from the ERC721A standard made by the Azuki project.
contract MoonwalkersA is ERC721A, PullPayment, Pausable, Ownable, ReentrancyGuard {
  using Strings for uint256;
  using SafeMath for uint256;

  /// @dev Sale configuration struct.
  struct SaleConfig {
    uint32 publicSaleStartTime;
    uint32 presaleStartTime;
    uint256 publicSaleMintPrice;
    uint256 presaleMintPrice;
    uint256 maxPublicMintPerWallet;
    uint256 maxPresaleMintPerWallet;
    uint256 maxBatchSize;
    bool revealed;
  }

  /// @dev Sale configuration instance.
  SaleConfig public saleConfig;

  /// @dev Sale State.
  bool private saleState;

  /// @dev Maximum public sale supply.
  uint256 private constant maxSupply = 10;

  /// @dev Merke root that contains all the whitelisted addresses.
  bytes32 private merkleRoot;

  /// @dev Mapping of the balance of NFT minted by an address during the presale.
  mapping(address => uint256) private addressMintedBalancePresale;

  /// @dev Mapping of the balance of NFT minted by an address during the public sale.
  mapping(address => uint256) private addressMintedBalancePublic;

  /// @dev Base token URI used as a prefix by tokenURI().
  string private baseTokenURI;

  /// @dev Base token URI used as a suffix by tokenURI().
  string private extensionTokenURI;

  /// @dev Unrevealed toke URI.
  string private unrevealedTokenURI;

  /// @dev Constructor.
  constructor() ERC721A("Moonwalkers", "MW") {
    baseTokenURI = "";
    extensionTokenURI = ".json";
    saleConfig.publicSaleMintPrice = 0.01 ether;
    saleConfig.presaleMintPrice = 0.008 ether;
    saleConfig.presaleStartTime = 1650058020;
    saleConfig.publicSaleStartTime = 1682006964;
    saleConfig.maxPresaleMintPerWallet = 2;
    saleConfig.maxPublicMintPerWallet = 3;
    saleConfig.maxBatchSize = 2;
    saleConfig.revealed = false;
    saleState = true;
    merkleRoot = 0x432eef6147ed482431347e58b9bf66304557b103e6c4ee0e4124dee4adf62484;
  }

  modifier whenNotLocked() {
      require(saleState, "Sale is locked");
      _;
  }

  /// @dev Pause the contract.
  function pause() external onlyOwner {
    _pause();
  }

  /// @dev Unpause the contract.
  function unpause() external onlyOwner {
    _unpause();
  }

  /// @dev Making the token transfer pausable.
  function _beforeTokenTransfers(address from, address to, uint256 startTokenId, uint256 quantity) internal whenNotPaused override {
    super._beforeTokenTransfers(from, to, startTokenId, quantity);
  }

  /// @dev Public sale mint function.
  function publicSaleMint(uint256 _mintAmount) external payable whenNotLocked {
    require(block.timestamp > saleConfig.publicSaleStartTime, "Mint hasn't stared yet"); // Need to take 1 second less cause it's a strict superior
    require(totalSupply().add(_mintAmount) < maxSupply.add(1), "Max supply reached");
    require(_mintAmount < saleConfig.maxBatchSize.add(1), "Max mint per transaction exceeded");
    require(msg.value == saleConfig.publicSaleMintPrice.mul(_mintAmount), "Transaction value did not equal the mint price");
    require(addressMintedBalancePublic[msg.sender].add(_mintAmount) < saleConfig.maxPublicMintPerWallet.add(1), "Max public sale mint per address exceeded");

    addressMintedBalancePublic[msg.sender] += _mintAmount;
    _safeMint(msg.sender, _mintAmount);
  }

  /// @dev Presale mint function. Only the whitelisted addresses are able to mint during the presale.
  function allowlistMint(uint256 _mintAmount, bytes32[] calldata _merkleProof) external payable whenNotLocked {
    require(block.timestamp > saleConfig.presaleStartTime && block.timestamp < saleConfig.publicSaleStartTime, "Presale hasn't stared yet or is closed");
    require(totalSupply().add(_mintAmount) < maxSupply.add(1), "Max supply reached");
    require(_mintAmount < saleConfig.maxBatchSize.add(1), "Max mint per transaction exceeded");
    require(msg.value == saleConfig.presaleMintPrice.mul(_mintAmount), "Transaction value did not equal the mint price");
    require(addressMintedBalancePresale[msg.sender].add(_mintAmount) < saleConfig.maxPresaleMintPerWallet.add(1), "Max presale mint per address reached");
    bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
    require(MerkleProof.verify(_merkleProof, merkleRoot, leaf), "Invalid Merkle Proof : not whitelisted");

    addressMintedBalancePresale[msg.sender] += _mintAmount;
    _safeMint(msg.sender, _mintAmount);
  }

  /// @dev Dev mint function for admin.
  function devMint(address recipient, uint256 _mintAmount) external onlyOwner {
    require(totalSupply().add(_mintAmount) < maxSupply.add(1), "Max supply reached");

    addressMintedBalancePublic[msg.sender] += _mintAmount;
    _safeMint(recipient, _mintAmount);
  }

  /// @dev Override the tokenURI to add our a custom base prefix and suffix.
  function tokenURI(uint256 tokenId) public view override returns (string memory) {
    require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

    if(!saleConfig.revealed) {
      return unrevealedTokenURI;
    }

    string memory baseURI = _baseURI();
    return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.toString(), extensionTokenURI)) : "";
  }

  /// @dev Returns an URI for a given token ID.
  function _baseURI() internal view override returns (string memory) {
    return baseTokenURI;
  }

  /// @dev Sets the base token URI prefix.
  function setBaseTokenURI(string memory _baseTokenURI) external onlyOwner {
    baseTokenURI = _baseTokenURI;
  }

  /// @dev Sets the base token URI suffix.
  function setExtensionTokenURI(string memory _extensionTokenURI) external onlyOwner {
    extensionTokenURI = _extensionTokenURI;
  }

  /// @dev Sets the unrevealed token URI.
  function setUnrevealedTokenURI(string memory _unrevealedTokenURI) external onlyOwner {
    unrevealedTokenURI = _unrevealedTokenURI;
  }

  /// @dev Sets the unrevealed state.
  function setRevealed(bool _revealedState) external onlyOwner {
    saleConfig.revealed = _revealedState;
  }

  /// @dev Sets the unrevealed state.
  function setPublicSaleMintPrice(uint64 _publicSaleMintPrice) external onlyOwner {
    saleConfig.publicSaleMintPrice = _publicSaleMintPrice;
  }

  /// @dev Sets the unrevealed state.
  function setPresaleMintPrice(uint64 _presaleMintPrice) external onlyOwner {
    saleConfig.presaleMintPrice = _presaleMintPrice;
  }

  /// @dev Sets the max batch size.
  function setMaxBatchSize(uint256 _maxBatchSize) external onlyOwner {
    saleConfig.maxBatchSize = _maxBatchSize;
  }

  /// @dev Sets the presale date.
  function setPresaleStartTime(uint32 _presaleStartTime) external onlyOwner {
    saleConfig.presaleStartTime = _presaleStartTime;
  }

  /// @dev Sets the presale date.
  function setPublicSaleStartTime(uint32 _publicSaleStartTime) external onlyOwner {
    saleConfig.publicSaleStartTime = _publicSaleStartTime;
  }

  /// @dev Sets the max public mint per wallet allowed.
  function setMaxPublicMintPerWallet(uint256 _maxPublicMintPerWallet) external onlyOwner {
    saleConfig.maxPublicMintPerWallet = _maxPublicMintPerWallet;
  }

  /// @dev Sets the max presale mint per wallet allowed.
  function setMaxPresaleMintPerWallet(uint256 _maxPresaleMintPerWallet) external onlyOwner {
    saleConfig.maxPresaleMintPerWallet = _maxPresaleMintPerWallet;
  }

  /// @dev Set the merkle root.
  function setMerkleRoot(bytes32 _merkleRoot) external onlyOwner {
    merkleRoot = _merkleRoot;
  }

  /// @dev Overridden in order to make it an onlyOwner function.
  function withdrawPayments(address payable payee) public override onlyOwner {
    super.withdrawPayments(payee);
  }

  /// @dev Withdraw the contract funds to the contract owner.
  function withdraw() external onlyOwner nonReentrant {
    uint256 balance = address(this).balance;
    payable(owner()).transfer(balance);
  }
}

