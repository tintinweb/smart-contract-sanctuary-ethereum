pragma solidity ^0.8.7;





/// @title StationXFactory Cloning Contract
/// @dev Contract create proxies of DAO Token and Governor contract
contract FactoryCloneContract {

    address ERC1155TradeableProxyAddress;
    address Implementation;
    address owner;
    address USDC;

    event CreatedGovernor(address indexed, address indexed);
    event ChangeOrganiser(address newOrganiser);
    event ChangeImplementationAddress(address newImplementation);
    event ChangedUSDCAddress(address USDC);

    /// @dev Initial Deployment of Token and Governor contract and storing their addresses for proxies creation
    constructor (address _USDC,address _implementation) {
        ERC1155TradeableProxyAddress = address(new ERC1155TradeableProxy());
                console.log("ERC1155TradeableProxy contract address:", ERC1155TradeableProxyAddress);       
        owner = msg.sender;
                console.log("Factory Owner Assign:", owner);
        USDC = _USDC;
                console.log("USDC contract set to:", USDC);
        Implementation = _implementation;
                console.log("Implementation contract set to:", Implementation);
    }

    /// @dev Modifier to check for Owner of contract
    modifier isOrganiser () {
        require(msg.sender == owner,"Only Owner Allowed");
        _;
    }

    /// @dev Function to assign new Owner
    /// @param _newOrganiser address of new owner
    function changeOrganiser(address _newOrganiser) external isOrganiser{
        if(_newOrganiser != address(0))
        {
            owner = _newOrganiser;
            emit ChangeOrganiser(_newOrganiser);
        }
    }

    /// @dev Function to change base Governor contract address
    /// @param _newDAO address of new Governor contract
    function changeDAOImplementation(address _newDAO) external isOrganiser{
        if(_newDAO != address(0))
        {
            ERC1155TradeableProxyAddress = _newDAO;
            emit ChangeImplementationAddress(_newDAO);
        }
    }

    /// @dev Function to create proxies and initialisation of Token and Governor contract
    function createDAO(
                string memory name, string memory symbol, 
                uint256 _totalDeposit,
                uint256 _minDeposit, uint256 _maxDeposit, 
                uint256 _ownerFee, uint256 _days,
                bool _feeUSDC,
                uint256 _quorum,uint256 _threshold,address _ownerAddress
                )external returns(address) {
        address tokenAddress;

        bytes memory data = abi.encodeWithSignature("initializeERC20(string,string,uint256,uint256,uint256,uint256,uint256,bool,uint256,uint256,address,address)",name,symbol,_totalDeposit,_minDeposit,_maxDeposit,_ownerFee,_days,_feeUSDC,_quorum,_threshold,_ownerAddress,USDC);

        tokenAddress = Clones.clone(ERC1155TradeableProxyAddress);
        address tempAddress = address(tokenAddress);
        address payable addr = payable(address(tempAddress));
        ERC1155TradeableProxy(addr).initialize(Implementation,owner,data);
        return addr;
    }

    /// @dev Function to assign USDC address
    /// @param _USDCAddress address of USDC token
    function changeUSDCAddress(address _USDCAddress) external isOrganiser{
        if(_USDCAddress != address(0))
        {
            USDC = _USDCAddress;
            emit ChangedUSDCAddress(_USDCAddress);
        }
    }

    /// @dev Function to get USDC address
    /// @return contract address of USDC  
    function getUSDCAddress() external view returns(address){
        return USDC;
    }
}