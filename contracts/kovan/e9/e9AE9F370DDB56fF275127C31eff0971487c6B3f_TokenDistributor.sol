/** 
 * @title - A retroactive ERC20 token distribution contract 
 * @author - [email protected]
 * @notice - Provided an EIP712 compliant signed message & token claim, distributes GTC tokens 
 **/

/**
* @notice interface for interacting with a Governance Token's delegate function
*/
interface IToken {
    function delegate(address) external;
}

contract TokenDistributor { 
    address immutable public token; 
    uint immutable public deployTime;
    address immutable public timeLockContract;
    bytes32 immutable public merkleRoot;

    // hash of the domain separator
    bytes32 DOMAIN_SEPARATOR;

    // This is a packed array of booleans.
    mapping(uint256 => uint256) private claimedBitMap;
    
    // EIP712 domain struct 
    struct EIP712Domain {
        string  name;
        string  version;
        uint256 chainId;
        address verifyingContract;
    }

    // Reward will be halved every HALVE_PERIOD
    uint public constant HALVE_PERIOD = 5 minutes; 
    // Initial reward is Ne18, i.e., N tokens.
    uint public constant INITIAL_AWARD = 500e18;

    // How long will this contract process token claims? 30 days
    uint public constant CONTRACT_ACTIVE = 30 days;

    // as required by EIP712, we create type hash that will be rolled up into the final signed message
    bytes32 constant EIP712DOMAIN_TYPEHASH = keccak256(
        "EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)"
    );

    // typehash for our token claim - matches the Claim struct  
    bytes32 constant GTC_TOKEN_CLAIM_TYPEHASH = keccak256(
        "Claim(uint32 user_id,address user_address,uint256 user_amount,address delegate_address,bytes32 leaf)"
    );
    
    // This event is triggered when a call to ClaimTokens succeeds.
    event Claimed(uint256 user_id, address account, uint256 amount, bytes32 leaf);

    // This event is triggered when unclaimed drops are moved to Timelock after CONTRACT_ACTIVE period 
    event TransferUnclaimed(uint256 amount);

    /**
     * @notice Construct a new TokenDistribution contract 
     * @param _token - address of ERC20 that claims will be distributed from
     * @param _timeLock - address of the timelock contract where unclaimed funds will be swept   
     **/
    constructor(address _token, address _timeLock, bytes32 _merkleRoot) public {
        token = _token;
        merkleRoot = _merkleRoot;
        timeLockContract = _timeLock;
        deployTime = block.timestamp; 
        
        DOMAIN_SEPARATOR = hash(EIP712Domain({
            name: "GTC",
            version: '1.0.0',
            chainId: 1,
            verifyingContract: address(this)
        }));

    }
    
    /**
    * @notice process incoming token claims, must be signed by <signer>  
    * @param user_id - serves as nonce - only one claim per user_id
    * @param delegate_to - address token claim will be deletaged too 
    * @param merkle_proof - proof hashes for leaf
    * @param temporary_address - temporary Ethereum address that's used only for signing
    * @param sender_signature - result of temporary_address signing msg.sender
    **/
    function claimTokens(
        uint32 user_id, 
        address delegate_to,
        bytes32[] calldata merkle_proof,
        address temporary_address,
        bytes calldata sender_signature
    ) external {
        // one claim per user  
        require(!isClaimed(user_id), 'TokenDistributor: Tokens already claimed.');

        require(temporary_address != address(0), 'TokenDistributor: Invalid temporary_address');
        
        bytes32 leaf = keccak256(abi.encode(user_id, temporary_address));
        // does the leaf exist on our tree? 
        require(MerkleProof.verify(merkle_proof, merkleRoot, leaf), 'TokenDistributor: Valid Proof Required.');

        bool isIt = isSignedBy(sender_signature, temporary_address);
        console.log("isSignedBy %s %s", isIt, temporary_address);
        require(isIt, 'TokenDistributor: Valid msg.sender Signature required');

        
        // process token claim !! 
        // TODO: is it OK to delegate before transfer?
        _delegateTokens(delegate_to); 
        _setClaimed(user_id);

        uint256 amount = currentAward();
   
        require(IERC20(token).transfer(msg.sender, amount), 'TokenDistributor: Transfer failed.');
        emit Claimed(user_id, msg.sender, amount, leaf);
    }

    /**
    * @notice verify that bytes of 'msg.sender' were signed by the temporary_address's private key
    * @return whether the signature is correct
    **/
    function isSignedBy(bytes calldata sender_signature, address expected_signer) private view returns (bool) {
        console.log("msg.sender %s", msg.sender);
        bytes32 hashed_sender = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n20", msg.sender));
        console.logBytes32(hashed_sender);
        console.logBytes(sender_signature);
        address signer = ECDSA.recover(hashed_sender, sender_signature);
        console.log("recovered signer %s", signer);
        return signer == expected_signer;
    }

    function checkSignedBy(bytes calldata sender_signature, address expected_signer) external view returns (bool) {
        return isSignedBy(sender_signature, expected_signer);
    }

    function packedSender() external view returns (bytes memory) {
        return abi.encodePacked("\x19Ethereum Signed Message:\n20", msg.sender);
    }

    function hashedSender() external view returns (bytes32) {
        return keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n20", msg.sender));
    }

    function senderSigner(bytes calldata sender_signature) external view returns (address) {
        bytes32 hashed_sender = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n20", msg.sender));
        address signer = ECDSA.recover(hashed_sender, sender_signature);
        return signer;
    }
    
    /**
    * @notice checks claimedBitMap to see if if user_id is 0/1
    * @dev fork from uniswap merkle distributor, unmodified
    * @return - boolean  
    **/
    function isClaimed(uint256 index) public view returns (bool) {
        uint256 claimedWordIndex = index / 256;
        uint256 claimedBitIndex = index % 256;
        uint256 claimedWord = claimedBitMap[claimedWordIndex];
        uint256 mask = (1 << claimedBitIndex);
        return claimedWord & mask == mask;
    }
    
    /**
    * @notice used to move any remaining tokens out of the contract after expiration   
    **/
    function transferUnclaimed() public {
        require(block.timestamp >= deployTime + CONTRACT_ACTIVE, 'TokenDistributor: Contract is still active.');
        // transfer all GTC to TimeLock
        uint remainingBalance = IERC20(token).balanceOf(address(this));
        require(IERC20(token).transfer(timeLockContract, remainingBalance), 'TokenDistributor: Transfer unclaimed failed.');
        emit TransferUnclaimed(remainingBalance);
    }

    /**
    * @notice - function can be used to create DOMAIN_SEPARATORs
    * @dev - from EIP712 spec, unmodified 
    **/
    function hash(EIP712Domain memory eip712Domain) internal pure returns (bytes32) {
        return keccak256(abi.encode(
            EIP712DOMAIN_TYPEHASH,
            keccak256(bytes(eip712Domain.name)),
            keccak256(bytes(eip712Domain.version)),
            eip712Domain.chainId,
            eip712Domain.verifyingContract
        ));
    }

    /**
    * Returns current size of the award in tokens
    **/
    function currentAward() public view returns (uint256) {
        uint deployedFor = block.timestamp - deployTime;
        // how many times do we have to reduce award in half
        uint halveTimes = deployedFor / HALVE_PERIOD;
        uint denominator = 2**halveTimes;

        if (denominator > INITIAL_AWARD) return 0;
        
        return INITIAL_AWARD / denominator;
    }

    /**
    * Returns timestamp when award will be halved
    */
    function nextHalvePeriod() public view returns (uint) {
        return block.timestamp + timeTillAwardHalve();
    }

    /**
    * Returns how much time left till award will be halved
    */
    function timeTillAwardHalve() public view returns (uint) {
        uint deployedFor = block.timestamp - deployTime;
        return HALVE_PERIOD - (deployedFor % HALVE_PERIOD);
    }

    /**
    * @notice Sets a given user_id to claimed 
    * @dev taken from uniswap merkle distributor, unmodified
    **/
    function _setClaimed(uint256 index) private {
        uint256 claimedWordIndex = index / 256;
        uint256 claimedBitIndex = index % 256;
        claimedBitMap[claimedWordIndex] = claimedBitMap[claimedWordIndex] | (1 << claimedBitIndex);
    }

    /**
    * @notice execute call on token contract to delegate tokens   
    */
    function _delegateTokens(address delegatee) private {
         IToken GovToken = IToken(token);
         GovToken.delegate(delegatee);
    }
}

