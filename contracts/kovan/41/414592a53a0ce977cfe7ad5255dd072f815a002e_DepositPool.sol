contract DepositPool is Initializable, UUPSUpgradeable, ERC721HolderUpgradeable, ERC20Upgradeable {
    using Concat for string;
    using Counters for Counters.Counter;
    
    FeedRegistryInterface registry = FeedRegistryInterface(0xAa7F6f7f507457a1EE157fE97F6c7DB2BEec5cD0);

    mapping(address => bool) public budgetApprovals;
    address public dao;
    address[] public assets;
    mapping(address => bool) public isAssetSupported;
    
    function initialize() public initializer {
        __ERC721Holder_init();
        __ERC20_init("Deposit Pool", "DP");
        dao = msg.sender;
        // addAssets([0xa36085F69e2889c224210F603D836748e7dC0088]);
    }

    function deposit() public payable {
        // if (!IDao(dao).contributors(msg.sender)) {
        //     IDao(dao).mintContributor(msg.sender);
        // }
        uint256 totalSupply = totalSupply();

        if (totalSupply == 0) {
            _mint(msg.sender, msg.value);
        }

        uint256 total = address(this).balance - msg.value;
        for (uint256 i = 0; i < assets.length; i++) {
            total += assetEthPrice(assets[i]) * IERC20Metadata(assets[i]).balanceOf(address(this)) / 1e8;
        }

        _mint(msg.sender, msg.value / total / totalSupply);
    }

    function depositToken(address erc20, uint256 amount) public payable {
        require(isAssetSupported[erc20], "Asset not support");
        require(IERC20Metadata(erc20).allowance(msg.sender, address(this)) >= amount, "not approve");
        _mint(msg.sender, amount / assetUnitPrice(erc20));
        IERC20Metadata(erc20).transferFrom(msg.sender, address(this), amount);
    }

    function canAddAsset(address erc20) public view returns (bool) {
        try registry.getFeed(erc20, Denominations.ETH) {
            return true;
        } catch (bytes memory /*lowLevelData*/) {
            return false;
        }
    }

    function addAssets(address[] calldata erc20s) public {
        for (uint256 i = 0; i < erc20s.length; i++) {
            require(canAddAsset(erc20s[i]) && !isAssetSupported[erc20s[i]], "Asset not support");
            assets.push(erc20s[i]);
            isAssetSupported[erc20s[i]] = true;
        }
    }

    function totalEthPrice() public view returns (uint256) {
        uint256 total = address(this).balance;
        for (uint256 i = 0; i < assets.length; i++) {
            total += assetEthPrice(assets[i]) * IERC20Metadata(assets[i]).balanceOf(address(this)) / 1e8;
        }
        return total;
    }

    function unitPrice() public view returns (uint256) {
        uint256 totalSupply = totalSupply();
        if (totalSupply == 0) {
            return 1;
        }
        return totalEthPrice() / totalSupply;
    }

    function assetEthPrice(address erc20) public view returns (uint256) {
        (, int price,,,) = registry.latestRoundData(erc20, Denominations.ETH);
        if (price > 0) {
            return uint256(price);
        }
        return 0;
    }

    function assetUnitPrice(address erc20) public view returns (uint256) {
        uint256 totalSupply = totalSupply();
        if (totalSupply == 0) {
            return 1;
        }
        return 1e8 / assetEthPrice(erc20) * unitPrice();
    }


    function _authorizeUpgrade(address newImplementation) internal override {}

    receive() external payable {}
}

