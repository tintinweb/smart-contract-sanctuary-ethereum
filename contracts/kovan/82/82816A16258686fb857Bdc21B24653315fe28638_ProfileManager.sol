// File contracts/ProfileManager.sol

pragma solidity ^0.8.0;

contract ProfileManager is Ownable {
    using Counters for Counters.Counter;
    using SafeERC20 for IERC20;

    Counters.Counter private _packageCounter;

    IUniswapV2Router02 public uniswapV2Router02;

    mapping(uint256 => address[]) public pairs;
    mapping(uint256 => uint16[]) public share; // share of each pair 0.1 = 100
    mapping(uint256 => address[]) public pairToken; // token of each pair

    constructor(IUniswapV2Router02 _uniswapV2Router02) {
        uniswapV2Router02 = _uniswapV2Router02;
    }

    function updateUniRouter(IUniswapV2Router02 _uniswapV2Router02)
        public
        onlyOwner
    {
        uniswapV2Router02 = _uniswapV2Router02;
    }

    function getPackageCounter() public view returns (uint256) {
        return _packageCounter.current();
    }

    function addPackage(address[] memory _pairs, uint16[] memory _share)
        public
        onlyOwner
    {
        uint64 totalShare;
        uint256 _pkgIndex = _packageCounter.current();
        address weth = uniswapV2Router02.WETH();
        for (uint64 i = 0; i < _share.length; i++) {
            address _token0 = IUniswapV2Pair(_pairs[i]).token0();
            address _token1 = IUniswapV2Pair(_pairs[i]).token1();
            require(
                _token0 == weth || _token1 == weth,
                "UniswapV2: PAIR_NOT_WETH"
            );
            address token = _token0 == weth ? _token1 : _token0;
            pairToken[_pkgIndex].push(token);
            require(_share[i] > 0, "Share must be greater than 0");
            totalShare += _share[i];
        }
        require(totalShare == 1000, "total share must be 1000");
        require(_pairs.length == _share.length, "invalid pair/share length");
        pairs[_pkgIndex] = _pairs;
        share[_pkgIndex] = _share;
        _packageCounter.increment();
    }

    function buyPackage(uint64 _pkgIndex) public payable {
        require(_pkgIndex < _packageCounter.current(), "invalid package index");
        address account = msg.sender;
        uint256 amount = msg.value;
        require(amount > 0, "amount too small");
        address[] memory _pairs = pairs[_pkgIndex];
        address weth = uniswapV2Router02.WETH();
        uint256 length = _pairs.length;
        for (uint64 i = 0; i < length; ) {
            address _token = pairToken[_pkgIndex][i];
            address[] memory path = new address[](2);
            path[0] = weth;
            path[1] = _token;
            uniswapV2Router02.swapExactETHForTokens{
                value: (amount * share[_pkgIndex][i]) / 1000
            }(0, path, account, block.timestamp);
            unchecked {
                ++i;
            }
        }
    }

    function sellPackage(uint64 _pkgIndex, uint256 sellAmount) public {
        require(_pkgIndex < _packageCounter.current(), "invalid package index");
        address account = msg.sender;
        address[] memory _pairs = pairs[_pkgIndex];
        address weth = uniswapV2Router02.WETH();
        uint256 length = _pairs.length;
        address[] memory path = new address[](2);
        for (uint64 i = 0; i < length; ) {
            address token = pairToken[_pkgIndex][i];
            uint256 amount = (sellAmount * share[_pkgIndex][i]) / 1000;
            path[0] = token;
            path[1] = weth;
            uint256[] memory amountIn = uniswapV2Router02.getAmountsIn(
                amount,
                path
            );
            require(amount > 0, "invalid amount");
            IERC20(token).safeTransferFrom(account, address(this), amountIn[0]);
            IERC20(token).approve(
                address(uniswapV2Router02),
                0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff
            );
            uniswapV2Router02.swapTokensForExactETH(
                amount,
                amountIn[0],
                path,
                account,
                block.timestamp
            );
            unchecked {
                ++i;
            }
        }
    }

    function getPackage(uint64 _pkgIndex)
        public
        view
        returns (
            address[] memory,
            uint16[] memory,
            address[] memory
        )
    {
        return (pairs[_pkgIndex], share[_pkgIndex], pairToken[_pkgIndex]);
    }
}