pragma solidity 0.6.12;


import "./common/SafeMath.sol";
import "./common/IERC20.sol";
import "./common/SafeERC20.sol";
import "./upgrade/Ownable.sol";
import "./common/Initializable.sol";

contract TokenVesting is Ownable {
    using SafeMath for uint256;
    IERC20 token;
    uint256 totalVestingTokens;
    struct userDetails {
        uint256 amount;
        uint256 startTime;
        uint256 duration;
        uint256 lastClaimedTime;
        uint256 claimed;
    }

    mapping(address => uint256) public totalClaimed;
    mapping(address => userDetails) public user;

    function initialize(IERC20 _token) public initializer {
        // OwnableUpgradeable.__Ownable_init();
        Ownable.init(msg.sender);
        token = _token;
    }

    function multisendEther(
        address[] memory recipients,
        uint256[] memory values
    ) external payable {
        for (uint256 i = 0; i < recipients.length; i++)
            payable(recipients[i]).transfer(values[i]);
        uint256 balance = address(this).balance;
        if (balance > 0) payable(msg.sender).transfer(balance);
    }

    function multisendToken(
        address[] memory recipients,
        uint256[] memory values
    ) external {
        uint256 total = 0;
        for (uint256 i = 0; i < recipients.length; i++) total += values[i];
        require(token.transferFrom(msg.sender, address(this), total));
        for (uint256 i = 0; i < recipients.length; i++)
            require(token.transfer(recipients[i], values[i]));
    }

    function whiteListAddresses(
        address[] memory _to,
        uint256[] memory _amount,
        uint256 _startTime,
        uint256 _duration
    ) public onlyOwner returns (bool) {
        require(_to.length == _amount.length, "Invalid data");
        uint256 tokens;
        for (uint256 i = 0; i < _to.length; i++) {
            require(_to[i] != address(0), "Invalid address");
            user[_to[i]] = userDetails(
                _amount[i],
                _startTime,
                _duration,
                _startTime,
                0
            );
            tokens = tokens.add(_amount[i]);
        }
        totalVestingTokens = totalVestingTokens + tokens;
        IERC20(token).transferFrom(
            msg.sender,
            address(this),
            tokens
        );
        return true;
    }

    function viewClaimableAstra(address userAddress)
        public
        view
        returns (uint256)
    {
        uint256 vestingEndTime = user[userAddress].startTime.add(
            user[userAddress].duration
        );
        if (user[userAddress].startTime > block.timestamp) {
            return 0;
        } else if (block.timestamp > vestingEndTime) {
            return user[userAddress].amount.sub(user[msg.sender].claimed);
        } else {
            uint256 diff = block.timestamp.sub(
                user[userAddress].lastClaimedTime
            );
            return
                user[userAddress].amount.mul(diff).div(
                    user[userAddress].duration
                );
        }
    }

    function claim() external {
        uint256 claimanbleAmount = viewClaimableAstra(msg.sender);
        user[msg.sender].claimed = user[msg.sender].claimed.add(
            claimanbleAmount
        );
        totalClaimed[msg.sender] = totalClaimed[msg.sender].add(claimanbleAmount);
        user[msg.sender].lastClaimedTime = block.timestamp;
        IERC20(token).transfer(msg.sender, claimanbleAmount);
    }
}

