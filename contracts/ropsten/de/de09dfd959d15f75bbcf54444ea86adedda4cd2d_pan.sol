     * @dev Converts a `uint256` to its ASCII `string` decimal representation.
     */
    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT licence
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }

    /**
     * @dev Converts a `uint256` to its ASCII `string` hexadecimal representation.
     */
    function toHexString(uint256 value) internal pure returns (string memory) {
        if (value == 0) {
            return "0x00";
        }
        uint256 temp = value;
        uint256 length = 0;
        while (temp != 0) {
            length++;
            temp >>= 8;
        }
        return toHexString(value, length);
    }

    /**
     * @dev Converts a `uint256` to its ASCII `string` hexadecimal representation with fixed length.
     */
    function toHexString(uint256 value, uint256 length) internal pure returns (string memory) {
        bytes memory buffer = new bytes(2 * length + 2);
        buffer[0] = "0";
        buffer[1] = "x";
        for (uint256 i = 2 * length + 1; i > 1; --i) {
            buffer[i] = _HEX_SYMBOLS[value & 0xf];
            value >>= 4;
        }
        require(value == 0, "Strings: hex length insufficient");
        return string(buffer);
    }
}

 contract IVP1155 is
    Initializable,
    ERC1155Upgradeable,
    OwnableUpgradeable,
    PausableUpgradeable,
    ERC1155BurnableUpgradeable,
    ERC1155SupplyUpgradeable
{
    string public name;
    mapping(uint256 => bool) public frozen;

    constructor() {}

    function initialize(
        string memory _name,
        string memory _uri,
        address _owner
    ) public virtual initializer {
        _transferOwnership(_owner);
        __ERC1155_init(_uri);
        __Ownable_init();
        __Pausable_init();
        __ERC1155Burnable_init();
        __ERC1155Supply_init();
        name = _name;
    }

    function freeze(uint256 _id) public virtual onlyManager {
        require(!frozen[_id], "alread freezed");
        frozen[_id] = true;
    }

    function unfreeze(uint256 _id) public virtual onlyManager {
        require(frozen[_id], "not freezed");
        frozen[_id] = false;
    }

    function setURI(string memory newuri) public virtual onlyOwner {
        _setURI(newuri);
    }

    function pause() public virtual onlyOwner {
        _pause();
    }

    function unpause() public virtual onlyOwner {
        _unpause();
    }

    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual {
     
            require(Manager[msg.sender], "can not mint to others");
            require(!exists(id), "token exists");
    
        _mint(account, id, amount, data);
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public virtual onlyManager {
        _mintBatch(to, ids, amounts, data);
    }

    function mintBatch2(
        address[] memory accounts,
        uint256[] memory ids,
        uint256[] memory amounts
    ) public virtual onlyManager {
        require(
            accounts.length == ids.length && ids.length == amounts.length,
            "invalid arguments"
        );
        for (uint256 i = 0; i < ids.length; i++) {
            _mint(accounts[i], ids[i], amounts[i], "0x00");
        }
    }

    function batchTransfer(
        address[] memory _from,
        address[] memory _to,
        uint256[] memory _ids,
        uint256[] memory _amounts
    ) public virtual {
        require(
            _from.length == _to.length &&
                _to.length == _ids.length &&
                _ids.length == _amounts.length,
            "invalid arguments"
        );
        for (uint256 i = 0; i < _from.length; i++) {
            safeTransferFrom(_from[i], _to[i], _ids[i], _amounts[i], "0x00");
        }
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    )
        internal
        virtual
        override(ERC1155Upgradeable, ERC1155SupplyUpgradeable)
        whenNotPaused
    {
        for (uint256 i = 0; i < ids.length; i++) {
            require(!frozen[ids[i]], "token freezed");
        }
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function uri(uint256 _id)
        public
        view
        virtual
        override
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    super.uri(_id),
                    StringsUpgradeable.toString(_id)
                )
            );
    }
}


 

 

interface IERC20 {

    function totalSupply() external view returns (uint256);

    function balanceOf(address account) external view returns (uint256);

    function transfer(address recipient, uint256 amount) external returns (bool);

    function allowance(address owner, address spender) external view returns (uint256);

    function approve(address spender, uint256 amount) external returns (bool);

 
    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool);

    event Transfer(address indexed from, address indexed to, uint256 value);

    event Approval(address indexed owner, address indexed spender, uint256 value);
}



 

contract pan is IVP1155  {

    event Merge(
        address _from,
        uint256[] ids,
        uint256[] amounts,
        uint256 newid,
        uint256 newamount
    );

    event Swap(
        address _from,
        uint256[] ids,
        uint256[] amounts,
        address contractAddr,
        uint256 amount
    );

    constructor( string memory _name
    ) {
 
        _transferOwnership(msg.sender);
     
    
        name = _name;
    }

    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual override onlyManager {
        _mint(account, id, amount, data);
    }

    function isApprovedForAll(address account, address operator)
        public
        view
        virtual
        override
        returns (bool)
    {
        return
            (msg.sender == owner()) ||
            super.isApprovedForAll(account, operator);
    }

    function merge(
        address _from,
        uint256[] memory ids,
        uint256[] memory amounts,
        uint256 newid,
        uint256 newamount
    ) public onlyManager {
        require(ids.length == amounts.length);
        for (uint256 i = 0; i < ids.length; i++) {
            _burn(_from, ids[i], amounts[i]);
        }
        _mint(_from, newid, newamount, "0x00");
        emit Merge(_from, ids, amounts, newid, newamount);
    }

    function swap(
        address _from,
        uint256[] memory ids,
        uint256[] memory amounts,
        address contractAddr,
        uint256 amount
    ) public onlyManager {
        require(ids.length == amounts.length);
        for (uint256 i = 0; i < ids.length; i++) {
            _safeTransferFrom(_from, owner(), ids[i], amounts[i], "0x00");
        }
        IERC20(contractAddr).transfer(_from, amount);
        emit Swap(_from, ids, amounts, contractAddr, amount);
    }

    function withdrawErc20(
        address _to,
        address _contract,
        uint256 amount
    ) public onlyManager {
        IERC20(_contract).transfer(_to, amount);
    }




    
}