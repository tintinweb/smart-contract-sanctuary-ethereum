pragma solidity ^0.8.0;
pragma abicoder v2;

import "../royalties/Royalties.sol";
import "../services/Services.sol";

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155SupplyUpgradeable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract ERC1155Minimal is Initializable, ERC1155SupplyUpgradeable, EIP712, OwnableUpgradeable {

    string private SIGNING_DOMAIN;
    string private constant SIGNATURE_VERSION = "1";

    address private royalties;
    address private services;

    mapping(uint256 => uint256) private royaltiesId;

    constructor() EIP712(SIGNING_DOMAIN, SIGNATURE_VERSION){}

    function __ERC1155Minimal_init(address _royalties, address _services, string memory _name, string memory _uri) public initializer{
        SIGNING_DOMAIN = _name;
        __ERC1155_init(_uri);
        __Ownable_init();
        royalties = _royalties;
        services = _services;
    }

    struct NFT1155 {
        /// @notice The id of the token to be redeemed. Must be unique - if another token with this ID already exists, the redeem function will revert.
        uint256 tokenId;
        /// @notice the value of the token of the total supply
        uint256 tokenValue;
        /// @notice The price (in wei) that the NFT creator is willing to accept for the initial sale of this NFT.
        uint256 price;
        /// @notice The metadata URI to associate with this token.
        string uri;
        /// @notice Percentage of royalties Ex: 1% is 100, 2.5% is 250, 10% is 1000
        uint32 royaltiesPercentage;
        /// @notice The ERC-20 token accept to pay.
        address ERC20Token;
        /// @notice the EIP-712 signature of all other fields in the NFT721 struct. For a voucher to be valid, it must be signed by an account with the MINTER_ROLE.
        bytes signature;
    }

    /// @notice sender creating an NFT.
    /// @param nft1155 A signed NFT721 that describes the NFT to be redeemed.
    function lazyMint(NFT1155 calldata nft1155, uint256 tokenAmount) public payable {
        address buyer = msg.sender;
        address signer = _verify(nft1155);
        require(!exists(nft1155.tokenId), "TokenId already exists");
        require(tokenAmount > 0,"Minimum purchase is 1");
        require(tokenAmount <= nft1155.tokenValue,"Buy don't exceed the maximum");
        if (nft1155.royaltiesPercentage > 0) {
            require(nft1155.royaltiesPercentage <= 1000,"Royalty fee not more than 10%");
        }
        address payable serviceOwner = payable(Services(services).recipients());
        uint256 totalPrice = nft1155.price * tokenAmount;
        uint256 fee = Services(services).feeCalculation(totalPrice);
        if (nft1155.ERC20Token != address(0)) {
            _mint(signer, nft1155.tokenId, nft1155.tokenValue, "");
            // transfer the token to the buyer
            _safeTransferFrom(signer, buyer, nft1155.tokenId, tokenAmount, "");
            uint256 amount = totalPrice - fee;
            // transfer tokens ERC20 to signer
            IERC20(nft1155.ERC20Token).transferFrom(buyer, signer, amount);
            // take service fee
            IERC20(nft1155.ERC20Token).transferFrom(buyer, serviceOwner, fee);
        } else {
            require(msg.value == totalPrice, "Insufficient funds");
            _mint(signer, nft1155.tokenId, nft1155.tokenValue, "");
            // transfer the token to the buyer
            _safeTransferFrom(signer, buyer, nft1155.tokenId, tokenAmount, "");
            uint256 amount = msg.value - fee;
            // transfer funds to seller
            address payable seller = payable(signer);
            seller.transfer(amount);
            // take service fee
            serviceOwner.transfer(fee);
        }
        if (nft1155.royaltiesPercentage > 0) {
            _setRoyalties(signer, nft1155.tokenId, nft1155.royaltiesPercentage);
        }
    }

    /// @notice Mint NFT with the usual way. Mint people will have to pay the cost gas
    /// @param tokenId, _royaltiesPercent, tokenValue
    function mint(uint256 tokenId, uint32 _royaltiesPercent, uint256 tokenValue) public {
        require(tokenId > 0, "tokenId must be greater than 0");
        require(!exists(tokenId), "TokenId already exists");
        require(tokenValue > 0, "tokenValue must be greater than 0");
        if (_royaltiesPercent > 0) {
            require(_royaltiesPercent <= 1000, "Royalty fee not more than 10%");
            _mint(msg.sender, tokenId, tokenValue, "");
            _setRoyalties(msg.sender, tokenId, _royaltiesPercent);
        } else {
            _mint(msg.sender, tokenId, tokenValue, "");
        }
    }

    /// @notice Update Royalties fee for 1 NFT
    /// @param tokenId and percent (uint32) with 100 is 1%, 1000 is 10%
    function _setRoyalties(address owner,uint256 tokenId, uint32 percent) internal {
        require(exists(tokenId), "TokenId don't exist");
        require(percent > 0, "Royalties not allowed");
        uint _royaltiesId = Royalties(royalties).setRoyalties(address(this), tokenId, owner, percent);
        royaltiesId[tokenId] = _royaltiesId;
    }

    /// @notice decrypt a signed transaction
    /// @param nft1155 NFT information
    function verify(NFT1155 calldata nft1155) public view returns (address) {
        address signer = _verify(nft1155);
        return signer;
    }

    function setRoyaltiesAddress(address _address) external onlyOwner {
        royalties = _address;
    }

    function getRoyaltiesId(uint256 _tokenId) public view returns (uint256){
        return royaltiesId[_tokenId];
    }

    /// @notice Returns a hash of the given NFT1155, prepared using EIP712 typed data hashing rules.
    /// @param nft1155 An NFT1155 to hash.
    function _hash(NFT1155 calldata nft1155) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        keccak256(
                            "NFT1155(uint256 tokenId,uint256 tokenValue,uint256 price,string uri,uint32 royaltiesPercentage,address ERC20Token)"
                        ),
                        nft1155.tokenId,
                        nft1155.tokenValue,
                        nft1155.price,
                        keccak256(bytes(nft1155.uri)),
                        nft1155.royaltiesPercentage,
                        nft1155.ERC20Token
                    )
                )
            );
    }

    /// @notice Verifies the signature for a given NFT1155, returning the address of the signer.
    /// @dev Will revert if the signature is invalid. Does not verify that the signer is authorized to mint NFTs.
    /// @param nft1155 describing an unminted NFT.
    function _verify(NFT1155 calldata nft1155) internal view returns (address) {
        bytes32 digest = _hash(nft1155);
        return ECDSA.recover(digest, nft1155.signature);
    }
    
}

