pragma solidity 0.8.10;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";

import "./interfaces/IABLP.sol";
import "./interfaces/IWallet.sol";
import "./utils/Math.sol";

import "hardhat/console.sol";

contract ABLP is IABLP, OwnableUpgradeable, ERC20Upgradeable, PausableUpgradeable, AccessControlUpgradeable {

    bytes32 public constant ABLP_BOT_ROLE = keccak256("ABLP_BOT_ROLE");
    bytes32 public constant COSIGNER_ROLE = keccak256("COSIGNER_ROLE");
    bytes32 public constant WITHDRAWER_ROLE = keccak256("WITHDRAWER_ROLE");

    uint256 constant public ONE_COIN = 1e18;

    modifier onlyAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "ABLP/DEFAULT_ADMIN_ROLE is required");
        _;
    }

    modifier onlyBot() {
        require(hasRole(ABLP_BOT_ROLE, msg.sender), "ABLP/ABLP_BOT_ROLE is required");
        _;
    }

    modifier onlyCoSigner() {
        require(hasRole(COSIGNER_ROLE, msg.sender), "ABLP/COSIGNER_ROLE is required");
        _;
    }

    modifier onlyWithdrawer() {
        require(hasRole(WITHDRAWER_ROLE, msg.sender), "ABLP/WITHDRAWER_ROLE is required");
        _;
    }

    IERC20  public baseAsset;
    IWallet public wallet;

    int256 public currentBalance;
    // average user share price [1e18]
    mapping(address => uint256) public rate;

    // System Commission [1e18]
    uint256 public performanceFee;
    uint256 public fees;

    // ID of the current Period, total number of Periods
    uint256 public totalPeriods;

    // periodId => Period
    mapping(uint256 => Period) public periods;

    bool public hasCoSigner;

    mapping(address => uint256) pendingWithdrawals;

    struct Withdrawal {
        uint256 amount;
        uint256 lockTs;
    }

    uint256 public totalPendingWithdrawals;
    uint256 public withdrawalLock;

    function initialize(address baseAsset_, bool hasCoSigner_)
    external
    initializer
    {
        __Ownable_init();
        __ERC20_init_unchained("ABLP LP Token", "ABLP");

        require(baseAsset_ != address(0), "ABLP/invalid-base-asset");
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        baseAsset = IERC20(baseAsset_); // USDT

        hasCoSigner = hasCoSigner_;

        periods[0].timeStart = block.timestamp;
    }

    function setWallet(
        address _wallet
    ) external onlyAdmin {
        wallet = IWallet(_wallet);

        emit WalletChanged(msg.sender, _wallet);
    }

    function setBot(
        address bot
    ) external onlyAdmin {
        require(bot != address(0), "ABLP/invalid-address");
        grantRole(ABLP_BOT_ROLE, bot);

        emit BotAdded(msg.sender, bot);
    }

    function removeBot(
        address bot
    ) external onlyAdmin {
        require(bot != address(0), "ABLP/invalid-address");
        require(hasRole(ABLP_BOT_ROLE, bot), "ABLP/not-bot");
        revokeRole(ABLP_BOT_ROLE, bot);
    }

    function setCoSignerAvailable(
        bool _hasCoSigner
    ) external onlyAdmin {
        hasCoSigner = _hasCoSigner;

        emit CoSignerChanged(msg.sender, _hasCoSigner);
    }

    function setWithdrawalLockTime(
        uint256 time
    ) external onlyAdmin {
        withdrawalLock = time;

        emit WithdrawalLockChanged(msg.sender, time);
    }

    /// @notice Close the current Period
    /// @param _periodId to close
    /// @param _matchedBalance Total balance matched in the Period
    /// @param _periodResult Total profit/loss generated in the Period
    /// @param _betsHash Hash of all the bets in the Period
    function closePeriod(
        uint256 _periodId,
        uint256 _matchedBalance,
        int256 _periodResult,
        bytes32 _betsHash
    ) external onlyBot {
        require(periods[_periodId].timeStart != 0, "ABLP/period-not-started");
        require(_periodId == totalPeriods, "ABLP/not-last-period");

        //update values for started period
        periods[_periodId].matchedBalance = _matchedBalance;
        periods[_periodId].pnl = _periodResult;
        periods[_periodId].betsHash = _betsHash;
        periods[_periodId].status = periodStatus.Pending;

        totalPeriods += 1;

        // start next period
        if (!hasCoSigner) {
            periods[_periodId].status = periodStatus.Accepted;

            currentBalance += _periodResult;
            //balance at the start of period
            periods[totalPeriods].totalBalance = currentBalance;
            if (currentBalance < 0) {
                currentBalance = 0;
            }

            // withdraw or deposit to wallet
            settlePnl(_periodResult);
        }
        periods[totalPeriods].timeStart = block.timestamp;

        emit PeriodClosed(_periodId, _periodResult, currentBalance);
    }

    /// @notice Confirm / decline closed Period by ABLP_COSIGNER_ROLE
    /// @param _periodId Period ID
    /// @param _pnl Total profit/loss generated in the Period
    /// @param _accepted Accepted or declined
    function confirmPeriod(
        uint256 _periodId,
        int256 _pnl,
        bool _accepted
    ) external onlyCoSigner {
        require(hasCoSigner, "ABLP/cosigner-not-required");
        require(periods[_periodId].status == periodStatus.Pending, "ABLP/period-not-closed");
        require(periods[_periodId].pnl == _pnl, "ABLP/pnl-mismatch");

        if (_accepted) {
            periods[_periodId].status = periodStatus.Accepted;

            currentBalance += _pnl;
            if (currentBalance < 0) {
                currentBalance = 0;
            }
            periods[_periodId].totalBalance = currentBalance;

            // withdraw or deposit to wallet
            settlePnl(_pnl);

            emit PeriodConfirmed(_periodId);
        } else {
            periods[_periodId].status = periodStatus.Declined;

            emit PeriodDeclined(_periodId);
        }
    }

    function settlePnl(int256 diff) internal {
        if (diff == 0) {
            return;
        }
        if (diff < 0) {
            require(baseAsset.transfer(address(wallet), uint256(-diff)), "ABLP/wallet-payment-failed");

            emit BalanceSettled(diff, currentBalance);
        } else {
            require(wallet.withdraw(address(baseAsset), uint256(diff)), "ABLP/wallet-payment-failed");
            uint256 fee = Math.mulDiv(uint256(diff), performanceFee, ONE_COIN);
            fees += fee;
            currentBalance -= int256(fee);
            if (currentBalance < 0) {
                currentBalance = 0;
            }
            emit PerformanceFeeCharged(totalPeriods, fee);
            emit BalanceSettled(diff, currentBalance);
        }
    }

    /// @notice Provide Liquidity to the Pool
    /// @param _amount Amount to send to the Pool
    function openPosition(uint256 _amount) external override {
        require(IERC20(baseAsset).transferFrom(msg.sender, address(this), _amount), "ABLP/transfer-failed");

        uint256 depositAmount = _amount;
        if (currentBalance < 0) {
            int256 diff = int256(_amount) - currentBalance;
            if (diff <= 0) {
                return;
            } else {
                depositAmount = uint256(diff);
            }
        }

        uint256 mintingAmount = depositAmount;
        // user get token amount equal to USDC amount
        if (uint256(currentBalance) > ONE_COIN) {
            uint256 share = Math.mulDiv(depositAmount, ONE_COIN, uint256(currentBalance));
            mintingAmount = Math.mulDiv(share, totalSupply(), ONE_COIN);
        }

        if (LPPrice() == 0) {
            rate[msg.sender] = 1e18;
        } else {
            uint256 userValue = getUserShareValue(msg.sender) + Math.mulDiv(mintingAmount, LPPrice(), ONE_COIN);
            rate[msg.sender] = Math.mulDiv(userValue, ONE_COIN, balanceOf(msg.sender) + mintingAmount);
        }

        currentBalance += int256(_amount);

        _mint(msg.sender, mintingAmount);

        emit Deposit(msg.sender, _amount, mintingAmount);
    }

    /// @notice Close Liquidity Position, confirmations from ABLP_FINANCE_ROLE will be required
    /// @param tokenAmount amount of tokens to burn
    function closePosition(uint256 tokenAmount) external override {
        uint256 toWithdraw = getTokensValue(tokenAmount);
        require(int256(toWithdraw) <= currentBalance, "ABLP/not-enough-balance");

        pendingWithdrawals[msg.sender] += toWithdraw;
        totalPendingWithdrawals += toWithdraw;

        _burn(msg.sender, tokenAmount);

        emit WithdrawalRequested(msg.sender, tokenAmount, toWithdraw);
    }


    function claim(uint256 amount) external override {
        require(amount <= available(), "ABLP/too-much-requested");
        totalPendingWithdrawals -= amount;
        pendingWithdrawals[msg.sender] -= amount;

        require(IERC20(baseAsset).transfer(msg.sender, amount), "ABLP/transfer-failed");

        emit Claim(msg.sender, amount);
    }

    function available() public override view returns(uint256) {
        return pendingWithdrawals[msg.sender];
    }

    function LPPrice() public view returns(uint256) {
        if (totalSupply() == 0) {
            return 0;
        }
        if (currentBalance <= 0) {
            return 0;
        }
        return Math.mulDiv(uint256(currentBalance), ONE_COIN, totalSupply());
    }

    function getTokensValue(uint256 amount) public override view returns (uint256) {
        return Math.mulDiv(amount, LPPrice(), ONE_COIN);
    }

    /**
    * @dev returns average user shares value in USDC  (ONE_COIN)
    */
    function getUserShareValue(address user) public view returns (uint256) {
        if (rate[user] != 0) {
            return Math.mulDiv(balanceOf(user), rate[user], ONE_COIN);
        } else {
            return balanceOf(user);
        }
    }

    /// @notice Get Periods
    /// @param _periodIdFrom Period ID from
    /// @param _periodIdTo Period ID to
    /// @return Period[]
    function getPeriods(uint256 _periodIdFrom, uint256 _periodIdTo) external view returns (Period[] memory) {
        int256 length = int256(_periodIdTo) - int256(_periodIdFrom);
        Period[] memory result = new Period[](_periodIdTo - _periodIdFrom);
        for (int256 i = 0; i < length; i++) {
            result[uint256(i)] = periods[uint256(i)];
        }
        return result;
    }

    /// @notice Get Profit / Loss of the Position for selected Periods
    /// @param _periodIdFrom Period ID from
    /// @param _periodIdTo Period ID to
    /// @return profit/loss
    function getPoolPerformance(uint256 _periodIdFrom, uint256 _periodIdTo) external override view returns (int256) {
        require(_periodIdFrom <= totalPeriods, "ABLP/incorrect-range");
        uint256 end = _periodIdTo;
        if (_periodIdTo > totalPeriods) {
            end = totalPeriods;
        }
        require(_periodIdTo >= end, "ABLP/incorrect-range");
        int256 accumulator;
        unchecked {
            uint256 size = _periodIdTo - end;
            for (uint256 i = 0; i <= size; i++) {
                accumulator += periods[_periodIdFrom + i].pnl;
            }
        }
        return accumulator;
    }


    /// @notice Withdraw Commission
    /// @param _account Account to send the commission to
    function withdrawCommission(
        address _account
    ) external onlyWithdrawer {
        uint256 toWithdraw = fees;
        fees = 0;
        require(baseAsset.transfer(_account, toWithdraw), "ABLP/withdraw-failed");

        emit FeesWithdrawal(msg.sender, _account, toWithdraw);
    }
}


