pragma solidity ^0.8.10;

import "./CoinTapNFT.sol";
import "./NFT.sol";
import "./interfaces/INFTMinter.sol";
import "./../services/FeesCharge.sol";
import "./../governance/Permissionable.sol";

/**
 * CoinTap NFT Minter
 */
contract NFTMinter is INFTMinter, FeesCharge, Permissionable, CoinTapNFT {
    constructor(address newConfig) {
        __Ownable_init();
        _setConfig(newConfig);
    }

    function updateFreeSlot(FreeSlot memory newFreeSlot)
        public
        onlyManagerOrOwner
    {
        _updateFreeSlot(newFreeSlot);
    }

    function setConfig(address newConfig) public onlyManagerOrOwner {
        _setConfig(newConfig);
    }

    /**
     * @dev Create new NFT
     *
     * @param name of NFT
     * @param symbol of NFT
     * @param baseUri of NFT
     * @return address new NFT
     */
    function newNFT(
        string memory name,
        string memory symbol,
        string memory baseUri
    ) public returns (address) {
        chargeFeeCoinTap(NFT_SERVICE);
        address owner = msg.sender;
        NFT nft = new NFT(name, symbol, baseUri, owner);
        emit NFTCreated(address(nft));
        return address(nft);
    }
}

