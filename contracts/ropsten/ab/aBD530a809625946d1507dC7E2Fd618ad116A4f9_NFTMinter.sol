import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./../libs/SafeArrays.sol";
import "./interfaces/ICoinTapConfig.sol";

/**
 * Contract charge fee service
 */
contract FeesCharge {
    bytes32 public constant NFT_SERVICE = bytes32(abi.encodePacked("nft"));
    bytes32 public constant TOKENIZATION_SERVICE =
        bytes32(abi.encodePacked("tokenization"));

    event FreeSlotChanged(uint32 defaultSlot, uint32 slots, uint256 startAt);

    error FeeIsNotEnough(bytes32 service, uint256 fee);

    using SafeERC20 for IERC20;
    using SafeArrays for address[];

    struct FreeSlot {
        uint32 defaultSlot; // default value for slots
        uint32 slots; // remain slot
        uint256 startAt;
        uint256 resetAt;
    }

    ICoinTapConfig public config; // address of CoinTap config
    FreeSlot public freeSlot;
    address[] public addresses; // Addresses created free

    constructor() {
        freeSlot = FreeSlot({
            defaultSlot: 5,
            slots: 5,
            startAt: block.timestamp,
            resetAt: 5 minutes
        });
    }

    function chargeFeeCoinTap(bytes32 service) internal virtual {
        IERC20 cointap = IERC20(config.cointap());
        uint256 fee = config.feesOf(service).token;
        uint256 balance = cointap.balanceOf(msg.sender);
        if (fee > 0 && fee > balance) {
            revert FeeIsNotEnough(service, balance);
        }
        // is free
        if (_free()) {
            unchecked {
                freeSlot.slots = freeSlot.slots - 1;
            }
            addresses.add(msg.sender);
            return;
        }
        cointap.safeTransferFrom(msg.sender, address(config), fee);
    }

    function chargeFeeNative(bytes32 service) internal virtual {
        uint256 fee = config.feesOf(service).native;
        if (fee > 0 && fee > msg.value) {
            revert FeeIsNotEnough(service, msg.value);
        }
        if (_free()) {
            unchecked {
                freeSlot.slots = freeSlot.slots - 1;
            }
            addresses.add(msg.sender);
            return;
        }
        payable(address(config)).transfer(msg.value);
    }

    function _setConfig(address newConfig) internal {
        config = ICoinTapConfig(newConfig);
    }

    function _free() internal returns (bool) {
        // Check remain free slot
        uint256 currentTime = block.timestamp;
        bool isNewMonth = currentTime - freeSlot.startAt > freeSlot.resetAt
            ? true
            : false;
        if (isNewMonth) {
            freeSlot.slots = freeSlot.defaultSlot;
            freeSlot.startAt = currentTime;
            delete addresses;
        }
        if (freeSlot.slots <= 0) {
            return false;
        }
        if (addresses.existed(msg.sender)) {
            return false;
        }
        return true;
    }

    function _updateFreeSlot(FreeSlot memory newFreeSlot) internal {
        freeSlot = newFreeSlot;
        emit FreeSlotChanged(
            newFreeSlot.defaultSlot,
            newFreeSlot.slots,
            newFreeSlot.startAt
        );
    }
}

