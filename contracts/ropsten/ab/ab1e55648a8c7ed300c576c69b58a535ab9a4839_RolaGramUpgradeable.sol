pragma solidity ^0.8.7;

import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

import "./BlackListUpgradeable.sol";
import "./IRolaGramUpgradeable.sol";

contract RolaGramUpgradeable is ERC721Upgradeable, PausableUpgradeable, OwnableUpgradeable, ReentrancyGuardUpgradeable, BlackListUpgradeable, IRolaGramUpgradeable {

    using CountersUpgradeable for CountersUpgradeable.Counter;

    // Index counter for the next NFT ID's
    CountersUpgradeable.Counter private nextTokenIndex;

    // Zero Address
    address constant ZERO_ADDRESS = address(0);

    IERC20Upgradeable public rolaContract;

    // Address of the maintainer
    address private maintainer;

    // Address of the treasury
    address private treasury;

    // Token Uri mapping correspond to token id
    mapping(uint => string) public rolaGramURI;

    function initialize(
        string memory tokenName,
        string memory tokenSymbol,
        address maintainerAddress,
        address treasuryAddress,
        address rolaContractAddress
    ) initializer external {
        require(maintainerAddress != ZERO_ADDRESS, "RolaCoaster: Cannot set maintainer with Zero Address.");
        require(treasuryAddress != ZERO_ADDRESS, "RolaCoaster: Cannot set treasury with Zero Address.");
        __ERC721_init(tokenName, tokenSymbol);
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();
        __BlackListEnabled_init();
        maintainer = maintainerAddress;
        treasury = treasuryAddress;
        rolaContract = IERC20Upgradeable(rolaContractAddress);
    }

    /// @dev This is the onlyMaintainer modifier. It is used by the backend to call contracts functions only by the maintainer address.

    modifier onlyMaintainer() {
        require(maintainer == _msgSender(), "RolaGram: caller is not the maintainer address");
        _;
    }

    /// @dev This is the uri function. It is used by any user to get the uri/metadata of the given tokenID.
    /// @param tokenId The unique ID of the token 

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "RolaGram: URI nonexistent token");
        return rolaGramURI[tokenId];
    }

    /// @dev This is the initializeMintRolaGram function. It is used for initialize the mint ROLAGRAM NFT by sending required ROLA to treasury.
    /// @param amountRola The amount of ROLA token to be sent to treasury for minting ROLAGRAM NFT

    function initializeMintRolaGram(uint256 amountRola) external override whenNotPaused nonReentrant {
        require(amountRola > 0, "RolaGram: ROLA amount should be greater than zero.");
        rolaContract.transferFrom(_msgSender(), treasury, amountRola);
        emit InitializeRolaGramMint(_msgSender(), amountRola);
    }

    /// @dev This is the endMintRolaGram function. It is used for completing the mint ROLAGRAM NFT.
    /// @dev Only the maintainer or artist can call this function
    /// @param account The account address to mint ROLAGRAM and transfer ROLA from it 
    /// @param tokenUri Token Uri of the metadata ROLAGRAM minted

    function endMintRolaGram(address account, string memory tokenUri) external override onlyMaintainer whenNotPaused nonReentrant whenNotBlackListedUser(account) {
        require(account != ZERO_ADDRESS, "RolaGram: Cannot mint RolaGram to Zero Address.");
        uint256 tokenID = nextTokenIndex.current();
        nextTokenIndex.increment();
        rolaGramURI[tokenID] = tokenUri;
        _safeMint(account, tokenID);
        emit NewRolaGramMinted(account, tokenID);
    }

    /// @dev This is the airdropRolaGram function. It is used by the owner to airdrop ROLAGRAM tokens to the `assigned` address respectively.
    /// @dev Only the owner can call this function
    /// @param assigned The address to be air dropped

    function airdropRolaGram(address[] memory assigned, string[] memory tokenUri) external override onlyOwner whenNotPaused nonReentrant {
        require(assigned.length == tokenUri.length, "RolaGram: Incorrect parameter length");
        for (uint8 index = 0; index < assigned.length; index++) {
            if(!_isBlackListUser(assigned[index])){
                uint256 tokenID = nextTokenIndex.current();
                nextTokenIndex.increment();
                rolaGramURI[tokenID] = tokenUri[index];
                _safeMint(assigned[index], tokenID);
                emit NewRolaGramMinted(assigned[index], tokenID);
            }
        }
    }

    /// @dev This is the updateMaintainerAddress function. It is used by the owner to update the maintainer address in the contract.
    /// @dev Only the owner or artist can call this function
    /// @param newMaintainerAddress New maintainer address for the RolaGram contract 

    function updateMaintainerAddress(address newMaintainerAddress) external override onlyOwner whenNotPaused {
        require(maintainer != newMaintainerAddress,"RolaGram: The new maintainer address must be different from the old one");
        require(newMaintainerAddress != ZERO_ADDRESS, "RolaGram: Cannot set maintainer with Zero Address.");
        maintainer = newMaintainerAddress;
        emit NewMaintainAddressSet(newMaintainerAddress);
    }

    /// @dev This is the getMaintainerAddress function. It is used to get the maintainer address in the contract.

    function getMaintainerAddress() external view override returns (address) {
        return maintainer;
    }

    /// @dev This is the updateTreasuryAddress function. It is used by the owner to update the treasury address in the contract.
    /// @dev Only the owner or artist can call this function
    /// @param newTreasuryAddress New maintainer address for the RolaGram contract 

    function updateTreasuryAddress(address newTreasuryAddress) external override onlyOwner whenNotPaused {
        require(treasury != newTreasuryAddress,"RolaGram: The new treasury address must be different from the old one");
        require(newTreasuryAddress != ZERO_ADDRESS, "RolaGram: Cannot set treasury with Zero Address.");
        treasury = newTreasuryAddress;
        emit NewTreasuryAddressSet(newTreasuryAddress);
    }

    /// @dev This is the getTreasuryAddress function. It is used to get the treasury address in the contract.

    function getTreasuryAddress() external view override returns (address) {
        return treasury;
    }

    /// @dev This function would add an address to the blacklist mapping
    /// @dev Only the owner can call this function
    /// @param user The account to be added to blacklist

    function addToBlackList(address[] memory user) external override onlyOwner whenNotPaused returns (bool) {
        for (uint256 index = 0; index < user.length; index++) {
            if( user[index] != ZERO_ADDRESS){
                _addToBlackList(user[index]);
            }
        }
        return true;
    }

    /// @dev This function would remove an address from the blacklist mapping
    /// @dev Only the owner can call this function
    /// @param user The account to be removed from blacklist

    function removeFromBlackList(address[] memory user) external override onlyOwner whenNotPaused returns (bool) {
        for (uint256 index = 0; index < user.length; index++) {
            if( user[index] != ZERO_ADDRESS){
                _removeFromBlackList(user[index]);
            }
        }
        return true;
    }


    function pause() external override onlyOwner {
        _pause();
    }

    function unpause() external override onlyOwner {
        _unpause();
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }
}

