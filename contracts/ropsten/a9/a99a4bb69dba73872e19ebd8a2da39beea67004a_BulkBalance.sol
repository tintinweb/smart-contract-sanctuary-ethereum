pragma solidity ^0.8.4;

contract BulkBalance {
    function _isOwnerERC1155(
        address tokenAddress,
        uint256 tokenID,
        address userAddress
    ) public view returns (bool) {
        IERC1155 token1155;
        if (_isContract(tokenAddress)) {
            token1155 = IERC1155(tokenAddress);
        } else {
            return false;
        }
        try token1155.balanceOf(userAddress, tokenID) returns (
            uint256 balance
        ) {
            if (balance > 0) {
                return true;
            }
        } catch (
            bytes memory /*lowLevelData*/
        ) {
            // return false;
        }
        return false;
    }

    function _isOwnerERC721(
        address tokenAddress,
        uint256 tokenID,
        address userAddress
    ) public view returns (bool) {
        IERC721 token721;
        if (_isContract(tokenAddress)) {
            token721 = IERC721(tokenAddress);
        } else {
            return false;
        }
        if (tokenID == 0) {
            try token721.balanceOf(userAddress) returns (uint256 balance) {
                if (balance > 0) {
                    return true;
                }
            } catch (
                bytes memory /*lowLevelData*/
            ) {
                // return false;
            }
            return false;
        }
        try token721.ownerOf(tokenID) returns (address owner) {
            if (owner == userAddress) {
                return true;
            }
        } catch (
            bytes memory /*lowLevelData*/
        ) {
            // return false;
        }
        return false;
    }

    function isOwner(
        address[] memory tokenAddress,
        uint256[] memory tokenID,
        address userAddress
    ) external view returns (bool) {
        uint256 i = 0;
        while (i < tokenAddress.length) {
            if (_isOwnerERC721(tokenAddress[i], tokenID[i], userAddress)) {
                return true;
            }
            if (_isOwnerERC1155(tokenAddress[i], tokenID[i], userAddress)) {
                return true;
            }
            i = i + 1;
        }
        return false;
    }

    function isOwner(
        address[] memory tokenAddress,
        uint256[] memory tokenID,
        address[] memory userAddress
    ) external view returns (bool) {
        uint256 i_w = 0;
        while (i_w < userAddress.length) {
            uint256 i_a = 0;
            while (i_a < tokenAddress.length) {
                if (
                    _isOwnerERC721(
                        tokenAddress[i_a],
                        tokenID[i_a],
                        userAddress[i_w]
                    )
                ) {
                    return true;
                }
                if (
                    _isOwnerERC1155(
                        tokenAddress[i_a],
                        tokenID[i_a],
                        userAddress[i_w]
                    )
                ) {
                    return true;
                }
                i_a = i_a + 1;
            }
            i_w = i_w + 1;
        }
        return false;
    }

    function getBalanceERC20(
        address tokenAddress,
        uint256 minBalance,
        address userAddress
    ) internal view returns (bool) {
        IERC20 token20 = IERC20(tokenAddress);
        try token20.balanceOf(userAddress) returns (uint256 balance) {
            if (balance > minBalance) {
                return true;
            }
        } catch (
            bytes memory /*lowLevelData*/
        ) {
            // return false;
        }
        return false;
    }

    function isOwnerERC20(
        address[] memory tokenAddress,
        uint256[] memory minBalance,
        address[] memory userAddress
    ) external view returns (bool) {
        uint256 i_w = 0;
        while (i_w < userAddress.length) {
            uint256 i_a = 0;
            while (i_a < tokenAddress.length) {
                if (
                    getBalanceERC20(
                        tokenAddress[i_a],
                        minBalance[i_a],
                        userAddress[i_w]
                    )
                ) {
                    return true;
                }
                i_a = i_a + 1;
            }
            i_w = i_w + 1;
        }
        return false;
    }

    function _isContract(address account) internal view returns (bool) {
        // This method relies on extcodesize/address.code.length, which returns 0
        // for contracts in construction, since the code is only stored at the end
        // of the constructor execution.
        return account.code.length > 0;
    }
