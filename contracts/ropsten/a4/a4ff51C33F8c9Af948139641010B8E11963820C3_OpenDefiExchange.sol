pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "./libraries/LibStorage.sol";

import "./interfaces/IBalance.sol";
import "./interfaces/IOpenDefiNFT.sol";
import "hardhat/console.sol";

contract OpenDefiExchange is
    Initializable,
    OwnableUpgradeable,
    ERC1155ReceiverUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using SafeMathUpgradeable for uint256;

    LibStorage.WhitelistTokens[] public supportedWhitelistTokens;
    // Array of struct Fees
    LibStorage.Fees[] public feeCollected;
    // Array of struct PurchaseDetails
    LibStorage.PurchaseDetails[] public purchases;
    // Stores the purchase id for each user
    mapping(address => uint256[]) public tokenPurchaseDetails;

    // Indicates if an address is whitelisted or not
    mapping(address => bool) public whitelistedBuyers;

    // flags when collection is private/public
    bool public isPrivateCollection;
    address public collection;
    address public collectionFactory;

    // Address variable to store insurance token address
    address public insuranceToken;
    // Variable to store the insurance time interval
    uint256 public insuranceInterval;
    // Variable to store the insurance price
    uint256 public insurancePrice;

    // Mapping from token id to insurance block status.
    mapping(uint256 => bool) public isInsuranceBlocked;
    // returns true of insurance is blocked.
    bool public isInsuranceGloballyBlocked;

    // [usdc, usdt, ufarm]
    address[] public feeTokens;
    mapping(address => uint256) public feeTokenId;

    // [100, 0, 500]
    mapping(uint256 => uint256) public feeTokenBuyLimits;

    // Mapping from token id to fee token and its prices.
    mapping(uint256 => mapping(address => uint256)) public nftPrices;

    uint256 public constant PERCENTAGE_MULTIPLIER = 1000;

    /**
     * @dev Modifier to make a function callable only when the contract is paused.
     *
     * Requirements:
     *
     * - The contract must be paused.
     */
    modifier whenNotPaused() {
        require(!IOpenDefiNFT(collection).paused(), "paused");
        _;
    }

    function __OpenDefiExchange_init(
        bool _isPrivateCollection,
        address _collection,
        address _insuranceToken,
        address _owner,
        uint256 _insuranceInterval,
        uint256 _insurancePrice,
        uint256[] memory _feeTokenBuyLimit,
        address[] memory _feeTokens
    ) external initializer {
        isPrivateCollection = _isPrivateCollection;
        collection = _collection;
        collectionFactory = _msgSender();

        _saveInsuranceDetails(
            _insuranceToken,
            _insuranceInterval,
            _insurancePrice
        );

        require(
            _feeTokenBuyLimit.length == _feeTokens.length,
            "Invalid fee details"
        );

        // to initialise 0th index
        feeTokens.push(address(0));

        for (uint256 index = 0; index < _feeTokenBuyLimit.length; index++) {
            require(_feeTokens[index] != address(0), "invalid token");
            feeTokens.push(_feeTokens[index]);
            feeTokenBuyLimits[index.add(1)] = _feeTokenBuyLimit[index];
            feeTokenId[_feeTokens[index]] = index.add(1);
        }

        __Ownable_init();
        _transferOwnership(_owner);
    }

    function updateFeeConfig(address feeToken, uint256 buyLimits)
        external
        onlyOwner
    {
        require(feeTokenId[feeToken] != 0, "token does not exist");

        // 0 limit set the limit to be max uint256
        feeTokenBuyLimits[feeTokenId[feeToken]] = buyLimits;
    }

    function addFeeTokens(address feeToken, uint256 buyLimits)
        external
        onlyOwner
    {
        require(feeTokenId[feeToken] == 0, "token exists");
        require(feeToken != address(0), "invalid token");

        // 0 limit set the limit to be max uint256
        feeTokens.push(feeToken);
        feeTokenId[feeToken] = feeTokens.length.sub(1);
        feeTokenBuyLimits[feeTokenId[feeToken]] = buyLimits;
    }

    function toggleCollectionType(bool isPrivate) external onlyOwner {
        require(isPrivateCollection != isPrivate, "no change");
        isPrivateCollection = isPrivate;
    }

    // Checks the eligibilty of the caller.
    function isEligible() public view returns (bool) {
        if (isPrivateCollection) {
            if (whitelistedBuyers[msg.sender]) return true;
            for (
                uint256 index = 0;
                index < supportedWhitelistTokens.length;
                index++
            ) {
                LibStorage.WhitelistTokens
                    memory details = supportedWhitelistTokens[index];
                if (
                    details.tokenType == 0 &&
                    IERC20Upgradeable(details.token).balanceOf(msg.sender) > 0
                ) {
                    return true;
                } else if (
                    details.tokenType == 1 &&
                    IBalance(details.token).balanceOf(msg.sender) > 0
                ) {
                    return true;
                } else if (
                    details.tokenType == 2 &&
                    IERC1155Upgradeable(details.token).balanceOf(
                        msg.sender,
                        details.tokenId
                    ) >
                    0
                ) {
                    return true;
                }
            }
            return false;
        }
        return true;
    }

    /**
     * @dev The function will set new values against the buyers added in the white list.
     * New values must be true inorder to whitelist a buyer.
     * Only whitelisted buyers will have the privilege to mint token
     */
    function updateWhitelistedBuyers(
        address[] calldata buyers,
        bool[] calldata status
    ) external onlyOwner {
        for (uint256 index = 0; index < buyers.length; index++) {
            whitelistedBuyers[buyers[index]] = status[index];
        }
    }

    /**
     * @dev The function will set token address whose holders will be able to buy NFT.
     */
    function updateEligibleTokenForWhitelisting(
        LibStorage.WhitelistTokens memory token
    ) external onlyOwner {
        require(token.token != address(0), "Invalid token");
        require(token.tokenType < 3, "Invalid token type");

        supportedWhitelistTokens.push(token);
    }

    /**
     * @dev Function to update insurance details
     *
     * Requirements:
     * Only owner can update
     */
    function updateInsuranceDetails(
        address _insuranceToken,
        uint256 _insuranceInterval,
        uint256 _insurancePrice
    ) external onlyOwner {
        _saveInsuranceDetails(
            _insuranceToken,
            _insuranceInterval,
            _insurancePrice
        );
    }

    /**
     * @dev Function to store the insurance related details like price, interval, address etc.
     *
     * Requirements:
     *
     * Insurance interval and price should be greater than zero.
     * Insurance address should be a valid address
     */
    function _saveInsuranceDetails(
        address _insuranceToken,
        uint256 _insuranceInterval,
        uint256 _insurancePrice
    ) internal {
        require(
            _insuranceInterval != 0 && _insurancePrice != 0,
            "Invalid insurance details"
        );
        require(_insuranceToken != address(0), "Invalid insurance token");

        insuranceToken = _insuranceToken;
        insuranceInterval = _insuranceInterval;
        insurancePrice = _insurancePrice;
    }

    function toggleInsuranceBlock(uint256 tokenId, bool status)
        external
        onlyOwner
    {
        isInsuranceBlocked[tokenId] = status;
    }

    function toggleGlobalInsuranceBlock(bool status) external onlyOwner {
        isInsuranceGloballyBlocked = status;
    }

    struct Referrals {
        address referee;
        address feeToken;
        uint256 amount;
        uint256 unlockDate;
    }

    mapping(address => Referrals[]) public referrals;

    /**
     * @dev function allows the buyers to puchase nft from a list of options.
     * Appropriate Fee is calculated and added to the purchase.
     * Minimum protection date period is calculated and added to each NFT purchases.
     */
    function buyNFT(
        address buyer,
        uint256 amount,
        uint256 tokenId,
        address feeToken,
        bool isInsured,
        bytes memory data,
        address referee,
        uint256 referralPercentage
    ) external whenNotPaused {
        require(isEligible(), "Not whitelisted");
        require(
            IERC1155Upgradeable(collection).balanceOf(address(this), tokenId) >=
                amount,
            "Already sold"
        );

        // transfer NFT
        IERC1155Upgradeable(collection).safeTransferFrom(
            address(this),
            buyer,
            tokenId,
            amount,
            data
        );

        // set fee details
        uint256 totalFee = nftPrices[tokenId][feeToken].mul(amount);

        require(feeTokenId[feeToken] != 0, "fee token invalid");
        uint256 amountReceived = _transferERC(
            _msgSender(),
            address(this),
            feeToken,
            totalFee
        );

        if (feeTokenBuyLimits[feeTokenId[feeToken]] != 0)
            feeTokenBuyLimits[feeTokenId[feeToken]] = feeTokenBuyLimits[
                feeTokenId[feeToken]
            ].sub(amount);

        uint256 unlockDate = block.timestamp;

        bool insuranceStatus = isInsured &&
            !isInsuranceGloballyBlocked &&
            !isInsuranceBlocked[tokenId];

        if (insuranceStatus) {
            _transferERC(
                msg.sender,
                address(this),
                insuranceToken,
                amount * insurancePrice
            );
            unlockDate = insuranceInterval.add(unlockDate);
        }

        //set referral details
        uint256 referralCut = 0;
        if (referee != address(0) && referralPercentage != 0) {
            referralCut = amountReceived.mul(referralPercentage).div(
                PERCENTAGE_MULTIPLIER
            );
            _setReferrals(referee, feeToken, referralCut, unlockDate);
        }

        uint256 feeId = _setFeeDetails(
            unlockDate,
            amountReceived.sub(referralCut),
            feeToken
        );

        // set purchase details
        _setPurchase(tokenId, feeId, false, false, insuranceStatus);
    }

    function _setReferrals(
        address referee,
        address feeToken,
        uint256 amount,
        uint256 unlockDate
    ) internal {
        Referrals memory details = Referrals(
            referee,
            feeToken,
            amount,
            unlockDate
        );
        referrals[referee].push(details);
    }

    /**
     * @dev function will transfer specified backed asset token while minting the new NFT by the owner.
     * Asset token address should be valid
     * Transfer amount should be greater than zero
     * Backed Asset amounts are transfered.
     */
    function _transferERC(
        address _from,
        address _to,
        address _asset,
        uint256 _amount
    ) internal returns (uint256 amount) {
        if (_asset == address(0)) return 0;
        if (_amount == 0) return 0;

        uint256 initialBalance = IERC20Upgradeable(_asset).balanceOf(_to);
        if (_from == address(this)) {
            IERC20Upgradeable(_asset).safeTransfer(_to, _amount);
        } else IERC20Upgradeable(_asset).safeTransferFrom(_from, _to, _amount);

        uint256 finalBalance = IERC20Upgradeable(_asset).balanceOf(_to);
        amount = finalBalance.sub(initialBalance);
    }

    /**
     * @dev Function will add purchase related data of nft
     */
    function _setPurchase(
        uint256 tokenId,
        uint256 feeId,
        bool isRedeemed,
        bool isBurnt,
        bool isInsured
    ) internal {
        LibStorage.PurchaseDetails memory purchase;

        purchase.feeId = feeId;
        purchase.isRedeemed = isRedeemed;
        purchase.isBurnt = isBurnt;
        purchase.isInsured = isInsured;
        purchase.tokenId = tokenId;

        purchases.push(purchase);
        tokenPurchaseDetails[_msgSender()].push(purchases.length.sub(1));
    }

    /**
     * @dev Function will set the fee details related to the nft purchase
     */
    function _setFeeDetails(
        uint256 unlockDate,
        uint256 amount,
        address feeToken
    ) internal returns (uint256) {
        LibStorage.Fees memory fee;

        fee.unlockDate = unlockDate;
        fee.amount = amount;
        fee.feeToken = feeToken;

        feeCollected.push(fee);
        return feeCollected.length.sub(1);
    }

    /**
     * @dev Function to save the initial NFT price
     */
    function saveNFTLaunchPrice(uint256 _id, uint256[] memory _prices) public {
        require(
            _msgSender() == address(collection) || _msgSender() == owner(),
            "only collection and owner can set prices"
        );
        for (uint256 index = 0; index < _prices.length; index++) {
            nftPrices[_id][feeTokens[index.add(1)]] = _prices[index];
        }
    }

    /**
     * @dev Function to get the NFT price
     *
     */
    function getNFTPrice(uint256 _id, address _feeToken)
        external
        view
        returns (uint256)
    {
        return nftPrices[_id][_feeToken];
    }

    /**
     * @dev To update the NFT prices '_nftPrice'
     *
     * Requirements:
     * Caller should be owner
     * token id og nft should be valid
     * fee token id should be valid
     */
    function updateNFTPrice(
        uint256 _id,
        address _feeToken,
        uint256 _nftPrice
    ) external onlyOwner {
        require(feeTokenId[_feeToken] != 0, "fee token do not exist");

        nftPrices[_id][_feeToken] = _nftPrice;
    }

    function _asSingletonArray(uint256 element)
        internal
        pure
        returns (uint256[] memory array)
    {
        array = new uint256[](1);
        array[0] = element;
    }

    function _asSingletonArray(address element)
        internal
        pure
        returns (address[] memory array)
    {
        array = new address[](1);
        array[0] = element;
    }

    /**
     * @dev Buyer can redeem the NFT which was bought before.
     * The contract not be paused for burn.
     * The buyer should have posses the token inorder to redeem it.
     */
    function redeemNFT(
        address buyer,
        uint256 amount,
        uint256 tokenId,
        uint256 purchaseId,
        bytes memory data
    ) external whenNotPaused {
        require(
            !isInsuranceGloballyBlocked && !isInsuranceBlocked[tokenId],
            "insurance blocked"
        );
        require(
            IERC1155Upgradeable(collection).balanceOf(buyer, tokenId) >= amount,
            "invalid balance"
        );

        LibStorage.PurchaseDetails storage purchase = purchases[
            tokenPurchaseDetails[buyer][purchaseId]
        ];
        LibStorage.Fees storage fee = feeCollected[purchase.feeId];

        require(
            purchase.isInsured && fee.unlockDate >= block.timestamp,
            "not insured"
        );

        require(
            !purchase.isRedeemed && !purchase.isBurnt,
            "refund not available"
        );

        IERC1155Upgradeable(collection).safeTransferFrom(
            buyer,
            address(this),
            tokenId,
            amount,
            data
        );

        uint256 toWithdraw = fee.amount;
        fee.amount = 0;
        purchase.isRedeemed = true;

        IERC20Upgradeable(fee.feeToken).safeTransfer(buyer, toWithdraw);
    }

    /**
     * @dev function allows the admin to claim the fees.
     * The fee id should be valid.
     * The fees associated with nfts are transferred back to the creators and referrees.
     */
    function claimOwnerFees(uint256 _fromFeeId, uint256 _toFeeId)
        external
        onlyOwner
        whenNotPaused
    {
        require(
            _toFeeId > _fromFeeId && _toFeeId <= feeCollected.length,
            "INVALID_ARRAY_INDEX"
        );

        for (uint256 index = _fromFeeId; index < _toFeeId; index++) {
            LibStorage.Fees storage fee = feeCollected[index];
            if (fee.unlockDate <= block.timestamp && fee.amount != 0) {
                IERC20Upgradeable(fee.feeToken).safeTransfer(
                    _msgSender(),
                    fee.amount
                );
                fee.amount = 0;
            }
        }
    }

    function claimFees(uint256 _fromFeeId, uint256 _toFeeId)
        external
        whenNotPaused
    {
        require(referrals[_msgSender()].length >= _toFeeId, "no fee to claim");

        for (uint256 index = _fromFeeId; index < _toFeeId; index++) {
            Referrals storage referral = referrals[_msgSender()][index];
            if (
                referral.unlockDate <= block.timestamp && referral.amount != 0
            ) {
                IERC20Upgradeable(referral.feeToken).safeTransfer(
                    _msgSender(),
                    referral.amount
                );
                referral.amount = 0;
            }
        }
    }

    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes calldata data
    ) external pure override returns (bytes4) {
        return
            bytes4(
                keccak256(
                    "onERC1155Received(address,address,uint256,uint256,bytes)"
                )
            );
    }

    function onERC1155BatchReceived(
        address operator,
        address from,
        uint256[] calldata ids,
        uint256[] calldata values,
        bytes calldata data
    ) external pure override returns (bytes4) {
        return
            bytes4(
                keccak256(
                    "onERC1155BatchReceived(address,address,uint256[],uint256[],bytes)"
                )
            );
    }

    function safeWithdrawNFT(uint256[] memory tokenIds, bytes memory data)
        external
        onlyOwner
    {
        uint256[] memory batchBalances = IERC1155Upgradeable(collection)
            .balanceOfBatch(_asSingletonArray(address(this)), tokenIds);

        IERC1155Upgradeable(collection).safeBatchTransferFrom(
            address(this),
            owner(),
            tokenIds,
            batchBalances,
            data
        );
    }

    function safeWithdrawInsuranceTokens() external onlyOwner {
        _transferERC(
            address(this),
            owner(),
            insuranceToken,
            IERC20Upgradeable(insuranceToken).balanceOf(address(this))
        );
    }

    function safeWithdrawFeeTokens() external onlyOwner {
        for (uint256 index = 1; index < feeTokens.length; index++) {
            _transferERC(
                address(this),
                owner(),
                feeTokens[index],
                IERC20Upgradeable(feeTokens[index]).balanceOf(address(this))
            );
        }
    }

    uint256[50] private __gap;
}

