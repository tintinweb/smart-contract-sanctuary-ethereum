     */
    function _afterTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {}
}

contract SimpleNFT is ERC721("SimpleNFT","SNFT"), Ownable {
    event Register(address user, uint256 _tokenId);

    uint256 public tokenId = 0;
    address[] public businessAddresses;

    struct Meta {
        address creator;
    }

    mapping(uint256 => Meta) public metadata;

    modifier onlyManager() {
        require(msg.sender == owner());
        _;
    }

    constructor()  {
        
    }

    function mint(address to, uint256 tokenId) public{
        super._mint(to, tokenId);
    }

    function _burnItem(address owner, uint256 tokenId) public {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721Burnable: caller is not owner nor approved");
        super._burn(tokenId);
    
    }

    // function transfer(address to, uint256 value) public returns (bool) {
    //     return super.transfer(to, value);
    // }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override{
        return super.transferFrom(from, to, tokenId);
    }
}

