pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "base64-sol/base64.sol";
import "./JsmnSolLib.sol";
import "./ERC721URIStorage_LolCat.sol";
import "./ILolCat_Items.sol";

contract LolCat_Quest is ERC721URIStorage, OwnableUpgradeable {
  using Counters for Counters.Counter;
  Counters.Counter private _tokenIds;
  Counters.Counter private _publicSaleId;

  string private _name;
  string private _symbol;
  uint mintRate;
  uint256 publicSaleSupply;

  mapping(address => bool) public uriUpdaters;

  ILolCat_Items public itemsContract;
  IERC20Upgradeable public gpContract;

  function initialize() public initializer {
    mintRate = 0.0069 ether;
    __Context_init_unchained();
    __Ownable_init_unchained();
    __ERC721_init_unchained("LolCat Quest", "LOLCAT");
  }

  modifier checkAmount(uint _mintAmt) {
    require(_mintAmt > 0);
    _;
  }

  function mintCustomLolCat(address recipient, string memory _tokenURI) public onlyOwner returns (uint256) {
    _tokenIds.increment();

    uint256 newItemId = _tokenIds.current();
    _mint(recipient, newItemId);
    _setTokenURI(newItemId, _tokenURI);

    return newItemId;
  }

  function safeMint(address to, uint _amtOfTokensToMint) 
    public payable checkAmount(_amtOfTokensToMint)
  {
    require(msg.value == (_amtOfTokensToMint * mintRate));
    for (uint i = 0; i < _amtOfTokensToMint; i++) {
      _tokenIds.increment();
      _publicSaleId.increment();
      uint256 tokenId = _tokenIds.current();
      _safeMint(to, tokenId);
    }
  }

  function withdraw() public payable onlyOwner {
    payable(owner()).transfer(address(this).balance);
  }

  function setMintRate(uint _mintRate) public onlyOwner {
    mintRate = _mintRate;
  }

  function setBaseURI(string memory _baseURI) public onlyOwner {
    _setBaseURI(_baseURI);
  }

  function setPublicSaleSupply(uint _publicSaleSupply) public onlyOwner {
    publicSaleSupply = _publicSaleSupply;
  }

  function addUpdater(address _wallet) public onlyOwner {
    uriUpdaters[_wallet]=true;
  }

  function removeUpdater(address _wallet) public onlyOwner {
    uriUpdaters[_wallet]=false;
  }

  function setItemsContract(ILolCat_Items _itemsContract) public onlyOwner {
    itemsContract = _itemsContract;
  }

  function setGpContract(IERC20Upgradeable _gpContract) public onlyOwner {
    gpContract = _gpContract;
  }

  function getItems() public view returns(string memory) {
    string memory items = itemsContract.getItems();
    return items;
  }

  function buildLolCat(uint256 backgroundId, uint hatId, string memory input) public view returns (string memory dataURI) {
    string memory dataURI = ' { "attributes": [ { "trait_type": "color", "value": "redish.." } ], "description": "test background", "image": "';
    string memory baseSvg = '<svg xmlns="http://www.w3.org/2000/svg" shape-rendering="crispEdges" viewBox="0 -0.5 64 64" width="64" height="64" xmlns:v="https://vecta.io/nano">';
    string memory svgEnd = '</svg>';
    string memory SVG_URI_PREFIX = "data:image/svg+xml;base64,";
    string memory JSON_URI_PREFIX = "data:application/json;base64,";

    // decode background & hat metadata
    string memory background = string(Base64.decode(itemsContract.getItem(backgroundId)));
    //string memory background = string(Base64.decode("iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVQYV2NgYAAAAAMAAWgmWQ0AAAAASUVORK5CYII="));
    //string memory input_test = string(Base64.decode(input));
    string memory hat = string(Base64.decode(itemsContract.getItem(hatId)));
    // get images from metadata
//  (,JsmnSolLib.Token[] memory backgroundJsonArray,) = JsmnSolLib.parse(background, 6);
//  JsmnSolLib.Token memory tBackground = backgroundJsonArray[3];
//  string memory backgroundImage = string(Base64.decode(JsmnSolLib.getBytes(background, tBackground.start, tBackground.end)));

//  (,JsmnSolLib.Token[] memory hatJsonArray,) = JsmnSolLib.parse(hat, 6);
//  JsmnSolLib.Token memory tHat = hatJsonArray[3];
//  string memory hatImage = string(Base64.decode(JsmnSolLib.getBytes(hat, tHat.start, tHat.end)));
    baseSvg = string(abi.encodePacked(
      baseSvg
//      , backgroundImage
//      , hatImage
      , svgEnd));
    baseSvg = Base64.encode(bytes(baseSvg));
    dataURI = string(abi.encodePacked(
        dataURI
      , SVG_URI_PREFIX
      , baseSvg
      , '", "name": "background" }'));
    dataURI = Base64.encode(bytes(dataURI));
    dataURI = string(abi.encodePacked(
      JSON_URI_PREFIX
      , dataURI
    ));
    return dataURI;
  }

  function getMintedTknsAmt() public view returns(uint256) {
    uint256 currentItem = _tokenIds.current();
    return currentItem;
  }

  function getPublicMintedTknsAmt() public view returns(uint256) {
    uint256 currentItem = _publicSaleId.current();
    return currentItem;
  }

  function equip(uint LolCatId, uint EquipmentId) public {
    require(msg.sender == ownerOf(LolCatId));
    require(msg.sender == ILolCat_Items(address(itemsContract)).ownerOf(EquipmentId));
  }
}

