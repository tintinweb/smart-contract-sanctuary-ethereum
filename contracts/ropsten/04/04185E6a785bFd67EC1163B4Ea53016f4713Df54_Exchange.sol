// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

import "../interface/IHeroNFT.sol";
import "../interface/ISkateNFT.sol";
import "../interface/IPropNFT.sol";
import "../interface/IExchangeData.sol";

contract Exchange is AccessControl {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    IERC20 public chc;
    IHeroNFT public hero;
    ISkateNFT public skate;
    IPropNFT public prop;
    address payable public fund;
    IExchangeData public exchangeData;

    event ItemNoLongerForSale(
        uint8 indexed tokenType, //token类型(英雄==>1;滑板==>2;道具==>3;)
        uint256 indexed itemId, //tokenID
        address indexed ownerAddress //拥有者地址
    );
    event ItemOffered(
        uint8 indexed tokenType,    //token类型(英雄==>1;滑板==>2;道具==>3;)
        uint256 indexed itemId,     //tokenID
        uint256 count,              //数量
        uint256 unitPrice,          //单价
        address indexed fromAddress //卖家地址
    );
    event ItemBought(
        uint8  tokenType,           //token类型(英雄==>1;滑板==>2;道具==>3;)
        uint256 indexed itemId,     //tokenID
        uint256 count,              //数量
        uint256 unitPrice,          //单价
        uint256 value,              //交易金额(扣除手续费)
        uint256 fee,                //手续费
        address indexed fromAddress,//卖家地址
        address indexed toAddress,  //买家地址
        address fundAddress         //资金管理地址
    );

    constructor(
        address token_,
        address hero_,
        address skate_,
        address prop_,
        address payable fund_,
        address exchangeData_
    ) {
        chc = IERC20(token_);
        hero = IHeroNFT(hero_);
        skate = ISkateNFT(skate_);
        prop = IPropNFT(prop_);
        fund = fund_;
        exchangeData = IExchangeData(exchangeData_);
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(MINTER_ROLE, _msgSender());
    }

    function itemNoLongerForSale(uint8 tokenType, uint256 itemId) public {
        if (tokenType == 1) {
            //英雄
            require(hero.ownerOf(itemId) == tx.origin, "You don't own this hero.");           
        } else if (tokenType == 2) {
            //滑板
            require(skate.ownerOf(itemId) == tx.origin, "You don't own this skate.");
        } else if (tokenType == 3) {
            //道具
            (uint256 total, uint256 onSale, ) = prop.getInfo(itemId, tx.origin);
            require(total > 0, "You don't own this prop.");
            require(onSale > 0, "This token isn't onSale");
        }
        setOfferStatus(tokenType, itemId, tx.origin, false, 0, 0);
        emit ItemNoLongerForSale(tokenType, itemId, tx.origin);
    }

    function offerItemForSale(uint8 _tokenType, uint256 _itemId, uint256 _unitPrice, uint256 _count) public {
         if (_tokenType == 1) {
            //英雄
            require(hero.ownerOf(_itemId) == _msgSender(), "You don't own this hero.");
            require(hero.getApproved(_itemId) == address(this) || hero.isApprovedForAll(_msgSender(), address(this)), "Approve this hero first.");
            (, , , , uint256 birthTime) = hero.getInfo(_itemId);
            uint256 growthTime = hero.getGrowthTime();
            require(birthTime + growthTime <= block.timestamp, "Hero is still growing.");
        } else if (_tokenType == 2) {
            //滑板
            require(skate.ownerOf(_itemId) == _msgSender(), "You don't own this skate.");
            require(skate.getApproved(_itemId) == address(this) || skate.isApprovedForAll(_msgSender(), address(this)), "Approve this skate first.");
        } else if (_tokenType == 3) {
            //道具
            (uint256 total, uint256 onSale, ) = prop.getInfo(_itemId, _msgSender());
            require(total > 0, "You don't own this prop.");
            require(onSale > 0, "This token isn't onSale");
            require(prop.isApprovedForAll(_msgSender(), address(this), _itemId), "Approve this prop first.");
        }
        uint256 minPrice = exchangeData.getMinPrice();
        require(_unitPrice >= minPrice, "Price reaches the lower limit.");
        uint256 maxPrice = exchangeData.getMaxPrice();
        require(_unitPrice <= maxPrice, "Price reaches the upper limit.");
        setOfferStatus(_tokenType, _itemId, _msgSender(), true, _count, _unitPrice);
        emit ItemOffered(_tokenType, _itemId, _count, _unitPrice, _msgSender());
    }

    function buyItem(uint8 _tokenType, uint256 _itemId, address _owner, uint256 _count, uint256 _unitPrice) external {
        address owner;
        if (_tokenType == 1) {
            //英雄
            owner = hero.ownerOf(_itemId);
        } else if (_tokenType == 2) {
            //滑板
            owner = skate.ownerOf(_itemId);
        } else if (_tokenType == 3) {
            //道具
            owner=_owner;
        }
        (address seller, uint256 count, uint256 unitPrice) = exchangeData.getOffer(_tokenType, _itemId, owner);

        require(count > 0, "This item not actually for sale.");
        require(seller != _msgSender(), "You own this item.");
        require(_count <= count, "This item is not enough for sale.");
        require(_unitPrice >= unitPrice, "You didn't send enough chc.");
        require(seller == owner, "Seller no longer owner of this item.");
        require(chc.transferFrom(_msgSender(), address(this), _unitPrice.mul(_count)), "CHC transfer failed");

        setOfferStatus(_tokenType, _itemId, seller, _count < count, count.sub(_count), _count < count? _unitPrice : 0);

        uint256 rates = exchangeData.getRates();
        uint256 fee = _unitPrice.mul(_count).mul(rates).div(10000);
        if (fee > 0) {
            require(chc.transfer(fund, fee), "Fee transfer failed");
        }
        uint256 leftAmount = _unitPrice.mul(_count).sub(fee);
        require(chc.transfer(seller, leftAmount), "CHC transfer failed");
         if (_tokenType == 1) {
            //英雄
            hero.safeTransferFrom(seller, _msgSender(), _itemId);
        } else if (_tokenType == 2) {
            //滑板
            skate.safeTransferFrom(seller, _msgSender(), _itemId);
        } else if (_tokenType == 3) {
            //道具
            prop.safeTransferFrom(seller, _msgSender(), _itemId, _count, "");
        }
        emit ItemBought(_tokenType, _itemId, _count, _unitPrice, leftAmount, fee, seller, _msgSender(), fund);
    }

    function setOfferStatus(uint8 _tokenType, uint256 _itemId, address _seller, bool _status, uint256 _count, uint256 _unitPrice) private {
        exchangeData.setOffer(_tokenType, _itemId, _seller, _count, _unitPrice);
        if (_tokenType == 1) {
            //英雄
            hero.setInfo(_itemId, _status);
        } else if (_tokenType == 2) {
            //滑板
             skate.setOnSale(_itemId, _status);
        } else if (_tokenType == 3) {
            //道具
            prop.setInfo_onSale(_itemId, _count);
            prop.setInfo_unitPrice(_itemId, _unitPrice);
        }
    }

}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

interface IExchangeData {
    function getRates() external view returns (uint256);

    function setRates(uint256 rates_) external;

    function getMinPrice() external view returns (uint256);

    function setMinPrice(uint256 price_) external;

    function getMaxPrice() external view returns (uint256);

    function setMaxPrice(uint256 price_) external;

    function getOffer(uint8 itemType,uint256 itemId,address addr)
        external
        view
        returns (
            address,
            uint256,
            uint256
        );

    function setOffer(
        uint8 itemType,
        uint256 itemId,
        address seller,
        uint256 count,
        uint256 unitPrice
    ) external;
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

interface IPropNFT is IERC1155 {
    function mint(address to, uint256 id, uint256 count) external returns (uint256,uint256);
    
    function mintBatch(address to, uint256[] memory ids, uint256[] memory counts) external returns (uint256[] memory, uint256[] memory);

    function burn(address owner, uint256 tokenId, uint256 count) external;
    
    function burnBatch(address owner, uint256[] memory tokenIds, uint256[] memory counts) external;

    function setInfo_onSale(uint256 _id, uint256 _sale) external;

    function setInfo_unitPrice(uint256 _id, uint256 _price) external;

    function getInfo(uint256 _id, address _addr) external view returns (uint256, uint256, uint256);

    function isApprovedForAll(address account, address operator, uint256 tokenId) external view returns (bool);

    function safeTransferFrom(address from, address to, uint256 id, uint256 amount, bytes memory data) external;
}

