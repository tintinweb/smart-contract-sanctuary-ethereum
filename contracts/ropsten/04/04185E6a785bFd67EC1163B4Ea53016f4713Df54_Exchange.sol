pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Enumerable.sol";

interface IHeroNFT is IERC721Enumerable {
    function getGrowthTime() external view returns (uint32);

    function mint(address to, uint256 tokenId, uint8 quality) external returns (uint256);

    function mintGenesis(address to) external returns (uint256);

    function burn(address owner, uint256 tokenId) external returns (bool);

    function getSuccessMaxCount() external view returns(uint16[] memory);

    function getSuccessPercentage() external view returns(uint8[] memory);

    function getInfo(uint256 tokenId) external view returns (bool, bool, uint8, uint16, uint64);

    function setInfo(uint256 tokenId, bool onSale) external;

    function setInfo(uint256 tokenId, uint16 evolutionaryValue) external;

    function setQuality(uint256 tokenId, uint8 quality) external;
}

