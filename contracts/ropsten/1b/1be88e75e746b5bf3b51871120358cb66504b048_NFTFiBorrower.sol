    function withdrawPledge(uint256 _orderId) public {
        //        map
        Order memory order = OrderIdToLender[_orderId];
        console.log("withdrawPledge ",order.borrower);
        require(address(0) != order.borrower, 'orderId borrower is null ');
        require(msg.sender == order.borrower, 'Only the borrower can withdrawPledge  NFT');
        delete OrderIdToLender[_orderId];
        IERC721(address(order.nftCollateralContract)).transferFrom(address(this), msg.sender, order.nftCollateralId);

        emit PledgeCancelled(_orderId);
    }


    function beginLoan(

        uint256 _orderId

    ) public whenNotPaused nonReentrant {


        Order memory order = OrderIdToLender[_orderId];
        require(address(0) != order.borrower, 'orderId borrower is null ');
        Loan memory loan = Loan({
        loanId: totalNumLoans, //currentLoanId,
        loanPrincipalAmount: order.loanPrincipalAmount,
