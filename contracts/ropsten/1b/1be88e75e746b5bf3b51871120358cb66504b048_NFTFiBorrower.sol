        });

        OrderIdToLender[totalOrderId] = order;
        totalOrderId = totalOrderId.add(1);
        IERC721(address(_nftCollateralContract)).transferFrom(msg.sender, address(this), _nftCollateralId);
        //     event PledgeCreated(address indexed borrower, address indexed nftCollateralContract, address indexed loanERC20Denomination, uint256 nftCollateralId, uint256 orderId, uint256 loanPrincipalAmount, uint256 loanInterestRateForDurationInBasisPoints, uint256 loanDuration);
        console.log(   "borrower %s ,orderId %s ",order.borrower,order.orderId);
