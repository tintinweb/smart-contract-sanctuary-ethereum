        nftCollateralId: order.nftCollateralId,
        loanStartTime: uint64(now), //_loanStartTime
        loanDuration: uint32(order.loanDuration),
        loanInterestRateForDurationInBasisPoints: uint32(order.loanInterestRateForDurationInBasisPoints),
        loanAdminFeeInBasisPoints: uint32(adminFeeInBasisPoints),
        nftCollateralContract: order.nftCollateralContract,
        loanERC20Denomination: order.loanERC20Denomination,
        borrower: order.borrower,
        interestIsProRated: true
        });

        loanIdToLoan[totalNumLoans] = loan;
        totalNumLoans = totalNumLoans.add(1);

        totalActiveLoans = totalActiveLoans.add(1);
        require(totalActiveLoans <= maximumNumberOfActiveLoans, 'Contract has reached the maximum number of active loans allowed by admins');

         IERC20(loan.loanERC20Denomination).transferFrom(msg.sender, loan.borrower, loan.loanPrincipalAmount);

        _mint(msg.sender, loan.loanId);
        console.log("beginLoan , borrower %s loanId :%s , loanAdminFeeInBasisPoints : %s   ",loan.borrower ,loan.loanId, loan.loanAdminFeeInBasisPoints );
        console.log(",loanPrincipalAmount :%s ",loan.loanPrincipalAmount);
        emit LoanStarted(
            loan.loanId,
            loan.borrower,
            msg.sender,
            loan.loanPrincipalAmount,
            loan.maximumRepaymentAmount,
            loan.nftCollateralId,
