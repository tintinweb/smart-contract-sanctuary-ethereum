pragma solidity 0.8.15;

import "@ensdomains/ens-contracts/contracts/registry/ENS.sol";
import "@ensdomains/ens-contracts/contracts/resolvers/Resolver.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./token/CNSToken.sol";
import "./libs/ENSController.sol";
import "./interfaces/IRegistrar.sol";

import "./structures/Domain.sol";
import "./structures/Subdomain.sol";

contract CNSController is Ownable, CNSToken, ENSController {
    /*
    |--------------------------------------------------------------------------
    | Constructor
    |--------------------------------------------------------------------------
    */
    constructor(
        address _ensAddr,
        address _baseRegistrarAddr,
        address _resolverAddr
    )
        CNSToken("Community Name Service", "CNS")
        ENSController(_ensAddr, _baseRegistrarAddr, _resolverAddr)
    {
        require(address(_ensAddr) != address(0), "Invalid address");
        require(address(_baseRegistrarAddr) != address(0), "Invalid address");
        require(address(_resolverAddr) != address(0), "Invalid address");
    }

    /*
    |--------------------------------------------------------------------------
    | Contract Controller
    |--------------------------------------------------------------------------
    */
    mapping(address => bool) internal controller;

    modifier onlyController() {
        require(controller[msg.sender]);
        _;
    }

    function addController(address _controller) public onlyOwner {
        controller[_controller] = true;
    }

    function removeController(address _controller) public onlyOwner {
        controller[_controller] = false;
    }

    /*
    |--------------------------------------------------------------------------
    | Domain Manager
    |--------------------------------------------------------------------------
    */
    Domain[] internal domains;

    function registerDomain(Domain memory _domain) public onlyController {
        domains.push(_domain);
    }

    function removeDomain(string memory _domain) public onlyController {
        uint256 index = getDomainId(_domain);
        delete domains[index];
    }

    function getDomainId(string memory _domain) public view returns (uint256) {
        for (uint256 i = 0; i < domains.length; i++) {
            if (
                keccak256(abi.encodePacked(domains[i].domain)) ==
                keccak256(abi.encodePacked(_domain))
            ) {
                return i;
            }
        }
        return 0;
    }

    function isRegisterDomain(string memory _domain)
        public
        view
        returns (bool)
    {
        return getDomainId(_domain) != 0;
    }

    /*
    |--------------------------------------------------------------------------
    | Subdomain Manager
    |--------------------------------------------------------------------------
    */

    Subdomain[] internal subdomains;

    /*
    |--------------------------------------------------------------------------
    | CNS Token (SBT)
    |--------------------------------------------------------------------------
    */

    function mint(address _to) internal onlyController {
        _mint(_to, currentId());
    }

    function burn(uint256 _tokenId) internal onlyController {
        _burn(_tokenId);
    }
}

