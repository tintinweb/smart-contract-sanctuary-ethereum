pragma solidity 0.8.10;

import "./OwnableContract.sol";
import "./interface/IBaseReNFT.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";

/**
 *
 * deploying minimal proxy contracts, also known as "clones".
 *
 * > To simply and cheaply clone contract functionality in an immutable way, this standard specifies
 * > a minimal bytecode implementation that delegates all calls to a known, fixed address.
 *
 * The library includes functions to deploy a proxy using either `create` (traditional deployment) or `create2`
 * (salted deterministic deployment). It also includes functions to predict the addresses of clones deployed using the
 * deterministic method.
 *
 * _Available since v3.4._
 */

contract ReNFTFactory is OwnableContract {
    /**nftAddress => (gameKey => reNFT) */
    mapping(address => mapping(string => address)) wrapReNFTMap;
    mapping(address => address) reNftToNft;
    mapping(address => address) reNftToTokenAddress;
    address private wrapReNftImplementation;

    constructor(){
        initOwnableContract();
    }

    function createWrapReNFT(address nftAddress, string calldata gameKey, string calldata name, string calldata symbol, uint minProfit, address tokenAddress) external returns (address) {
        require(IERC165(nftAddress).supportsInterface(type(IERC721).interfaceId), "no 721");
        require(wrapReNFTMap[nftAddress][gameKey] == address(0), "already create");
        address clone = Clones.clone(wrapReNftImplementation);
        IBaseReNFT(clone).init(nftAddress, name, symbol);
        wrapReNFTMap[nftAddress][gameKey] = clone;
        reNftToNft[clone] = nftAddress;
        reNftToTokenAddress[nftAddress] = tokenAddress;
        return clone;
    }

    function setWrapReNFTImplementation(address imp) public onlyAdmin {
        wrapReNftImplementation = imp;
    }

    function getWrapReNFTImplementation() public view returns (address){
        return wrapReNftImplementation;
    }

    function getWrapReNFTImplementation(address nftAddress, string calldata gameKey) public view returns (address){
        return wrapReNFTMap[nftAddress][gameKey];
    }

//    function getWrapReNFTImplementation(address nftAddress, string calldata gameKey) public view returns (address){
//        return wrapReNFTMap[nftAddress][gameKey];
//    }

    function getWrapReNftTokenAddress(address nftAddress) public view returns (address){
        return reNftToTokenAddress[nftAddress];
    }


}

