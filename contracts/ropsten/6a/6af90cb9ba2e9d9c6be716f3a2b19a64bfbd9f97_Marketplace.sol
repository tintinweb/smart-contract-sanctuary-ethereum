// File: contracts/Marketplace.sol



pragma solidity ^0.8.4;





contract Marketplace is ERC721Holder {

    struct Listing {
        address tokenAddress;
        uint256 tokenId;
        uint256 amount;
        uint256 priceETH;
        address ownerAddress;
        bool nft;
    }

    mapping(uint256 => Listing) public listings;
    uint256 private currentListing = 0;

    function listFT(address _tokenAddress, uint256 _amount, uint256 _priceETH) external {
        require(_amount > 0, "Bruh wyd");
        require(_priceETH > 0, "Bruh wyd");
        IERC20(_tokenAddress).transferFrom(msg.sender, address(this), _amount);
        listings[currentListing] = Listing(_tokenAddress, 0, _amount, _priceETH, msg.sender, false);
        currentListing = SafeMath.add(currentListing, 1);
    }

    function delistFT(uint256 _listingId) external {
        require(_listingId <= currentListing, "Bruh wyd");
        Listing memory listing = listings[_listingId];
        require(listing.amount == 0, "Listing not available.");
        IERC20(listing.tokenAddress).transferFrom(address(this), msg.sender, listing.amount);
        delete listings[_listingId]; // Sets all members of struct to 0
    }

    function listNFT(address _tokenAddress, uint256 _tokenId, uint256 _priceETH) external {
        require(_priceETH > 0, "Bruh wyd");
        IERC721(_tokenAddress).safeTransferFrom(msg.sender, address(this), _tokenId);
        listings[currentListing] = Listing(_tokenAddress, _tokenId, 1, _priceETH, msg.sender, true);
        currentListing += 1;
    }

    function delistNFT(uint256 _listingId) external {
        require(_listingId <= currentListing, "Bruh wyd");
        Listing memory listing = listings[_listingId];
        require(listing.amount == 0, "Listing not available.");
        IERC721(listing.tokenAddress).safeTransferFrom(address(this), msg.sender, listing.tokenId);
        delete listings[_listingId];
    }

    function buy(uint256 _listingId) external payable {
        require(_listingId <= currentListing, "Bruh wyd");
        Listing memory listing = listings[_listingId];
        require(listing.amount == 0, "Listing not available.");
        require(msg.value >= listing.priceETH, "Insufficient funds!");
        payable(listing.ownerAddress).transfer(listing.priceETH);
        if (listing.nft) {
            IERC721(listing.tokenAddress).safeTransferFrom(address(this), msg.sender, listing.tokenId);
        } else {
            IERC20(listing.tokenAddress).transferFrom(address(this), msg.sender, listing.amount);
        }
    }


}