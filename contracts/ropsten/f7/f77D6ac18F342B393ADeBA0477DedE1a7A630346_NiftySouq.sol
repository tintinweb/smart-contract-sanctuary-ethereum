        uint256 quantity;
    }

    modifier validRevenues(uint256[] calldata revenues, address[] calldata investors){
        // make sure revenues and creators length are same.
        require (revenues.length == investors.length, "corrupted revenues and investors");
        
        uint256 sum = 0;
        // make sure all revenues and creators are non zero.
        for (uint256 i = 0; i < revenues.length; i++) {
            require (revenues[i] > 0, "zero revenue is invalid");
            require (investors[i] != address(0), "null creator is invalid");

            sum = sum + revenues[i];
        }

        require (sum <= PERCENT_UNIT, "revenue overflow");
        _;
    }

    modifier onlyWert() {
        require (wert == msg.sender, "not an wert");
        _;
    }

    address public admin;
    address public wert;
    mapping (uint256 => TokenData) internal _tokenData;
    bool public is721;

    function __Base_init(string memory domain, string memory version, bool is721_, address admin_) internal onlyInitializing
    {
        __EIP712_init(domain, version);
        _setupRole(ADMIN_ROLE, admin_);

        wert = 0x48402530d59B99966f6e277854856f166cb92018;
        admin = admin_;
        is721 = is721_;
    }

    /// @dev Returns a hash of the given NFTVoucher, prepared using EIP712 typed data hashing rules.
    function _hash(LazyMintData calldata lazyData) internal view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(
            keccak256("LazyMintData(uint256 minPrice,uint256[] revenues,address[] investors,uint256[] royalties,address[] creators)"),
            lazyData.minPrice,
            keccak256(abi.encodePacked(lazyData.revenues)),
            keccak256(abi.encodePacked(lazyData.investors)),
            keccak256(abi.encodePacked(lazyData.royalties)),
            keccak256(abi.encodePacked(lazyData.creators))
        )));
    }

    /// @dev Verifies the signature for a given NFTVoucher, returning the address of the signer.
    ///      Will revert if the signature is invalid. Does not verify that the signer is authorized to mint NFTs.
    function _verify(LazyMintData calldata lazyData) internal view returns (address) {
        bytes32 digest = _hash(lazyData);
        return digest.toEthSignedMessageHash().recover(lazyData.signature);
    }

    function _pERCent(uint256 value1, uint256 value2)
        internal virtual pure
        returns (uint256)
    {
        uint256 result = value1.mul(value2).div(PERCENT_UNIT);
        return (result);
    }
    function _withdraw(uint256 price, address owner, uint256[] calldata revenues, address[] calldata investors) internal {
        uint256 revenueSum;
        uint256 revenueFee;
        uint256[] memory revenueFees = new uint256[](revenues.length);
        for (uint256 i = 0; i < revenues.length; i++) {
            revenueFee = _pERCent(price, revenues[i]);
            revenueFees[i] = revenueFee;
            revenueSum = revenueSum.add(revenueFee);
        }
        uint256 netfee = price.sub(revenueSum);

        require (price == netfee.add(revenueSum), "invalid revenues");

        if (netfee != 0) {
            require (owner != address(0), "invalid owner");
            payable(owner).transfer(netfee);
        }

        for (uint256 i = 0; i < investors.length; i++) {
            if (revenueFees[i] > 0) {
                require (investors[i] != address(0), "invalid creator");
                payable(investors[i]).transfer(revenueFees[i]);
            }
        }
    }

    function getTokenData(uint256 tokenId) public view returns (TokenData memory) {
        TokenData storage data = _tokenData[tokenId];

        if (data.royalties.length == 0) {
            for (uint256 i = tokenId; i > 0; i--) {
                data = _tokenData[i];
                if (data.royalties.length != 0) {
                    break;
                }
            }
        }
        return data;
    }

    function _setTokenData(uint256 tokenId, uint256 quantity, uint256[] calldata royalties, address[] calldata creators) internal {
        _tokenData[tokenId].royalties = royalties;
        _tokenData[tokenId].creators = creators;
        _tokenData[tokenId].quantity = quantity;
    }

    function setWert(address wert_) external onlyWert {
        wert = wert_;
    }

    function withdrawWert() onlyWert external {
        payable(wert).transfer(address(this).balance);
    }
}

