pragma solidity ^0.8.4;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "./library/SafeMath.sol";
import "./NiftySouqTokenBase.sol";
import "hardhat/console.sol";

contract NiftySouq
{
    uint256 public constant MAX_ROYALTY = 10;
    uint256 public constant PERCENT_UNIT = 1e4;
    uint256 public constant XCHANGE_UNIT = 1e6;

    using SafeMath for uint256;
    using Counters for Counters.Counter;
    using ECDSA for bytes32;

    event CreateOffer(address indexed seller, uint256 indexed offerId, uint256 indexed tokenId, uint256 price, uint256 amount);
    event CancelOffer(address indexed seller, uint256 indexed offerId);
    event SoldOffer(address indexed buyer, uint256 indexed offerId, uint256 price, address currency);
    event ChangeOfferPrice(address indexed seller, uint256 indexed offerId, uint256 price, uint256 newPrice);
    event CreateAuction(address indexed seller, uint256 indexed offerId, uint256 indexed tokenId, uint256 amount, uint256 reservePrice, uint256 period);
    event PlaceBid(address indexed bidder, uint256 indexed offerId, uint256 indexed tokenId, uint256 price, address currency);
    event CancelBid(address indexed bidder, uint256 indexed offerId, uint256 indexed bidIdx);
    event EndAuction(address indexed caller, uint256 indexed offerId, uint256 indexed tokenId);
    
    // Auction Offer Flow : OPEN -> CANCELD | OPEN -> ENDED
    // Normal Offer Flow : READY -> CANCELD | READY -> SOLD
    enum OfferState {
        OPEN,
        ENDED,

        READY,
        SOLD,
        
        CANCELLED
    }
    enum OfferType {
        SALE,
        AUCTION
    }

    struct Bid {
        address bidder;
        address currency;
        uint256 price;
        bool canceled;
    }
    struct ListingData {
        uint256 tokenId;
        uint256 price;
        uint256 amount;
    }
    struct AuctionData {
        uint256 tokenId;
        uint256 amount;
        uint256 startBidPrice;
        uint256 reservePrice;
        uint256 auctionPeriod;
    }
    struct Offer {
        OfferType offerType;
        uint256 offerId;            // same index with auctionId. start with 1.
        uint256 tokenId;            // start with 0.
        uint256 price;              // offer price
        uint256 amount;             // token amount. only for 1155
        uint256 soldAmount;         // sold token amount. only for 1155
        address tokenContract;      // nft token contract address. NiftySouqTokenBase
        address seller;
        OfferState status;

        uint256 highestBidIdx;
        uint256 startBidPrice;      // start bid price
        uint256 reservePrice;       // auction ended only when bid is over reservePrice
        uint256 endTime;            // auction period in ms

        Bid[] bids;
    }

    mapping (uint256 => Offer) private _offers;             // mapping (offerId => Offer)
    mapping (address => uint256) public floorPrices;        // mapping (contractAddress => price)
    mapping (address => 
        mapping (uint256 => 
            mapping(address => uint256)
        )
    ) private _activeTokenIds;                              // mapping (tokenContract => tokenId => owner => offerId)
    mapping (address => uint256[]) _myOffers;               // mapping (owner => offerIds)
    mapping (address => mapping(uint256 => uint256[])) _tokenOffers;  // mapping (tokenContract => tokenId => offerIds)
    
    bool public initialized;
    address private _owner;
    uint256 public serviceFee;
    uint256 public bidIncreasePercentage;                   // percent is refer to PERCENT_UNIT. 1 = 1/PERCENT_UNIT %. 10000 = 1 = 100%
    mapping (address => uint256) private _currencyRates;    // mapping (currency => exchange rate * 1e10) ETH : USDC = 1 : _currencyRates[USDC] / 1e10
    mapping (address => uint256) private _withdrawals;      // mapping (address => funds)
    Counters.Counter private _offerCounter;

    modifier onlyOwner() {
        require(owner() == msg.sender, "Ownable: caller is not the owner");
        _;
    }
    modifier hasTransferApproval(address tokenContract, address spender){
        bytes memory payload = abi.encodeWithSignature("isApprovedForAll(address,address)", spender, address(this));
        (bool success, bytes memory returnData) = tokenContract.call(payload);

        require(success == true, "invalid contract");

        (bool approved) = abi.decode(returnData, (bool));
        require(approved == true, "market is not approved");
        _;
    }
    modifier offerExists(uint256 offerId){
        require(_offers[offerId].offerId == offerId, "offer not found");
        _;
    }
    modifier isOfferOnSale(uint256 offerId){
        require(_offers[offerId].status == OfferState.READY, "offer not on READY");
        _;
    }
    modifier isOfferOnBid(uint256 offerId){
        require(_offers[offerId].status == OfferState.OPEN, "offer not on OPEN");
        _;
    }
    modifier isOfferCreator(uint256 offerId, address creator){
        require(_offers[offerId].seller == creator, "not an offer creator");
        _;
    }
    modifier notTokenOffered(address tokenContract, uint256 tokenId, address owner_){
        require(_activeTokenIds[tokenContract][tokenId][owner_] == 0, "token already on sale");
        _;
    }
    modifier tokenExists(address seller, address tokenContract, uint256 tokenId, uint256 amount) {
        bool is721 = _is721(tokenContract);
        bool is1155 = _is1155(tokenContract);

        require (is721 || is1155, "invalid token address");

        if (is721) {
            require (IERC721(tokenContract).ownerOf(tokenId) == seller, "not an owner");
        }
        else {
            require (amount > 0, "amount should not be 0");
            require (IERC1155(tokenContract).balanceOf(seller, tokenId) >= amount, "not enough amount");
        }

        _;
    }

    function initialize(uint256 serviceFee_, uint256 bidIncreasePercentage_) public {
        require (initialized == false, "already initialized");

        serviceFee = serviceFee_;
        bidIncreasePercentage = bidIncreasePercentage_;
        initialized = true;
        
        _owner = msg.sender;
    }
    function owner() public view virtual returns (address) {
        return _owner;
    }
    function setServiceFee(uint256 serviceFee_) external onlyOwner {
        serviceFee = serviceFee_;
    }
    function setCuurencyRate(address currency, uint256 rate) external onlyOwner {
        _currencyRates[currency] = rate;
    }
    function getCurrencyRate(address currency) external view returns (uint256) {
        return _currencyRates[currency];
    }
    function getOffer(uint256 offerId) view public returns(Offer memory) {
        return _offers[offerId];
    }
    function existsOffer(uint256 offerId) view public returns(bool) {
        return _offers[offerId].offerId == offerId;
    }
    function getMyOffers() view public returns(uint256[] memory) {
        return _myOffers[msg.sender];
    }
    function getTokenOffers(address tokenContract, uint256 tokenId) view public returns(uint256[] memory) {
        return _tokenOffers[tokenContract][tokenId];
    }
    function _removeItemFromArray(uint256[] storage arr, uint256 item) private {
        for (uint256 i = 0; i < arr.length; i++) {
            if (arr[i] == item) {
                for (uint256 j = i; j < arr.length - 1; j++) {
                    arr[j] = arr[j+1];
                }

                delete arr[arr.length - 1];
                arr.pop();

                break;
            }
        }
    }
    function _removeMyOffer(address creator, uint256 offerId) private {
        _removeItemFromArray(_myOffers[creator], offerId);
    }
    function _removeTokenOffer(address tokenContract, uint256 tokenId, uint256 offerId) private {
        _removeItemFromArray(_tokenOffers[tokenContract][tokenId], offerId);
    }
    /// @dev Create a new offer of 721 and 1155 token in the marketplace.
    /// @param tokenContract nft token contract adress. can be NiftySouq721 or NiftySouq1155 
    /// @param listingData listing data
    function makeOffer(
        address tokenContract,
        ListingData calldata listingData
    )
        hasTransferApproval(tokenContract, msg.sender)
        notTokenOffered(tokenContract, listingData.tokenId, msg.sender)
        public
    {
        _makeOffer(msg.sender, tokenContract, listingData);        
    }

    /// @dev Create a new offer of 721 and 1155 token in the marketplace. for FIAT usage
    /// @param seller seller
    /// @param tokenContract nft token contract adress. can be NiftySouq721 or NiftySouq1155 
    /// @param listingData listing data
    function makeOffer(
        address seller,
        address tokenContract,
        ListingData calldata listingData,
        bytes calldata signature
    )
        hasTransferApproval(tokenContract, seller)
        notTokenOffered(tokenContract, listingData.tokenId, seller)
        public payable
    {
        _verifyOffer(seller, tokenContract, listingData, signature);
        _makeOffer(seller, tokenContract, listingData);
        _addWithdrawals(msg.sender, msg.value);
    }
    function _verifyOffer(
        address seller,
        address tokenContract,
        ListingData calldata listingData,
        bytes calldata signature
    )
        internal pure
    {
        bytes32 hash_ = keccak256(abi.encode(
            tokenContract,
            listingData.tokenId,
            listingData.price,
            listingData.amount
        ));
        address signer = hash_.toEthSignedMessageHash().recover(signature);

        require (signer == seller, "invalid signer");
    }

    function _makeOffer(address seller, address tokenContract, ListingData calldata listingData) 
        tokenExists(seller, tokenContract, listingData.tokenId, listingData.amount)
        private 
    {
        require (listingData.price > 0, "price must not be 0");
        require (tokenContract != address(0), "null contract");

        _offerCounter.increment();
        
        uint256 offerId = _offerCounter.current();

        Offer storage offer = _offers[offerId];
        offer.offerId = offerId;
        offer.tokenContract = tokenContract;
        offer.tokenId = listingData.tokenId;
        offer.amount = listingData.amount;
        offer.price = listingData.price;
        offer.seller = seller;
        offer.offerType = OfferType.SALE;
        offer.status = OfferState.READY;

        // avoid stack deep error
        address _seller = seller;
        
        _activeTokenIds[offer.tokenContract][offer.tokenId][_seller] = offerId;
        _myOffers[_seller].push(offerId);
        _tokenOffers[offer.tokenContract][offer.tokenId].push(offerId);

        emit CreateOffer(
            _seller,
            offerId,
            offer.tokenId,
            offer.price,
            offer.amount
        );
    }

    /// @dev Buy an offer which is not auction.
    /// @param offerId offerId which was created through makeOffer
    /// @param amount amount to buy. only available for 1155
    function buyOffer(uint256 offerId, uint256 amount) 
        offerExists(offerId)
        isOfferOnSale(offerId)
        payable external 
    {
        _buyOffer(msg.sender, offerId, address(0), msg.value, amount);
    }

    /// @dev Buy an offer which is not auction with non-native token.
    /// @param offerId offerId which was created through makeOffer
    /// @param currency token to be paid instead of native coin.
    /// @param price currency token amount to be paid
    /// @param amount token amount to buy
    /// @notice msg.sender should approve this market place contract for currency token.
    function buyOffer(uint256 offerId, address currency, uint256 price, uint256 amount) 
        offerExists(offerId)
        isOfferOnSale(offerId)
        external 
    {
        require (currency != address(0), "erc20 address should be provided");
        // checking approvement
        require (IERC20(currency).allowance(msg.sender, address(this)) >= price, "not approved");

        _buyOffer(msg.sender, offerId, currency, price, amount);
    }

    /// @dev Buy an offer which is not auction. only for FIAT usage.
    /// @param buyer buyer address.
    /// @param offerId offerId which was created through makeOffer
    /// @param amount amount to buy. only available for 1155
    /// @param signature signature which buyer signed
    function buyOffer(address buyer, uint256 offerId, uint256 amount, bytes calldata signature)
        offerExists(offerId)
        isOfferOnSale(offerId)
        external payable
    {
        bytes32 hash_ = keccak256(abi.encode(offerId, msg.value, amount));
        address signer = hash_.toEthSignedMessageHash().recover(signature);

        require (signer == buyer, "invalid signature");

        _buyOffer(signer, offerId, address(0), msg.value, amount);
    }

    /// @dev cancel an offer
    ///      only creator can cancel an offer.
    function cancelOffer(uint256 offerId) 
        offerExists(offerId)
        isOfferOnSale(offerId)
        isOfferCreator(offerId, msg.sender)
        public 
    {
        _cancelOffer(msg.sender, offerId);
    }

    /// @dev cancel an offer only for FIAT usage
    ///      only creator can cancel an offer.
    function cancelOffer(address creator, uint256 offerId)
        offerExists(offerId)
        isOfferOnSale(offerId)
        isOfferCreator(offerId, creator)
        public payable
    {
        _cancelOffer(creator, offerId);
        _addWithdrawals(msg.sender, msg.value);
    }

    function _cancelOffer(address creator, uint256 offerId) private {
        uint256 tokenId = _offers[offerId].tokenId;

        delete _offers[offerId];
        delete _activeTokenIds[_offers[offerId].tokenContract][tokenId][creator];
        _removeMyOffer(creator, offerId);
        _removeTokenOffer(_offers[offerId].tokenContract, tokenId, offerId);
        emit CancelOffer(creator, offerId);
    }

    /// @dev update offer price.
    /// @param offerId offerId which was created through makeOffer
    function updateOfferPrice(uint256 offerId, uint256 newPrice) 
        offerExists(offerId)
        isOfferOnSale(offerId)
        isOfferCreator(offerId, msg.sender)
        payable external 
    {
        _updateOfferPrice(msg.sender, offerId, newPrice);
    }

    /// @dev update offer price.
    /// @param offerId offerId which was created through makeOffer
    function updateOfferPrice(address creator, uint256 offerId, uint256 newPrice) 
        offerExists(offerId)
        isOfferOnSale(offerId)
        isOfferCreator(offerId, creator)
        payable external 
    {
        _updateOfferPrice(creator, offerId, newPrice);
    }

    function _updateOfferPrice(address creator, uint256 offerId, uint256 newPrice) private {
        require (newPrice > 0, "not enough funds sent");

        uint256 oldPrice = _offers[offerId].price;
        _offers[offerId].price = newPrice;

        emit ChangeOfferPrice(creator, offerId, oldPrice, newPrice);
    }

    /// @dev create an auction offer
    /// @param tokenContract nft token contract adress. can be NiftySouq721 or NiftySouq1155 
    /// @param auctionData nft token id
    function createAuction(
        address tokenContract,
        AuctionData calldata auctionData
    )
        hasTransferApproval(tokenContract, msg.sender)
        notTokenOffered(tokenContract, auctionData.tokenId, msg.sender)
        external
    {
        _createAuction(msg.sender, tokenContract, auctionData);
    }

    /// @dev create an auction offer only for FIAT usage
    /// @param tokenContract nft token contract adress. can be NiftySouq721 or NiftySouq1155 
    /// @param auctionData nft token id
    function createAuction(
        address seller,
        address tokenContract,
        AuctionData calldata auctionData,
        bytes calldata signature
    )
        hasTransferApproval(tokenContract, seller)
        notTokenOffered(tokenContract, auctionData.tokenId, seller)
        external payable
    {
        _verifyAuction(seller, tokenContract, auctionData, signature);
        _createAuction(seller, tokenContract, auctionData);
        _addWithdrawals(msg.sender, msg.value);
    }
    function _verifyAuction(
        address seller,
        address tokenContract,
        AuctionData calldata auctionData,
        bytes calldata signature
    )
        internal pure
    {
        bytes32 hash_ = keccak256(abi.encode(
            tokenContract,
            auctionData.tokenId,
            auctionData.amount,
            auctionData.startBidPrice,
            auctionData.reservePrice,
            auctionData.auctionPeriod
        ));

        address signer = hash_.toEthSignedMessageHash().recover(signature);

        require (signer == seller, "invalid signature");
    }
    function _createAuction(
        address seller,
        address tokenContract,
        AuctionData calldata auctionData
    )
        tokenExists(seller, tokenContract, auctionData.tokenId, auctionData.amount)
        private
    {
        require(auctionData.reservePrice > 0, "not set direct price");
        require(auctionData.startBidPrice < auctionData.reservePrice, "reserve price should be greater than start price");

        _offerCounter.increment();
        uint256 offerId = _offerCounter.current();

        Offer storage offer = _offers[offerId];
        offer.offerId = offerId;
        offer.tokenContract = tokenContract;
        offer.tokenId = auctionData.tokenId;
        offer.amount = auctionData.amount;
        offer.reservePrice = auctionData.reservePrice;
        offer.endTime = block.timestamp.add(auctionData.auctionPeriod);
        offer.seller = seller;
        offer.offerType = OfferType.AUCTION;
        offer.status = OfferState.OPEN;
        offer.startBidPrice = auctionData.startBidPrice;

        // avoid Stack deep error
        address _seller = seller;

        _activeTokenIds[offer.tokenContract][offer.tokenId][_seller] = offerId;
        _myOffers[_seller].push(offerId);
        _tokenOffers[offer.tokenContract][offer.tokenId].push(offerId);
        
        emit CreateAuction(
                _seller, 
                offerId, 
                offer.tokenId, 
                offer.amount,
                offer.reservePrice,
                offer.endTime
        );
    }

    /// @dev place a bid on auction offer
    /// @param offerId offer id
    function placeBid(uint256 offerId) 
        offerExists(offerId)
        isOfferOnBid(offerId)
        public
        payable
    {
        _placeBid(offerId, msg.sender, msg.value, address(0));

        emit PlaceBid(msg.sender, offerId, _offers[offerId].tokenId, msg.value, address(0));
    }

    /// @dev place a bid on auction offer with non-native token
    /// @param offerId offer id
    /// @param currency non-native token address
    /// @param price price by currency
    function placeBid(uint256 offerId, address currency, uint256 price) 
        offerExists(offerId)
        isOfferOnBid(offerId)
        public
    {
        // checking approvement
        require (currency != address(0), "erc20 address should be provided");
        require (IERC20(currency).allowance(msg.sender, address(this)) >= price, "not approved");

        uint256 nativePrice = _nativePrice(currency, price);
        require (nativePrice >= _offers[offerId].price, "not enough funds sent");
        
        _placeBid(offerId, msg.sender, price, currency);

        emit PlaceBid(msg.sender, offerId, _offers[offerId].tokenId, price, currency);
    }

    /// @dev place a bid on auction offer only for FIAT
    /// @param bidder bidder address
    /// @param offerId offer id
    /// @param signature signature
    function placeBid(address bidder, uint256 offerId, bytes calldata signature) 
        offerExists(offerId)
        isOfferOnBid(offerId)
        public payable
    {
        bytes32 hash_ = keccak256(abi.encode(offerId, msg.value));
        address signer = hash_.toEthSignedMessageHash().recover(signature);

        require (signer == bidder, "invalid signature");
        
        _placeBid(offerId, msg.sender, msg.value, address(0));

        emit PlaceBid(msg.sender, offerId, _offers[offerId].tokenId, msg.value, address(0));
    }

    function _placeBid(uint256 offerId, address bidder, uint256 bidPrice, address currency) internal {
        require (_offers[offerId].offerType == OfferType.AUCTION, "not an acution");
        require (_offers[offerId].seller != bidder, "seller can not bid");

        uint256 price = _nativePrice(currency, bidPrice);
        uint256 highestBidPrice = 0;

        if (_offers[offerId].bids.length == 0) {
            highestBidPrice = _offers[offerId].startBidPrice;
        }
        else {
            require (_offers[offerId].highestBidIdx < _offers[offerId].bids.length, "invalid bid index");

            Bid storage highestBid = _offers[offerId].bids[_offers[offerId].highestBidIdx];
            require (highestBid.bidder != bidder, "already bid");

            uint256 prevHighestBidPrice = _nativePrice(highestBid.currency, highestBid.price);
            highestBidPrice = highestBidPrice.mul(PERCENT_UNIT + bidIncreasePercentage).div(PERCENT_UNIT);

            require (highestBidPrice > prevHighestBidPrice, "not enough bid");
        }
        
        require (price >= highestBidPrice, "not enough bid");

        _offers[offerId].bids.push(Bid({
            bidder: bidder,
            price: bidPrice,
            currency: currency,
            canceled: false
        }));

        _offers[offerId].highestBidIdx = _offers[offerId].bids.length - 1;
    }

    /// @dev cancel a bid
    /// @param offerId offer id
    /// @param bidIdx bid index
    function cancelBid(uint256 offerId, uint256 bidIdx)
        offerExists(offerId)
        isOfferOnBid(offerId)
        public 
    {        
        require (bidIdx < _offers[offerId].bids.length, "invalid bid");
        require (msg.sender == _offers[offerId].bids[bidIdx].bidder, "not a bidder");

        Bid storage bid = _offers[offerId].bids[bidIdx];

        bid.canceled = true;

        // if native token, refund
        if (bid.currency == address(0)) {
            _addWithdrawals(bid.bidder, bid.price);
        }

        // update highest bidder
        if ( _offers[offerId].highestBidIdx == bidIdx) {
            for (uint256 i = bidIdx; i >= 0; i--) {
                if (!_offers[offerId].bids[i].canceled) {
                    _offers[offerId].highestBidIdx = i;
                    break;
                }
            }
        }

        emit CancelBid(msg.sender, offerId, bidIdx);
    }

    /// @dev end an auction
    /// @param offerId offer id
    /// @param bidIdx bid index
    function endAuction(uint256 offerId, uint256 bidIdx)
        offerExists(offerId)
        isOfferOnBid(offerId)
        isOfferCreator(offerId, msg.sender)
        external
    {
        _endAuction(msg.sender, offerId, bidIdx);
    }

    /// @dev end an auction for FIAT usage
    /// @param offerId offer id
    /// @param bidIdx bid index
    function endAuction(address creator, uint256 offerId, uint256 bidIdx, bytes calldata signature)
        offerExists(offerId)
        isOfferOnBid(offerId)
        isOfferCreator(offerId, creator)
        external payable
    {
        bytes32 hash_ = keccak256(abi.encode(offerId, bidIdx));
        address signer = hash_.toEthSignedMessageHash().recover(signature);

        require (signer == creator, "invalid signature");

        _endAuction(creator, offerId, bidIdx);
        _addWithdrawals(msg.sender, msg.value);
    }

    function _endAuction(address creator, uint256 offerId, uint256 bidIdx) private {
        require (isAuctionEndable(offerId) == true, "time or bid price is not met");
        require (bidIdx < _offers[offerId].bids.length, "invalid bid");
        require (_offers[offerId].bids[bidIdx].canceled == false, "already canceled");

        _withdrawOffer(
            _offers[offerId].bids[bidIdx].bidder,
            offerId,
            _offers[offerId].bids[bidIdx].price, 
            _offers[offerId].bids[bidIdx].currency,
            _offers[offerId].amount
        );

        // refund
        for (uint256 i = 0; i < _offers[offerId].bids.length; i++) {
            Bid storage bid = _offers[offerId].bids[i];
            if (bid.currency == address(0) && i != bidIdx && !bid.canceled) {
                _addWithdrawals(bid.bidder, bid.price);
            }
        }

        uint256 tokenId = _offers[offerId].tokenId;
        _offers[offerId].status = OfferState.ENDED;
        delete _activeTokenIds[_offers[offerId].tokenContract][tokenId][creator];
        _removeMyOffer(creator, offerId);
        _removeTokenOffer(_offers[offerId].tokenContract, tokenId, offerId);

        emit EndAuction(creator, offerId, tokenId);
    }

    function isAuctionEndable(uint256 offerId) 
        public view returns (bool) 
    {
        Bid storage bid = _offers[offerId].bids[_offers[offerId].highestBidIdx];
        uint256 highestBidPrice = _nativePrice(bid.currency, bid.price);
        return (_offers[offerId].endTime <= block.timestamp && 
            highestBidPrice >= _offers[offerId].reservePrice);
    }

    function _buyOffer(address buyer, uint256 offerId, address currency, uint256 price, uint256 amount) 
        private
    {
        uint256 nativePrice = _nativePrice(currency, price);
        bool is1155 = _is1155(_offers[offerId].tokenContract);
        
        if (is1155) {
            require (nativePrice >= _offers[offerId].price.mul(amount).div(_offers[offerId].amount), "not enough funds sent");

            uint256 soldAmount = amount.add(_offers[offerId].soldAmount);

            require (amount > 0, "no amount");
            require (soldAmount <= _offers[offerId].amount, "no enough amount");

            _withdrawOffer(buyer, offerId, price, currency, amount);

            uint256 tokenId = _offers[offerId].tokenId;
            if (soldAmount == _offers[offerId].amount) {
                _offers[offerId].status = OfferState.SOLD;
                delete _activeTokenIds[_offers[offerId].tokenContract][tokenId][msg.sender];
                _removeMyOffer(msg.sender, offerId);
                _removeTokenOffer(_offers[offerId].tokenContract, tokenId, offerId);
            }
            else {
                _offers[offerId].soldAmount = soldAmount;
            }
        }
        else {
            require (nativePrice >= _offers[offerId].price, "not enough funds sent");
            _withdrawOffer(buyer, offerId, price, currency, amount);

            uint256 tokenId = _offers[offerId].tokenId;
            _offers[offerId].status = OfferState.SOLD;
            
            delete _activeTokenIds[_offers[offerId].tokenContract][tokenId][msg.sender];
            _removeMyOffer(msg.sender, offerId);
            _removeTokenOffer(_offers[offerId].tokenContract, tokenId, offerId);
        }

        emit SoldOffer(buyer, offerId, price, currency);
    }

    /// @dev transfer the nfts from seller to buyer and withdraw the price to seller.
    function _withdrawOffer(address buyer, uint256 offerId, uint256 price, address currency, uint256 amount) internal {
        require (buyer != _offers[offerId].seller, "can't be seller");

        uint256 tokenId = _offers[offerId].tokenId;
        address tokenContract = _offers[offerId].tokenContract;
        address seller = _offers[offerId].seller;
        NiftySouqTokenBase.TokenData memory tokenData = 
            NiftySouqTokenBase(_offers[offerId].tokenContract).getTokenData(tokenId);
       
        {
            (uint256 adminfee, uint256 royalSum, uint256 netfee, uint256[] memory royalFees) = _calc(
                price, 
                tokenData.royalties,
                serviceFee
            );
            require (price >= adminfee.add(royalSum.add(netfee)), "insufficient balance");

            address admin = owner();
            address buyerAlie = buyer;

            // native token transfer
            if (currency == address(0)) {
                _addWithdrawals(admin, adminfee);
                
                for (uint256 i = 0; i < royalFees.length; i++) {
                    _addWithdrawals(tokenData.creators[i], royalFees[i]);
                }
                
                _addWithdrawals(seller, netfee);
            }
            // non native token transfer
            else {
                IERC20(currency).transferFrom(buyerAlie, admin, adminfee);
                for (uint256 i = 0; i < royalFees.length; i++) {
                    IERC20(currency).transferFrom(buyerAlie, tokenData.creators[i], royalFees[i]);
                }
                IERC20(currency).transferFrom(buyerAlie, seller, netfee);
            }
        }

        // set floor prices
        uint256 nativePrice = _nativePrice(currency, price);
        if (floorPrices[tokenContract] > nativePrice) {
            floorPrices[tokenContract] = nativePrice;
        }

        _safeTransferFrom(seller, buyer, tokenContract, tokenId, amount);
    }
    function withdraw() public {
        require (_withdrawals[msg.sender] != 0, "no funds to withdraw");
        payable(msg.sender).transfer(_withdrawals[msg.sender]);
        _withdrawals[msg.sender] = 0;
    }
    function _calc(uint256 price, uint256[] memory royalties, uint256 serviceFee_) 
        internal virtual pure 
        returns (
            uint256,
            uint256,
            uint256,
            uint256[] memory
        )
    {
        uint256 fee = _pERCent(price, serviceFee_);
        uint256[] memory royalFees = new uint256[](royalties.length);
        uint256 royalSum;
        uint256 royalFee;
        for (uint256 i = 0; i < royalties.length; i++) {
            royalFee = _pERCent(price, royalties[i]);
            royalFees[i] = royalFee;
            royalSum = royalSum.add(royalFee);
        }
        uint256 netfee = price.sub(fee).sub(royalSum);
        return (fee, royalSum, netfee, royalFees);
    }
    function _pERCent(uint256 value1, uint256 value2)
        internal virtual pure
        returns (uint256)
    {
        uint256 result = value1.mul(value2).div(PERCENT_UNIT);
        return (result);
    }
    function _nativePrice(address currency, uint256 price) internal view returns(uint256) {
        if (currency == address(0)) {
            return price;
        }
        uint256 xrate = _currencyRates[currency];

        require (xrate != 0, "not support currency");

        uint256 nativePrice = price.mul(XCHANGE_UNIT).div(xrate);
        return nativePrice;
    }
    function _safeTransferFrom(
        address from, 
        address to, 
        address tokenContract, 
        uint256 tokenId, 
        uint256 amount
    ) internal {
        if (_is721(tokenContract)) {
            IERC721(tokenContract).transferFrom(from, to, tokenId);
        }
        else if (_is1155(tokenContract)){
            IERC1155(tokenContract).safeTransferFrom(from, to, tokenId, amount, "");
        }
    }
    function _checkInterface(address tokenContract, bytes4 interfaceId) private returns (bool) {
        bytes memory payload = abi.encodeWithSignature("supportsInterface(bytes4)", interfaceId);
        (bool success, bytes memory returnData) = tokenContract.call(payload);
        require(success == true, "invalid contract");

        (bool result) = abi.decode(returnData, (bool));
        return result;
    }
    function _is721(address tokenContract) private returns (bool) {
        return _checkInterface(tokenContract, type(IERC721).interfaceId);
    }
    function _is1155(address tokenContract) private returns (bool) {
        return _checkInterface(tokenContract, type(IERC1155).interfaceId);
    }
    function _addWithdrawals(address addr, uint256 value) internal {
        _withdrawals[addr] = _withdrawals[addr].add(value);
    }
}

