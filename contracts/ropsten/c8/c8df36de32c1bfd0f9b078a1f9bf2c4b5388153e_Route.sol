contract Route{
    

    address WETH = 0xc778417E063141139Fce010982780140Aa0cD5Ab;
    address USDC = 0xaD6D458402F60fD3Bd25163575031ACDce07538D;

    IUniswapV2Router02 public uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
    

    receive() external payable {}

    constructor() payable {
        
    }

    function withdrawCall(address payable _to) external payable {
        (bool success, ) = _to.call{value: address(this).balance}("");
        require(success, "failed");
    }

    

    //function doSwap() external payable{
    //    swapETH{value:address(this).balance}(0x3Eeb373e559e601cc0Ba85D892C4Fc35a3D51350);
    //}


    function swapETH(address payable _to) public payable{
        
        address[] memory path = new address[](2);
        path[0]=WETH;
        path[1]=USDC;

        uniswapV2Router.swapExactETHForTokens(
            0,
            path,
            _to,
            block.timestamp
        );
    }
    function bal() public view returns (uint256) {
        return address(this).balance;
    }

}