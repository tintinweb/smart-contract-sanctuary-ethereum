pragma solidity ^0.8.15;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "hardhat/console.sol";

interface IMegaNFT is IERC721 {
    function levelOfId(uint256 id) external view returns (uint level);
}

contract MegaStaking is Ownable, IERC721Receiver, ReentrancyGuard {
    using SafeMath for uint256;
    
    IMegaNFT public nftContract;     // interface for MegaNFT
    bool public stakeStarted = false;

    struct Staker {
        uint256[] tokenIdsStaked;
        uint256 stakedCountTotal;
        mapping(uint256=>uint256) stakedCountPerLevel;
    }
    mapping(address => Staker) private stakerFromAddress;
    mapping(uint256 => address) public tokenOwnerForId;     //  MegaNFT owner address for tokenId already staked    

    uint256 public totalNFTstakedCnt;                           // total MegaNFT staked
    uint256[] public totalNFTIdsStaked;
    mapping(uint256=>uint256) public totalNFTstakedPerLevel;

    // modifier for check if staking is started
    modifier hasStarted() {
        require(stakeStarted, "NftStaking: staking not started");
        _;
    }

    modifier hasNotStarted() {
        require(!stakeStarted, "NftStaking: staking has started");
        _;
    }

    event Started();
    event NftStaked(address staker, uint256 tokenId);
    event NftUnstaked(address staker, uint256 tokenId);
    
    constructor (address addr) {
        nftContract = IMegaNFT(addr);        
    }

    // start staking
    function start() public onlyOwner hasNotStarted {
        stakeStarted = true;
        emit Started();
    }

    // important!!!    in case MegaNFT recieved to staking contract for stake,  user must send it as safetransferfrom to call this function.
    function onERC721Received(
        address ,
        address from,
        uint256 tokenId,
        bytes calldata 
    ) external override returns (bytes4){
        _stakeNFT(tokenId, from);
        return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    }

    // list NFT ids staked by address
    function listNFTStakedForAddress(address addr) public view returns (uint256 [] memory) {
        return stakerFromAddress[addr].tokenIdsStaked;
    }

    // list total NFT staked and owner
    function listAllNFTAndOwner() public view returns (uint256 [] memory, address [] memory, uint [] memory) {
        uint length = totalNFTIdsStaked.length;
        address [] memory ownerArr = new address[](length);
        uint [] memory lvlArr = new uint[](length);
        for(uint i = 0; i < totalNFTIdsStaked.length; i ++){
            ownerArr[i] = tokenOwnerForId[totalNFTIdsStaked[i]];          
            lvlArr[i] = nftContract.levelOfId(totalNFTIdsStaked[i]);
        }
        return (totalNFTIdsStaked, ownerArr, lvlArr);
    }

    // stake NFT
    function _stakeNFT(uint256 id, address from) internal hasStarted {
        require(address(nftContract) == msg.sender, "NftStaking: not correct NFT staked!");   
        stakerFromAddress[from].tokenIdsStaked.push(id);
        totalNFTIdsStaked.push(id);
        stakerFromAddress[from].stakedCountTotal = stakerFromAddress[from].stakedCountTotal.add(1);
        uint levelPerId = nftContract.levelOfId(id);
        stakerFromAddress[from].stakedCountPerLevel[levelPerId] = stakerFromAddress[from].stakedCountPerLevel[levelPerId].add(1);
        totalNFTstakedCnt = totalNFTstakedCnt.add(1);
        totalNFTstakedPerLevel[levelPerId] = totalNFTstakedPerLevel[levelPerId].add(1);        
        tokenOwnerForId[id] = from;        
        emit NftStaked(from, id);
    }    

    // batch stake NFT
    function batchStakeNFT(uint256[] memory tokenIds) external hasStarted {        
        for (uint256 i=0; i < tokenIds.length; i ++){
            nftContract.safeTransferFrom(msg.sender, address(this), tokenIds[i]);
        }
    }

    // unstake NFT, transfer back nft to original owner
    function unstakeNft(uint256 id, address to) public hasStarted {        
        require(tokenOwnerForId[id] == msg.sender, "NftStaking: token not staked or incorrect token owner!");
        uint levelPerId = nftContract.levelOfId(id);
        tokenOwnerForId[id] = address(0);     
        stakerFromAddress[msg.sender].stakedCountTotal = stakerFromAddress[msg.sender].stakedCountTotal.sub(1);      
        stakerFromAddress[msg.sender].stakedCountPerLevel[levelPerId] = stakerFromAddress[msg.sender].stakedCountPerLevel[levelPerId].sub(1);  
        totalNFTstakedCnt = totalNFTstakedCnt.sub(1);
        totalNFTstakedPerLevel[levelPerId] = totalNFTstakedPerLevel[levelPerId].sub(1);
        for (uint i = 0; i < stakerFromAddress[msg.sender].tokenIdsStaked.length; i ++){
            if (stakerFromAddress[msg.sender].tokenIdsStaked[i] == id){
                stakerFromAddress[msg.sender].tokenIdsStaked[i] = stakerFromAddress[msg.sender].tokenIdsStaked[stakerFromAddress[msg.sender].tokenIdsStaked.length - 1];
                break;
            }
        }
        stakerFromAddress[msg.sender].tokenIdsStaked.pop();
        for (uint i = 0; i < totalNFTIdsStaked.length; i ++){
            if (totalNFTIdsStaked[i] == id){
                totalNFTIdsStaked[i] = totalNFTIdsStaked[totalNFTIdsStaked.length - 1];
                break;
            }
        }
        totalNFTIdsStaked.pop();
        try nftContract.safeTransferFrom(address(this), to, id)  {} catch {            
            nftContract.transferFrom(address(this), to, id);
        }
        emit NftUnstaked(msg.sender, id);
    }

    // batch unstake all NFT for msg.sender
    function batchUnstakeNFT(uint256[] memory tokenIds, address to) external hasStarted {
        for (uint256 i=0; i < tokenIds.length; i ++){
            unstakeNft(tokenIds[i], to);
        }
    }    

    function getTotalValueStaked() external view returns (uint256) {
        uint256 lvl_1_cnt = totalNFTstakedPerLevel[1];
        uint256 lvl_2_cnt = totalNFTstakedPerLevel[2];
        uint256 lvl_3_cnt = totalNFTstakedPerLevel[3];
        uint256 lvl_4_cnt = totalNFTstakedPerLevel[4];
        uint256 value = lvl_1_cnt.mul(1).add(lvl_2_cnt.mul(2)).add(lvl_3_cnt.mul(3)).add(lvl_4_cnt.mul(4));
        return value;
    }
}

