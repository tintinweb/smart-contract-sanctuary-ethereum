// File contracts/Apartments/Apartments.sol





pragma solidity ^0.8.0;

contract Apartments is OwnableAccessControl, IApartments, ONFT721 {

    // keeps track of the ID based on apartment type.
    mapping (ApartmentType => uint256) public countersMapping;

    // tracks how many of each type have been minted.
    mapping (ApartmentType => uint256) public mintedPerType;

    // enforces the max amount that can be minted for each type of apartment.
    mapping (ApartmentType => uint256) public maxPerType;

    // roles
    bytes32 constant public override APARTMENT_MINTER_ROLE = keccak256("APARTMENT_MINTER_ROLE");
    bytes32 constant public override APARTMENT_BURNER_ROLE = keccak256("APARTMENT_BURNER_ROLE");
    string private _URI;

    uint256 public maxSupply = 9500;

    constructor(string memory name, string memory symbol, string memory uri, address endpoint) 
    ONFT721(name, symbol, endpoint) {
        setURI(uri);
        _initializeTokenIds();
    }

    function _initializeTokenIds() internal {
        countersMapping[ApartmentType.small] = 0;
        countersMapping[ApartmentType.medium] = 4500;
        countersMapping[ApartmentType.large] = 7500;
        countersMapping[ApartmentType.penthouse] = 9000;

        maxPerType[ApartmentType.small] = 4500;
        maxPerType[ApartmentType.medium] = 3000;
        maxPerType[ApartmentType.large] = 1500;
        maxPerType[ApartmentType.penthouse] = 500;
    }

    function mint(address to, ApartmentType apartmentType) public onlyRole(APARTMENT_MINTER_ROLE) returns(uint tokenId) {
        require(totalSupply() < maxSupply, "Max supply reached");
        require(mintedPerType[apartmentType] < maxPerType[apartmentType], "Max per type");
        //todo: uncomment line below so minting only happens on mainnnet (or ETH rinkeby for)
        // require(block.chainid == 1 || block.chainid == 4, "Not ETH chain");
        mintedPerType[apartmentType] += 1;
        countersMapping[apartmentType] += 1;
        tokenId = countersMapping[apartmentType];
        _mint(to, tokenId);
    }

    function burn(uint tokenId) public override onlyRole(APARTMENT_BURNER_ROLE) {
        _burn(tokenId);
    }

    function setURI(string memory uri) public override onlyOwner {
        _URI = uri;
    }

    function _baseURI() internal view override returns(string memory) {
        return _URI;
    }

    function _beforeTokenTransfer(address from, address to, uint tokenId) internal override(ERC721Enumerable) {
        ERC721Enumerable._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public override(AccessControl, ONFT721) view returns(bool) {
        return super.supportsInterface(interfaceId);
    }
}