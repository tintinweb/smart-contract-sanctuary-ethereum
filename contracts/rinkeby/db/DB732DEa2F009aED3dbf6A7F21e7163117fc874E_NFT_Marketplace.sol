//Pragma
pragma solidity ^0.8.4;

//Imports
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./NFT_Token.sol";

//Errors
error NFT_Marketplace_Not_Seller();
error NFT_Marketplace_Not_Owner();
error NFT_Marketplace_Product_Exists();
error NFT_Marketplace_Index_Out_Of_Bounds();
error NFT_Marketplace_Not_Enough_Money();
error NFT_Marketplace_Not_Token_Owner();
error NFT_Marketplace_Sale_Over();
error NFT_Marketplace_Time_Error();
//Global Structs
struct Product {
    uint productId;
    string productName;
    string productDescription;
}
struct Sale {
    uint256 saleId;
    uint256 productId;
    uint32 count;
    uint256 price;
    uint256 startTime;
    uint256 endTime;
    address seller;
    string imgURI;
}
struct Item {
    uint256 itemId;
    NFT_Token nft;
    uint tokenId;
    address seller;
    bool sold;
}

//Contract
contract NFT_Marketplace is ReentrancyGuard {
    //Constants

    //Immutable Variables
    address public immutable i_owner;
    uint32 public immutable i_feeAmount;
    uint256 public immutable i_sellerFee;

    //State variables
    uint256 itemCount;
    Product[] public s_products;
    mapping(string => uint256) public s_productName;
    mapping(address => bool) public s_sellers;
    mapping(address => uint256) public s_accounts;
    //   mapping(address => mapping(uint => uint32)) s_onCount;
    Sale[] public s_sales;
    Item[] public s_items;

    //Events
    event Offers(
        uint256 indexed productId,
        address indexed seller,
        string productName,
        uint32 count,
        uint256 price
    );
    event Bought(
        address indexed nftAddress,
        address indexed seller,
        address indexed buyer,
        uint256 tokenId,
        string productName,
        uint256 productId,
        uint32 count,
        uint256 price
    );
    event Products(uint256 indexed productId);
    event Sales(
        uint256 indexed saleId,
        uint32 count,
        uint256 price,
        uint256 startTime,
        uint256 endTime,
        string imgURI
    );
    //Function Modifiers
    modifier onlySeller() {
        if (!s_sellers[msg.sender]) {
            revert NFT_Marketplace_Not_Seller();
        }
        _;
    }
    modifier onlyOwner() {
        if (msg.sender != i_owner) {
            revert NFT_Marketplace_Not_Owner();
        }
        _;
    }

    //Constructor
    constructor(uint32 _feeAmount, uint256 _sellerFee) {
        i_owner = msg.sender;
        i_feeAmount = _feeAmount;
        i_sellerFee = _sellerFee;
    }

    //Fallback/Receive

    //External visible gfnctions
    function add_Product(
        string memory _productName,
        string memory _productDescription
    ) external onlySeller returns (uint256 idProduct) {
        if (
            s_products.length > 0 &&
            (s_productName[_productName] != 0 ||
                keccak256(abi.encodePacked(s_products[0].productName)) ==
                keccak256(abi.encodePacked(_productName)))
        ) {
            revert NFT_Marketplace_Product_Exists();
        }
        Product memory p = Product(
            s_products.length,
            _productName,
            _productDescription
        );
        s_products.push(p);
        s_productName[_productName] = p.productId;
        idProduct = p.productId;
        emit Products(idProduct);
    }

    function add_Seller() external payable {
        if (msg.value < i_sellerFee) {
            revert NFT_Marketplace_Not_Enough_Money();
        }
        s_sellers[msg.sender] = true;
    }

    function add_Sale(
        uint productId,
        uint32 count,
        uint256 price,
        uint256 startTime,
        uint256 endTime,
        string memory imgURI
    ) external onlySeller {
        if (productId >= s_products.length) {
            revert NFT_Marketplace_Index_Out_Of_Bounds();
        }
        if (startTime >= endTime || endTime < block.timestamp) {
            revert NFT_Marketplace_Time_Error();
        }
        Sale memory sale = Sale(
            s_sales.length,
            productId,
            count,
            price,
            startTime,
            endTime,
            msg.sender,
            imgURI
        );

        s_sales.push(sale);

        emit Sales(productId, count, price, startTime, endTime, imgURI);
    }

    // function dodaj_Na_Stanju(uint256 idProizvoda, uint32 kolicina)
    //     external
    //     onlySeller
    // {
    //  //   s_naStanju[msg.sender][idProizvoda] += kolicina;
    // }

    function make_Item(address _nftAddress, uint _tokenId)
        external
        nonReentrant
    {
        NFT_Token _nft = NFT_Token(_nftAddress);

        if (_nft.ownerOf(_tokenId) != msg.sender) {
            revert NFT_Marketplace_Not_Token_Owner();
        }
        // transfer nft
        _nft.transferFrom(msg.sender, address(this), _tokenId);
        // add new item to items mapping

        uint256 saleId = _nft._getSaleId(_tokenId);
        if (saleId >= s_sales.length) {
            revert NFT_Marketplace_Index_Out_Of_Bounds();
        }
        uint256 productId = s_sales[saleId].productId;
        if (s_sales[saleId].endTime < block.timestamp) {
            revert NFT_Marketplace_Sale_Over();
        }
        s_items.push(Item(itemCount, _nft, _tokenId, msg.sender, false));
        itemCount++;
        // emit Offered event
        emit Offers(
            productId,
            msg.sender,
            s_products[productId].productName,
            s_sales[saleId].count,
            s_sales[saleId].price
        );
    }

    function buy_Item(uint256 itemId) external payable {
        if (itemId >= s_items.length) {
            revert NFT_Marketplace_Index_Out_Of_Bounds();
        }
        Item memory item = s_items[itemId];
        uint256 saleId = s_items[itemId].nft._getSaleId(
            s_items[itemId].tokenId
        );
        if (msg.value != s_sales[saleId].price) {
            revert NFT_Marketplace_Not_Enough_Money();
        }
        if (s_sales[saleId].endTime < block.timestamp) {
            revert NFT_Marketplace_Sale_Over();
        }
        item.nft.transferFrom(address(this), msg.sender, item.tokenId);
        s_accounts[item.nft._getTokenCreator(item.tokenId)] += msg.value;
        uint256 tokenId = item.tokenId;
        uint256 productId = s_sales[saleId].productId;
        emit Bought(
            address(item.nft),
            item.seller,
            msg.sender,
            tokenId,
            s_products[productId].productName,
            productId,
            s_sales[saleId].count,
            s_sales[saleId].price
        );
    }

    //Public visible functions

    //Internal visible functions

    //Private visible functions
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import {Sale} from "./NFT_Marketplace.sol";

error NFT_Token_Invalid_Id();

contract NFT_Token is ERC721URIStorage {
    address public immutable i_contracOwner;
    uint public tokenCount;
    mapping(uint256 => uint256) private s_saleIDs;
    mapping(uint256 => address) private s_tokenCreator;

    event Minted(uint256 indexed tokenId, address indexed creator);

    constructor() ERC721("STORE NFT", "S_NFT") {
        i_contracOwner = msg.sender;
    }

    function mint(string memory tokenURI, uint256 saleId)
        external
        returns (uint)
    {
        tokenCount++;
        _safeMint(msg.sender, tokenCount);
        _setTokenURI(tokenCount, tokenURI);
        _setSaleId(tokenCount, saleId);
        _setTokenCreator(tokenCount, msg.sender);
        emit Minted(tokenCount, msg.sender);
        return (tokenCount);
    }

    function _getSaleId(uint256 tokenId)
        public
        view
        virtual
        returns (uint256 saleId)
    {
        _requireMinted(tokenId);

        saleId = s_saleIDs[tokenId];
    }

    function _setSaleId(uint256 tokenId, uint256 saleId) internal virtual {
        require(
            _exists(tokenId),
            "ERC721URIStorage: URI set of nonexistent token"
        );
        s_saleIDs[tokenId] = saleId;
    }

    function _burn(uint256 tokenId) internal virtual override {
        super._burn(tokenId);
        delete s_saleIDs[tokenId];
    }

    function _setTokenCreator(uint256 tokenId, address _creator)
        internal
        virtual
    {
        require(
            _exists(tokenId),
            "ERC721URIStorage: URI set of nonexistent token"
        );
        s_tokenCreator[tokenId] = _creator;
    }

    function _getTokenCreator(uint256 tokenId)
        external
        virtual
        returns (address)
    {
        _requireMinted(tokenId);
        return s_tokenCreator[tokenId];
    }
}

