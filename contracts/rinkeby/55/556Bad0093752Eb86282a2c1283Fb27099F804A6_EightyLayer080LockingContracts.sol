pragma solidity ^0.8.7;

import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "../signers/Eighty80LockingContractSigner.sol";
import "../utils/NonblockingLzApp.sol";
import "../utils/Ownable.sol";
import "hardhat/console.sol";

contract EightyLayer080LockingContracts is Ownable, Eighty80LockingContractSigner, NonblockingLzApp {

    bytes4 private constant _ERC721_RECEIVED = 0x150b7a02;
    IERC721Upgradeable public nft;
    uint256 public gas ;
    address public mainSigner;
    bool hasStarted;
    mapping (address => bool) public whitelistedProjects;
    mapping (address => mapping (uint16 => bytes)) public destinationRoutes;
    mapping (address => mapping (uint16 => address)) public sourceRoutes;
    mapping (address => mapping (uint => bool)) public nonceChecker;
    event Locked(address indexed user, address[] indexed nftContractAddresses, uint[] indexed tokenList);
    event Withdrawn(address indexed user, address[] indexed nftContractAddresses, uint[] indexed tokenList);


    modifier onlyOnce {
        require (!hasStarted,'Error: Contract Already Initialised');
        _;
    }


    function initialize(address[] memory _whitelistedProjects, address _mainSigner, address endpointAddress, address _admin, string memory domain, string memory version ) external  {
        require (_mainSigner!=address(0),'!Allowed');
        Ownableinitialize();
        __Eighty80LockingContractSigner_init(domain, version);
        __nonblockingLzApp_init(endpointAddress);
        transferOwnership(_admin);
        for (uint i=0;i< _whitelistedProjects.length;i++)
            whitelistedProjects[_whitelistedProjects[i]] = true;
        mainSigner = _mainSigner;
        hasStarted = true;
        gas = 3500000;
    }

    function lockAndMigrateTokens(uint[] memory tokenIds, address _nftAddress, uint16 destinationChainId, bytes memory _userAddress) external payable {
        require (whitelistedProjects[_nftAddress],'Error: Project Not Whitelisted');
        address convertedBytesToAddress;
        assembly {
            convertedBytesToAddress := mload(add(_userAddress,20))
        }
        require ( convertedBytesToAddress == msg.sender,"Error: Bytes doesn't match owner address" );

        for (uint i = 0;i<tokenIds.length;i++) {
            require (IERC721Upgradeable(_nftAddress).ownerOf(tokenIds[i]) == msg.sender,'Not the owner');
            IERC721Upgradeable(_nftAddress).safeTransferFrom(msg.sender, address(this), tokenIds[i]);
        }

        bytes memory payload = abi.encode(_userAddress,tokenIds);
        uint16 version = 1;
        bytes memory adaptorParams = abi.encodePacked(version,gas);
        (uint256 transactionCost, ) = lzEndpoint.estimateFees(destinationChainId, address(this), payload, false, adaptorParams);
        require (msg.value > transactionCost,'Error: Migration Transaction Underpriced');
       _lzSend(destinationChainId,payload,payable(msg.sender),address(0),adaptorParams,destinationRoutes[_nftAddress][destinationChainId]);
    }

    function releaseMigratedTokens(address _toSend, address _nftAddress, uint[] memory tokenIds) internal {

        require(whitelistedProjects[_nftAddress], 'Error: Project Not Whitelisted');
        for (uint i = 0; i < tokenIds.length; i++) {
            IERC721Upgradeable(_nftAddress).transferFrom(address(this), _toSend, tokenIds[i]);
        }
    }

    function _nonblockingLzReceive(
        uint16 sourceChainId,
        bytes memory _sourceAddress,
        uint64,
        bytes memory _payload
    ) internal override virtual{
        address _to;
        address source;
//        uint[] memory tokenIds;
        (bytes memory userAddress, uint[] memory _tokenIds) = abi.decode(_payload,(bytes,uint256[]));
        assembly {
            _to:= mload(add(userAddress,20))
        }
        assembly {
            source := mload(add(_sourceAddress,20))
        }
        releaseMigratedTokens(_to,sourceRoutes[source][sourceChainId],_tokenIds);
    }

    function forceReleaseToken(address _to, address _nftAddress, uint tokenId) external onlyOwner {
        IERC721Upgradeable(_nftAddress).transferFrom(address(this),_to,tokenId);
    }

    //setters

    function addSourceRoutes(address _destinationAddress, uint16 chainId, address sourceAddress) external onlyOwner {
        sourceRoutes[_destinationAddress][chainId] = sourceAddress;
    }

    function setDestinationRoutes (address source, uint16 chainIds, bytes memory destination) external onlyOwner {
        destinationRoutes[source][chainIds] = destination;
    }

    function setWhitelistedProjects (address[] memory partnerProjects) external onlyOwner {
        for (uint i=0;i< partnerProjects.length;i++)
            whitelistedProjects[partnerProjects[i]] = true;
    }

    function blacklistWhitelistedProjects (address[] memory partnerProjects) external onlyOwner {
        for (uint i=0;i< partnerProjects.length;i++)
            whitelistedProjects[partnerProjects[i]] = false;
    }

    function changeGasPrice(uint price) external onlyOwner {
        gas = price;
    }

    function setNftAddress (address _nftAddress) external onlyOwner {
        nft =IERC721Upgradeable(_nftAddress);
    }

    function setMainSigner (address _mainSigner) external onlyOwner {
        mainSigner = _mainSigner;
    }

    function doEncryption(address[] memory _nftContractAddress) external pure returns (bytes memory) {
        return abi.encode(_nftContractAddress);
    }

    // Endpoint.sol estimateFees() returns the fees for the message
    function estimateFees(
        uint16 _dstChainId,
        address _userAddress,
        uint[] memory tokenIds
    ) external view returns (uint256 nativeFee, uint256 zroFee) {
        return
        lzEndpoint.estimateFees(
            _dstChainId,
            address(this),
            abi.encode(_userAddress,tokenIds),
            false,
            abi.encodePacked(uint16(1),uint256(gas*tokenIds.length))
        );
    }

    //@dev Receive the tokens
    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) public pure returns (bytes4) {
        return _ERC721_RECEIVED;
    }
}

