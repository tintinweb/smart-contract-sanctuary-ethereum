pragma solidity ^0.8.7;
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";

///@author Anyx
contract Eighty80LockingContractSigner is EIP712Upgradeable{

    string public SIGNING_DOMAIN; //= "Eighty80Locker";
    string public SIGNATURE_VERSION;// = "1";

    struct Locker{
        address userAddress;
        uint gas;
        uint16 destinationChainId;
        bytes tokenIds;
        bytes nftSignature;
        bytes signature;
    }

    function __Eighty80LockingContractSigner_init(string memory domain, string memory version) internal initializer {
        SIGNING_DOMAIN = domain;
        SIGNATURE_VERSION = version;
        __EIP712_init(domain, version);
    }


    function getDetails() public view returns (string memory, string memory) {
        return (SIGNING_DOMAIN,SIGNATURE_VERSION);
    }

    function getSigner(Locker memory locker) public view returns(address) {
        return _verify(locker);
    }

    /// @notice Returns a hash of the given Locker, prepared using EIP712 typed data hashing rules
    function _hash(Locker memory locker) internal view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(
                keccak256("Locker(address userAddress,uint256 gas,uint16 destinationChainId,bytes tokenIds,bytes nftSignature)"),
                locker.userAddress,
                locker.gas,
                locker.destinationChainId,
                keccak256(locker.tokenIds),
                keccak256(locker.nftSignature)
            )));
    }

    function _verify(Locker memory locker) internal view returns (address) {
        bytes32 digest = _hash(locker);
        return ECDSAUpgradeable.recover(digest, locker.signature);
    }

}

