// Import this file to use console.log
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

contract Identity is Ownable {
    using EnumerableSet for EnumerableSet.Bytes32Set;
    string public blsPublicKey;
    string public publicKey;

    event ChangedBLSPublicKey(string blsPublicKey);
    event ChangedPublicKey(string publicKey);

    struct Credential {
        string encryptedDocument;
        string holderPublicKey;
        uint256 timestamp;
        uint256 latestVerification;
    }
    mapping(uint256 => Credential) public credentials; // [Credential id] => Credential
    event MintCredential(
        uint256 credentialId,
        string encryptedDocument,
        string holderPublicKey,
        uint256 timestamp
    );

    struct Verification {
        uint256 credentialId;
        uint256 previousVerification;
        string encryptedDocument;
        uint256 timestamp;
        uint256 verifierPublicKeyHash;
    }
    mapping(uint256 => Verification) public verifications; // [Verification id] => Verification
    event MintVerification(
        uint256 credentialId,
        uint256 verificationId,
        uint256 previousVerification,
        string encryptedDocument,
        uint256 verifierPublicKeyHash,
        uint256 timestamp
    );

    constructor(string memory blsKey, string memory publicKey_) {
        blsPublicKey = blsKey;
        emit ChangedBLSPublicKey(blsKey);
        publicKey = publicKey_;
        emit ChangedPublicKey(publicKey);
    }

    //***** Owner management *****//

    /// @notice Sets the BLS public key
    /// @dev Only the owner can set the public key
    /// @param blsPublicKey_ The BLS public key to be set

    function setBLSPublicKey(string memory blsPublicKey_) external onlyOwner {
        blsPublicKey = blsPublicKey_;
        emit ChangedBLSPublicKey(blsPublicKey);
    }

    /// @notice Sets the public key
    /// @dev Only the owner can set the public key
    /// @param publicKey_ The public key to be set
    function setPublicKey(string memory publicKey_) external onlyOwner {
        publicKey = publicKey_;
        emit ChangedPublicKey(publicKey);
    }

    //***** PRIVATE FUNCTIONS *****//

    /// @dev Internal function to mint a root
    /// @param credentialId The Credential id
    /// @param encryptedDocument The label specifying the root
    /// @param holderPublicKey The label specifying the root
    /// @param timestamp The label specifying the root
    function mintCredential(
        uint256 credentialId,
        string memory encryptedDocument,
        string memory holderPublicKey,
        uint256 timestamp
    ) public onlyOwner {
        require(
            credentials[credentialId].timestamp == 0,
            "Credential already exists"
        );

        // We won't get here if something is wrong;
        credentials[credentialId].encryptedDocument = encryptedDocument;
        credentials[credentialId].holderPublicKey = holderPublicKey;
        credentials[credentialId].timestamp = timestamp;

        emit MintCredential(
            credentialId,
            encryptedDocument,
            holderPublicKey,
            timestamp
        );
    }

    /// @dev Internal function to mint a root
    /// @param credentialId The Credential id
    /// @param verificationId The Credential id
    /// @param previousVerificationId The Credential id
    /// @param encryptedDocument The label specifying the root
    /// @param timestamp The label specifying the root
    function mintVerification(
        uint256 credentialId,
        uint256 verificationId,
        uint256 previousVerificationId,
        string memory encryptedDocument,
        uint256 timestamp,
        uint256 verifierPublicKeyHash
    ) public onlyOwner {
        require(
            credentials[credentialId].timestamp != 0,
            "Credential already exists"
        );
        require(
            verifications[verificationId].timestamp == 0,
            "Verification already exists"
        );
        uint256 latestVerification = _getCredentialLatestVerification(
            credentialId
        );
        // So we don't have to worry about a pending TX
        require(
            latestVerification == previousVerificationId,
            "Wrong root verification"
        );
        verifications[verificationId].credentialId = credentialId;
        verifications[verificationId].encryptedDocument = encryptedDocument;
        verifications[verificationId].timestamp = timestamp;
        verifications[verificationId].previousVerification = latestVerification;
        verifications[verificationId]
            .verifierPublicKeyHash = verifierPublicKeyHash;
        credentials[credentialId].latestVerification = verificationId;

        emit MintVerification(
            credentialId,
            verificationId,
            latestVerification,
            encryptedDocument,
            verifierPublicKeyHash,
            timestamp
        );
    }

    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns empty string if does or attribute does not exists
    /// @param credentialId Node id from which info will be obtained
    /// @return verificationId obtained
    function _getCredentialLatestVerification(uint256 credentialId)
        internal
        view
        returns (uint256 verificationId)
    {
        verificationId = credentials[credentialId].latestVerification;
    }

    //***** PUBLIC FUNCTIONS *****//
    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns empty string if does or attribute does not exists
    /// @param credentialId Node id from which info will be obtained
    /// @return timestamp obtained
    function getCredentialTimestamp(uint256 credentialId)
        external
        view
        returns (uint256 timestamp)
    {
        timestamp = credentials[credentialId].timestamp;
    }

    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns empty string if does or attribute does not exists
    /// @param credentialId Node id from which info will be obtained
    /// @return encryptedDocument obtained
    function getCredentialEncryptedDocument(uint256 credentialId)
        external
        view
        returns (string memory encryptedDocument)
    {
        encryptedDocument = credentials[credentialId].encryptedDocument;
    }

    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns 0 if does or attribute does not exists
    /// @param credentialId Node id from which info will be obtained
    /// @return verificationId obtained
    function getCredentialLatestVerification(uint256 credentialId)
        external
        view
        returns (uint256 verificationId)
    {
        verificationId = _getCredentialLatestVerification(credentialId);
    }

    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns empty string if does or attribute does not exists
    /// @param credentialId Node id from which info will be obtained
    /// @return holderPublicKey obtained
    function getCredentialHolderPublicKey(uint256 credentialId)
        external
        view
        returns (string memory holderPublicKey)
    {
        holderPublicKey = credentials[credentialId].holderPublicKey;
    }

    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns empty string if does or attribute does not exists
    /// @param verificationId Node id from which info will be obtained
    /// @return credentialId obtained
    function getVerificationCredentialId(uint256 verificationId)
        external
        view
        returns (uint256 credentialId)
    {
        credentialId = verifications[verificationId].credentialId;
    }

    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns empty string if does or attribute does not exists
    /// @param verificationId Node id from which info will be obtained
    /// @return previousVerification obtained
    function getVerificationPreviousVerification(uint256 verificationId)
        external
        view
        returns (uint256 previousVerification)
    {
        previousVerification = verifications[verificationId]
            .previousVerification;
    }

    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns empty string if does or attribute does not exists
    /// @param verificationId Node id from which info will be obtained
    /// @return encryptedDocument obtained
    function getVerificationEncryptedDocument(uint256 verificationId)
        external
        view
        returns (string memory encryptedDocument)
    {
        encryptedDocument = verifications[verificationId].encryptedDocument;
    }

    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns empty string if does or attribute does not exists
    /// @param verificationId Node id from which info will be obtained
    /// @return timestamp obtained
    function getVerificationTimestamp(uint256 verificationId)
        external
        view
        returns (uint256 timestamp)
    {
        timestamp = verifications[verificationId].timestamp;
    }

    /// @notice Gets information stored in an attribute of a given node
    /// @dev Returns empty string if does or attribute does not exists
    /// @param verificationId Node id from which info will be obtained
    /// @return verifierPublicKeyHash obtained
    function getVerificationVerifierPublicKeyHash(uint256 verificationId)
        external
        view
        returns (uint256 verifierPublicKeyHash)
    {
        verifierPublicKeyHash = verifications[verificationId]
            .verifierPublicKeyHash;
    }
}

