pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import "openzeppelin-solidity/contracts/interfaces/IERC1271.sol";
import "openzeppelin-solidity/contracts/interfaces/IERC2981.sol";
import "openzeppelin-solidity/contracts/token/ERC1155/IERC1155.sol";

contract Marketplace1155 is AccessControlUpgradeable, ReentrancyGuardUpgradeable {

  bytes32 private constant SELLABLE = keccak256("SELLABLE");

  bytes4 constant internal ERC1271_MAGICVALUE = bytes4(keccak256("isValidSignature(bytes32,bytes)"));

  bytes32 constant internal EIP712_DOMAIN_TYPEHASH = keccak256(
      "EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)");

  bytes32 constant internal PROPOSAL_TYPEHASH = keccak256(
      "Proposal(address seller,address optionalBuyer,address tokenAddress,uint tokenId,uint price,uint expirationTime)");

  function eip712DomainSeparator() internal view returns (bytes32) {
    return keccak256(abi.encode(
        EIP712_DOMAIN_TYPEHASH,
        keccak256(bytes("MetaMural Marketplace 1155")),
        keccak256(bytes("1")),
        block.chainid,
        address(this)));
  }

  // TODO: Update to allow buyer-initiated proposals.
  struct Proposal {
    address seller;
    address optionalBuyer; // Buyer address, or zero
    address tokenAddress;
    uint tokenId;
    uint price;
    uint expirationTime;
  }

  event ProposalApproved(
      address approver,
      address seller,
      address optionalBuyer,
      address indexed tokenAddress,
      uint indexed tokenId,
      uint price,
      uint indexed expirationTime);
  event ProposalRevoked(
      address revoker,
      address seller,
      address optionalBuyer,
      address indexed tokenAddress,
      uint indexed tokenId,
      uint price,
      uint indexed expirationTime);

  function proposalHash(Proposal memory proposal) public pure returns (bytes32) {
    return keccak256(abi.encode(
        PROPOSAL_TYPEHASH,
        proposal.seller,
        proposal.optionalBuyer,
        proposal.tokenAddress,
        proposal.tokenId,
        proposal.price,
        proposal.expirationTime));
  }

  function checkProposal(Proposal memory proposal) internal view {
    require(hasRole(SELLABLE, proposal.tokenAddress),
        "checkProposal: Token cannot be sold on this marketplace");
    require(proposal.seller != proposal.optionalBuyer,
        "checkProposal: seller and buyer may not be the same");
    require(AddressUpgradeable.isContract(proposal.tokenAddress),
        "checkProposal: tokenAddress must refer to a contract");
    require(proposal.tokenId != 0, "checkProposal: tokenId must be nonzero");
    require(proposal.price != 0, "checkProposal: price must be nonzero");
    require(proposal.expirationTime > block.timestamp,
        "checkProposal: expirationTime must be in the future ");
    IERC1155 token = IERC1155(proposal.tokenAddress);
    require(token.balanceOf(proposal.seller, proposal.tokenId) > 0,
        "checkProposal: seller does not own the referenced token");
    require(token.isApprovedForAll(proposal.seller, address(this)),
        "checkProposal: seller must grant approval to the marketplace contract");
  }

  // approverAddress => proposalHash => isApproved
  mapping(address => mapping(bytes32 => bool)) approvals;
  // approverAddress => proposalHash => isCompleted
  mapping(address => mapping(bytes32 => bool)) completed;

  function initialize() public initializer {
    ReentrancyGuardUpgradeable.__ReentrancyGuard_init();

    _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
  }

  function isApproved(Proposal memory proposal, address approver) public view returns (bool) {
    return isApprovedHash(proposalHash(proposal), approver);
  }

  function isApprovedHash(bytes32 hash, address approver) public view returns (bool) {
    return approvals[approver][hash] && !completed[approver][hash];
  }

  // TODO: Update to offer EIP-1271 contract signing
  function approveProposal(
      Proposal memory proposal,
      address approver,
      bytes calldata signature) external {
    checkProposal(proposal);
    bytes32 hash = proposalHash(proposal);
    require(approver == proposal.seller || approver == proposal.optionalBuyer,
        "approveProposal: Approver is not a party to this Proposal");

    if (approver == _msgSender()) {
      // Approval by sender
      approvals[_msgSender()][hash] = true;
    } else if (AddressUpgradeable.isContract(approver)) {
      // Approval by ERC-1271 contract
      require(IERC1271(approver).isValidSignature(
          ECDSAUpgradeable.toTypedDataHash(eip712DomainSeparator(), hash),
          signature) == ERC1271_MAGICVALUE,
          "approveProposal: Approver signature is not valid");
    } else {
      // Approval by ECDSA signature
      require(
          ECDSAUpgradeable.recover(
                  ECDSAUpgradeable.toTypedDataHash(eip712DomainSeparator(), hash), signature)
              == approver,
          "approveProposal: Approver signature is not valid");
      approvals[approver][hash] = true;
    }

    emit ProposalApproved(
        approver,
        proposal.seller,
        proposal.optionalBuyer,
        proposal.tokenAddress,
        proposal.tokenId,
        proposal.price,
        proposal.expirationTime);
  }

  function revokeProposal(Proposal memory proposal) external {
    checkProposal(proposal);
    bytes32 hash = proposalHash(proposal);

    // Revoked by sender
    require(_msgSender() == proposal.seller || _msgSender() == proposal.optionalBuyer,
        "revokeProposal: Sender is not a party to this Proposal");
    completed[_msgSender()][hash] = true;
    emit ProposalRevoked(
        _msgSender(),
        proposal.seller,
        proposal.optionalBuyer,
        proposal.tokenAddress,
        proposal.tokenId,
        proposal.price,
        proposal.expirationTime);
  }

  // Must be done by the buyer, so that they can send the ETH necessary to complete  the transaction
  function acceptProposal(Proposal memory proposal) external virtual payable nonReentrant {
    checkProposal(proposal);
    bytes32 hash = proposalHash(proposal);

    // Require approval by seller
    require(isApprovedHash(hash, proposal.seller) == true,
        "acceptProposal: Seller has not approved (or has revoked) the proposal");
    // Require acceptance by buyer, if specified in the proposal
    require(proposal.optionalBuyer == address(0) || _msgSender() == proposal.optionalBuyer,
        "acceptProposal: Sender cannot accept this proposal");

    // ETH from the transaction is accepted as the payment price, and credited to the seller
    require(proposal.price == msg.value, "acceptProposal: Incorrect value supplied");
    (address royaltyAddress, uint royaltyAmount) =
        IERC2981(proposal.tokenAddress).royaltyInfo(proposal.tokenId, proposal.price);
    require(royaltyAmount < proposal.price, "acceptProposal: Royalty amount is too high");
    // Royalties are paid
    payable(royaltyAddress).transfer(royaltyAmount);
    // Seller gets paid
    payable(proposal.seller).transfer(proposal.price - royaltyAmount);
    // The NFT is transferred from the seller to the buyer
    IERC1155(proposal.tokenAddress)
        .safeTransferFrom(proposal.seller, _msgSender(), proposal.tokenId, 1, bytes(""));
    completed[proposal.seller][hash] = true;
  }
}

