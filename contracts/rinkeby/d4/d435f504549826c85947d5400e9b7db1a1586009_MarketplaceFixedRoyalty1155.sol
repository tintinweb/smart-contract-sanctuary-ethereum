pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "openzeppelin-solidity/contracts/token/ERC1155/IERC1155.sol";
import "@prb/math/contracts/PRBMathUD60x18.sol";

import "./Marketplace1155.sol";

contract MarketplaceFixedRoyalty1155 is Marketplace1155 {

  address royaltyAddress;
  uint royaltyFee;

  function setRoyaltyParams(address royaltyAddress_, uint royaltyFee_) external onlyRole(DEFAULT_ADMIN_ROLE) {
    royaltyAddress = royaltyAddress_;
    royaltyFee = royaltyFee_;
  }

  // Must be done by the buyer, so that they can send the ETH necessary to complete  the transaction
  function acceptProposal(Proposal memory proposal) external override payable nonReentrant {
    checkProposal(proposal);
    bytes32 hash = proposalHash(proposal);

    require(royaltyAddress != address(0),
        "acceptProposal: Royalties are not configured.");

    // Require approval by seller
    require(isApprovedHash(hash, proposal.seller) == true,
        "acceptProposal: Seller has not approved (or has revoked) the proposal");
    // Require acceptance by buyer, if specified in the proposal
    require(proposal.optionalBuyer == address(0) || _msgSender() == proposal.optionalBuyer,
        "acceptProposal: Sender cannot accept this proposal");

    // ETH from the transaction is accepted as the payment price, and credited to the seller
    require(proposal.price == msg.value, "acceptProposal: Incorrect value supplied");
    uint royaltyAmount = PRBMathUD60x18.mul(proposal.price, royaltyFee);
    require(royaltyAmount < proposal.price, "acceptProposal: Royalty amount is too high");
    // Royalties are paid
    payable(royaltyAddress).transfer(royaltyAmount);
    // Seller gets paid
    payable(proposal.seller).transfer(proposal.price - royaltyAmount);
    // The NFT is transferred from the seller to the buyer
    IERC1155(proposal.tokenAddress)
        .safeTransferFrom(proposal.seller, _msgSender(), proposal.tokenId, 1, bytes(""));
    completed[proposal.seller][hash] = true;
  }
}

