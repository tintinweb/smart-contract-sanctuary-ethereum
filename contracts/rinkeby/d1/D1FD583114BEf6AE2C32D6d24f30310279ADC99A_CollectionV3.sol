pragma solidity 0.8.12;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "../lib/AdressSet.sol";

contract CollectionV3 is ERC1155Upgradeable {
    using EnumerableSet for EnumerableSet.UintSet;
    using AddressSet for Set;

/*================================ Events ========================================*/

    event Minted(
        address indexed operator,
        address indexed to,
        uint256 indexed id,
        uint256 amount,
        uint256 time
    );
    
    event Sold(
        address indexed operator,
        address indexed to,
        uint256 indexed id,
        uint256 amount,
        uint256 time
    );

    event SetWhitelisted(address whiteListedUSer);
    event SetBatchWhitelisted(address[] whiteListedUsers);
    event StartTimeChanged(uint256 startTime);
    event EndTimeChanged(uint256 endTime);
    event WhitelistedStartTimeChanged(uint256 whiteListedTime);

/*================================ State Variables ========================================*/

    EnumerableSet.UintSet soldCards;

    bytes32 public constant MINTER_ROLE = bytes32(keccak256("MINTER_ROLE"));

    uint256 public amount; // Should be set as Dollar 10**8
    uint256 public percent;
    uint256 public available;
    uint256 public sold;
    uint256 public total;
    uint256 public whitelistedStartTime;
    uint256 public startTime;
    uint256 public endTime;
    uint256 public shareAmount;
    uint256 private _totalReleased;

    address public facAddress;
    address public ernTreasure;
    address public admin;

    bool public isOnlyWhitelisted;

    mapping(address => bool) private _whiteListed;

/*================================ Modifiers ========================================*/
    modifier onlyAdmin() {
        require(msg.sender == admin, "Not Authorized Access!");
        _;
    }

    modifier onlyFactory() {
        require(
            msg.sender == facAddress,
            "This function can only be called by factory contract"
        );
        _;
    }

/*================================ Constructor(initialize) ========================================*/
    function initialize(
        string memory uri,
        uint256 _total,
        uint256 _whitelistedStartTime,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _amount,
        uint256 _percent,
        address _admin,
        address _facAddress
    ) public virtual initializer {
        __CollectionV3_init_unchained(
            uri,
            _total,
            _whitelistedStartTime,
            _startTime,
            _endTime,
            _amount,
            _percent,
            _admin,
            _facAddress
        );
    }

    function __CollectionV3_init_unchained(
        string memory uri,
        uint256 _total,
        uint256 _whitelistedStartTime,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _amount,
        uint256 _percent,
        address _admin,
        address _facAddress
    ) internal onlyInitializing {
        __ERC1155_init(uri);
        amount = _amount;
        available = _total;
        whitelistedStartTime = _whitelistedStartTime;
        total = _total;
        startTime = _startTime;
        endTime = _endTime;
        percent = _percent;
        facAddress = _facAddress;
        admin = _admin;
    }

/*================================ Functions ========================================*/

/*================================ External,Public Functions ========================================*/

    function addExternalAddresses(address _treasure) external onlyAdmin {
        ernTreasure = _treasure;
    }

    function recoverToken(address _token) external onlyAdmin {
        uint256 _amount = ERC20(_token).balanceOf(address(this));
        ERC20(_token).transfer(msg.sender, _amount);
    }

    function changeOnlyWhitelisted(bool _status) external onlyAdmin {
        isOnlyWhitelisted = _status;
    }

    function buy(address buyer, uint256 _id) external onlyFactory {
        require(!(soldCards.contains(_id)), "This card already sold");
        require(available > 0, "Sold Out");
        uint256 time = block.timestamp;

        if (isOnlyWhitelisted == true) {
            require(
                _whiteListed[buyer] &&
                    time >= whitelistedStartTime &&
                    endTime > time,
                "Only for WL users and in the sale time"
            );
        } else {
            if (!_whiteListed[buyer]) {
                require(
                    startTime <= time && endTime > time,
                    "Only in the sale time"
                );
            } else {
                require(
                    time >= whitelistedStartTime &&
                        endTime > time,
                    "Only in the sale time even for WL users!"
                );
            }
        }

        _mint(buyer, _id, 1, "");

        available -= 1;
        sold += 1;
        soldCards.add(_id);

        emit Sold(address(this), buyer, _id, amount,time);
    }

    function mint(address to, uint256 _id) external onlyAdmin {
        require(!(soldCards.contains(_id)), "This card already sold");
        require(available > 0, "Sold Out");

        _mint(to, _id, 1, "");

        available -= 1;
        sold += 1;
        soldCards.add(_id);

        emit Minted(address(this), to, _id, amount,block.timestamp);
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amount_
    ) external onlyAdmin {
        require(available > ids.length, "Sold Out");

        for (uint256 i = 0; i < ids.length; i++) {
            require(!(soldCards.contains(ids[i])), "This card already sold");
            soldCards.add(ids[i]);
        }

        _mintBatch(to, ids, amount_, "");

        available -= ids.length;
        sold += ids.length;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC1155Upgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function setStarTime(uint256 _starTime) external onlyAdmin {
        startTime = _starTime;
        emit StartTimeChanged(_starTime);
    }

    function setEndTime(uint256 _endTime) external onlyAdmin {
        endTime = _endTime;
        emit EndTimeChanged(_endTime);
    }

    function setWhiteListUser(address _addr) external onlyAdmin {
        _whiteListed[_addr] = true;
        emit SetWhitelisted(_addr);
    }

    function setBatchWhiteListUser(address[] calldata _addr)
        external
        onlyAdmin
    {
        for (uint16 i = 0; i < _addr.length; i++) {
            _whiteListed[_addr[i]] = true;
        }

        emit SetBatchWhitelisted(_addr);
    }

    function setAmount(uint256 _amount) external onlyAdmin {
        amount = _amount;
    }

    function setWlStartTime(uint256 _wlStartTime) external onlyAdmin {
        whitelistedStartTime = _wlStartTime;
        emit WhitelistedStartTimeChanged(_wlStartTime);
    }

/*================================ View Functions ========================================*/

    function isWhitelisted(address _user) external view returns (bool) {
        return _whiteListed[_user];
    }
}

