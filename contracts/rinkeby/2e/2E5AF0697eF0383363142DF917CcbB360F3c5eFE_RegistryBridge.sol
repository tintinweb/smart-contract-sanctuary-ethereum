pragma solidity ^0.8.4;
import "./NFTCarbonMarket.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import {CreditRequest, CreditStatus} from "./utils/structs.sol";

contract RegistryBridge is AccessControl {
  event CreditsRequested(
    address owner,
    uint256 indexed tokenId,
    uint256 amount,
    bytes32 indexed cid,
    bytes32 indexed serialNumber
  );
  event CreditsVerified(
    address verifier,
    address owner,
    uint256 indexed tokenId,
    uint256 amount,
    bytes32 indexed cid,
    bytes32 indexed serialNumber
  );
  event CreditsRejected(address verifier, address indexed user, bytes32 indexed serialNumber);
  event NftMarketUpdated(address indexed _oldNftMarket, address _newNftMarket);

  bytes32 public constant VERIFIER_ROLE = keccak256("VERIFIER_ROLE");
  bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

  mapping(string => CreditRequest) public creditRequests;
  address public nftMarket;

  constructor() {
    _setRoleAdmin(VERIFIER_ROLE, ADMIN_ROLE);
    _grantRole(ADMIN_ROLE, msg.sender);
  }

  /**
   * @notice Allows real world carbon credit holders to request the on-chain minting of carbon credits
   * @param _serialNumber The serial number associated with the carbon credits
   *
   * Emits a CreditsRequested event
   *
   * Requirements
   * - A credit request must not already be in progress
   */
  function requestCredits(
    uint256 _tokenId,
    uint256 _amount,
    string memory _serialNumber,
    string memory _cid
  ) public {
    require(
      creditRequests[_serialNumber].status == CreditStatus.Rejected,
      "RegistryBridge: Credits for this serial number are already used or in progress"
    );
    require(_amount > 0, "RegistryBridge: Amount requested must be greater than zero");
    creditRequests[_serialNumber] = CreditRequest(msg.sender, _tokenId, _amount, _cid, CreditStatus.Pending);
    emit CreditsRequested(msg.sender, _tokenId, _amount, stringToBytes32(_cid), stringToBytes32(_serialNumber));
  }

  function verifyCredits(string memory _serialNumber) public onlyRole(VERIFIER_ROLE) {
    CreditRequest memory request = creditRequests[_serialNumber];
    require(request.status == CreditStatus.Pending, "RegistryBridge: Credit request is not pending");
    require(address(nftMarket) != address(0), "RegistryBridge: NFTCarbonMarket not set");

    if (bytes(request.cid).length > 0) {
      NFTCarbonMarket(nftMarket).setTokenURI(request.tokenId, request.cid);
    }
    creditRequests[_serialNumber].status = CreditStatus.Confirmed;
    NFTCarbonMarket(nftMarket).mintToken(request.owner, request.tokenId, request.amount, "");

    emit CreditsVerified(
      msg.sender,
      request.owner,
      request.tokenId,
      request.amount,
      stringToBytes32(request.cid),
      stringToBytes32(_serialNumber)
    );
  }

  function rejectCredits(string memory _serialNumber) public onlyRole(VERIFIER_ROLE) {
    CreditRequest memory request = creditRequests[_serialNumber];
    require(request.status == CreditStatus.Pending, "RegistryBridge: Credit request is not pending");

    delete creditRequests[_serialNumber];
    emit CreditsRejected(msg.sender, request.owner, stringToBytes32(_serialNumber));
  }

  /**
   * @notice Sets the NFTCarbonMarket collection to control
   * @param _nftMarket The NFTCarbonMarket collection
   *
   * Emits a NftMarketUpdated event
   *
   * Requirements
   * - The address cannot be zero
   */
  function setNFTMarket(address _nftMarket) external onlyRole(ADMIN_ROLE) {
    require(_nftMarket != address(0), "RegistryBridge: NFTCarbonMarket address cannot be zero");
    emit NftMarketUpdated(nftMarket, _nftMarket);
    nftMarket = _nftMarket;
  }

  function stringToBytes32(string memory source) public pure returns (bytes32 result) {
    bytes memory tempEmptyStringTest = bytes(source);
    if (tempEmptyStringTest.length == 0) {
      return 0x0;
    }

    assembly {
      result := mload(add(source, 32))
    }
  }
}

