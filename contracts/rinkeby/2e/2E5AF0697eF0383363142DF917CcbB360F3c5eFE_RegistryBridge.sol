// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;
import "@openzeppelin/contracts/access/AccessControl.sol";
import "hardhat/console.sol";


import { ProjectStatus, 
         ProjectMetadata,
         ProjectType,
         VintageMetadata
         } from "./utils/structs.sol";


/**
 * @notice This contract keeps track of all states of projects and their associated vintages
 *
 */

contract ProjectVintageController is AccessControl {

    error InvalidProjectType();

    event NewIoTProject(uint256 indexed _projectId, address indexed _projectOwner, ProjectMetadata _projectData);
    event NewRegistryProject(uint256 indexed _projectId, address indexed _projectOwner, ProjectMetadata _projectData);



    event ProjectStatusUpdated(uint256 indexed _projectId, ProjectStatus indexed oldStatus, ProjectStatus indexed _newStatus );
 
    event ProjectOwnerUpdated(uint256 indexed _projectId, address _oldOwner, address _newOwner);

    //moved here
    event ProjectProposed(uint256 indexed _projectId, address _proposedBy);
    event ProjectEndorsed(uint256 indexed _projectId, address _endorsedBy);
    event ProjectDeleted(uint256 indexed _projectId, address _deletedBy);



    event VintageProposed(uint256 indexed _projectId, uint256 _vintageId, address _proposedBy);
    event VintageEndorsed(uint256 indexed _projectId, uint256 _vintageId, address _endorsedBy);


    bytes32 public constant MARKET_AUTHORITY = keccak256("MARKET_AUTHORITY");
    bytes32 public constant PROJECT_OWNER = keccak256("PROJECT_OWNER");


    // from projectId => metadata
    mapping(uint256 => ProjectMetadata) public projects;
    // from tokenId => metadata
    mapping(uint256 => VintageMetadata) public vintages;


    //temprary UI mappings
    mapping(address => uint256[]) public projectsOwned;
    mapping(uint256 => uint256[]) public projectVintages; // this can be moved to project metadata
    mapping(uint256 => string) public projectStaticData; //for opensea display
    
    uint256[] private projectIds; 

    constructor(){

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MARKET_AUTHORITY, msg.sender);
        // set MARKET_AUTHORITY to be the adminRole of PROJECT_OWNER
        _setRoleAdmin(PROJECT_OWNER, MARKET_AUTHORITY);

    }
    


    // -- project Level---


    /**
    * @notice Allows a user to create a new project
    * @dev A project is the combination of a project and vintage
    * @param _newProject The data that defines the project and vintage
    *
    * Emits a ProjectProposed event
    *
    * Requirements
    * - Only the PROJECT_OWNER can execute
    * - The projectId must not already exist
    * - The projectId must not be 0
    * - The project status must be pending
    * - The projectId must be set to taken
    * - Max quantity must be greater than 0
    */
    function proposeProject(
        uint256 _projectId, 
        ProjectMetadata memory _newProject,
        string memory _projectStaticData
    ) external onlyRole(PROJECT_OWNER){

        ProjectType _type = _newProject.projectType;
        // validate input data
        require(_newProject.status == ProjectStatus.Pending, "ProjectVintageController: Project status must be set to pending");

        require(_projectId != 0,  "ProjectVintageController: Project id cannot be zero");

        require(projects[_projectId].isIdTaken == false, 
        "ProjectVintageController: Project id already exists");


        if (_type == ProjectType.IOT){
             // basic validation for IOT project
            emit NewIoTProject(_projectId, msg.sender, _newProject);
            } else if (_type == ProjectType.REGISTRY){
             // basic validation for registry project
            emit NewRegistryProject(_projectId, msg.sender, _newProject);

            } else {
                revert InvalidProjectType();
            }

        
        // update state
        projects[_projectId] = _newProject;
        projects[_projectId].projectOwner = msg.sender;
        projects[_projectId].isIdTaken = true;

        projectStaticData[_projectId] = _projectStaticData;
        // Add this to the list of projects I own
        projectsOwned[msg.sender].push(_projectId);
        projectIds.push(_projectId);

    }


    /**
    * @notice Allows a manager to endorse a project
    * @param _projectId The project to endorse
    *
    * Requirements
    * - Only the MARKET_AUTHORITY can execute
    * - The project must be pending endorsement
    */
    function endorseProject(uint256 _projectId) external onlyRole(MARKET_AUTHORITY) {
        ProjectStatus status = projects[_projectId].status;
        require(status == ProjectStatus.Pending, "ProjectVintageController: This project is not pending");

        projects[_projectId].endorser = msg.sender;

        //@audit - two events are emitted with this update
        emit ProjectEndorsed(_projectId, msg.sender);
        _updateProjectStatus(_projectId, status, ProjectStatus.Confirmed);
    }



    /**
    * @notice Allows a manager to reject a project
    * @param _projectId The project to reject
    *
    * Requirements
    * - Only the MARKET_AUTHORITY can execute
    * - The project must not be rejected
    */
    function rejectProject(uint256 _projectId) external onlyRole(MARKET_AUTHORITY) {
      ProjectStatus status = projects[_projectId].status;
      require(status != ProjectStatus.Rejected, "ProjectVintageController: This project is already rejected");
      _updateProjectStatus(_projectId, status, ProjectStatus.Rejected);
    }



    /**
    * @notice Allows a manager to delete a project
    * @param _projectId The project to delete
    *
    * Requirements
    * - Only the MARKET_AUTHORITY can execute
    * - The project must rejected to be deleted
    */
    function deleteProject(uint256 _projectId) external onlyRole(MARKET_AUTHORITY) {
      ProjectStatus status = projects[_projectId].status;
      require(status == ProjectStatus.Rejected, "ProjectVintageController: The project must rejected to be deleted");
      _updateProjectStatus(_projectId, status, ProjectStatus.Deleted);
      //@audit - swap and pop logic for the arrays
      emit ProjectDeleted(_projectId, msg.sender);
    }


    /**
    * @notice Updates the project owner
    * @param _projectId The project whose owner is being updated
    * @param _owner The address of the new owner
    *
    * Emits a ProjectOwnerUpdated event
    *
    * Requirements
    * - A valid project id
    * - Owner address cannot be zero
    */
    function updateProjectOwner(uint256 _projectId, address _owner)
    external
    onlyRole(MARKET_AUTHORITY)
    projectIsValid(_projectId)
    {
    require(_owner != address(0), "ProjectVintageController: Owner address cannot be zero");

    emit ProjectOwnerUpdated(_projectId, projects[_projectId].projectOwner, _owner);
    projects[_projectId].projectOwner = _owner;
    }




    // ---- Vintage Level states----

    // vintageKey is the tokenId for the market
    //@audit need to make sure this is one-to-one map with no duplicates
    function computerTokenId(uint256 _projectId, uint256 _vintageId) public pure returns (uint256) {
      return (_projectId * 1000000) + _vintageId;
    }



    function proposeVintage(uint256 _projectId, VintageMetadata memory vintageMetadata) external onlyRole(PROJECT_OWNER) {
      require(projects[_projectId].isIdTaken == true, "ProjectVintageController: ProjectId could not be found");
      require(vintageMetadata.isApproved == false, "ProjectVintageController: Vintage not approved");
      require(
        vintageMetadata.vintageId > 0 && vintageMetadata.vintageId < 1000,
        "ProjectVintageController: VintageId must be between 1 and 999"
      );

      uint256 tokenId = this.computerTokenId(_projectId, vintageMetadata.vintageId);
      vintages[tokenId] = vintageMetadata;

      // Track the vitnages belonging to a project
      projectVintages[_projectId].push(vintageMetadata.vintageId);
      // Emit some kind of event
      emit VintageProposed(_projectId, vintageMetadata.vintageId, msg.sender);
    }



    // this is only needed for IoT project but not registry project
    function endorseVintage(uint256 _projectId, uint256 _vintageId) external onlyRole(MARKET_AUTHORITY) {
      require(projects[_projectId].isIdTaken == true, "ProjectVintageController: ProjectId could not be found");

      uint256 tokenId = this.computerTokenId(_projectId, _vintageId);
      vintages[tokenId].isApproved = true;
      vintages[tokenId].approvedBy = msg.sender;
      // Set the current vintage, so that we can start minting credits
      projects[_projectId].currentVintageId = _vintageId;

      // Emit some kind of event
      emit VintageEndorsed(_projectId, _vintageId, msg.sender);
    }




    /**
    * @notice Gets the owner of a project
    * @param _projectId The project whose owner is being requested
    *
    * Requirements
    * - A valid project id
    */
    function getProjectVintages(uint256 _projectId) public view projectIsValid(_projectId) returns (uint256[] memory) {
      return projectVintages[_projectId];
    }

    /**
    * @notice Gets the metadata of a project
    * @param _projectId The project whose owner is being requested
    *
    * Requirements
    * - A valid project id
    */
    function getProjectMetadata(uint256 _projectId)
      public
      view
      projectIsValid(_projectId)
      returns (ProjectMetadata memory)
    {
      return projects[_projectId];
    }

    function getVintageMetadata(uint256 _projectId, uint256 _vintageId) public view returns (VintageMetadata memory) {
      require(projects[_projectId].isIdTaken == true, "ProjectVintageController: ProjectId could not be found");

      uint256 tokenId = this.computerTokenId(_projectId, _vintageId);
      VintageMetadata memory found = vintages[tokenId];

      require(found.vintageId != 0, "Unable to find vintage");
      return found;
    }



    // --- public view functions ---- 


    /**
    * @notice Gets all the projectIds that have been proposed or endorsed
    */
    function getAllProjectIds() public view returns (uint256[] memory) {
        //@audit - unbounded array could cause issues
        return projectIds;
    }

    /**
    * @notice Gets all the projectIds that I have proposed
    */
    function getMyProjectIds() public view returns (uint256[] memory) {
        return projectsOwned[msg.sender];
    }

    /**
    * @notice Gets the status of a project
    * @param _projectId The project whose status is being requested
    *
    * Requirements
    * - A valid project id
    */
    function getProjectStatus(uint256 _projectId) public view projectIsValid(_projectId) returns (ProjectStatus) {
        return projects[_projectId].status;
    }

    /**
    * @notice Gets the owner of a project
    * @param _projectId The project whose owner is being requested
    *
    * Requirements
    * - A valid project id
    */
    function getProjectOwner(uint256 _projectId) public view projectIsValid(_projectId) returns (address) {
        return projects[_projectId].projectOwner;
    }


    function getProjectEndorser(uint256 _projectId) external view projectIsValid(_projectId) returns (address) {
        return projects[_projectId].endorser;
    }

    /**
    * @notice Returns the static data for the NFTCarbonMarket
    * @param id The project whose owner is being updated
    */
    function getStaticData(uint256 id) public view returns (string memory) {
      return projectStaticData[id];
    }


    // --- internal helper functions ---- 

    function _updateProjectStatus(uint256 _projectId, ProjectStatus _oldStatus, ProjectStatus _newStatus) internal {

        require(_oldStatus != _newStatus);
        projects[_projectId].status = _newStatus;
        emit ProjectStatusUpdated(_projectId, _oldStatus, _newStatus);
    }





    // -- contract Level configuration---



    modifier projectIsValid(uint256 _projectId) {
        require(projects[_projectId].isIdTaken, "ProjectVintageController: ProjectId already exists");
        _;
    }

}

