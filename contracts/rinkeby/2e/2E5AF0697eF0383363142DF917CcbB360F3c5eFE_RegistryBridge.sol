import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "./ProjectVintageController.sol";
import "./interfaces/INFTCarbonMarket.sol";

/**
 * @notice GLOBAL ANZ Market Place contract
 */
contract NFTCarbonMarket is ERC1155, AccessControl, Pausable, ERC1155Burnable, ERC1155Supply, ReentrancyGuard {
  // Notes: Need to add ownable or it wont show on OpenSea

  //roles
  bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
  bytes32 public constant BRIDGE_ROLE = keccak256("BRIDGE_ROLE");

  //state variables
  mapping(address => bool) private bridges;
  mapping(uint256 => string) private _tokenURIs;

  string public name;
  string public symbol;
  string public baseUri;
  address public owner;

  constructor() ERC1155("") {
    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _grantRole(ADMIN_ROLE, msg.sender);
    _setRoleAdmin(BRIDGE_ROLE, ADMIN_ROLE);

    owner = msg.sender;
    name = "Carbon Market";
    symbol = "CMARKET";
    baseUri = "https://ipfs.io/ipfs/";
  }

  /**
   * @notice Mint a specific quanity to a specific tokenId
   */
  function mintToken(
    address to,
    uint256 id,
    uint256 amount,
    bytes memory data
  ) public onlyRole(BRIDGE_ROLE) nonReentrant returns (bool) {
    //@audit - this potentially allow any RIDGE_ROLE to mint, instead of minting only through bridge contract.
    _mint(to, id, amount, data);
    return true;
  }

  function supportsInterface(bytes4 interfaceId) public view override(ERC1155, AccessControl) returns (bool) {
    return super.supportsInterface(interfaceId);
  }

  function pause() public onlyRole(ADMIN_ROLE) {
    _pause();
  }

  function unpause() public onlyRole(ADMIN_ROLE) {
    _unpause();
  }

  function uri(uint256 _tokenId) public view override returns (string memory) {
    string memory _tokenURI = _tokenURIs[_tokenId];
    return string(abi.encodePacked(baseUri, _tokenURI));
  }

  function setBaseURI(string memory _newBaseURI) public onlyRole(ADMIN_ROLE) {
    baseUri = _newBaseURI;
  }

  function setTokenURI(uint256 tokenId, string memory _tokenURI) public onlyRole(BRIDGE_ROLE) {
    _tokenURIs[tokenId] = _tokenURI;
  }

  function _beforeTokenTransfer(
    address operator,
    address from,
    address to,
    uint256[] memory ids,
    uint256[] memory amounts,
    bytes memory data
  ) internal override(ERC1155, ERC1155Supply) whenNotPaused {
    super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
  }
}

