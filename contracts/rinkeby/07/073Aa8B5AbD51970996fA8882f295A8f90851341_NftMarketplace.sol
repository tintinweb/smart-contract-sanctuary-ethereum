// it inherited just from ERC721URIStorage before...
contract TattooCollectionContract is Initializable, ERC721URIStorageUpgradeable{

    // counters allow us to keep track of tokenIds
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    
    address public owner;
    address public marketContractAddress = 0x40c5E71290257d3bC08D08202bb46343Eb78AAd2;              // adjust based on the actual address
    address public factoryContract = 0x63cdddf9d5DfD27165Ca0d4508C077450Ca63414;                    // adjust based on the actual address

    mapping(uint256 => Tattoo) public TattooCollection;
    mapping(uint256 => bool) Lock;

    struct Tattoo{
        uint256 inked;
        uint256 inkDate;
        bool isListedByArtist;

        //string title;           // this will be removed
        //string country;         // this will be removed
        //string city;            // this will be removed
        //string style;           // this will be removed
        //string description;     // this will be removed
        //uint256 artistRoyalty;        //this royalty is for our marketplace and in percentage
    }
    
    modifier OnlyMarket(){
        require(msg.sender == marketContractAddress, "caller is not marketplace contract");
        _;
    }

    modifier OnlyOwner(){
        require(msg.sender == owner, "caller is not owner");
        _;
    }

    modifier OnlyOwnerOrMarketOrFactory(){
        require(msg.sender == marketContractAddress || msg.sender == owner || msg.sender == factoryContract, "caller not marketplace or owner");
        _;
    }
    
    function initialize(string calldata name, string calldata symbol, address _owner) public initializer{
      
        //require(msg.sender == factoryContract, "Only the factory contract can call this function!!");
        // __ERC721_init_unchained(name,symbol);
        __ERC721_init(name,symbol);

        owner = _owner;                                
    }



    

    function setLocked(uint256 tokenId, bool lock) external OnlyMarket {
        Lock[tokenId] = lock;
    }

    // overriding the OZ's `_beforeTokenTransfer` to check if the item is locked (listed or on auction) - prevent transfer in that case
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override {
        require(!Lock[tokenId], "Cannot transfer - token currently locked");
    }

    // overriding the OZ's `setApprovalForAll`, so that we can forward the owner's approval from the clone factory
    // NOTE: making this `OnlyMarket` would make the marketplace the only place eligible to trade this collection (because user wouldn't be able to give permission to other marketplaces)
    function setApprovalForAll(address operator, bool approved) public override OnlyOwnerOrMarketOrFactory {
        _setApprovalForAll(owner, operator, approved);
    }






    function setListedStatus(uint256 tokenId, bool newStat) external OnlyMarket {
        TattooCollection[tokenId].isListedByArtist = newStat;
    }

    //custom total supply to save gas    
    function totalSupply() public view returns(uint256){
        return _tokenIds.current();
    }

    //the function to burn the token
    function burnToken(uint256 tokenId) external OnlyOwner {
        _burn(tokenId);
        delete TattooCollection[tokenId];
        _tokenIds.decrement();
    }

    // the function to destroy the contract
    function killTheContract() external OnlyOwner {
        require(totalSupply() == 0, "Supply must be 0");
        selfdestruct(payable(msg.sender));
    }
   
    
    /*
        OBJ: give the NFT market the ability to transact with tokens or change ownership
        SetApprovalForAll allows us to do that with the contract address

        constructor to set up our address

        constructor(address marketplaceAddress){
        
            marketContractAddress = marketplaceAddress;
            owner = msg.sender;
        }
    */

    function mintToken(string calldata tokenURI, uint256 inkDate) public OnlyOwner returns(uint){

        uint256 newItemId = _tokenIds.current();
        _tokenIds.increment(); 

        _mint(msg.sender, newItemId); // this is the original minting code (line 63 should also be commented in if this line is commented)
        //_mint(marketContractAddress,newItemId);
        //Set the token URI :id and url
        _setTokenURI(newItemId, tokenURI);
        

        TattooCollection[newItemId] = Tattoo(0, inkDate, false);

        // give marketplace the approval to transfer all tokens from this collection
        //setApprovalForAll(marketContractAddress, true); 

        // give the marketplace the approval to transact between users 
        // setApprovalForAll(contractAddress,true); 
        // mint the token and set it for sale - return the id to do so
        return newItemId;
    }

    function updateInk(uint256 tokenId) external OnlyOwner(){
        TattooCollection[tokenId].inked++;
    }

    function updateInkDate(uint256 tokenId, uint256 extendedTime) external OnlyOwner(){
        TattooCollection[tokenId].inkDate = block.timestamp + extendedTime;
    }

    function isPristine(uint256 tokenId) external view returns(bool){
        return ((TattooCollection[tokenId].inked == 0) ? true : false);
    }

    function isInkable(uint256 tokenId) external view returns(bool){
        return ((TattooCollection[tokenId].inkDate > block.timestamp) ? true : false);
    }

    function isListedByArtist(uint256 tokenId) external view OnlyMarket returns(bool){          // do we need OnlyMarket?
        return TattooCollection[tokenId].isListedByArtist;
    }

    function showArtist() external view OnlyMarket returns(address){                            // do we need OnlyMarket?
        return owner;
    }

    // this function will be able to see all the Tattoos that were minted
    function showAllProjects() external view returns(Tattoo[] memory){

        uint256 totalItemCount = _tokenIds.current();           //this is the total item count of our Tattoos (or minted tokens we can say)
        Tattoo[] memory items = new Tattoo[](totalItemCount);

        for(uint256 i = 0; i < totalItemCount; i++){
            items[i] = TattooCollection[i];
        }

        return items;
    }
}

contract StoreFactory {

    using Counters for Counters.Counter;
    Counters.Counter public proxyIds;

    address public impl;
    address public owner;
    mapping(uint256=> address) createdProxies;

    modifier OnlyOwner(){
        require(owner == msg.sender,"Only the owner can call this function");
        _;
    }

    constructor() {
        owner = msg.sender;
    }

    event cloneCreated(address cloneAddress);

    function setImplementationAddress(address _impl) external OnlyOwner{
        impl = _impl;
    }

    function cloneStore(string calldata name, string calldata symbol) public {
        
        uint256 pid = proxyIds.current();
        proxyIds.increment();

        createdProxies[pid] =  ClonesUpgradeable.clone(impl);
        TattooCollectionContract(createdProxies[pid]).initialize(name,symbol,msg.sender);
        emit cloneCreated(createdProxies[pid]);
    //    address createdProxy = ClonesUpgradeable.clone(impl);
    //    TattooCollection(createdProxy).initialize(name,symbol,msg.sender);
    //    emit cloneCreated(createdProxy);
    }

    function getCreatedProxies(uint256 indexer) external view OnlyOwner  returns(address){
        //if the artist destroys his collection from his contract we can still observe the destroyed proxy contract address from createdProxies mapping..This is intentional
        return createdProxies[indexer];
    }
}

