error PriceNotMet(address nftAddress, uint256 tokenId, uint256 price);
error ItemNotForSale(address nftAddress, uint256 tokenId);
error NotListed(address nftAddress, uint256 tokenId);
error AlreadyListed(address nftAddress, uint256 tokenId);
error NoProceeds();
error NotOwner();
error NotApprovedForMarketplace();
error ApprovedForMarketplace();
error PriceMustBeAboveZero();
error TimeMustBePositive();

contract Withdrawable {

    mapping(address => uint256) private _pendingWithdrawals;

    event PendingWithdrawal(address beneficiary, uint256 amount);

    /// Beneficiaries can withdraw any Ether held for them
    function withdraw() external {
        uint256 amountToWithdraw = _pendingWithdrawals[msg.sender];
        // Remember to zero the pending refund before sending to prevent re-entrancy attacks
        delete _pendingWithdrawals[msg.sender];
        payable(msg.sender).transfer(amountToWithdraw);
        emit PendingWithdrawal(msg.sender, 0);
    }

    /// Gets amount of Ether waiting for beneficiary to withdraw
    function getPendingWithdrawal(address beneficiary) public view returns (uint256 amount) {
        return _pendingWithdrawals[beneficiary];
    }

    /// Amount of Ether waiting for beneficiaries to withdraw is increased
    function _increasePendingWithdrawal(address beneficiary, uint256 amount) internal {
        _pendingWithdrawals[beneficiary] += amount;
        emit PendingWithdrawal(beneficiary, _pendingWithdrawals[beneficiary]);
    }
}


contract NftMarketplace is Withdrawable, ReentrancyGuard {

    struct Listing {
        uint256 price;
        address seller;
    }

    struct Offer {
        uint256 amount;
        address bidder;
    }

    struct Auction {
        uint256 amount;
        uint256 reserved;
        address seller;
        address bidder;
        uint256 deadline;
        bool extendOnBid;
    }

    event ItemListed(address indexed seller, address indexed nftAddress, uint256 indexed tokenId, uint256 price);
    event ItemCanceled(address indexed seller, address indexed nftAddress, uint256 indexed tokenId);
    event ItemBought(address indexed buyer, address indexed nftAddress, uint256 indexed tokenId, uint256 price);
    event OfferCanceled(address indexed bidder, address indexed nftAddress, uint256 indexed tokenId);
    event OfferUpdated(address indexed bidder, address indexed nftAddress, uint256 indexed tokenId,  uint256 lockedBid);
    event AuctionStarted(address indexed seller, address indexed nftAddress, uint256 indexed tokenId);
    event AuctionUpdated(address indexed seller, address indexed nftAddress, uint256 indexed tokenId);
    event AuctionCanceled(address indexed canceller, address indexed nftAddress, uint256 indexed tokenId);
    event AuctionBid(address indexed bidder, address indexed nftAddress, uint256 indexed tokenId);

    // State Variables
    mapping(address => mapping(uint256 => Listing)) private s_listings;      
    mapping(address => mapping(uint256 => Offer)) private s_Offers;   
    mapping(address => mapping(uint256 => Auction)) private s_Auction;       


    uint256 private commission;

    uint256 private numberOfDiscountedCollections;
    mapping(uint256 => address) private collectionsWithDiscount;
    mapping(address => uint256) private commissionDeduction;        // IF a seller has a totem NFT -> lower fee (have it settable)
                                                                    // in case you hold multiple different collections or nfts, just pick the highest discount

    address payable ownerMarketplace;         
                                                            
    constructor(){
        ownerMarketplace = payable(msg.sender);
        commission = 50;    // 5%
    }


    // Function modifiers
    modifier OnlyNotListed(address nftAddress, uint256 tokenId, address owner) {
        if (s_listings[nftAddress][tokenId].price > 0) {
            revert AlreadyListed(nftAddress, tokenId);
        }
        _;
    }

    modifier OnlyOwner(address nftAddress, uint256 tokenId, address spender) {
        IERC721 nft = IERC721(nftAddress);
        address owner = nft.ownerOf(tokenId);
        if (spender != owner) {
            revert NotOwner();
        }
        _;
    }

    modifier OnlyListed(address nftAddress, uint256 tokenId) {
        if (s_listings[nftAddress][tokenId].price <= 0) {
            revert NotListed(nftAddress, tokenId);
        }
        _;
    }

    modifier OnlyMarketplaceOwner(){
        require(msg.sender == ownerMarketplace, "caller is not marketplace owner");
        _;
    }


    // General Functions
    function doesSellerOwnNFT(address seller, address nftAddress, uint256 tokenId) internal view returns(bool){
        IERC721 nft = IERC721(nftAddress);
        address ownerOfNFT = nft.ownerOf(tokenId);
        return (seller == ownerOfNFT);
    }

    function GetCommision(address sellerWallet) internal view returns(uint256) {
        return commission - GetDiscount(sellerWallet);
    }

    function GetDiscount (address sellerWallet) internal view returns(uint256) {

        uint256 highestDiscount=0;

        for (uint256 i = 0; i < numberOfDiscountedCollections; i++){

            address collection = collectionsWithDiscount[i];

            if(CheckIfWalletHoldsNFTFromCollection(sellerWallet, collection)){
                uint256 discount = commissionDeduction[collection];

                if(discount > highestDiscount){highestDiscount = discount;}
            }

        }

        return highestDiscount;
    }

    function CheckIfWalletHoldsNFTFromCollection(address wallet, address nftAddress) internal view returns(bool){
        IERC721 nft = IERC721(nftAddress);
        return (nft.balanceOf(wallet) == 0) ? false : true;
    }

    function AdjustCommisionForHoldersOfSpecificCollection(address nftAddress, uint256 newCommision) external OnlyMarketplaceOwner {
        require(0 <= newCommision && newCommision <= 50, "Commision should be between 0 and 5% (50)");

        for(uint256 i = 0; i < numberOfDiscountedCollections; i++){
            if(collectionsWithDiscount[i] == nftAddress){
                commissionDeduction[nftAddress] = newCommision;
                return;
            }
        }

        collectionsWithDiscount[numberOfDiscountedCollections] = nftAddress;
        commissionDeduction[nftAddress] = newCommision;
        numberOfDiscountedCollections++;
    }



    //-------------------------------------------
    //          LISTINGS SPECIFIC PART
    //-------------------------------------------

    function listItem(address nftAddress, uint256 tokenId, uint256 price)
        external
        OnlyNotListed(nftAddress, tokenId, msg.sender)
        OnlyOwner(nftAddress, tokenId, msg.sender)
    {
        if (price <= 0) {
            revert PriceMustBeAboveZero();
        }
    
        // lock the NFT - disable the transfer while listed
        TattooCollectionContract tattooContract = TattooCollectionContract(nftAddress);
        tattooContract.setLocked(tokenId, true);

        s_listings[nftAddress][tokenId] = Listing(price, msg.sender);
        emit ItemListed(msg.sender, nftAddress, tokenId, price);
    }

    function cancelListing(address nftAddress, uint256 tokenId)
        external
        OnlyOwner(nftAddress, tokenId, msg.sender)
        OnlyListed(nftAddress, tokenId)
    {
        // unlock the NFT
        TattooCollectionContract tattooContract = TattooCollectionContract(nftAddress);
        tattooContract.setLocked(tokenId, false);

        delete (s_listings[nftAddress][tokenId]);
        emit ItemCanceled(msg.sender, nftAddress, tokenId);
    }

    function buyItem(address nftAddress, uint256 tokenId)
        external
        payable
        OnlyListed(nftAddress, tokenId)
        nonReentrant
    {
        Listing memory listedItem = s_listings[nftAddress][tokenId];

        // check that the seller still holds this NFT
        require(doesSellerOwnNFT(listedItem.seller, nftAddress, tokenId), "Seller is no longer the owner of this nft");   

        // Set approval just in case it is false
        IERC721 nft = IERC721(nftAddress);
        nft.setApprovalForAll(address(this), true);

        // unlock the NFT - make the transfer available
        TattooCollectionContract tattooContract = TattooCollectionContract(nftAddress);
        tattooContract.setLocked(tokenId, false);

        if (msg.value < listedItem.price) {
            revert PriceNotMet(nftAddress, tokenId, listedItem.price);
        }

        // release funds to the seller
        _increasePendingWithdrawal(ownerMarketplace, (msg.value / 1000) * GetCommision(listedItem.seller)) ;
        (payable(listedItem.seller)).transfer((msg.value / 1000) * (1000 - GetCommision(listedItem.seller)));

        // remove the listing + offer
        delete (s_listings[nftAddress][tokenId]);
        delete (s_Offers[nftAddress][tokenId]);

        IERC721(nftAddress).safeTransferFrom(listedItem.seller, msg.sender, tokenId);
        emit ItemBought(msg.sender, nftAddress, tokenId, listedItem.price);
    }

    function updateListing(address nftAddress, uint256 tokenId, uint256 newPrice)
        external
        OnlyListed(nftAddress, tokenId)
        nonReentrant
        OnlyOwner(nftAddress, tokenId, msg.sender)
    {
        if (newPrice == 0) {
            revert PriceMustBeAboveZero();
        }

        s_listings[nftAddress][tokenId].price = newPrice;
        emit ItemListed(msg.sender, nftAddress, tokenId, newPrice);
    }

    function getListing(address nftAddress, uint256 tokenId) external view returns (Listing memory){
        return s_listings[nftAddress][tokenId];
    }

    function getListingPrice(address nftAddress, uint256 tokenId) external view returns (uint256){
        return s_listings[nftAddress][tokenId].price;
    }




    //-------------------------------------------
    //          OFFERS SPECIFIC PART
    //-------------------------------------------

    /// Anyone can commit more than the existing offer for a token. Previous offer will be refunded
    function offer(address nftAddress, uint256 tokenId) external payable {

        Offer memory existingLockedBid = s_Offers[nftAddress][tokenId];
        require(msg.value > existingLockedBid.amount, "Bid too low");

        // remove old bid, if it exists
        if (existingLockedBid.amount > 0) {
            _increasePendingWithdrawal(existingLockedBid.bidder, existingLockedBid.amount); // return funds to the previous bidder
        }

        // Enter new bid
        _setOffer(nftAddress, tokenId, msg.sender, msg.value);
    }

    /// Anyone can add more value to their existing offer
    function offerIncrease(address nftAddress, uint256 tokenId) external payable {

        Offer memory existingLockedBid = s_Offers[nftAddress][tokenId];

        require(msg.value > 0, "Can only add positive amount to the bid");
        require(msg.sender == existingLockedBid.bidder, "Only the bidder may increase their bid");

        uint256 newBidAmount = existingLockedBid.amount + msg.value;

        // set new bid
        _setOffer(nftAddress, tokenId, msg.sender, newBidAmount);
    }

    /// A bidder can revoke their offer
    function offerCancel(address nftAddress, uint256 tokenId) external {

        Offer memory existingLockedBid = s_Offers[nftAddress][tokenId];

        require(msg.sender == existingLockedBid.bidder, "Only the bidder may revoke their bid");

        _increasePendingWithdrawal(msg.sender, existingLockedBid.amount);

        // set bid to 0 - aka not existing
        // _setBid(nftAddress, tokenId, address(0), 0);
        delete (s_Offers[nftAddress][tokenId]);

        emit OfferCanceled(msg.sender, nftAddress, tokenId);
    }

    // consider adding the amount as a argument (read from current offer)
    // why: what if the seller has just canceled the offer, than the seller would accept a 0 offer and NFT would go to the address(0)????
    function offerAccept(address nftAddress, uint256 tokenId) OnlyOwner(nftAddress, tokenId, msg.sender) external {

        // Set approval just in case it is false
        IERC721 nft = IERC721(nftAddress);
        nft.setApprovalForAll(address(this), true);

        // unlock the NFT
        TattooCollectionContract tattooContract = TattooCollectionContract(nftAddress);
        tattooContract.setLocked(tokenId, false);

        // get the amount
        Offer memory existingLockedBid = s_Offers[nftAddress][tokenId];

        // release funds to the seller
        _increasePendingWithdrawal(ownerMarketplace, (existingLockedBid.amount / 1000) * GetCommision(msg.sender)) ;
        (payable(msg.sender)).transfer((existingLockedBid.amount / 1000) * (1000 - GetCommision(msg.sender)));

        // remove the listing + offer
        delete (s_Offers[nftAddress][tokenId]);
        delete(s_listings[nftAddress][tokenId]);
        
        // transfer the token
        IERC721(nftAddress).safeTransferFrom(msg.sender, existingLockedBid.bidder, tokenId);
        emit ItemBought(existingLockedBid.bidder, nftAddress, tokenId, existingLockedBid.amount);
    }

    function _setOffer(address nftAddress, uint256 tokenId, address bidder, uint256 amount) private {
        s_Offers[nftAddress][tokenId].bidder = bidder;
        s_Offers[nftAddress][tokenId].amount = amount;
        emit OfferUpdated(bidder, nftAddress, tokenId, amount);
    }

    function getTopOffer(address nftAddress, uint256 tokenId) external view returns (Offer memory){
        return s_Offers[nftAddress][tokenId];
    }




    //-------------------------------------------
    //          AUCTION SPECIFIC PART
    //-------------------------------------------

    function auctionStart(address nftAddress, uint256 tokenId, uint256 reservedPrice, uint256 timeUntilEnd, bool extendOnBid) OnlyOwner(nftAddress, tokenId, msg.sender) external {

        if (reservedPrice <= 0) {
            revert PriceMustBeAboveZero();
        }

        require(0 < timeUntilEnd, "Time until the end of auction has to be positive");
                
        // lock the NFT - disable the transfer while auctioned
        TattooCollectionContract tattooContract = TattooCollectionContract(nftAddress);
        tattooContract.setLocked(tokenId, true);

        s_Auction[nftAddress][tokenId].reserved = reservedPrice;
        s_Auction[nftAddress][tokenId].deadline = timeUntilEnd + block.timestamp;
        s_Auction[nftAddress][tokenId].seller = msg.sender;
        s_Auction[nftAddress][tokenId].extendOnBid = extendOnBid;

        emit AuctionStarted(msg.sender, nftAddress, tokenId);
    }

    function auctionBid(address nftAddress, uint256 tokenId) payable external {

        Auction memory auction = s_Auction[nftAddress][tokenId];

        // check if deadline has passed already
        require(block.timestamp < auction.deadline, "Auction has finished");

        // check that the amount is bigger
        require(msg.value >= auction.reserved && msg.value > auction.amount, "Bid too low");       

        // remove old bid, if it exists
        if (auction.amount > 0) {
            _increasePendingWithdrawal(auction.bidder, auction.amount); // return funds to the previous bidder
        }

        // replace the bidder
        s_Auction[nftAddress][tokenId].amount = msg.value;
        s_Auction[nftAddress][tokenId].bidder = msg.sender;

        // extend the deadline if there is less than 15min (900s)
        if(auction.extendOnBid){

            if(auction.deadline - block.timestamp < 900){
                s_Auction[nftAddress][tokenId].deadline = block.timestamp + 900;    // reset to 15min time left
            }
        }

        emit AuctionBid(msg.sender, nftAddress, tokenId);
    }

    function auctionClaim(address nftAddress, uint256 tokenId) external {

        Auction memory auction = s_Auction[nftAddress][tokenId];

        // Set approval just in case it is false
        IERC721 nft = IERC721(nftAddress);
        nft.setApprovalForAll(address(this), true);

        // check that the seller still holds this NFT
        require(doesSellerOwnNFT(auction.seller, nftAddress, tokenId), "Seller is no longer the owner of this nft"); 

        require(block.timestamp > auction.deadline, "Auction has not finished yet");

        // send funds to the seller
        _increasePendingWithdrawal(ownerMarketplace, (auction.amount / 1000) * GetCommision(auction.seller)) ;
        (payable(auction.seller)).transfer((auction.amount / 1000) * (1000 - GetCommision(auction.seller)));

        // remove the auction
        delete(s_Auction[nftAddress][tokenId]);
        
        // transfer the token
        IERC721(nftAddress).safeTransferFrom(auction.seller, auction.bidder, tokenId);
        emit ItemBought(auction.bidder, nftAddress, tokenId, auction.amount);
    }

    function auctionAcceptTopBid(address nftAddress, uint256 tokenId) OnlyOwner(nftAddress, tokenId, msg.sender) external {
    
        // Set approval just in case it is false
        IERC721 nft = IERC721(nftAddress);
        nft.setApprovalForAll(address(this), true);

        // unlock the NFT
        TattooCollectionContract tattooContract = TattooCollectionContract(nftAddress);
        tattooContract.setLocked(tokenId, false);

        Auction memory auction = s_Auction[nftAddress][tokenId];
        
        // send funds to the seller
        _increasePendingWithdrawal(ownerMarketplace, (auction.amount / 1000) * GetCommision(auction.seller)) ;
        (payable(auction.seller)).transfer((auction.amount / 1000) * (1000 - GetCommision(auction.seller)));

        // remove the auction
        delete(s_Auction[nftAddress][tokenId]);
        
        // transfer the token
        IERC721(nftAddress).safeTransferFrom(auction.seller, auction.bidder, tokenId);
        emit ItemBought(auction.bidder, nftAddress, tokenId, auction.amount);
    }

    function auctionCancel(address nftAddress, uint256 tokenId) OnlyOwner(nftAddress, tokenId, msg.sender) external {

        Auction memory auction = s_Auction[nftAddress][tokenId];

        require(auction.amount == 0, "First Bid is in, cannot cancel auction anymore");

        // unlock the NFT
        TattooCollectionContract tattooContract = TattooCollectionContract(nftAddress);
        tattooContract.setLocked(tokenId, false);

        // remove the auction
        delete(s_Auction[nftAddress][tokenId]);

        emit AuctionCanceled(msg.sender, nftAddress, tokenId); 
    }

    function auctionUpdate(address nftAddress, uint256 tokenId, uint256 reservedPrice, uint256 timeUntilEnd, bool extendOnBid) OnlyOwner(nftAddress, tokenId, msg.sender) external {

        Auction memory auction = s_Auction[nftAddress][tokenId];

        require(auction.amount == 0, "First Bid is in, cannot change auction anymore");
        require(block.timestamp < auction.deadline, "Auction has finished");

        s_Auction[nftAddress][tokenId].reserved = reservedPrice;
        s_Auction[nftAddress][tokenId].deadline = timeUntilEnd + block.timestamp;
        s_Auction[nftAddress][tokenId].extendOnBid = extendOnBid;

        emit AuctionUpdated(msg.sender, nftAddress, tokenId); 
    }


    function GetAuctionTopBid(address nftAddress, uint256 tokenId) external view returns(uint256){
        return s_Auction[nftAddress][tokenId].amount;
    }

    function GetAuctionTimeLeft(address nftAddress, uint256 tokenId) external view returns(uint256){
        uint256 timeLeft = s_Auction[nftAddress][tokenId].deadline - block.timestamp;
        return (timeLeft > 0 ? timeLeft : 0);
    }

    function GetAuctionReservedPrice(address nftAddress, uint256 tokenId) external view returns(uint256){
        return s_Auction[nftAddress][tokenId].reserved;
    }

    function isAuctionExtendable(address nftAddress, uint256 tokenId) external view returns(bool){
        return s_Auction[nftAddress][tokenId].extendOnBid;
    }

}

