pragma solidity ^0.8.7;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "../interfaces/ILayerZeroReceiver.sol";
import "../interfaces/ILayerZeroEndpoint.sol";

// deploy this contract to 2+ chains for testing.
//
// sendTokens() function works like this:
//  1. burn local tokens on the source chain
//  2. send a LayerZero message to the destination OmniChainToken contract on another chain
//  3. mint tokens on destination in lzReceive()
contract OmniMint is ERC721URIStorage, ILayerZeroReceiver {

    event LzReceiveLog(string omniId);

    struct OmniTokenData {
        string chainSymbol;
        string tokenURI;
        address destinationAddress;
    }

    ILayerZeroEndpoint public endpoint;
    mapping(uint16 => bytes) public remotes;
    mapping(string => uint256) private _chainSymbolToId;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    address private _owner;
    uint256 private _contractChainId;

    constructor() ERC721("Omni NFT", "ONFT") {
        // TODO: (Must) .env Now Rinkeby endpoint address:
        endpoint = ILayerZeroEndpoint(address(0x6Fcb97553D41516Cb228ac03FdC8B9a0a9df04A1));
        // 0x79a63d6d8BBD5c6dfc774dA79bCcD948EAcb53FA for Rinkeby endpoint
        // 0x6Fcb97553D41516Cb228ac03FdC8B9a0a9df04A1 for BSC Testnet endpoint
        _contractChainId = 10002;
        // BSC Testnet ChainId: Use from https://layerzero.gitbook.io/docs/technical-reference/testnet/testnet-addresses
        _owner = msg.sender;

        _chainSymbolToId["eth"] = 10001;
        // Rinkeby
        _chainSymbolToId["bsc"] = 10002;
        // BSC Testnet
    }

    modifier onlyOwner() {
        require(msg.sender == _owner);
        _;
    }

    // send NFT to another chain.
    // this function sends the token from your address to the same address on the destination.
    function omniMint(OmniTokenData[] calldata _tokensData) public payable {
        require(_tokensData.length > 0, "Provide at least 1 NFT data");

        for (uint i = 0; i < _tokensData.length; i++) {
            OmniTokenData calldata tokenData = _tokensData[i];
            require(bytes(tokenData.chainSymbol).length > 0 && bytes(tokenData.tokenURI).length > 0 && tokenData.destinationAddress != address(0), "Token data incomplete");
            require(_chainSymbolToId[tokenData.chainSymbol] > 0, "Invalid chain symbol");

            if (_chainSymbolToId[tokenData.chainSymbol] == _contractChainId) {
                _tokenIds.increment();
                uint256 newItemId = _tokenIds.current();
                _mint(msg.sender, newItemId);
                _setTokenURI(newItemId, tokenData.tokenURI);
            } else {
                // abi.encode() the payload with the values to send
                bytes memory payload = abi.encode(msg.sender, tokenData.tokenURI, tokenData.destinationAddress);
                uint16 mappedChainId = uint16(_chainSymbolToId[tokenData.chainSymbol]);

                // send LayerZero message
                endpoint.send{value : msg.value}(
                    mappedChainId, // destination chainId
                    remotes[mappedChainId], // destination address of OmniMint LzReceiver Contract
                    payload, // abi.encode()'ed bytes
                    payable(msg.sender), // refund address (LayerZero will refund any superflous gas back to caller of send()
                    address(0x0), // 'zroPaymentAddress' unused for this mock/example
                    bytes("")                       // 'txParameters' unused for this mock/example
                );
            }
        }
    }

    // _chainId - the chainId for the remote contract
    // _remoteAddress - the contract address on the remote chainId
    // the owner must set remote contract addresses.
    // in lzReceive(), a require() ensures only messages
    // from known contracts can be received.
    function setRemote(uint16 _chainId, bytes calldata _remoteAddress) external onlyOwner {
        require(remotes[_chainId].length == 0, "The remote address has already been set for the chainId!");
        remotes[_chainId] = _remoteAddress;
    }

    function updateChainSymbolToId(uint16 _chainId, string memory _symbol) external onlyOwner {
        require(bytes(_symbol).length > 0, "Symbol cannot be empty");
        _chainSymbolToId[_symbol] = _chainId;
    }

    function lzReceive(
        uint16 _srcChainId,
        bytes memory _srcAddress,
        uint64,
        bytes memory _payload
    ) override external {
        emit LzReceiveLog("Receiver log");

        require(msg.sender == address(endpoint));
        // boilerplate! lzReceive must be called by the endpoint for security
        // owner must have setRemote() to allow its remote contracts to send to this contract
        require(
            _srcAddress.length == remotes[_srcChainId].length && keccak256(_srcAddress) == keccak256(remotes[_srcChainId]),
            "Invalid remote sender address. owner should call setRemote() to enable remote contract"
        );

        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();

        // decode
        (address user, string memory tokenURI, address receiver) = abi.decode(_payload, (address, string, address));

        if (user == receiver) {
            _mint(user, newItemId);
        } else {
            _mint(receiver, newItemId);
        }
        _setTokenURI(newItemId, tokenURI);
    }

    function testOmniMint(OmniTokenData[] calldata _tokensData) external {
        require(_tokensData.length > 0, "Provide at least 1 NFT data");

        for (uint i = 0; i < _tokensData.length; i++) {
            OmniTokenData calldata tokenData = _tokensData[i];
            require(bytes(tokenData.chainSymbol).length > 0 && bytes(tokenData.tokenURI).length > 0 && tokenData.destinationAddress != address(0x0), "Token data incomplete");
            require(_chainSymbolToId[tokenData.chainSymbol] > 0, "Invalid chain symbol");

            _tokenIds.increment();
            uint256 newItemId = _tokenIds.current();

            if (_chainSymbolToId[tokenData.chainSymbol] == _contractChainId) {
                _mint(msg.sender, newItemId);
                _setTokenURI(newItemId, tokenData.tokenURI);
            } else {
                if (msg.sender == tokenData.destinationAddress) {
                    _mint(msg.sender, newItemId);
                } else {
                    _mint(tokenData.destinationAddress, newItemId);
                }
                _setTokenURI(newItemId, tokenData.tokenURI);
            }
        }
    }
}

