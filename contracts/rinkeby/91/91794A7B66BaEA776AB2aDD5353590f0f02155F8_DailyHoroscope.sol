contract DailyHoroscope is Ownable, ERC721A, ReentrancyGuard, DailyHoroscopeSVG, DailyHoroscopeDesc {
  using Strings for uint256;

  mapping(address => uint8) public allowlist;
  
  constructor() ERC721A("DailyHoroscope", "Horoscope") {}

  modifier callerIsUser() {
    require(tx.origin == msg.sender, "The caller is another contract");
    _;
  }
  //......
  function mint(uint8 quantity, string memory nickName) external onlyOwner {
    _safeMint(msg.sender, quantity, nickName);
      
  }

  // function clientMint(uint8 quantity, string memory receiverName) external payable callerIsUser {
  //   require(quantity > 0 && allowlist[msg.sender] >= quantity, "not eligible for allowlist mint");
  //   allowlist[msg.sender] -= quantity;
  //   _safeMint(msg.sender, quantity, receiverName);
  // }
  
  function seedAllowlist(address[] memory addresses, uint8[] memory numSlots, string[] memory receiverNames) external onlyOwner {
    require(
      addresses.length == numSlots.length && numSlots.length == receiverNames.length,
      "length does not match"
    );
    for (uint256 i = 0; i < addresses.length; i++) {
      allowlist[addresses[i]] = numSlots[i];
    }
  }
  //......

  // function setPersonalInfo(string memory nickName, uint256 month, uint256 day) external {
  //   require(isValidDate(month, day));
  // }

  function isValidDate(uint256 month, uint256 day) internal pure returns (bool valid){
    if (
        month == 1 ||
        month == 3 ||
        month == 5 ||
        month == 7 ||
        month == 8 ||
        month == 10 ||
        month == 12
    ) {
        if (day < 32 && day > 0) {
          valid == true;
        }
    } else if (month != 2 && month < 13) {
        if (day < 31 && day > 0) {
          valid == true;
        }
    } else if (month == 2 && month < 13) {
        if (day < 30 && day > 0) {
          valid == true;
        }
    }
  }

  // metadata URI

  function tokenURI(uint256 tokenId) public view override returns (string memory) {
    require(_exists(tokenId));
    uint256 currentUnix = block.timestamp;
    // uint256 dayOfWeek = DateTime.getDayOfWeek(currentUnix);
    string memory dayOfWeekAbbr = DateTime.getDayOfWeekAbbr(currentUnix);
    // uint256 month = DateTime.getMonth(currentUnix);
    string memory monthAbbr = DateTime.getMonthAbbr(currentUnix);
    uint256 day = DateTime.getDay(currentUnix);
    TokenOwnership memory ownershipData = getOwnershipData(tokenId);
    string memory dateInfo = string(abi.encodePacked(dayOfWeekAbbr, ". ", day.toString(), " ", monthAbbr));
    string memory nickName = ownershipData.nickName;
    SVGParams memory params = SVGParams(dateInfo, nickName);
    (string memory name, string memory desc) = generateDesc(tokenId, ownershipData.nickName);
    JSONGenerator.TokenUriParams memory tokenUriParams = JSONGenerator.TokenUriParams(name, desc, generateSVG(params));
    return JSONGenerator.generateUri(tokenUriParams);
  }

  function numberMinted(address owner) public view returns (uint256) {
    return _numberMinted(owner);
  }

  function numberBurned(address owner) public view returns (uint256) {
    return _numberBurned(owner);
  }

  function getOwnershipData(uint256 tokenId) public view returns (TokenOwnership memory) {
    return ownershipOf(tokenId);
  }

}

