pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/IERC165Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";

import "hardhat/console.sol";

import "./interfaces/IMNFTStrategyManager.sol";
import "./interfaces/ITransferManagerNFT.sol";
import "./interfaces/IMNFT721.sol";
import "./interfaces/IMNFT1155.sol";
import "./interfaces/IMNFTFactory.sol";
import "./interfaces/IMNFTCurrencyManager.sol";
import "./interfaces/IMNFTRoyaltyManager.sol";
import "./libraries/OrderType.sol";
import "./libraries/SignatureChecker.sol";

contract MNFTMarketplace is OwnableUpgradeable, AccessControlEnumerableUpgradeable, ReentrancyGuardUpgradeable {
   using SafeERC20Upgradeable for IERC20Upgradeable;

   IERC20Upgradeable public WETH;
   IMNFTCurrencyManager private currencyManager;
   IMNFTStrategyManager private strategyManager;
   IMNFTRoyaltyManager private royaltyManager;
   address private ERC721Manager;
   address private ERC1155Manager;
   address private MNFT721;
   address private MNFT1155;
   address private MNFTFactory;
   address private fundAddress;

   bytes32 private OWNER_ROLE;
   bytes32 private MODERATE_ROLE;
   bytes32 private DOMAIN_SEPARATOR;

   uint256 public MNFT721Price;
   uint256 public MNFT1155Price;

   // ERC721 interfaceID
   bytes4 public INTERFACE_ID_ERC721;
   // ERC1155 interfaceID
   bytes4 public INTERFACE_ID_ERC1155;

   uint16 private ROYALTY_MIN;     // over 1e3, 500 means 0.5%
   uint16 private ROYALTY_MAX;     // over 1e3, 10000 means 10%
   uint16 public royaltyRate;   // over 1e3, 500 means 0.5%

   event MintMNFT721(
      string[] tokenURIs,
      address minter
   );

   event MintMNFT1155(
      string tokenURI,
      uint256 amount,
      address minter
   );

   event CreateMNFT721(
      string name,
      string symbol,
      address owner,
      address tokenAddress
   );

   event CreateMNFT1155(
      address owner,
      address tokenAddress,
      uint256 amount
   );

   event BuyNonFindgibleToken(
      address seller,
      address buyer,
      address tokenAddress,
      uint256 price,
      uint256 tokenID
   );

   event MatchAuction(
      address maker,
      address bidder,
      address tokenAddress,
      uint256 tokenID,
      uint256 price
   );

   event AcceptOffer(
      address maker,
      address bidder,
      address makerTokenAddress,
      address bidderTokenAddress,
      uint256 makerTokenID,
      uint256 bidderTokenID,
      uint256 makerPrice,
      uint256 bidderPrice
   );

   function initialize() public initializer {
      __Ownable_init();

      MNFT721Price = 1e15;
      MNFT1155Price = 1e15;
      OWNER_ROLE = keccak256("OWNER_ROLE");
      MODERATE_ROLE = keccak256("MODERATE_ROLE");
      _setupRole(OWNER_ROLE, msg.sender);

      // Calculate the domain separator
      DOMAIN_SEPARATOR = keccak256(
         abi.encode(
            0x8b73c3c69bb8fe3d512ecc4cf759cc79239f7b179b0ffacaa9a75d522b39400f, // keccak256("EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)")
            0xb222be2004e3f228a1a1e9abc911cd6048d3bcc0aceabe1ea26f611270b6fa9e, // keccak256("MNFTMarketplace")
            0xc89efdaa54c0f20c7adf612882df0950f5a951637e0307cdcb4c672f298b8bc6, // keccak256(bytes("1")) for versionId = 1
            block.chainid,
            address(this)
         )
      );

      INTERFACE_ID_ERC721 = 0x80ac58cd;
      INTERFACE_ID_ERC1155 = 0xd9b67a26;
      ROYALTY_MIN = 5 * 1e2;
      ROYALTY_MAX = 1 * 1e4;
      royaltyRate = ROYALTY_MIN;
   }

   function setBasicData(
      address strategyManager_,
      address currencyManager_,
      address royaltyManager_,
      address ERC721Manager_,
      address ERC1155Manager_,
      address ERC721Address_,
      address ERC1155Address_,
      address MNFTFactoryAddress_,
      address WETH_,
      address multisig_
   ) external onlyOwner {
      strategyManager = IMNFTStrategyManager(strategyManager_);
      currencyManager = IMNFTCurrencyManager(currencyManager_);
      royaltyManager = IMNFTRoyaltyManager(royaltyManager_);
      ERC721Manager = ERC721Manager_;
      ERC1155Manager = ERC1155Manager_;
      MNFT721 = ERC721Address_;
      MNFT1155 = ERC1155Address_;
      MNFTFactory = MNFTFactoryAddress_;
      WETH = IERC20Upgradeable(WETH_);
      fundAddress = msg.sender;
      
      _setupRole(OWNER_ROLE, multisig_);
   }

   function checkPermission() internal view {
      address sender = msg.sender;
      require (hasRole(OWNER_ROLE, sender) || hasRole(MODERATE_ROLE, sender), 'no permission');
   }

   function checkScammer() internal view {
      require (strategyManager.isStrategyblacklisted(msg.sender) == false, 'scammer address');
   }

   function getScammers() external view returns(address[] memory) {
      return strategyManager.viewblacklistedStrategies();
   }

   function addModerate(address newUser_) external {
      checkPermission();
      _setupRole(MODERATE_ROLE, newUser_);
   }

   function setMNFT721Price(uint256 newPrice_) external onlyOwner {
      MNFT721Price = newPrice_;
   }

   function setMNFT1155Price(uint256 newPrice_) external onlyOwner {
      MNFT1155Price = newPrice_;
   }

   function setFundAddress(address fundAddress_) external onlyRole(OWNER_ROLE) {
      fundAddress = fundAddress_;
   }

   function addToBlackList(address user_) external {
      checkPermission();
      strategyManager.addStrategy(user_);
   }

   function removeFromBlackList(address user_) external {
      checkPermission();
      strategyManager.removeStrategy(user_);
   }

   function createMNFT721(
      string calldata name_,
      string calldata symbol_,
      string calldata tokenURI_
   ) external {
      checkScammer();
      address tokenAddress = IMNFTFactory(MNFTFactory).createMNFT721(name_, symbol_, tokenURI_, msg.sender);
      currencyManager.addUserCurrency(tokenAddress, msg.sender);
      emit CreateMNFT721(name_, symbol_, msg.sender, tokenAddress);
   }

   function createMNFT1155(
      string calldata name_,
      string calldata symbol_,
      string calldata tokenURI_,
      uint256 amount_
   ) external {
      checkScammer();
      address tokenAddress = IMNFTFactory(MNFTFactory).createMNFT1155(name_, symbol_, tokenURI_, amount_, msg.sender);
      currencyManager.addUserCurrency(tokenAddress, msg.sender);
      emit CreateMNFT1155(msg.sender, tokenAddress, amount_);
   }

   function addCollection(address tokenAddress_) external {
      require (tokenAddress_ != address(0), 'wrong token');
      require (
         IERC165Upgradeable(tokenAddress_).supportsInterface(INTERFACE_ID_ERC1155) ||
         IERC165Upgradeable(tokenAddress_).supportsInterface(INTERFACE_ID_ERC721),
         'not NFT token'
      );
      currencyManager.addCurrency(tokenAddress_);
   }

   function mintMNFT721(
      address tokenAddress_,
      string[] calldata tokenURIs_
   ) external payable {
      checkScammer();
      require (
         (tokenAddress_ == MNFT721 ||
         currencyManager.isUserCurrencyWhitelisted(tokenAddress_, msg.sender) == true) && 
         IERC165Upgradeable(tokenAddress_).supportsInterface(INTERFACE_ID_ERC721),
         'not allowed currency'
      );
      uint256 amount = tokenURIs_.length;
      require(amount > 0, "Amount must be greater than zero");
      require(amount * MNFT721Price <= msg.value, 'not enough money');

      IMNFT721(tokenAddress_).mintNFT(tokenURIs_, msg.sender);

      emit MintMNFT721(tokenURIs_, msg.sender);
   }

   function mintMNFT1155(
      address tokenAddress_,
      string calldata tokenURI_,
      uint256 amount_
   ) external payable {
      checkScammer();
      require (
         (tokenAddress_ == MNFT1155 ||
         currencyManager.isUserCurrencyWhitelisted(tokenAddress_, msg.sender) == true) && 
         IERC165Upgradeable(tokenAddress_).supportsInterface(INTERFACE_ID_ERC1155),
         'not allowed currency'
      );
      require(amount_ > 0, "Amount must be greater than zero");
      require(amount_ * MNFT1155Price <= msg.value, 'not enough money');

      IMNFT1155(tokenAddress_).mintNFT(msg.sender, tokenURI_, amount_);

      emit MintMNFT1155(tokenURI_, amount_, msg.sender);
   }

   function setRoyalty(uint16 royalty_) external onlyRole(OWNER_ROLE) {
      royaltyManager.updateGlobalRoyalty(royalty_);
   }

   function setUserRoyalty(
      address tokenAddress_,
      uint16 royalty_
   ) external {
      checkScammer();
      royaltyManager.setUserRoyalty(msg.sender, tokenAddress_, royalty_);
   }

   function getWhitelistedCurrencies() external view returns(address[] memory) {
      checkScammer();
      return currencyManager.viewUserWhitelistedCurrencies(msg.sender);
   }

   function buyNonFindgibleToken(
      OrderType.MakerOrder calldata maker_
   ) external payable nonReentrant {
      checkScammer();
      require (msg.value >= maker_.price, 'not enough money');
      _matchMakerWithTakerByETHAndWETH(maker_, msg.sender, true);
      
      emit BuyNonFindgibleToken(
         maker_.signer,
         msg.sender,
         maker_.tokenAddress,
         maker_.price,
         maker_.tokenID
      );
   }

   function matchAuction(
      OrderType.MakerOrder calldata maker_,
      OrderType.MakerOrder calldata taker_
   ) external nonReentrant {
      checkScammer();
      require (msg.sender == taker_.signer, 'wrong caller');
      
      bytes32 makerHash = OrderType.hash(maker_);
      _validateOrder(maker_, makerHash);

      bytes32 biderHash = OrderType.hash(taker_);
      _validateOrder(taker_, biderHash);

      require (
         maker_.tokenAddress == taker_.tokenAddress &&
         maker_.tokenID == taker_.tokenID &&
         maker_.tokenAmount == taker_.tokenAmount &&
         maker_.isETH == taker_.isETH,
         'not match between maker and bider'
      );

      OrderType.MakerOrder memory txOrder = maker_;
      txOrder.price = taker_.price;

      _makeTransactionByETHAndWETH(txOrder, taker_.signer, false);

      emit MatchAuction(
         maker_.signer,
         msg.sender,
         maker_.tokenAddress,
         maker_.tokenID,
         taker_.price
      );
   }

   function acceptOffer(
      OrderType.MakerOrder calldata maker_,
      OrderType.TakeOfferOrder calldata taker_
   ) external nonReentrant {
      checkScammer();
      require (msg.sender == maker_.signer, 'wrong caller');
      require (taker_.signer != address(0), 'wrong signer');
      require (
         maker_.tokenAddress == taker_.targetTokenAddress &&
         maker_.tokenID == taker_.targetTokenID,
         'wrong taker'
      );

      // maker
      _matchMakerWithTakerByETHAndWETH(maker_, taker_.signer, false);

      // taker
      bytes32 hash = OrderType.hashTakeOffer(taker_);
      require (
         SignatureChecker.verify(
            hash, 
            taker_.signer, 
            taker_.v, 
            taker_.r, 
            taker_.s,
            DOMAIN_SEPARATOR
         ),
         'verification failed'
      );

      OrderType.MakerOrder memory taker = OrderType.MakerOrder({
         signer: taker_.signer,
         tokenAddress: taker_.tokenAddress,
         tokenID: taker_.tokenID,
         price: taker_.price,
         tokenAmount: taker_.tokenAmount,
         r: taker_.r,
         s: taker_.s,
         v: taker_.v,
         isETH: taker_.isETH
      });

      _makeTransactionByETHAndWETH(taker, maker_.signer, false);

      emit AcceptOffer(
         maker_.signer, 
         taker_.signer, 
         maker_.tokenAddress, 
         taker_.tokenAddress, 
         maker_.tokenID, 
         taker_.tokenID, 
         maker_.price, 
         taker_.price
      );
   }

   // for only ERC721
   function batchTransfer(
      address to_,
      address[] calldata tokenAddress_,
      uint256[] calldata tokenID_
   ) external {
      checkScammer();
      require (to_ != address(0), 'wrong address');
      require (tokenAddress_.length == tokenID_.length, 'wrong token info');

      for (uint256 i = 0; i < tokenAddress_.length; i ++) {
         require (currencyManager.isCurrencyWhitelisted(tokenAddress_[i]), 'not added token');
         IERC721(tokenAddress_[i]).transferFrom(msg.sender, to_, tokenID_[i]);
      }
   }

   function _matchMakerWithTakerByETHAndWETH(
      OrderType.MakerOrder memory maker_,
      address taker_,
      bool isETH_
   ) internal {
      bytes32 hash = OrderType.hash(maker_);
      _validateOrder(maker_, hash);

      _makeTransactionByETHAndWETH(maker_, taker_, isETH_);
   }

   function _makeTransactionByETHAndWETH(
      OrderType.MakerOrder memory maker_,
      address taker_,
      bool isETH_
   ) internal {
      require (
         currencyManager.isCurrencyWhitelisted(maker_.tokenAddress) == true ||
         maker_.tokenAddress == MNFT1155 || maker_.tokenAddress == MNFT721, 
         'not added token'
      );
      if (maker_.price > 0) {
         if (maker_.isETH == false) {
            WETH.transferFrom(taker_, address(this), maker_.price);
         }
         _transferFeesAndFunds(
            maker_.signer, 
            maker_.tokenAddress,
            maker_.price, 
            isETH_
         );
      }

      if (maker_.tokenAddress != address(0)) {
         _transferNonFundgibleToken(
            maker_.signer, 
            taker_, 
            maker_.tokenAddress, 
            maker_.tokenID, 
            maker_.tokenAmount
         );
      }
   }

   function _transferFeesAndFunds(
      address to_,
      address tokenAddress_,
      uint256 amount_,
      bool isETH_
   ) internal {
      (
         address receiver,
         uint256 royaltyFee,
         uint256 userFee
      ) = royaltyManager.getRoyaltyFee(tokenAddress_, amount_);

      amount_ = amount_ - royaltyFee - userFee;
      
      if (isETH_) {
         payable(to_).transfer(amount_);
         if (receiver != address(0)) {
            payable(receiver).transfer(userFee);
         }
      } else {
         WETH.safeTransfer(to_, amount_);
         if (receiver != address(0)) {
            WETH.safeTransfer(receiver, userFee);
         }
      }
   }

   function _transferNonFundgibleToken(
      address from_,
      address to_,
      address tokenAddress_,
      uint256 tokenID_,
      uint256 tokenAmount_
   ) internal {
      ITransferManagerNFT manager;
      if (IERC165Upgradeable(tokenAddress_).supportsInterface(INTERFACE_ID_ERC721)) {
         manager = ITransferManagerNFT(ERC721Manager);
         require (tokenAmount_ == 1, 'wrong amount');
      } else if (IERC165Upgradeable(tokenAddress_).supportsInterface(INTERFACE_ID_ERC1155)) {
         manager = ITransferManagerNFT(ERC1155Manager);
      } else {
         revert("not Non Fundgible Token");
      }

      manager.transferNonFungibleToken(tokenAddress_, from_, to_, tokenID_, tokenAmount_);
   }

   function _validateOrder(
      OrderType.MakerOrder memory maker_,
      bytes32 orderHash_
   ) internal view {
      require (maker_.signer != address(0), 'wrong maker');

      require (
         SignatureChecker.verify(
            orderHash_, 
            maker_.signer, 
            maker_.v, 
            maker_.r, 
            maker_.s,
            DOMAIN_SEPARATOR
         ),
         'verification failed'
      );
   }

   function withDraw() external onlyRole(OWNER_ROLE) {
      uint256 ETHBal_ = address(this).balance;
      uint256 WETHBal_ = WETH.balanceOf(address(this));

      payable(fundAddress).transfer(ETHBal_);
      WETH.transfer(fundAddress, WETHBal_);
   }

   receive() external payable { }
}

