            abi.encodePacked(
                "<text x='10%' y='10%' class='base' dominant-baseline='middle' text-anchor='middle'>#",
                nftNumber,
                "</text>"
            )
        );

        string memory nftOwnerData = string(
            abi.encodePacked(
                "<text x='50%' y='60%' style='font-size:28px' class='base' dominant-baseline='middle' text-anchor='middle'>",
                name,
                "</text><text x='50%' y='66.5%' style='font-size:10px' class='base' dominant-baseline='middle' text-anchor='middle'>",
                position,
                "</text>"
            )
        );

