import {Base64} from "./libraries/Base64.sol";

contract EniaNFT is ERC721, ERC721URIStorage {
    using Counters for Counters.Counter;

    Counters.Counter public _tokenIdCounter;
    mapping(address => bool) public nftOwner;

    uint256 public constant MAX_SUPPLY = 5000;

    constructor() ERC721("Enia", "ENIA") {}

    function mint(string memory name, string memory position) public {
        require(!nftOwner[msg.sender], "You can mint only 1 NFT!");

        require(
            _tokenIdCounter.current() <= MAX_SUPPLY,
            "All NFTs have been minted!"
        );

        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();

        _safeMint(msg.sender, tokenId);
        nftOwner[msg.sender] = true;

        string memory nftNumber = Strings.toString(tokenId);

        string
            memory baseSvg = "<svg xmlns='http://www.w3.org/2000/svg' preserveAspectRatio='xMinYMin meet' viewBox='0 0 350 350'><style>.base{fill:#fff;font-family:helvetica;font-size:20px}</style><rect width='100%' height='100%' fill='#4381bd'/>";

