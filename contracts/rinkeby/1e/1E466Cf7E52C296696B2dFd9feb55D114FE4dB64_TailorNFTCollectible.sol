    using Counters for Counters.Counter;
    using Strings for uint256;

    struct Collection {
        address collectionCreator;
        string baseURI;
        string metadataURI;
        bool soldItem;
        // mapping(NFTCATEGORYID_128 => Counters.Counter) alreadyMinted;
        mapping(uint128 => Counters.Counter) alreadyMinted;
        // mapping(NFTCATEGORYID_128 => uint32) maxSupplies;
        mapping(uint128 => uint32) maxSupplies;
        // mapping(index => NFTCATEGORYID_128) _nftCategoryIdx;
        mapping(uint16 => uint128) _nftCategoryIdx;
        uint16 nbCategories;
    }

    function serializeCollection(Collection storage collection)
        internal
        view
        returns (string memory)
    {
        uint16 _currentIdx;
        uint128 nftCategoryId = collection._nftCategoryIdx[_currentIdx];
        string memory minted_per_category = string(
            abi.encodePacked(
                '{"',
                Strings.toString(nftCategoryId),
                '":',
                Strings.toString(
                    collection.alreadyMinted[nftCategoryId].current()
                )
            )
        );
        string memory maxSupplies = string(
            abi.encodePacked(
                '{"',
                Strings.toString(nftCategoryId),
                '":',
                Strings.toString(collection.maxSupplies[nftCategoryId])
            )
        );
        _currentIdx += 1;

        do {
            nftCategoryId = collection._nftCategoryIdx[_currentIdx];
            uint32 capacity = collection.maxSupplies[nftCategoryId];
            uint32 capacity_minted = uint32(
                collection.alreadyMinted[nftCategoryId].current()
            );

            minted_per_category = string(
                abi.encodePacked(
                    minted_per_category,
                    ', "',
                    Strings.toString(nftCategoryId),
                    '":',
                    Strings.toString(capacity_minted)
                )
            );

            maxSupplies = string(
                abi.encodePacked(
                    maxSupplies,
                    ', "',
                    Strings.toString(nftCategoryId),
                    '":',
                    Strings.toString(capacity)
                )
            );

            _currentIdx += 1;
        } while (_currentIdx != collection.nbCategories);

        minted_per_category = string(
            abi.encodePacked(minted_per_category, "}")
        );

        maxSupplies = string(abi.encodePacked(maxSupplies, "}"));

        return
            string(
                abi.encodePacked(
                    '{"creator": "',
                    Strings.toHexString(
                        uint160(collection.collectionCreator),
                        20
                    ),
                    '", "ticketsSold": ',
                    Strings.boolToString(collection.soldItem),
                    ', "uri": "',
                    abi.encodePacked(
                        collection.baseURI,
                        collection.metadataURI
                    ),
                    '", "maxSupplies": ',
                    abi.encodePacked(maxSupplies),
                    ', "mintedPerCategory": ',
                    abi.encodePacked(minted_per_category),
                    "}"
                )
            );
    }
}

