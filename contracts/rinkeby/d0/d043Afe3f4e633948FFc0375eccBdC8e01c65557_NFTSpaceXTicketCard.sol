pragma solidity ^0.8.0;

import "../access/Ownable.sol";
import "../access/NFTSpaceXAccessControls.sol";
import "../interfaces/INFTSpaceXTicketCard.sol";
import "../token/ERC1155/ERC1155.sol";
import "../utils/math/SafeMath.sol";
import "../utils/libraries/Strings.sol";
import "./meta-transaction/ContextMixin.sol";
import "./meta-transaction/NativeMetaTransaction.sol";

contract NFTSpaceXTicketCard is INFTSpaceXTicketCard, ERC1155, ContextMixin, NativeMetaTransaction, Ownable {
  using Strings for string;
  using SafeMath for uint256;

  NFTSpaceXAccessControls public nftspacexAccessControls;
  string public baseTokenURI;

  string private _name;
  string private _symbol;
  uint256 private _currentId;

  mapping(uint256 => address) public creator;
  mapping(uint256 => uint256) public tokenSupply;
  mapping(uint256 => uint256) public expiryId;

  event BaseTokenURIChanged(string baseTokenURI);

  modifier creatorOnly(uint256 _id) {
    require(creator[_id] == _msgSender(), "NTC: only creator");
    _;
  }

  modifier ownersOnly(uint256 _id) {
    require(balanceOf(_msgSender(), _id) > 0, "NTC: only owners");
    _;
  }

  modifier onlyAdminOrTokenMinter() {
    require(nftspacexAccessControls.hasAdminRole(_msgSender()) ||
      nftspacexAccessControls.hasTokenMinterRole(_msgSender()),
      "NST: only admin or token minter role"
    );
    _;
  }

  constructor (
    string memory name_,
    string memory symbol_,
    string memory baseTokenURI_,
    address nftspacexAccessControls_
  ) ERC1155("") {
    _name = name_;
    _symbol = symbol_;
    baseTokenURI = baseTokenURI_;
    nftspacexAccessControls = NFTSpaceXAccessControls(nftspacexAccessControls_);
    _initializeEIP712(name_);
  }

  function name() public view override returns (string memory) {
    return _name;
  }

  function symbol() public view override returns (string memory) {
    return _symbol;
  }

  function factorySchemaName() public pure override returns (string memory) {
    return "ERC1155";
  }

  function tokenURI(uint256 id) public view override returns (string memory) {
    return string(abi.encodePacked(baseTokenURI, Strings.toString(id), ".json"));
  }

  function totalSupply(uint256 id) public view override returns (uint256) {
    return tokenSupply[id];
  }

  function currentIdSupported() public view override returns (uint256) {
    return _currentId;
  }

  function isIdExpired(uint256 id) public view override returns (bool expiry) {
    expiry = _isIdExpired(id);
  }

  function setBaseTokenURI(string memory _baseTokenURI) public override onlyOwner {
    baseTokenURI = _baseTokenURI;
    emit BaseTokenURIChanged(_baseTokenURI);
  }

  function create(
    address initialOwner,
    uint256 initialSupply,
    uint256 expiry,
    bytes memory data
  ) public override onlyAdminOrTokenMinter {
    require(expiry > block.timestamp, "NTC: expiry invalid");

    ++_currentId;
    creator[_currentId] = _msgSender();
    expiryId[_currentId] = expiry;

    _mint(initialOwner, _currentId, initialSupply, data);

    tokenSupply[_currentId] = initialSupply;
  }

  function mint(
    address to,
    uint256 quantity,
    bytes memory data
  ) public virtual override creatorOnly(_currentId) {
    require(!_isIdExpired(_currentId), "NTC: id expired");
    _mint(to, _currentId, quantity, data);
    tokenSupply[_currentId] = tokenSupply[_currentId].add(quantity);
  }

  function burn(address account, uint256 id, uint256 amount) public override {
    require(_isIdExpired(id), "NTC: id not expired");
    require(
      account == _msgSender() || isApprovedForAll(account, _msgSender()),
      "ERC1155: transfer caller is not owner nor approved"
    );
    require(balanceOf(account, id) >= amount, "NTC: insufficient");

    _burn(account, id, amount);
    tokenSupply[id] = tokenSupply[id].sub(amount);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 id,
    uint256 amount,
    bytes memory data
  ) public virtual override(ERC1155, IERC1155) {
    require(!_isIdExpired(id), "NTC: id expired");
    require(
      from == _msgSender() || isApprovedForAll(from, _msgSender()),
      "ERC1155: caller is not owner nor approved"
    );
    _safeTransferFrom(from, to, id, amount, data);
  }

  function safeBatchTransferFrom(
    address from,
    address to,
    uint256[] memory ids,
    uint256[] memory amounts,
    bytes memory data
  ) public virtual override(ERC1155, IERC1155) {
    for (uint256 i = 0; i < ids.length; ++i) {
      require(!_isIdExpired(ids[i]), "NTC: id expired");
      require(
        from == _msgSender() || isApprovedForAll(from, _msgSender()),
        "ERC1155: transfer caller is not owner nor approved"
      );
      _safeTransferFrom(from, to, ids[i], amounts[i], data);
    }
  }

  function setCreator(address _to, uint256[] memory _ids) public {
    require(_to != address(0), "NTC: zero address");
    for (uint256 i = 0; i < _ids.length; i++) {
      uint256 id = _ids[i];
      _setCreator(_to, id);
    }
  }

  function _setCreator(address _to, uint256 _id) internal creatorOnly(_id) {
    creator[_id] = _to;
  }

  function _isIdExpired(uint256 _id) internal view returns (bool) {
    if (!_exists(_id)) {
      return false;
    }
    return expiryId[_id] < block.timestamp;
  }

  function _exists(uint256 _id) internal view returns (bool) {
    return creator[_id] != address(0);
  }

  function _msgSender() internal override view returns (address sender) {
    return ContextMixin.msgSender();
  }
}

