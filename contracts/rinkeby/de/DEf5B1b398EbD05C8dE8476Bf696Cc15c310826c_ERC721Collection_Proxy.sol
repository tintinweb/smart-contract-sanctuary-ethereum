/**
 * This interface is used to deploy the TransparentUpgradeableProxy contract
 */
interface IProxyFactory {
    function deployProxyContract(
        address _owner,
        address _logic,
        bytes memory _data
    ) external returns (ERC721Collection_Proxy _proxy);
}

contract ERC721Collection_Proxy is
    NFTSetter,
    OwnableUpgradeable
{
    using Counters for Counters.Counter;
    Counters.Counter private tokenId;

    // Initialize the initial data of a contract
    function initialize(
        address _owner,
        string memory _name,
        string memory _symbol,
        uint256 _tokenSupply,
        string memory _uri,
        uint96 _royaltyRate
    ) external initializer {
        __ERC721_init(_name, _symbol);
        _grantRole(ADMIN_ROLE, _owner);
        _setRoleAdmin(ADMIN_ROLE, ADMIN_ROLE);
        _transferOwnership(_owner);
        _setDefaultRoyalty(_owner, _royaltyRate);
        TOKEN_SUPPLY = _tokenSupply;
        baseURI = _uri;
    }

    // User with Minter Role can mint token by calling this function
    function mintToken(address _receiver) external onlyRole(MINTER_ROLE) {
        require(_receiver != address(0), "Receiver required");
        uint256 _tokenId = tokenMinted();
        require(_tokenId < TOKEN_SUPPLY, "Reached max supply");
        _safeMint(_receiver, _tokenId);
        tokenId.increment();
    }

    // Every marketplace looks for this function to get the uri of a token
    function tokenURI(uint256 _tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(_tokenId),
            "Invalid TokenId"
        );
        string memory notRevealedUri = _baseURI();
        if (!revealed) return notRevealedUri;

        string memory token_URI = _tokenURI[_tokenId];
        return bytes(token_URI).length > 0 ? token_URI : "";
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function tokenMinted() public view returns (uint256) {
        return tokenId.current();
    }

}

contract ERC721Factory_Proxy /* is OwnableUpgradeable */ {
    struct TokenDetails {
        address tokenAddress;
        string name;
        string symbol;
        uint256 tokenSupply;
    }

    IProxyFactory public proxyFactory;
    address public owner;
    bool private initialized;

    mapping(address => TokenDetails[]) userNFTCollection;

    // event CreateERC721Collection(
    //     address indexed _tokenAddress,
    //     uint256 _tokenSupply,
    //     string _baseURI,
    //     uint96 _royaltyRate,
    //     address indexed _owner
    // );

    // replacement of constructer, this function must be called only once.
    // required if you want your contract compatible to upgradeability
    function initialize(IProxyFactory _proxyFactory) external /* initializer */ {
        require(!initialized, "Already initialized");
        owner = msg.sender;
        proxyFactory = _proxyFactory;
        initialized = true;
    }

    // to update the address of Proxy Factory
    // may not be used ever
    function setProxyFactoryAddress(IProxyFactory _proxyFactory) external /* onlyOwner */ {
        require(msg.sender == owner, "only owner");
        proxyFactory = _proxyFactory;
    }

    // This function deploys the child contract
    function createERC721Collection(
        string memory _name,
        string memory _symbol,
        uint256 _tokenSupply,
        string memory _baseURI,
        uint96 _royaltyRate
    ) external {
        // erc721 contract will be deployed
        ERC721Collection_Proxy token = new ERC721Collection_Proxy();
        // pass token contract address as parameter to deploy the proxy contract.
        ERC721Collection_Proxy proxy = IProxyFactory(proxyFactory).deployProxyContract(msg.sender, address(token), "");
        // once proxy is deployed, call the initialize function to set initial values
        proxy.initialize(
            msg.sender,
            _name,
            _symbol,
            _tokenSupply,
            _baseURI,
            _royaltyRate
        );
        userNFTCollection[msg.sender].push(
            TokenDetails({
                tokenAddress: address(proxy),
                name: _name,
                symbol: _symbol,
                tokenSupply: _tokenSupply
            })
        );
        // emit CreateERC721Collection(
        //     address(token),
        //     _tokenSupply,
        //     _baseURI,
        //     _royaltyRate,
        //     msg.sender
        // );
    }

    // List of contract details deployed from this factory
    function getUserCollections(address _owner)
        external
        view
        returns (TokenDetails[] memory collectionList)
    {
        collectionList = userNFTCollection[_owner];
        return collectionList;
    }
}

