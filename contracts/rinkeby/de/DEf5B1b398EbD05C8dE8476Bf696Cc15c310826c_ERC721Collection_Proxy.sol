pragma solidity ^0.8.11;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";

abstract contract NFTSetter is AccessControlUpgradeable, ERC2981, ERC721Upgradeable {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    uint256 public TOKEN_SUPPLY;

    string public contractURI;
    string public baseURI;

    bool public revealed;

    mapping(uint256 => string) internal _tokenURI;

    // Can be used to set contract level metadata (required for opensea to set royalty fee)
    function setContractURI(string calldata _contractURI)
        external
        onlyRole(ADMIN_ROLE)
    {
        contractURI = _contractURI;
    }

    // to set or update default royalty fee for every token
    function setDefaultRoyalty(address _receiver, uint96 _royaltyRate)
        external
        onlyRole(ADMIN_ROLE)
    {
        require(_receiver != address(0), "Receiver required");
        _setDefaultRoyalty(_receiver, _royaltyRate);
    }

    // to set or update royalty fee of a specific token id
    function setTokenRoyalty(
        uint256 _tokenId,
        address _receiver,
        uint96 _royaltyRate
    ) external onlyRole(ADMIN_ROLE) {
        require(
            _exists(_tokenId),
            "Invalid TokenId"
        );
        require(_receiver != address(0), "Receiver required");
        _setTokenRoyalty(_tokenId, _receiver, _royaltyRate);
    }

    // to set or update total token supply
    function setTokenSupply(uint256 _tokenSupply)
        external
        onlyRole(ADMIN_ROLE)
    {
        TOKEN_SUPPLY = _tokenSupply;
    }

    // to reveal the actual NFT. DEFAULT_VALUE: FALSE
    function setReveal(bool _revealed) external onlyRole(ADMIN_ROLE) {
        // require(revealed != _revealed, "Value should not be same");
        revealed = _revealed;
    }

    // to set or update the baseUri.
    function setBaseURI(string memory _uri) external onlyRole(ADMIN_ROLE) {
        baseURI = _uri;
    }

    // to set or update the uri of a specific token id
    function setTokenURI(uint256 _tokenId, string memory _uri)
        external
        onlyRole(ADMIN_ROLE)
    {
        require(
            _exists(_tokenId),
            "Invalid TokenId"
        );
        _tokenURI[_tokenId] = _uri;
    }

    // required
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721Upgradeable, AccessControlUpgradeable, ERC2981)
        returns (bool)
    {
        return
            ERC721Upgradeable.supportsInterface(interfaceId) ||
            ERC2981.supportsInterface(interfaceId);
    }
}

