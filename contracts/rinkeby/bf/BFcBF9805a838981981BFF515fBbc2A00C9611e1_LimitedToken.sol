	function _safeMintBatch(
		address account,
		uint256[] memory ids,
		uint256[] memory amounts,
		bytes memory data
	) internal virtual {
		_mintBatch(account, ids, amounts, data);

		_doSafeBatchTransferAcceptanceCheck(_msgSender(), address(0), account, ids, amounts, data);
	}

