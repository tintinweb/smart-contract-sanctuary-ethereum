pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
import "./SafeMath.sol";
import "./ContextMixin.sol";

/// @custom:security-contact [emailÂ protected]
contract DJOBTokenUpgradeableV3 is
    ERC1155PausableUpgradeable,
    OwnableUpgradeable,
    ContextMixin
{
    uint256 private _currentTokenID;

    string public name;
    string public symbol;
    string private _generic_uri;
    string private _generic_contract_uri;
    address private _creator;

    struct Metadata {
        string _uri;
        string _offchain_hash; // this is calculated off-chain, only stored on-chain for proof of integrity of uri
        bool _revoked;
        bool _transferable;
        bool _soulbound;
    }

    // only one issuer per token id
    mapping(uint256 => address) public issuers; // issuers[tokenId] < > address of issuer
    mapping(uint256 => uint256) public tokenCapSupply; // tokenCapSupply[tokenId] < > quantity
    mapping(uint256 => uint256) public tokenExistingSupply; // tokenExistingSupply[tokenId] < > quantity
    mapping(uint256 => Metadata) public tokenMetadata; // tokenMetadata[tokenId] < > Metadata
    mapping(uint256 => uint256) public royalties; // royalties[tokenId] < > basisPoints
    mapping(uint256 => address) private _royaltiesRecipient; // royaltiesRecipient[tokenId] < > address of recipient

    // ADDED IN V2
    uint16 private _version;

    modifier onlyCreator() {
        require(msg.sender == _creator, "ONLY_CREATOR_ALLOWED");
        _;
    }

    modifier onlyIssuer(uint256 _id) {
        require(issuers[_id] == msg.sender, "ONLY_ISSUER_ALLOWED");
        _;
    }

    event CreatorTransferred(
        address indexed previousOwner,
        address indexed newOwner
    );

    event SetRoyaltiesBasisPoints(
        uint256 _tokenId,
        uint256 previousBasisPoints,
        uint256 newBasisPoints
    );

    function initialize(string memory _name, string memory _symbol)
        public
        initializer
    {
        _currentTokenID = 0;
        name = _name;
        symbol = _symbol;
        _generic_uri = "http://example.com/foo/path/uri.json";
        _generic_contract_uri = "https://metadata-url.com/my-metadata";
        _creator = msg.sender;
        __ERC1155_init(_generic_uri);
        __Ownable_init();
    }

    // OWNER SPAN OF CONTROL

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function setIssuer(address _to, uint256[] memory _ids) public onlyOwner {
        require(_to != address(0), "setIssuer: INVALID_ADDRESS.");
        for (uint256 i = 0; i < _ids.length; i++) {
            uint256 id = _ids[i];
            _setIssuer(_to, id);
        }
    }

    function _setIssuer(address _to, uint256 _id) internal {
        issuers[_id] = _to;
    }

    function setCreator(address newOwner) public onlyOwner {
        _setCreator(newOwner);
    }

    function _setCreator(address newOwner) internal {
        require(newOwner != address(0), "setCreator: INVALID_ADDRESS.");
        emit CreatorTransferred(_creator, newOwner);
        _creator = newOwner;
    }

    // CREATOR SPAN OF CONTROL

    function setGenerics(
        string memory generic_uri,
        string memory generic_contract_uri
    ) public onlyCreator {
        _generic_uri = generic_uri;
        _generic_contract_uri = generic_contract_uri;
    }

    function create(
        address _issuer,
        uint256 _tokenCapSupply,
        string memory _uri,
        string memory _offchain_hash,
        bool _transferable,
        bool _soulbound,
        uint256 _royaltiesBasisPoints
    ) external onlyCreator returns (uint256) {
        uint256 _id = _getNextTokenID();
        _currentTokenID = _id;
        issuers[_id] = _issuer;

        if (bytes(_uri).length == 0) {
            _uri = _generic_uri;
        }
        royalties[_id] = _royaltiesBasisPoints;
        tokenMetadata[_id]._uri = _uri;
        tokenMetadata[_id]._offchain_hash = _offchain_hash;
        tokenMetadata[_id]._transferable = _transferable;
        tokenMetadata[_id]._soulbound = _soulbound;
        emit URI(_uri, _id);

        tokenCapSupply[_id] = _tokenCapSupply;
        return _id;
    }

    function batchCreate(
        address _issuer,
        uint256[] memory _tokensCapSupply,
        string[] memory _uris,
        string[] memory _offchain_hashes,
        bool _transferable,
        bool _soulbound,
        uint256[] memory _royaltiesBasisPoints
    ) external onlyCreator returns (uint256) {
        require(
            (_uris.length == _offchain_hashes.length &&
                _uris.length == _tokensCapSupply.length),
            "MISMATCH_ARRAY_SIZE"
        );
        uint256 _id;
        for (uint256 i = 0; i < _uris.length; i++) {
            _id = _getNextTokenID();
            _currentTokenID = _id;
            issuers[_id] = _issuer;
            if (bytes(_uris[i]).length == 0) {
                tokenMetadata[_id]._uri = _generic_uri;
            } else {
                tokenMetadata[_id]._uri = _uris[i];
            }
            royalties[_id] = _royaltiesBasisPoints[i];
            tokenCapSupply[_id] = _tokensCapSupply[i];
            tokenMetadata[_id]._offchain_hash = _offchain_hashes[i];
            tokenMetadata[_id]._transferable = _transferable;
            tokenMetadata[_id]._soulbound = _soulbound;

            emit URI(_uris[i], _id);
        }
        return _id;
    }

    // ISSUERS SPAN OF CONTROL

    function setTokenUriHash(
        uint256 _id,
        string memory _uri,
        string memory _offchain_hash
    ) public onlyIssuer(_id) {
        require(_exists(_id), "NONEXISTENT_TOKEN");
        tokenMetadata[_id]._uri = _uri;
        tokenMetadata[_id]._offchain_hash = _offchain_hash;
        emit URI(_uri, _id);
    }

    function setTokenCapSupply(uint256 _id, uint256 _quantityToAdd)
        public
        onlyIssuer(_id)
    {
        require(_exists(_id), "NONEXISTENT_TOKEN");
        tokenCapSupply[_id] = SafeMath.add(tokenCapSupply[_id], _quantityToAdd);
    }

    function setBooleanRevoked(uint256 _id, bool _revoked)
        public
        onlyIssuer(_id)
    {
        require(_exists(_id), "NONEXISTENT_TOKEN");
        tokenMetadata[_id]._revoked = _revoked;
    }

    function setBooleanTransferable(uint256 _id, bool _transferable)
        public
        onlyIssuer(_id)
    {
        require(_exists(_id), "NONEXISTENT_TOKEN");
        tokenMetadata[_id]._transferable = _transferable;
    }

    function setBooleanSoulbound(uint256 _id, bool _soulbound)
        public
        onlyIssuer(_id)
    {
        require(_exists(_id), "NONEXISTENT_TOKEN");
        tokenMetadata[_id]._soulbound = _soulbound;
    }

    function mint(
        // ASSUME all tokenIds have the same unique issuer
        uint256 _id,
        uint256 _quantity,
        bytes memory _data
    ) public onlyIssuer(_id) {
        require(
            (tokenExistingSupply[_id] + _quantity) <= tokenCapSupply[_id],
            "EXCEEDED_TOKEN_CAP"
        );
        _mint(issuers[_id], _id, _quantity, _data);
        tokenExistingSupply[_id] = SafeMath.add(
            tokenExistingSupply[_id],
            _quantity
        );
    }

    function batchMint(
        uint256[] memory _ids,
        uint256[] memory _quantities,
        bytes memory _data
    ) public onlyIssuer(_ids[0]) {
        require((_ids.length == _quantities.length), "MISMATCH_ARRAY_SIZE");
        address _issuer = issuers[_ids[0]];
        for (uint256 i = 0; i < _ids.length; i++) {
            require(
                (tokenExistingSupply[_ids[i]] + _quantities[i]) <=
                    tokenCapSupply[_ids[i]],
                "EXCEEDED_TOKEN_CAP"
            );
            // ASSUME all tokenIds have the same unique issuer, as the first element in the _ids list
            require(issuers[_ids[i]] == _issuer, "ISSUERS MISMATCH");
            uint256 _id = _ids[i];
            uint256 quantity = _quantities[i];
            tokenExistingSupply[_id] = SafeMath.add(
                tokenExistingSupply[_id],
                quantity
            );
        }
        _mintBatch(_issuer, _ids, _quantities, _data);
    }

    // PUBLIC FONCTIONS

    function uri(uint256 _id) public view override returns (string memory) {
        require(_exists(_id), "NONEXISTENT_TOKEN");
        return tokenMetadata[_id]._uri;
    }

    function tokenUriHash(uint256 _id)
        public
        view
        returns (string memory, string memory)
    {
        require(_exists(_id), "NONEXISTENT_TOKEN");
        return (tokenMetadata[_id]._uri, tokenMetadata[_id]._offchain_hash);
    }

    function getCreator() public view returns (address) {
        return _creator;
    }

    // INTERNAL / PRIVATE WITHOUT MODIFIERS

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function _exists(uint256 _id) internal view returns (bool) {
        return issuers[_id] != address(0);
    }

    function _getNextTokenID() private view returns (uint256) {
        return SafeMath.add(_currentTokenID, 1);
    }

    /** @dev EIP2981 royalties implementation. */

    // Maintain flexibility to modify royalties basis points
    function setRoyaltiesBasisPoints(uint256 _tokenId, uint256 _basisPoints)
        external
        onlyCreator
    {
        require(_exists(_tokenId), "NONEXISTENT_TOKEN");
        emit SetRoyaltiesBasisPoints(
            _tokenId,
            royalties[_tokenId],
            _basisPoints
        );
        royalties[_tokenId] = _basisPoints;
    }

    function setRoyaltiesRecipient(uint256 _tokenId, address _recipient)
        external
        onlyCreator
    {
        require(_exists(_tokenId), "NONEXISTENT_TOKEN");
        _royaltiesRecipient[_tokenId] = _recipient;
    }

    // EIP2981 standard royalties return.
    function royaltyInfo(uint256 _tokenId, uint256 _salePrice)
        external
        view
        returns (address receiver, uint256 royaltyAmount)
    {
        address _recipient = _creator;
        if (_royaltiesRecipient[_tokenId] != address(0)) {
            _recipient = _royaltiesRecipient[_tokenId];
        }
        return (
            _recipient,
            SafeMath.div(SafeMath.mul(_salePrice, royalties[_tokenId]), 10000)
        );
    }

    // Adds EIP2981 to ERC1155 and ERC165 Interface returns.
    // interfaceId 0xd9b67a26 -> ERC1155
    // interfaceId 0x2a55205a -> IERC2981
    // interfaceId 0x0e89341c -> ERC-165
    // interfaceId 0x01ffc9a7 -> EIP165
    // interfaceId 0x80ac58cd -> ERC721
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override
        returns (bool)
    {
        return (interfaceId == type(IERC2981).interfaceId ||
            super.supportsInterface(interfaceId));
    }

    // https://docs.opensea.io/docs/polygon-basic-integration
    // https://github.com/alxrnz2/ERC1155-with-EIP2981-for-OpenSea#meta-transactions-in-parkpicssol-via-import-of-contextmixinsol
    /** @dev Meta-transactions override for OpenSea. */

    function _msgSender() internal view override returns (address sender) {
        return ContextMixin.msgSender();
    }

    /** @dev Contract-level metadata for OpenSea. */

    // Update for collection-specific metadata.
    // This is just for OpenSea to find your metadata containing the royalties.
    // This metadata is about the contract and not the individual NFTs
    function contractURI() public view returns (string memory) {
        return _generic_contract_uri;
    }

    // added in V2
    function getVersion() public view returns (uint16) {
        return _version;
    }

    function setVersion(uint16 version) public onlyOwner {
        _version = version;
    }

    function getCharles() public pure returns (string memory) {
        return "charless";
    }
    // COMMENT
}

