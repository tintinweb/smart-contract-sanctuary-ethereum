        rates[_size][_rarity].cost = _cost;
        rates[_size][_rarity].total = _total;
        rates[_size][_rarity].valid = true;
    }
    function deleteRate(uint256 _size, string memory _rarity) public  {
        delete rates[_size][_rarity];
    }
	function mintSuperRare(uint256 _mintAmount, uint256 _size, bytes32[] calldata proof) public payable nonReentrant{
        string memory rarity = "Super Rare";
        require(rates[_size][rarity].valid == true, "Plot size not valid" );
        uint256 balance = SafeMath.mul(msg.value,10 ** 18);
        uint256 s = rates[_size][rarity].sold;
        require(status != 0, "Sale not started yet." );
        if(status == 1) require(isWhitelisted(msg.sender, proof, _mintAmount), "Not whitelisted");
        require(_mintAmount > 0, "0" );
        require(_mintAmount <= maxMint, "Too many" );
        require(s + _mintAmount <= rates[_size][rarity].total, "Max" );
        require(balance >= SafeMath.mul(rates[_size][rarity].cost,_mintAmount));
        for (uint256 i = 0; i < _mintAmount; ++i) {
        _safeMint(msg.sender, s + i, "");
        rates[_size][rarity].sold++;
        }
        delete s;   
        emit _mintSuperRare(msg.sender, rarity, _size, _mintAmount);  
	}
