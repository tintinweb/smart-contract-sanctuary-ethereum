    function addTokens(string calldata _uri, uint256 _amount, uint256 _price)
      public onlyRole(TEAM_ROLE) {
        if (status == Status.Live || status == Status.Done)
            revert PadNotPreparing();
        IERC20(padToken).transferFrom(_msgSender(), address(this), _amount);
        Item storage tokenItem = items[0];
        tokenItem.amount += _amount;
        tokenItem.pricePerUnit = _price;
        emit PadFunded(_uri, _amount, _price);
    }

    ///@dev buy tokens of the crowdsale
    ///@dev payed with the token defined
    ///@param _amount of tokens required
    function buyTokens(uint256 _amount) public nonReentrant padChecks {
        if (items[0].pricePerUnit == 0) revert NoTokenPriceSet();
        address sender = _msgSender();
        Stake storage user = staked[sender];
        uint256 cost = investmentChecks(sender, 0, _amount);
        uint256 amountCorrected = _amount * 10 ** padTokenDecimals;
        processPayment(cost);
        uint256 returnAmount = amountCorrected * percenteageReturn / 1000;
        IERC20(padToken).transfer(sender, returnAmount);
        items[0].amount -= amountCorrected;
        if (percenteageReturn != 1000 && user.vestContract == address(0)) {
          address _clone = ClonesUpgradeable.clone(vestWallet);
          address payable newVest = payable(_clone);
          SimpleVest(newVest).init(sender, startVest, durationVest);
          user.vestContract = address(newVest);
          emit VestCreated(sender, _clone);
        }
        IERC20(padToken).transfer(user.vestContract, amountCorrected - returnAmount);
        emit TokensBought(sender, _amount);
    }

