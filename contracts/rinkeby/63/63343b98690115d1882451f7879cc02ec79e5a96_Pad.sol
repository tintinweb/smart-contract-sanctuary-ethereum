      uint256 _lotteryAnswer
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
      lotteryAnswer = _lotteryAnswer;
      lotteryDone = true;
    }

    ///@dev reveal chainlinkVRF value and sets winners
    ///@param winners amount of winners desired
    function revealLottery(uint256 winners) external onlyRole(KAPPATEAM_ROLE){
      if (!lotteryDone) revert NotAllowed();
      for (uint256 i = 0; i < winners; i++) {
        for (uint256 j = 0; j < ticketSum;) {
          uint256 winNumber = (uint256(keccak256(abi.encode(lotteryAnswer, j))) % ticketSum);
          address user = lotteryDistribution[winNumber];
          if (!lotteryWinners.contains(user)) {
            lotteryWinners.add(user);
            emit LotteryWinner(user);
            break;
          } else {
            unchecked {
              j++;
            }
          }
        }
      }
    }

    ///@dev TEST to prepare lottery users and data
    ///@dev will create the lotteryDistribution and lottery and sum ticketSum
    function applyToLottery() public nonReentrant {
      if (status != Status.Live) revert PadIsNotLive();
      if (ticketPrice == 0) revert NoTokenPriceSet();
      address sender = _msgSender();
      if (lottery.contains(sender)) revert NotAllowed();
      uint256 stakedValue = staked[sender].amount;
      if (stakedValue >= lastTierMinAllocation) revert NotAllowed();
      if (stakedValue < ticketPrice) revert ValueError();
      lottery.add(sender);
      uint256 tickets = stakedValue / ticketPrice;
      for (uint256 j = 0; j <= tickets; j++) {
        lotteryDistribution[ticketSum + j] = sender;
      }
      ticketSum += tickets;
    }

