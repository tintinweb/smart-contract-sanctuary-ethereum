    function tierValue(uint256 amount, address who) internal view returns(uint8 tier) {
      if(whitelist[who]) return tier = 5; // only if limits are different
      else if (amount >= firstTierMinAllocation) {
          tier = 0;
      } else if (amount >= secondTierMinAllocation) {
          tier = 1;
      } else if (amount >= thirdTierMinAllocation) {
          tier = 2;
      } else if (amount >= lastTierMinAllocation) {
          tier = 3;
      } else {
          tier = 4; // address for lottery & only registered users
      }
    }

    ///@dev checks the investment values
    function investmentChecks(
      address sender,
      uint256 _itemId,
      uint256 _amount
    ) internal returns(uint256 cost) {
      if (_amount == 0) revert ValueError();
      uint256 itemsAvailable = items[_itemId].amount;
      if (itemsAvailable < _amount) revert ItemAmountNotAvailable();
      Stake storage user = staked[sender];
      cost = _amount * items[_itemId].pricePerUnit;
      uint256 tierId;
      if(whitelist[sender]){
        tierId = 5;
      } else {
        tierId = getTier(sender);
      }
      uint256 newTotal = user.invested + cost;
      if (newTotal < minInvest[tierId]) revert UserMinLimitNotReached();
      if (newTotal > maxInvest[tierId]) revert UserMaxLimitReached();
      user.invested = newTotal;
    }

