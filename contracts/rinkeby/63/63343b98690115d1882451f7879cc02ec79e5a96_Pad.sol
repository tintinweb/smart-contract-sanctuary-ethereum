    function setLotteryWinners(address[] calldata _addressList)
      public
      onlyRole(KAPPATEAM_ROLE)
    {
      for (uint256 i =0; i < _addressList.length;) {
        address check = _addressList[i];
        if (!lotteryWinners.contains(check)) {
          lotteryWinners.add(check);
        }
        unchecked {
          i++;
        }
      }
    }
    ///@dev automatic function to get the chainlinkVRF value
