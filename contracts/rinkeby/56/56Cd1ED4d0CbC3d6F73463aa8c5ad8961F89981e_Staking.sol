pragma solidity 0.8.7;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

/// @title StartMining NFTs Collection
/// @author cd33
contract StartMining1155 is Ownable, ERC1155 {
    using Strings for uint256;

    using SafeERC20 for IERC20;
    IERC20 private usdt;
    // IERC20 private usdt = IERC20(0x55d398326f99059fF775485246999027B3197955); // USDT sur la BSC
    address private constant recipient =
        0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266; // Client
    AggregatorV3Interface internal priceFeed =
        AggregatorV3Interface(0x8A753747A1Fa494EC906cE90E9f37563A8AF630e); // Chainlink ETH/USD

    bool public notPaused;

    struct Pool {
        // attribuer le meme pour les deux nft de la meme pool ?
        uint8 tokenId; // faire un uint8[] ids ?
        string typeMachine;
        string area;
        uint256 costElectricity;
        uint256 lifeTime;
    }

    uint8 public tokenIdMachine = 1; // NFT Minage Paris
    uint8 public tokenIdInfra = 2; // NFT Infra Paris
    uint16 public nextNFTMachine;
    uint16 public nextNFTInfra;
    uint16 public limitNFTMachine = 125; // passer en uint8 ?
    uint16 public limitNFTInfra = 250; // passer en uint8 ?

    uint8[] public existingNFT = [tokenIdMachine, tokenIdInfra];

    uint16 public publicSalePrice = 100; // 100$ USD

    string public baseURI;

    /// @notice mapping of a pool to an id.
    mapping(uint8 => Pool) public poolInfos;

    /// @notice event emitted when a new pool of NFT is ready to be minted.
    event InitializedMintMachineInfra(
        uint8 _tokenIdMachine,
        uint8 _tokenIdInfra
    );

    /**
     * @notice Constructor of the contract ERC1155.
     * @param _baseURI Metadatas for the ERC1155.
     */
    constructor(string memory _baseURI, IERC20 _usdt) ERC1155(_baseURI) {
        // TESTS: IERC20 usdt Ã  supprimer
        baseURI = _baseURI;
        poolInfos[tokenIdMachine] = Pool(
            tokenIdMachine,
            "machine incroyable",
            "Paris",
            50,
            1722079628
        );
        poolInfos[tokenIdInfra] = Pool(
            tokenIdInfra,
            "infra de fou",
            "Paris",
            300,
            1722079628
        );
        usdt = _usdt;
    }

    /**
     * @notice Enables only externally owned accounts (= users) to mint.
     */
    modifier callerIsUser() {
        require(tx.origin == msg.sender, "Caller is a contract");
        _;
    }

    /**
     * @notice Secures the mint.
     * @param _tokenId Id of the token to mint.
     * @param _quantity Number of tokens to mint.
     */
    modifier mintRequires(uint8 _tokenId, uint16 _quantity) {
        _isAuthorized(_tokenId, _quantity);
        _;
        _mintAccepted(_tokenId, _quantity);
    }

    /**
     * @notice Set of requires before a mint.
     * @param _tokenId Id of the token to mint.
     * @param _quantity Number of tokens to mint.
     */
    function _isAuthorized(uint8 _tokenId, uint16 _quantity) private view {
        require(
            _tokenId != 0 &&
                (_tokenId == tokenIdMachine || _tokenId == tokenIdInfra),
            "Wrong NFT"
        );
        require(_quantity > 0, "Quantity min 1");
        require(notPaused, "Sale unavailable for the moment");
        if (_tokenId == tokenIdMachine) {
            require(nextNFTMachine + _quantity <= limitNFTMachine, "Sold out");
        }
        if (_tokenId == tokenIdInfra) {
            require(nextNFTInfra + _quantity <= limitNFTInfra, "Sold out");
        }
    }

    /**
     * @notice Increments variables and mint after checks.
     * @param _tokenId Id of the token to mint.
     * @param _quantity Number of tokens to mint.
     */
    function _mintAccepted(uint8 _tokenId, uint16 _quantity) private {
        if (_tokenId == tokenIdMachine) {
            nextNFTMachine += _quantity;
        }
        if (_tokenId == tokenIdInfra) {
            nextNFTInfra += _quantity;
        }
        _mint(msg.sender, _tokenId, _quantity, "");
    }

    /**
     * @notice Get the current BNB/USD price.
     * @dev The function uses the chainlink aggregator.
     * @return int Price value.
     */
    function getLatestPrice() public view returns (int256) {
        (, int256 price, , , ) = priceFeed.latestRoundData();
        return price;
    }

    /**
     * @notice Requires approve from msg.sender to this contract upstream.
     * @param _tokenamount Dollar amount sent from msg.sender to recipient.
     */
    function _acceptPayment(uint256 _tokenamount) private {
        usdt.safeTransferFrom(msg.sender, recipient, _tokenamount);
    }

    /**
     * @notice Changes the variable notPaused to allow or not the sale.
     */
    function setPause() external onlyOwner {
        notPaused = !notPaused;
    }

    /**
     * @notice Change the base URI.
     * @param _newBaseURI New base URI.
     **/
    function setBaseUri(string calldata _newBaseURI) external onlyOwner {
        baseURI = _newBaseURI;
    }

    /**
     * @notice Change the token's image URI, override for OpenSea traits compatibility.
     * @param _tokenId Id of the token.
     * @return string Token's metadatas URI.
     */
    function uri(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(
            _tokenId > 0 && _tokenId < 256 && _doesNftExist(uint8(_tokenId)),
            "NFT doesn't exist"
        );
        return string(abi.encodePacked(baseURI, _tokenId.toString(), ".json"));
    }

    /**
     * @notice Change the publicSalePrice.
     * @param _newPrice New public sale price.
     **/
    function setPublicSalePrice(uint16 _newPrice) external onlyOwner {
        publicSalePrice = _newPrice;
    }

    /**
     * @notice Change the Pool informations of an NFT.
     * @param _tokenId Id of the token.
     * @param _area The area where the machine is located.
     * @param _costElectricity The average cost of electricity.
     **/
    function setPoolInformation(
        uint8 _tokenId,
        string calldata _area,
        uint256 _costElectricity
    ) external onlyOwner {
        require(_doesNftExist(_tokenId), "NFT doesn't exist");
        // poolInfos[_tokenId] = Pool(_tokenId, _typeMachine, _area, _costElectricity, _lifeTime);
        poolInfos[_tokenId].area = _area;
        poolInfos[_tokenId].costElectricity = _costElectricity;
    }

    /**
     * @notice Checks if the NFT exists.
     * @param _tokenId Id of the token.
     * @return bool NFT exists(true) or not(false).
     */
    function _doesNftExist(uint8 _tokenId) private view returns (bool) {
        for (uint8 i = 0; i < existingNFT.length; i++) {
            if (_tokenId == existingNFT[i]) {
                return true;
            }
        }
        return false;
    }

    // Ajouter publicSalePrice ? Vont ils changer les prix entre chaque collection ?

    /**
     * @notice Initialize a new pool of NFT ready to be minted.
     * @dev It's possible to create a single NFT on both.
     * @param _tokenIdMachine Id of the token machine.
     * @param _tokenIdInfra Id of the token infra.
     * @param _limitMachine Maximum number of units machine.
     * @param _limitInfra Maximum number of units infra.
     * @param _typeMachine The type of machine.
     * @param _area The area where the machine is located.
     * @param _costElectricity The average cost of electricity.
     * @param _lifeTime The life time.
     **/
    function initMintMachineInfra(
        uint8 _tokenIdMachine,
        uint8 _tokenIdInfra,
        uint16 _limitMachine,
        uint16 _limitInfra,
        string calldata _typeMachine,
        string calldata _area,
        uint256 _costElectricity,
        uint256 _lifeTime
    ) external onlyOwner {
        if (_tokenIdMachine == 0) {
            require(
                _limitMachine == 0 &&
                    _tokenIdInfra != 0 &&
                    _tokenIdInfra < 255 &&
                    _tokenIdInfra % 2 == 0 &&
                    _limitInfra != 0,
                "Violation of the rules of init"
            );
        }
        if (_tokenIdInfra == 0) {
            require(
                _limitInfra == 0 &&
                    _tokenIdMachine != 0 &&
                    _tokenIdMachine < 254 &&
                    _tokenIdMachine % 2 != 0 &&
                    _limitMachine != 0,
                "Violation of the rules of init"
            );
        }
        if (_tokenIdMachine != 0 && _tokenIdInfra != 0) {
            require(
                _limitMachine != 0 && _limitInfra != 0,
                "Limits can't be zero"
            );
            require(
                _tokenIdMachine + 1 == _tokenIdInfra && _tokenIdInfra % 2 == 0,
                "NFTs of the pool must be such: X(odd),Y(Even=X+1)"
            );
        }
        tokenIdMachine = _tokenIdMachine;
        tokenIdInfra = _tokenIdInfra;
        limitNFTMachine = _limitMachine;
        limitNFTInfra = _limitInfra;
        nextNFTMachine = 0;
        nextNFTInfra = 0;
        // publicSalePrice = _publicSalePrice;
        if (_tokenIdMachine != 0) {
            if (!_doesNftExist(_tokenIdMachine)) {
                existingNFT.push(_tokenIdMachine);
                poolInfos[_tokenIdMachine] = Pool(
                    _tokenIdMachine,
                    _typeMachine,
                    _area,
                    _costElectricity,
                    _lifeTime
                );
            } else {
                poolInfos[_tokenIdMachine].area = _area;
                poolInfos[_tokenIdMachine].costElectricity = _costElectricity;
            }
        }
        if (_tokenIdInfra != 0) {
            if (!_doesNftExist(_tokenIdInfra)) {
                existingNFT.push(_tokenIdInfra);
                poolInfos[_tokenIdInfra] = Pool(
                    _tokenIdInfra,
                    _typeMachine,
                    _area,
                    _costElectricity,
                    _lifeTime
                );
            } else {
                poolInfos[_tokenIdInfra].area = _area;
                poolInfos[_tokenIdInfra].costElectricity = _costElectricity;
            }
        }
        emit InitializedMintMachineInfra(_tokenIdMachine, _tokenIdInfra);
    }

    // MINTS
    /**
     * @notice Mint in XXX during the public sale.
     * @param _tokenId Id of the token to mint.
     * @param _quantity Number of tokens to mint.
     */
    function mintMachineInfra(uint8 _tokenId, uint16 _quantity)
        external
        payable
        callerIsUser
        mintRequires(_tokenId, _quantity)
    {
        // require(
        //     msg.value >=
        //         (_quantity * publicSalePrice * 10**26) /
        //             uint256(getLatestPrice()),
        //     "Not enough funds"
        // );

        // TESTS
        require(
            msg.value >=
                (uint256(_quantity) * uint256(publicSalePrice) * 10**26) /
                    162150000000,
            "Not enough funds"
        );
        // TESTS
        payable(recipient).transfer(address(this).balance);
    }

    /**
     * @notice Mint in USDT during the public sale.
     * @param _tokenId Id of the token to mint.
     * @param _quantity Number of tokens to mint.
     */
    function mintMachineInfraUSDT(uint8 _tokenId, uint16 _quantity)
        external
        payable
        callerIsUser
        mintRequires(_tokenId, _quantity)
    {
        _acceptPayment(uint256(_quantity) * uint256(publicSalePrice) * 10**18);
    }

    /**
     * @notice Allows the owner to offer NFTs. Attention, this function is not subject to the limit number of each NFT
     * @param _to Receiving address.
     * @param _tokenId Id of the token.
     * @param _quantity Number of tokens to mint.
     */
    function gift(
        // mintRequires ?
        address _to,
        uint8 _tokenId,
        uint16 _quantity
    ) external onlyOwner {
        require(_quantity > 0, "Quantity min 1");
        require(_tokenId > 0 && _doesNftExist(_tokenId), "NFT doesn't exist");
        _mint(_to, _tokenId, _quantity, "");
    }
}

