pragma solidity 0.8.7;

import "./StartMining1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "hardhat/console.sol";

contract Staking is Ownable, ERC1155Holder, ReentrancyGuard {
    StartMining1155 private nft;

    uint256 public stakingUnlock = 1 days;

    bool public notPaused;

    struct ItemInfo {
        uint8 tokenId;
        uint16 amount;
        uint256 timestamp;
        string addressBTC;
    }

    struct Staker {
        address owner;
        // string addressBTC;
        mapping(uint256 => ItemInfo[]) multiToken;
        uint8[] ownedMultiTokens;
    }

    /// @notice mapping of a staker to its wallet.
    mapping(address => Staker) public stakers;

    /// @notice event emitted when a user has staked a nft.
    event Staked1155(address owner, uint8 id, uint16 amount);

    /// @notice event emitted when a user has staked several nft.
    event StakedBatch1155(address owner, uint256[] ids, uint256[] amounts);

    /// @notice event emitted when a user has unstaked a nft.
    event Unstaked1155(address owner, uint8 id, uint16 amount);

    /// @notice event emitted when a user has unstaked several nft.
    event UnstakedBatch1155(address owner, uint256[] ids, uint256[] amounts);

    /**
     * @notice Constructor of the contract Staking.
     * @param _nft Address of the mint contract.
     */
    constructor(StartMining1155 _nft) {
        nft = _nft;
    }

    /**
     * @notice Enables only externally owned accounts (= users) to mint.
     */
    modifier callerIsUser() {
        require(tx.origin == msg.sender, "Caller is a contract");
        _;
    }

    /**
     * @notice Changes the variable notPaused to allow or not the staking.
     */
    function setPause() external onlyOwner {
        notPaused = !notPaused;
    }

    /**
     * @notice Changes the minimum period before it's possible to unstake.
     */
    function setUnlockPeriod(uint256 _period) external onlyOwner {
        stakingUnlock = _period;
    }

    /**
     * @notice Allows you to stake an NFT in the desired quantity.
     * @param tokenId Id of the token to stake.
     * @param tokenAmount Number of tokens to stake.
     * @param addressBTC BTC address that will receive the rewards.
     */
    function stakeERC1155(
        uint8 tokenId,
        uint16 tokenAmount,
        string calldata addressBTC
    ) external callerIsUser {
        require(notPaused, "Staking unavailable for the moment");
        require(tokenAmount > 0, "Limit min 1");
        require(
            bytes(addressBTC).length > 25 && bytes(addressBTC).length < 36,
            "Wrong length address"
        );
        require(nft.balanceOf(msg.sender, tokenId) > 0, "Not owner");
        _stakeERC1155(tokenId, tokenAmount, addressBTC);
    }

    /**
     * @notice Private function used in stakeERC1155.
     * @param _tokenId Id of the token to stake.
     * @param _tokenAmount Number of tokens to stake.
     * @param _addressBTC BTC address that will receive the rewards.
     */
    function _stakeERC1155(
        uint8 _tokenId,
        uint16 _tokenAmount,
        string calldata _addressBTC
    ) private nonReentrant {
        nft.safeTransferFrom(
            msg.sender,
            address(this),
            _tokenId,
            _tokenAmount,
            ""
        );
        Staker storage staker = stakers[msg.sender];
        ItemInfo memory info = ItemInfo(
            _tokenId,
            _tokenAmount,
            block.timestamp,
            _addressBTC
        );
        if (staker.owner == address(0)) {
            staker.owner = msg.sender;
        }
        if (staker.multiToken[_tokenId].length == 0) {
            staker.ownedMultiTokens.push(_tokenId);
        }
        staker.multiToken[_tokenId].push(info);
        // if(keccak256(bytes(staker.addressBTC)) != keccak256(bytes(_addressBTC))) {staker.addressBTC = _addressBTC;}
        emit Staked1155(msg.sender, _tokenId, _tokenAmount);
    }

    /**
     * @notice Allows you to unstake an NFT staked.
     * @param tokenId Id of the token to unstake.
     * @param tokenIndex Index of the token to unstake in the mapping.
     */
    function unstakeERC1155(uint8 tokenId, uint16 tokenIndex)
        external
        callerIsUser
    {
        require(notPaused, "Staking unavailable for the moment");
        require(
            stakers[msg.sender].multiToken[tokenId].length != 0,
            "No NFT staked"
        );
        require(
            tokenIndex < stakers[msg.sender].multiToken[tokenId].length,
            "Out of bounds token index"
        );
        require(
            stakers[msg.sender].multiToken[tokenId][tokenIndex].amount != 0,
            "Not owner"
        ); // Not sure if it's really useful

        uint256 elapsedTime = block.timestamp -
            stakers[msg.sender].multiToken[tokenId][tokenIndex].timestamp;
        require(
            stakingUnlock < elapsedTime,
            "Unable to unstake before the minimum period"
        );
        _unstakeERC1155(tokenId, tokenIndex);
    }

    /**
     * @notice Private function used in unstakeERC1155.
     * @param _tokenId Id of the token to unstake.
     * @param _tokenIndex Index of the token to unstake in the mapping.
     */
    function _unstakeERC1155(uint8 _tokenId, uint16 _tokenIndex)
        private
        nonReentrant
    {
        Staker storage staker = stakers[msg.sender];
        uint16 tokenAmount = staker.multiToken[_tokenId][_tokenIndex].amount;

        staker.multiToken[_tokenId][_tokenIndex] = staker.multiToken[_tokenId][
            staker.multiToken[_tokenId].length - 1
        ];
        staker.multiToken[_tokenId].pop();

        if (staker.multiToken[_tokenId].length == 0) {
            for (uint256 _j = 0; _j < staker.ownedMultiTokens.length; _j++) {
                if (staker.ownedMultiTokens[_j] == _tokenId) {
                    staker.ownedMultiTokens[_j] = staker.ownedMultiTokens[
                        staker.ownedMultiTokens.length - 1
                    ];
                    staker.ownedMultiTokens.pop();
                    break;
                }
            }
        }

        nft.safeTransferFrom(
            address(this),
            msg.sender,
            _tokenId,
            tokenAmount,
            ""
        );
        emit Unstaked1155(msg.sender, _tokenId, tokenAmount);
    }

    /**
     * @notice Allows you to stake several NFT in the desired quantity.
     * @param tokenIds Ids of the tokens to stake.
     * @param tokenAmounts Numbers of tokens to stake.
     * @param addressBTC BTC address that will receive the rewards.
     */
    function batchStakeERC1155(
        uint256[] memory tokenIds,
        uint256[] memory tokenAmounts,
        string calldata addressBTC
    ) external callerIsUser {
        require(notPaused, "Staking unavailable for the moment");
        require(
            tokenIds.length == tokenAmounts.length,
            "Ids and amounts length mismatch"
        );
        require(
            bytes(addressBTC).length > 25 && bytes(addressBTC).length < 36,
            "Wrong length address"
        );
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(nft.balanceOf(msg.sender, tokenIds[i]) > 0, "Not owner");
            require(tokenAmounts[i] > 0, "Limit min 1");
        }
        _batchStakeERC1155(tokenIds, tokenAmounts, addressBTC);
    }

    /**
     * @notice Private function used in batchStakeERC1155.
     * @param _tokenIds Ids of the token to stake.
     * @param _tokenAmounts Numbers of tokens to stake.
     * @param _addressBTC BTC address that will receive the rewards.
     */
    function _batchStakeERC1155(
        uint256[] memory _tokenIds,
        uint256[] memory _tokenAmounts,
        string calldata _addressBTC
    ) private nonReentrant {
        nft.safeBatchTransferFrom(
            msg.sender,
            address(this),
            _tokenIds,
            _tokenAmounts,
            ""
        );
        Staker storage staker = stakers[msg.sender];

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            uint8 tokenId = uint8(_tokenIds[i]);
            ItemInfo memory info = ItemInfo(
                tokenId,
                uint16(_tokenAmounts[i]),
                block.timestamp,
                _addressBTC
            );
            if (staker.owner == address(0)) {
                staker.owner = msg.sender;
            }
            if (staker.multiToken[tokenId].length == 0) {
                staker.ownedMultiTokens.push(tokenId);
            }
            staker.multiToken[tokenId].push(info);
        }

        emit StakedBatch1155(msg.sender, _tokenIds, _tokenAmounts);
    }

    /**
     * @notice Allows you to unstake several NFT staked.
     * @param tokenIds Ids of the token to unstake.
     * @param tokenIndexes Indexes of the tokens to unstake in the mapping.
     */
    function batchUnstakeERC1155(
        uint256[] memory tokenIds,
        uint256[] memory tokenIndexes
    ) external callerIsUser {
        require(notPaused, "Staking unavailable for the moment");
        require(
            tokenIds.length == tokenIndexes.length,
            "Ids and indexes length mismatch"
        );
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                stakers[msg.sender].multiToken[tokenIds[i]].length != 0,
                "No NFT staked"
            );
            require(
                tokenIndexes[i] <
                    stakers[msg.sender].multiToken[tokenIds[i]].length,
                "Out of bounds token index"
            );
            require(
                stakers[msg.sender]
                .multiToken[tokenIds[i]][tokenIndexes[i]].amount != 0,
                "Not owner"
            ); // Not sure if it's really useful

            uint256 elapsedTime = block.timestamp -
                stakers[msg.sender]
                .multiToken[tokenIds[i]][tokenIndexes[i]].timestamp;
            require(
                stakingUnlock < elapsedTime,
                "Unable to unstake before the minimum period"
            );
        }
        _batchUnstakeERC1155(tokenIds, tokenIndexes);
    }

    /**
     * @notice Private function used in batchUnstakeERC1155.
     * @param _tokenIds Ids of the tokens to unstake.
     * @param _tokenIndexes Indexes of the tokens to unstake in the mapping.
     */
    function _batchUnstakeERC1155(
        uint256[] memory _tokenIds,
        uint256[] memory _tokenIndexes
    ) private nonReentrant {
        Staker storage staker = stakers[msg.sender];
        uint256[] memory tokenAmounts = new uint256[](_tokenIds.length);

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            uint256 tokenId = _tokenIds[i];
            tokenAmounts[i] = staker
            .multiToken[tokenId][_tokenIndexes[i]].amount;

            staker.multiToken[tokenId][_tokenIndexes[i]] = staker.multiToken[
                tokenId
            ][staker.multiToken[tokenId].length - 1];
            staker.multiToken[tokenId].pop();

            if (staker.multiToken[tokenId].length == 0) {
                for (
                    uint256 _j = 0;
                    _j < staker.ownedMultiTokens.length;
                    _j++
                ) {
                    if (staker.ownedMultiTokens[_j] == tokenId) {
                        staker.ownedMultiTokens[_j] = staker.ownedMultiTokens[
                            staker.ownedMultiTokens.length - 1
                        ];
                        staker.ownedMultiTokens.pop();
                        break;
                    }
                }
            }
        }
        nft.safeBatchTransferFrom(
            address(this),
            msg.sender,
            _tokenIds,
            tokenAmounts,
            ""
        );
        emit UnstakedBatch1155(msg.sender, _tokenIds, tokenAmounts);
    }

    /**
     * @notice Returns the ItemInfo[] of a specific NFT staked.
     * @param _user Address of the user.
     * @param _tokenId Id of the token.
     * @return ItemInfo[] Details of tokenId.
     */
    function getStakedERC1155(address _user, uint8 _tokenId)
        external
        view
        returns (ItemInfo[] memory)
    {
        return stakers[_user].multiToken[_tokenId];
    }

    /**
     * @notice Returns the list of NFT staked by the user.
     * @param _user Address of the user.
     * @return uint8[] List of tokenIds.
     */
    function getAllStakedERC1155(address _user)
        external
        view
        returns (uint8[] memory)
    {
        return stakers[_user].ownedMultiTokens;
    }
}

