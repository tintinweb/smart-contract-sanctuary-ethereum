pragma solidity ^0.8.0;

/// @artist: Anna Riddler
/// @author: manifold.xyz

import "@prb/math/contracts/PRBMathSD59x18.sol";
import "./Trigonometry.sol";

contract Solstice {
    using PRBMathSD59x18 for int256;

    // Seasons
    uint8 constant WINTER = 1;
    uint8 constant SPRING = 2;
    uint8 constant SUMMER = 3;
    uint8 constant FALL = 4;
    
    // Floating Point Constants
    int256 constant DAY_IN_SECONDS = 8640000000000000000000;
    int256 constant JULIAN_EPOCH = 2440587500000000000000000;
    int256 constant JULIAN_2000 = 2451545000000000000000000;
    int256 constant JULIAN_CENTURY = 36525000000000000000000;
    
    int256 immutable D2R = PRBMathSD59x18.pi().div(180000000000000000000);

    // Integer Constants
    int256 constant EPOCH_YEAR = 1970;
    int256 constant YEAR_IN_SECONDS = 31536000;

    int256[5] SpringConstants = [int(2451623809840000000000000), int(365242374040000000000000), int(51690000000000000), int(-4110000000000000), int(-570000000000000)];
    int256[5] SummerConstants = [int(2451716567670000000000000), int(365241626030000000000000), int(3250000000000000), int(8880000000000000), int(-300000000000000)];
    int256[5] FallConstants = [int(2451810217150000000000000), int(365242017670000000000000), int(-115750000000000000), int(3370000000000000), int(780000000000000)];
    int256[5] WinterConstants = [int(2451900059520000000000000), int(365242740490000000000000), int(-62230000000000000), int(-8230000000000000), int(320000000000000)];
    
    int256[3][24] terms = 
        [
            [int(485000000000000000000), int(324960000000000000000), int(1934136000000000000000)],
            [int(203000000000000000000), int(337230000000000000000), int(32964467000000000000000)],
            [int(199000000000000000000), int(342080000000000000000), int(20186000000000000000)],
            [int(182000000000000000000), int(27850000000000000000), int(445267112000000000000000)],
            [int(156000000000000000000), int(73140000000000000000), int(45036886000000000000000)],
            [int(136000000000000000000), int(171520000000000000000), int(22518443000000000000000)],
            [int(77000000000000000000), int(222540000000000000000), int(65928934000000000000000)],
            [int(74000000000000000000), int(296720000000000000000), int(3034906000000000000000)],
            [int(70000000000000000000), int(243580000000000000000), int(9037513000000000000000)],
            [int(58000000000000000000), int(119810000000000000000), int(33718147000000000000000)],
            [int(52000000000000000000), int(297170000000000000000), int(150678000000000000000)],
            [int(50000000000000000000), int(21020000000000000000), int(2281226000000000000000)],
            [int(45000000000000000000), int(247540000000000000000), int(29929562000000000000000)],
            [int(44000000000000000000), int(325150000000000000000), int(31555956000000000000000)],
            [int(29000000000000000000), int(60930000000000000000), int(4443417000000000000000)],
            [int(18000000000000000000), int(155120000000000000000), int(67555328000000000000000)],
            [int(17000000000000000000), int(288790000000000000000), int(4562452000000000000000)],
            [int(16000000000000000000), int(198040000000000000000), int(62894029000000000000000)],
            [int(14000000000000000000), int(199760000000000000000), int(31436921000000000000000)],
            [int(12000000000000000000), int(95390000000000000000), int(14577848000000000000000)],
            [int(12000000000000000000), int(287110000000000000000), int(31931756000000000000000)],
            [int(12000000000000000000), int(320810000000000000000), int(34777259000000000000000)],
            [int(9000000000000000000), int(227730000000000000000), int(1222114000000000000000)],
            [int(8000000000000000000), int(15450000000000000000), int(16859074000000000000000)]
        ];

    function currentSeason() public view returns (uint8) {
        int256 currentYear = timestampToYear(block.timestamp);
        int256 julianDay = timestampToJulianDay(block.timestamp);

        if (julianDay < springEquinox(currentYear)) {
            return WINTER;
        } else if (julianDay < summerSolstice(currentYear)) {
            return SPRING;
        } else if (julianDay < fallEquinox(currentYear)) {
            return SUMMER;
        } else if (julianDay < winterSolstice(currentYear)) {
            return FALL;
        }
        return WINTER;
    }

    function springEquinox(int256 year) internal view returns (int256) {
        return eventJulianDay(year - 2000000000000000000000, SpringConstants);
    }

    function summerSolstice(int256 year) internal view returns (int256) {
        return eventJulianDay(year - 2000000000000000000000, SummerConstants);
    }

    function fallEquinox(int256 year) internal view returns (int256) {
        return eventJulianDay(year - 2000000000000000000000, FallConstants);
    }

    function winterSolstice(int256 year) internal view returns (int256) {
        return eventJulianDay(year - 2000000000000000000000, WinterConstants);
    }

    function eventJulianDay(int256 year, int256[5] storage term) internal view returns (int256) {
        int256 J0 = horner(year.mul(1000000000000000), term);
        int256 T = J2000Century(J0);
        int256 W = D2R.mul(T).mul(35999373000000000000000) - D2R.mul(2470000000000000000);
        int256 dL = 1000000000000000000 + Trigonometry.cos(uint(W)).mul(33400000000000000) + Trigonometry.cos(uint(W.mul(2000000000000000000))).mul(700000000000000);
        int256 S = 0;
        for (uint256 i = 24; i > 0; i--) {
            int256[3] storage t = terms[i-1];
            S += t[0].mul(Trigonometry.cos(uint((t[1] + T.mul(t[2])).mul(D2R))));
        }
        return J0 + S.div(dL).mul(10000000000000);
    }

    function horner(int256 x, int256[5] storage c) internal view returns (int256) {
        uint256 i = c.length - 1;
        int256 y = c[i];
        while (i > 0) {
            i--;
            y = y.mul(x) + c[i];
        }
        return y;
    }

    function J2000Century(int256 jde) internal pure returns (int256) {
        return (jde - JULIAN_2000).div(JULIAN_CENTURY);
    }

    function timestampToYear(uint256 timestamp) internal pure returns (int256) {
        return PRBMathSD59x18.fromInt(EPOCH_YEAR + int(timestamp) / YEAR_IN_SECONDS);
    }

    function timestampToJulianDay(uint256 timestamp) internal pure returns (int256) {
        return PRBMathSD59x18.fromInt(int(timestamp)).div(DAY_IN_SECONDS) + JULIAN_EPOCH;
    }
}

