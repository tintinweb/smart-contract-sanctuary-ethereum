contract RedPacket {
    
    address private owner;		//
    
    uint uid = 0;
    address[] userAddressList;

    mapping(uint => address) id2UserDict;
    mapping(address => uint) user2IdDict;
    mapping(address => RedList) use2RedListDict;
    
    struct RedEnvelope{
        IERC20  tokenAddress;
        mapping(address => uint) rewards;       //rewards
        uint leftMoney;		//leftMoney
        string passwd;
        uint _type;
        uint maxReceiver;
        uint averageMoney;
        uint receivedNum;		// num
        uint[] result;
        address[] receiverList;
        uint[] passworkList;  
        uint endTime;      
    }

    
    struct RedList{
        RedEnvelope[] red;
        uint[] lm;
        uint reid;
    }

    
   
    function createRedEnvelope(uint __type,IERC20 tokenAddress,uint amount,string memory passwd, uint maxReceiver, uint timeLength) public returns(uint,uint) {
        
        bool flag = true;
        
        for (uint i = 0; i < userAddressList.length; i++){
            if (userAddressList[i] == msg.sender){
                flag = false;
                break;
            }
        }
        
        if (flag){
            userAddressList.push(msg.sender);
            id2UserDict[uid] = msg.sender;
            user2IdDict[msg.sender] = uid;
            uid++;    
        }
        
        
        require(
            amount > 0,
            "The red envelope's money should lager than 0!"
        );
        
        require(
            maxReceiver > 0,
            "You can't set the receiver number as 0"
        );
        require(
            __type == 0 || __type == 1,
            "Please enter 0 or 1 to select the red envelope type, 0 for randomly, 1 for evenly!"
        );
        
    
        initRedEnvelopeInner(amount,tokenAddress, passwd,__type, maxReceiver,amount/ maxReceiver, block.timestamp + timeLength,msg.sender);
        tokenAddress.transferFrom(msg.sender, address(this), amount);
        
        emit create(msg.sender, user2IdDict[msg.sender], use2RedListDict[msg.sender].reid);
        use2RedListDict[msg.sender].reid++;
        use2RedListDict[msg.sender].lm.push(amount);
    }

     function createEtherRedEnvelope(uint __type,string memory passwd, uint maxReceiver, uint timeLength) public payable returns(uint,uint) {
        
        bool flag = true;
        
        for (uint i = 0; i < userAddressList.length; i++){
            if (userAddressList[i] == msg.sender){
                flag = false;
                break;
            }
        }
        
        if (flag){
            userAddressList.push(msg.sender);
            id2UserDict[uid] = msg.sender;
            uid++;    
        }
        
        
        require(
            msg.value > 0,
            "The red envelope's money should lager than 0!"
        );
        
        require(
            maxReceiver > 0,
            "You can't set the receiver number as 0"
        );
        require(
            __type == 0 || __type == 1,
            "Please enter 0 or 1 to select the red envelope type, 0 for randomly, 1 for evenly!"
        );
        
    
        initRedEnvelopeInner(msg.value,IERC20(address(0)), passwd,__type, maxReceiver,msg.value/ maxReceiver, block.timestamp + timeLength,msg.sender);
        emit create(msg.sender, uid-1, use2RedListDict[msg.sender].reid);
        use2RedListDict[msg.sender].reid++;
        use2RedListDict[msg.sender].lm.push(msg.value);
    }

    function initRedEnvelopeInner(uint256 leftMoney,
        IERC20  _tokenAddress,		
        string memory passwd,
        uint _type,
        uint maxReceiver,
        uint averageMoney,	
        uint endTime,
        address sender ) internal {
            RedEnvelope storage r = use2RedListDict[sender].red.push();
            r.tokenAddress = _tokenAddress;
            r.leftMoney = leftMoney;
            r.passwd = passwd;
            r._type = _type;
            r.maxReceiver = maxReceiver;
            r.averageMoney = averageMoney;
            r.receivedNum = 0;
            r.result = new uint[](0);
            r.receiverList = new address[](0);
            r.endTime = endTime;
        }
 
    function getRewardStatus(uint uid,uint reid) public view returns(uint){
         address user = id2UserDict[uid]; 
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        if(re.rewards[msg.sender] == 0){
            return 0;
        } else {
            return 1;
        }
    }

    function get(uint uid, uint reid, uint signStr) public returns(uint){
        address user = id2UserDict[uid]; 
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        
        // require(
        //     re.passwd == password,
        //     "Wrong Password!"    
        // );
        
        // require(
        //     re._type == 0,
        //     "This red envelope is randomly distributed!" 
        // );
        
        require(
            re.rewards[msg.sender] == 0,
            "You have received your red envelope!"
        );
        require(
            block.timestamp < re.endTime,
            "This red envelope is expired!"
        );
        require(
            re.leftMoney > 0,
            "The red envelope is empty!"
            );		//
            
        if (re._type == 0){
         getRandomMoney(uid, reid);
         
        }
        
        else if (re._type == 1){
         getAverageMoney(uid, reid);
        }
            
    }
 
 
    function getRandomMoney(uint uid, uint reid) internal{
        address user = id2UserDict[uid]; 
        RedList storage rel = use2RedListDict[user];
        RedEnvelope storage re = rel.red[reid];
        
        if (re.receivedNum == re.maxReceiver - 1){		//555
        	re.rewards[msg.sender] = re.leftMoney;
        	re.result.push(re.leftMoney);
        	re.receiverList.push(msg.sender);
            if(address(re.tokenAddress) == address(0)){
                disperseEther(msg.sender, re.leftMoney);
            } else{
                disperseToken(re.tokenAddress,msg.sender,re.leftMoney);
            }
            
            emit getRed(msg.sender, uid, reid,re.leftMoney);
            rel.lm[reid] = 0;
            
            re.leftMoney = 0;
            isDelete(user, rel, re);
            
            
        }

        //0~99
        uint random = uint8(uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty, re.receivedNum)))%100);
        uint temp = random * re.leftMoney / 100; 		//
        
        re.leftMoney -= temp;		//
        if(address(re.tokenAddress) == address(0)){
                disperseEther(msg.sender, temp);
            } else{
                disperseToken(re.tokenAddress,msg.sender,temp);
            }
        rel.lm[reid] -= temp;
        
        re.rewards[msg.sender] = temp;		//
        re.result.push(temp);
        re.receiverList.push(msg.sender);
        emit getRed(msg.sender, uid, reid,temp);
        re.receivedNum++;

    }
    

    function getAverageMoney(uint uid, uint reid) internal{
        address user = id2UserDict[uid]; 
        RedList storage rel = use2RedListDict[user];
        RedEnvelope storage re = rel.red[reid];
        
        require(
            re.receivedNum < re.maxReceiver,
            "This red envelope reached its limit on the maximum number of receivers"
        );

        re.leftMoney -= re.averageMoney;
        if(address(re.tokenAddress) == address(0)){
                disperseEther(msg.sender, re.averageMoney);
            } else{
                disperseToken(re.tokenAddress,msg.sender,re.averageMoney);
            }
        emit getRed(msg.sender, uid, reid,re.averageMoney);
        use2RedListDict[id2UserDict[uid]].lm[reid] -= re.averageMoney;
        
        re.rewards[msg.sender] = re.averageMoney;		//
        re.result.push(re.averageMoney);
        re.receiverList.push(msg.sender);
        
        re.receivedNum++;
        if (re.receivedNum == re.maxReceiver && re.leftMoney != 0){
            re.rewards[msg.sender] += re.leftMoney;
            re.result[re.receivedNum-1] += re.leftMoney;
            if(address(re.tokenAddress) == address(0)){
                disperseEther(msg.sender, re.leftMoney);
            } else{
                disperseToken(re.tokenAddress,msg.sender,re.leftMoney);
            }
            emit getRed(msg.sender, uid, reid,re.leftMoney);
            use2RedListDict[id2UserDict[uid]].lm[reid] = 0;
            
            re.leftMoney = 0;
        }
        
        isDelete(user, rel, re);
      
    }

    function disperseEther(address recipient, uint256 value) internal {
        payable(recipient).transfer(value);
    }

    function disperseToken(IERC20 token, address recipient, uint256 value) internal {
        require(token.transfer(recipient, value));
    }


    
    
    function isDelete(address user, RedList storage rel, RedEnvelope storage re) internal {
        
            bool flag = true;
            if (re.leftMoney == 0){
                uint index;
                
                for (uint i = 0; i < userAddressList.length; i++){
                    if (user == userAddressList[i]){
                        index = i;
                        break;
                    }
                }
                
                for (uint j = 0; j < rel.lm.length; j++){
                    if (rel.lm[j] != 0){
                        flag = false;
                        break;
                    }
                }
                
                if (flag){
                    for (uint k = index; k < userAddressList.length - 1; k++){
                        userAddressList[k] = userAddressList[k+1];
                    }
                    delete userAddressList[userAddressList.length - 1];
                }
            }
    }
   
    
    
    function getSender() public view returns(address [] memory) {
        return userAddressList;
    }
    
    //
    function getRewards(uint uid, uint reid) public view returns(address [] memory,uint [] memory) {
        address user = id2UserDict[uid]; 
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        
        return (re.receiverList,re.result);
    }
    
    event create(address _from, uint uid, uint redid);

    event getRed(address to, uint uid, uint redid,uint money);
    
    function getLeftmoney(uint uid, uint reid) public view returns(uint) {
        address user = id2UserDict[uid]; 
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        
        return re.leftMoney;
    }
    
    function getClaimStatus(uint uid,uint reid) public view returns(uint){
        uint status = 1;
        address user = id2UserDict[uid];
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        if(block.timestamp < re.endTime){
            status = 0;
        }
        if(re.rewards[msg.sender] > 0){
             status = 0;
        }
        if(re.leftMoney <= 0){
             status = 0;
        }
        return status;
       
    }

    function getWithdrawStatus(uint uid, uint reid) public view returns(uint){
        address user = id2UserDict[uid];
        uint status =0;
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        if(block.timestamp < re.endTime){
            status = 0;
        } else {
            if(re.leftMoney == 0){
                status = 2;
            } else status = 1;
                
        }
        return status;
            
    }
    
    
    function withdraw(uint uid, uint reid) public{
        address user = id2UserDict[uid];
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        
        require(
            block.timestamp > re.endTime,
            "You can't withdraw your money before the red envelope is due!"
        );
        require(
            re.leftMoney > 0,
            "This red envelope is empty!"
        );
        require(
            msg.sender == user,
            "You are not the owner of the red envelope!"
        );
        if(address(re.tokenAddress) == address(0)){
                disperseEther(msg.sender, re.leftMoney);
            } else{
                disperseToken(re.tokenAddress,msg.sender,re.leftMoney);
            }
        re.leftMoney = 0;
        use2RedListDict[user].lm[reid] = 0;
        isDelete(user, use2RedListDict[user], re);
        }
}