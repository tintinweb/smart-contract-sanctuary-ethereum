pragma solidity ^0.8.15;
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Royalty.sol";

import "./SignatureValidator.sol";
import "./TokenAsDate.sol";
import "./BigDayToken/BigDayToken.sol";

/// @title The BigDay 
/// @author A/S: AddressedToTheSubject
/// @notice The BigDay NFT project's contract
contract BigDay is  Ownable, ERC721Royalty, Pausable, ReentrancyGuard, SignatureValidator, TokenAsDate {
  using BigDayTokens for BigDayTokens.BigDayToken;
  using Covers for Covers.Cover;
  struct MintPayload{
    uint256 tokenId;
    string label;
    string desc;
    string dateDispl;
    string year;
    string month;
    string day;
    string dropId;
    uint256 timestamp;
    bytes32 nonce;
  }

  string private _contractMetadataURI;
  bool public isPublicSalesOpen = false;
  string public wepAppBaseURL = "ipfs://contract.metadata";
  uint256 public maxCoversAmount = 24;

  
  
  // token contracts by uint
  mapping (uint256 => BigDayTokens.BigDayToken) private _tokens;
  
  modifier _isTokenOwner(uint256 tokenId) {
    require(ownerOf(tokenId) == _msgSender(), "Access restricted to the token owner");
    _;
  }

  modifier _isTokenExists(uint256 tokenId) {
    require(_exists(tokenId), "nonexistent token");
    _;
  }

  constructor(string memory collectionName, string memory tokenName,  string memory _metadataURI, string memory _wepAppBaseURL) SignatureValidator(string.concat(collectionName,":",tokenName)) ERC721(collectionName, tokenName) {
    _contractMetadataURI = _metadataURI;
    wepAppBaseURL = _wepAppBaseURL;
    _setDefaultRoyalty(address(this), 10*_feeDenominator()/100); // 10%
  }

  function mint(MintPayload calldata voucher, bytes calldata signature) external payable whenNotPaused{
        require(isPublicSalesOpen == true, "Public sales are closed now");
        _verifySignature(voucher.tokenId,voucher.label,voucher.desc,voucher.dateDispl,voucher.year,voucher.month,voucher.day,voucher.nonce,signature);
        uint256 price = getAvailability(voucher.tokenId);
        require(price > 0, "The token already taken");
        require(msg.value >= price, "No enough funds");
       
        _safeMint(msg.sender, voucher.tokenId);
        
        BigDayTokens.BigDayToken storage token = _tokens[voucher.tokenId];
        token.covers.push(Covers.Cover(address(this), voucher.tokenId, voucher.label, voucher.desc, "", false));
        token.id = voucher.tokenId;
        token.tokenActiveCover = 0;
        token.dateDisplay = voucher.dateDispl;
        token.attributes = BigDayTokens.Attributes(voucher.year, voucher.month, voucher.day, voucher.dateDispl, voucher.timestamp, voucher.dropId);

        emit BigDayTokens.CoverAdded(voucher.tokenId, 0);
        _incrPriceMultiplier();
  }

  //TODO mintPrivate should take an array of tokens

  function getAvailability(uint256 day)

        public
        view
        _dateBounds(day)
        returns (uint256)
    {
        if (_exists(day)) {
            return 0;
        }
        return _getPrice();
    }

  function tokenURI(uint256 tokenId)
    public
    view
    override
    _isTokenExists(tokenId)
    returns (string memory)
  {
    return _tokens[tokenId].generateMetadataJSON(wepAppBaseURL);
  }
    
  // function _burn(uint256 _tokenId) internal override(ERC721, ERC721Royalty) _isTokenExists(_tokenId) _isTokenOwner(_tokenId) whenNotPaused{
  //   transferFrom(msg.sender, owner(), _tokenId);
  // }


  
  // 
  // TOKEN CONTROL
  // 
  function addCover(
      uint256 tokenId,
      address collection,
      uint256 artId,
      string memory label, 
      string memory desc
    ) external _isTokenOwner(tokenId)
  {
  _tokens[tokenId].addCover(
                          maxCoversAmount,
                          collection,
                          artId,
                          label, 
                          desc,
                          msg.sender
                    );
  }

  function getCovers(uint256 tokenId) public view returns (Covers.Cover[] memory) {
      return _tokens[tokenId].getCovers(_msgSender() == ownerOf(tokenId));
  }
 
  function updateImageUrl(uint256 tokenId, uint256 coverIndex, string memory _imageUrl) external onlyOwner{
    return _tokens[tokenId].updateImageUrl(coverIndex, _imageUrl);
  }

  function setCoversStates(uint256 tokenId, bool[] calldata _visibility) external _isTokenOwner(tokenId) {
    return _tokens[tokenId].setCoversStates(_visibility);
  }

  function changeActiveCover(uint256 tokenId, uint256 newCoverIndex)  external _isTokenOwner(tokenId) {
    return _tokens[tokenId].changeActiveCover(newCoverIndex);
  }
    
  // 
  // ADMIN
  // 

  function pause() public virtual onlyOwner {
    _pause();
  }

  function unpause() public virtual onlyOwner {
    _unpause();
  }

  function setWebAppURL(string memory _wepAppBaseURL) external onlyOwner {
    wepAppBaseURL = _wepAppBaseURL;
  }

  function setPublicSales(bool _value) public onlyOwner {
    isPublicSalesOpen = _value;
  }

  function setMaxCoversAmount(uint256 _maxCoversAmount) external onlyOwner {
    maxCoversAmount = _maxCoversAmount;
  }

  //
  // SYSTEM
  //

  function supportsInterface(bytes4 interfaceId)
    public
    view
    override
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }

  function contractURI() public view returns (string memory) {
    return _contractMetadataURI;
  }

  // TODO withdraw erc20?
  function withdraw() public onlyOwner returns (bool success) {
    uint256 amount = payable(address(this)).balance;
    address account = _msgSender();
    (bool sent, ) = account.call{ value: amount, gas: 20317 }("");
    require(sent, "Exchange: Failed to send Ether");
    return true;
  }

  receive() external payable{} 

  
  fallback() external payable {}
}

