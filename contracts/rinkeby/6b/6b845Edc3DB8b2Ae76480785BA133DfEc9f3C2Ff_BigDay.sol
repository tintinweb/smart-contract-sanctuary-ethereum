pragma solidity ^0.8.15;

import "@openzeppelin/contracts/utils/Counters.sol";
abstract contract TokenAsDate {
    using Counters for Counters.Counter;
    uint256 private constant _HALF_MAX_DAYS = 1_000_000; // 2740 years
    uint256 private _priceBase = 10**16; // 0.01Eth
    uint256 private _basePriceStep = 10**15; // 0.001Eth. 1 eth for 1000 purchases
    Counters.Counter private _basePriceStepMultiplier;
    
    modifier _dateBounds(uint256 day) {
        require(day >= 0 && day <= _HALF_MAX_DAYS * 2, "Out of date bounds");
        _;
    }

    function _getPrice() internal view returns (uint256) {
        return _priceBase + (_basePriceStep * _basePriceStepMultiplier.current());
    }

    function _incrPriceMultiplier() internal {
        _basePriceStepMultiplier.increment();
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.15;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "solidity-json-writer/contracts/JsonWriter.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
// import "@openzeppelin/contracts/utils/Address.sol";

import "hardhat/console.sol";


library Covers {
 struct Cover {
        address collection;
        uint256 artTokenId;
        string label;
        string desc;
        string imageUrl;
        bool hidden;
    }
}

library BigDayTokens {    
    
    using JsonWriter for JsonWriter.Json;
    using Covers for Covers.Cover;
    using BigDayTokens for BigDayToken;

    // events
    event CoverAdded(uint256 indexed tokenId, uint256 coverIndex);
    event CoverUpdated(uint256 indexed tokenId, uint256 coverIndex);
    event ActiveCoverChanged(uint256 indexed tokenId, uint256 fromIndex, uint256 toIndex);
    event coversStatesUpdated(uint256 indexed tokenId);

    struct BigDayToken {
        uint256 id;
        Covers.Cover[] covers;
        uint256 tokenActiveCover; 
        string dateDisplay;
        // string _tokenIdStr;
        Attributes attributes;
    }

    struct Attributes {
        string year;
        string month;
        string day;
        string dateDisplay;
        uint256 timestamp;
        string dropId;
    }

    modifier _isValidCoverIndex(BigDayToken storage self, uint256 coverIndex) {
      require(coverIndex < self.covers.length, "Wrong cover index");
      _;
    }

    function addCover(
        BigDayToken storage self,
        uint256 _maxCoversAmount,
        address _collection,
        uint256 _artId,
        string memory _label, 
        string memory _desc,
        address _caller
    ) public returns (uint256 coverIndex){
        IERC721 collContract = IERC721(_collection);
        address _owner = collContract.ownerOf(_artId);
        require(_owner == _caller, "BDToken: sender have to be an owner");
        Covers.Cover memory cover = Covers.Cover({
            collection: _collection,
            artTokenId: _artId,
            label: _label,
            desc: _desc,
            imageUrl: "",
            hidden: false
        });
        self.covers.push(cover);

        if(self.covers.length >= _maxCoversAmount){
            // save the first cover and shift others
            for(uint i=1; i<self.covers.length; i++){
                self.covers[i]=self.covers[i+1];
            }
            self.covers.pop();
        }
        coverIndex = self.covers.length - 1;
        self.tokenActiveCover = coverIndex;
        emit CoverAdded(self.id, coverIndex);
    }
    function getCovers(BigDayToken storage self, bool includeHidden) external view returns (Covers.Cover[] memory){
        uint coversAmount = 0;
        uint256 totalCovers = self.covers.length;

        if (includeHidden) {
            coversAmount = totalCovers;
        } else {
            for(uint i=0;i<totalCovers; i++){
                if(!self.covers[i].hidden){
                    coversAmount++;
                }
            }
        }
        Covers.Cover[] memory results = new Covers.Cover[](coversAmount);
        coversAmount = 0;
         if (includeHidden) {
            results =  self.covers;
        } else {
            for(uint i=0;i<totalCovers; i++){
                if(!self.covers[i].hidden){
                     results[coversAmount++] = self.covers[i];
                }
            }
        }
        return results;
    }

    function getCover(BigDayToken storage self) external view returns (Covers.Cover memory cover){
        cover = self.covers[self.tokenActiveCover];
    }

    function updateImageUrl(BigDayToken storage self, uint256 coverIndex, string memory _imageUrl)
        public
        _isValidCoverIndex(self, coverIndex)
    {
        self.covers[coverIndex].imageUrl = _imageUrl;
        emit CoverUpdated(self.id, coverIndex);
    }

    function changeActiveCover(BigDayToken storage self, uint256 newIndex)
        internal
        _isValidCoverIndex(self, newIndex)
    {
        require(!self.covers[newIndex].hidden,"Hidden cover could not set as active");
        
        if(self.tokenActiveCover != newIndex){
            emit ActiveCoverChanged(self.id, self.tokenActiveCover, newIndex);
            self.tokenActiveCover = newIndex;
        }
    }
    
    function setCoversStates(
            BigDayToken storage self,
            bool[] calldata _visibility
        ) external {
            require(_visibility.length == self.covers.length - 1, "Wrong array length");
            require(self.tokenActiveCover == 0 || _visibility[self.tokenActiveCover - 1] == true, "Active cover could not be hidden");
            uint256 amount = _visibility.length;
            for(uint256 i = 0; i < amount; i++){
                self.covers[i+1].hidden = !_visibility[i];
            }
            emit coversStatesUpdated(self.id);
    }

    function generateMetadataJSON(BigDayToken storage self, string memory _webAppUrl) external view returns (string memory) {
        JsonWriter.Json memory writer;
        Covers.Cover storage activeCover  = self.covers[self.tokenActiveCover];
        writer = writer.writeStartObject();
        writer = writer.writeStringProperty("name", activeCover.label);
        writer = writer.writeStringProperty("description", activeCover.desc); // string.concat("BigDay token of the " , attributes.dateDisplay)
        writer = writer.writeStringProperty("external_url", _webAppUrl);
        writer = writer.writeStringProperty("image", activeCover.imageUrl);
        writer = writer.writeStringProperty("animation_url", string.concat(_webAppUrl,"/token?d=", Strings.toString(self.id)));
        
        writer = self._generateAttributes(writer);
        writer = writer.writeEndObject();
        return string(
            abi.encodePacked(
                "data:application/json;base64,",
                Base64.encode(abi.encodePacked(writer.value))
            )
        );
    }

    function _addStringAttribute(BigDayToken storage, JsonWriter.Json memory _writer, string memory key, string memory value) internal pure returns (JsonWriter.Json memory writer) {
        writer = _writer.writeStartObject();
        writer = writer.writeStringProperty("trait_type", key);
        writer = writer.writeStringProperty("value", value);
        writer = writer.writeEndObject();
    }

    function _addNumberAttribute(BigDayToken storage, JsonWriter.Json memory _writer, string memory key, uint256 value) internal pure returns (JsonWriter.Json memory writer) {
        writer = _writer.writeStartObject();
        writer = writer.writeStringProperty("trait_type", key);
        writer = writer.writeStringProperty("display_type", "number");
        writer = writer.writeUintProperty("value", value);
        writer = writer.writeEndObject();
    }    

    function _addDateAttribute(BigDayToken storage, JsonWriter.Json memory _writer, string memory key, uint256 value) internal pure returns (JsonWriter.Json memory writer) {
        writer = _writer.writeStartObject();
        writer = writer.writeStringProperty("trait_type", key);
        writer = writer.writeStringProperty("display_type", "date");
        writer = writer.writeUintProperty("value", value);
        writer = writer.writeEndObject();
    }    
    

    function _generateAttributes(BigDayToken storage self, JsonWriter.Json memory _writer) internal view returns (JsonWriter.Json memory writer){
        writer = _writer.writeStartArray("attributes");
        self._addDateAttribute(writer, "Date", self.attributes.timestamp);
        self._addStringAttribute(writer, "Year", self.attributes.year);
        self._addStringAttribute(writer, "Month", self.attributes.month);
        self._addStringAttribute(writer, "Day", self.attributes.day);
        if(bytes(self.attributes.dropId).length > 0){
            self._addStringAttribute(writer, "Drop", self.attributes.dropId);
        }
        self._addNumberAttribute(writer, "Covers", self.covers.length);

        writer = writer.writeEndArray();
    }    
}

