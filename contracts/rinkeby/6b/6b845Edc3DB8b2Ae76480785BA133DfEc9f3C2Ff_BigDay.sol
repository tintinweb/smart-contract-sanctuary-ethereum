pragma solidity ^0.8.9;

import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";
contract SignatureValidator is EIP712 {
  using Address for address;

  mapping(bytes32 => bool) private _expired;

  bytes32 private constant PERMIT_SIGNATURE = keccak256("EIP712(uint256 tokenId,string label,string desc,string dateDispl,string year,string month,string day,bytes32 nonce)");

  constructor(string memory name) EIP712(name, "1.0.0") {}

  function _verifySignature(
    uint256 tokenId,
    string memory label,
    string memory desc,
    string memory dateDispl,
    string memory year,
    string memory month,
    string memory day,
    bytes32 nonce,
    bytes calldata signature
  ) internal {
    require(!_expired[nonce], "SignatureValidator: Expired signature");
    _expired[nonce] = true;

    bool isVerified = _verify(msg.sender, _hash(tokenId, label, desc, dateDispl, year, month, day, nonce), signature);
    require(isVerified, "SignatureValidator: Invalid signature");
  }

  function _hash(
    uint256 tokenId,
    string memory label,
    string memory desc,
    string memory dateDispl,
    string memory year,
    string memory month,
    string memory day,
    bytes32 nonce
  ) private view returns (bytes32) {
    return _hashTypedDataV4(
      keccak256(
        abi.encode(
          PERMIT_SIGNATURE,
          tokenId,
          keccak256(bytes(label)),
          keccak256(bytes(desc)),
          keccak256(bytes(dateDispl)),
          keccak256(bytes(year)),
          keccak256(bytes(month)),
          keccak256(bytes(day)),
          nonce
        )
      )
    );
  }

  function _verify(
    address signer,
    bytes32 digest,
    bytes memory signature
  ) private view returns (bool) {
    return SignatureChecker.isValidSignatureNow(signer, digest, signature);
  }
}

