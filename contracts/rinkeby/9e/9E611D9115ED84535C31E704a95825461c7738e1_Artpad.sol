// File: Artpad.sol


pragma solidity ^0.8.9;







interface IStaking {
    function getHolderCf(address holder) external view returns (uint);
    function getHolderTier(address holder) external view returns (uint);
    function getHolderAmount(address holder) external view returns (uint);
    function getHolderStakingTime(address holder) external view returns (uint);
}


contract Artpad is Ownable {
    using SafeMath for uint;
    using ECDSA for bytes32;

    uint256 public totalTokens;
    uint256 public _price;
    uint256 public _totalUsers;

    bytes32 private merkleRoot;

    mapping(address => User) public users;
    mapping(uint => address) public idToUsers;

    bool public sellingIsActive;

    IERC20 _usdt;
    IERC20 _usdc;

    IERC20 wARTR;
    IStaking staking;

    IERC20 importToken;

    struct User {
        uint id;
        address referrer;
        uint allocation;
        bool registratedBasic;
        bool registratedWhitelist;
        uint256 stakedTokensCount;
        uint256 stakedTokensTime;

        address [] referrersFirsLine;
        address [] referrersSecondLine;
        address [] referrersThirdLine;
    }

    constructor(address _staking, address _wARTR, address _importToken) {
        _usdt = IERC20(0xdAC17F958D2ee523a2206206994597C13D831ec7);
        _usdc = IERC20(0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48);

        staking = IStaking(_staking);
        wARTR = IERC20(_wARTR);
        importToken = IERC20(_importToken);
    }

    //Registration

    function registerUserBasic(address userAddress, address referrerAddress) public {
        require(users[referrerAddress].registratedBasic || users[referrerAddress].registratedWhitelist, "referral address is wrong");
        users[userAddress].registratedBasic = true;

        users[userAddress].referrer = referrerAddress;

        users[referrerAddress].referrersFirsLine.push(userAddress);

        if (isUserExists(users[referrerAddress].referrer)) {
            users[users[referrerAddress].referrer].referrersSecondLine.push(userAddress);
        }
        if (isUserExists(users[users[referrerAddress].referrer].referrer)) {
            users[users[users[referrerAddress].referrer].referrer].referrersThirdLine.push(userAddress);
        }
        _totalUsers++;
        users[userAddress].id = _totalUsers;
        idToUsers[_totalUsers] = userAddress;
    }

    function registerUserWhitelist(address userAddress, address referrerAddress, bytes32[] calldata _merkleProof) public {
        require(users[referrerAddress].registratedBasic || users[referrerAddress].registratedWhitelist, "referral address is wrong");
        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
        require(MerkleProof.verify(_merkleProof, merkleRoot, leaf), "Tokens must be minted from our website");
        users[userAddress].registratedWhitelist = true;
        
        users[userAddress].referrer = referrerAddress;

        users[referrerAddress].referrersFirsLine.push(userAddress);

        if (isUserExists(users[referrerAddress].referrer)) {
            users[users[referrerAddress].referrer].referrersSecondLine.push(userAddress);
        }
        if (isUserExists(users[users[referrerAddress].referrer].referrer)) {
            users[users[users[referrerAddress].referrer].referrer].referrersThirdLine.push(userAddress);
        }
        _totalUsers++;
        users[userAddress].id = _totalUsers;
        idToUsers[_totalUsers] = userAddress;
    }

    function registerAdmin(address userAddress) public onlyOwner {
        users[userAddress].registratedBasic = true;
        users[userAddress].registratedWhitelist = true;
    }

    //Info

    function isUserExists(address userAddress) public view returns (bool) {
        return (users[userAddress].id != 0);
    }

    function referralRewardPayment(address user) public returns (uint) {
        users[user].stakedTokensCount = staking.getHolderAmount(user);
        users[user].stakedTokensTime = staking.getHolderStakingTime(user);
        require(users[user].stakedTokensCount > 15000, "you do not have required quantity of staked coins");
        uint referralReward;
        if (users[user].stakedTokensCount < 50000) {
            if (users[user].stakedTokensTime == 3) {
                referralReward = users[user].stakedTokensCount * 2/100;
            } else {
                if (users[user].stakedTokensTime == 6) {
                    referralReward = users[user].stakedTokensCount * 3/100;
                } else {
                    if (users[user].stakedTokensTime == 12) {
                        referralReward = users[user].stakedTokensCount * 4/100;  
                    }
                }
            }
        } else {
            if (users[user].stakedTokensCount < 100000) {
                if (users[user].stakedTokensTime == 3) {
                referralReward = users[user].stakedTokensCount * 1/100;
                } else {
                    if (users[user].stakedTokensTime == 6) {
                        referralReward = users[user].stakedTokensCount * 2/100;
                    } else {
                        if (users[user].stakedTokensTime == 12) {
                            referralReward = users[user].stakedTokensCount * 1/40;
                        }
                    }
                }
            } else {
                if (users[user].stakedTokensTime == 3) {
                    referralReward = users[user].stakedTokensCount * 1/200;
                } else {
                    if (users[user].stakedTokensTime == 6) {
                        referralReward = users[user].stakedTokensCount * 1/100;
                    } else {
                        if (users[user].stakedTokensTime == 12) {
                            referralReward = users[user].stakedTokensCount * 3/250;
                        }
                    }
                }
            }
        }
        return referralReward;
    }

    function getAllocation(address user) public returns (uint) {
        uint totalBonusAllocation;
        for (uint i = 1; i <=_totalUsers; i++) {

            address userAddress = idToUsers[i];
            uint tier = staking.getHolderTier(user);
            uint timeCf = staking.getHolderCf(user);

            if (tier > 1) {
        
            uint baseAllocation = totalTokens/_totalUsers;

            if (tier == 10) {
                uint bonusAllocation = tier/4 * baseAllocation * (timeCf / 10);
                users[userAddress].allocation += bonusAllocation;
                totalBonusAllocation += bonusAllocation;
            }
            else {
                uint bonusAllocation = tier * baseAllocation * (timeCf / 10);
                users[userAddress].allocation += bonusAllocation;
                totalBonusAllocation += bonusAllocation;
                }    
            }
        }

        uint baseUserAllocation = (totalTokens - totalBonusAllocation)/_totalUsers;
        for (uint i = 1; i <= _totalUsers; i++) {
            users[idToUsers[i]].allocation += baseUserAllocation;
        }        

        return(users[user].allocation);
    }

    //Basic

    function sellingActiveToggle() public onlyOwner {
        sellingIsActive = !sellingIsActive;
    }

    function buyForBNB(uint256 amount) public payable {
        address account = msg.sender;
        uint256 weiAmount = msg.value;
        require(account != address(0), "Receiver is the zero address");
        require(users[account].registratedBasic || users[account].registratedWhitelist, "User doesn't registered");
        require(users[account].allocation <= amount, "Exceeded allocation");
        require(sellingIsActive, "Selling is not active");
        require(weiAmount >= amount * _price, "Exceeded balance");
        importToken.transfer(account, amount);
        totalTokens += amount;
    }

    function buyForUSDT(uint256 amount) public payable {
        address account = msg.sender;
        uint256 weiAmount = _usdt.balanceOf(account);
        require(account != address(0), "Receiver is the zero address");
        require(users[account].registratedBasic || users[account].registratedWhitelist, "User doesn't registered");
        require(users[account].allocation <= amount, "Exceeded allocation");
        require(sellingIsActive, "Selling is not active");
        require(weiAmount >= amount * _price, "Exceeded balance");
        _usdt.transferFrom(account, address(this), weiAmount);
        importToken.transfer(account, amount);
        totalTokens += amount;
    }

    function buyForUSDC(uint256 amount) public payable {
        address account = msg.sender;
        uint256 weiAmount = _usdc.balanceOf(account);
        require(account != address(0), "Receiver is the zero address");
        require(users[account].registratedBasic || users[account].registratedWhitelist, "User doesn't registered");
        require(users[account].allocation <= amount, "Exceeded allocation");
        require(sellingIsActive, "Selling is not active");
        require(weiAmount >= amount * _price, "Exceeded balance");
        _usdc.transferFrom(account, address(this), weiAmount);
        importToken.transfer(account, amount);
        totalTokens += amount;
    }

    //Settings

    function setPrice(uint256 newPrice) public onlyOwner {
        _price = newPrice;
    }

    function setNewRoot(bytes32 _newRoot) public onlyOwner {
        merkleRoot = _newRoot;
    }

    function setUSDC(address _new) public onlyOwner {
        _usdc = IERC20(_new);
    }

    function setUSDT(address _new) public onlyOwner {
        _usdt = IERC20(_new);
    }

    function setwARTR(address _new) public onlyOwner {
        wARTR = IERC20(_new);
    }

    function setStaking(address _new) public onlyOwner {
        staking = IStaking(_new);
    }

    //For tests

    function setUserCountTime(address user, uint count, uint time) public onlyOwner {
        users[user].stakedTokensCount = count;
        users[user].stakedTokensTime = time;
    }

    function setUserAllocation(address user, uint all) public onlyOwner {
        users[user].allocation = all;
    }
}