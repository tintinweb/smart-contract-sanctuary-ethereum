contract Catlantic is ERC721, Ownable, ReentrancyGuard {
    using Strings for uint256;

    uint256 private maxTokens;
    uint256 private maxPresaleTokens;
    uint256 private maxBonusTokens;

    uint256 private saleTokens;

    mapping(uint256 => string) private customTokensURIs;
    uint256 private bonusTokensLength;

    bool private _saleEnabled = false;
    bool private _presaleEnabled = false;
    bool private _presaleEnabled2 = false;
    
    string private _contractURI;
    string  private _baseTokenURI;
    
    address private chestContract = address(0);

    address private serverAddress = address(0);

    address private supportAddress = address(0);

    address private oldContract = address(0);
    
    mapping(bytes => bool) private signatureUsed;

    bool private _tokenRecoverEnabled = false;

    mapping (uint256 => uint256) prices;
    mapping (uint256 => uint256) presalePrices;

    
    constructor(uint256 maxTokens_, uint256 maxPresaleTokens_, uint256 maxBonusTokens_) ERC721("Catlantic","Cat") {
        maxTokens = maxTokens_;
        maxPresaleTokens = maxPresaleTokens_;
        maxBonusTokens = maxBonusTokens_;

        saleTokens = maxTokens - maxBonusTokens;

        presalePrices[1] = 1 ether;
        prices[1] = 1 ether;
        presalePrices[3] = 1 ether;
        prices[3] = 1 ether;
        presalePrices[5] = 1 ether;
        prices[5] = 1 ether;
        presalePrices[10] = 1 ether;
        prices[10] = 1 ether;
    }

    function setOldContract(address target) external onlyOwner {
        oldContract = target;
    }

    function setSupportAddress(address target) external onlyOwner {
        supportAddress = target;
    }

    function setChestContract(address target) external onlyOwner {
        chestContract = target;
    }

    function setServerAddress(address target) external onlyOwner {
        serverAddress = target;
    }

    function setMaxTokens(uint256 _maxTokens) external onlyOwner {
        maxTokens = _maxTokens;
        saleTokens = maxTokens - maxBonusTokens;
    }

    function getMaxTokens()  external view returns(uint256) {
        return maxTokens;
    }

    function setPresaleTokens(uint256 _maxTokens) external onlyOwner {
        maxPresaleTokens = _maxTokens;
    }

    function setBonusTokens(uint256 _maxTokens) external onlyOwner {
        maxBonusTokens = _maxTokens;
        saleTokens = maxTokens - maxBonusTokens;
    }
    
    function saleEnabled() external view returns(bool){
        return _saleEnabled;
    }
    
    function saleOn() external onlyOwner{
        _saleEnabled = true;
    }
    
    function saleOff() external onlyOwner{
        _saleEnabled = false;
    }

    function presaleEnabled() external view returns(bool){
        return _presaleEnabled;
    }
    
    function presaleOn() external onlyOwner{
        _presaleEnabled = true;
    }
    
    function presaleOff() external onlyOwner{
        _presaleEnabled = false;
    }

    function presale2Enabled() external view returns(bool){
        return _presaleEnabled2;
    }
    
    function presale2On() external onlyOwner{
        _presaleEnabled2 = true;
    }
    
    function presale2Off() external onlyOwner{
        _presaleEnabled2 = false;
    }

    function tokenRecoverEnabled() external view returns(bool){
        return _tokenRecoverEnabled;
    }
    
    function tokenRecoverOn() external onlyOwner{
        _tokenRecoverEnabled = true;
    }
    
    function tokenRecoverOff() external onlyOwner{
        _tokenRecoverEnabled = false;
    }

    function totalSupply() public view returns(uint256) {
        return maxTokens;
    }

    function getBonusTokensLength() internal view returns(uint256) {
        return bonusTokensLength;
    }
    
    function setPrices(uint256[][] memory prices_) external onlyOwner {
        for (uint256 i; i < prices_.length; i++) {
            prices[prices_[i][0]] = prices_[i][1];
        }
    }
    
    function setPresalePrices(uint256[][] memory prices_) external onlyOwner {
        for (uint256 i; i < prices_.length; i++) {
            presalePrices[prices_[i][0]] = prices_[i][1];
        }
    }
    
    function loadTokens(address[] memory addresses) external onlyOwner {
        for (uint256 i; i < addresses.length; i++) {
            _mint(addresses[i]);
        }
    }


    function mintA(address _to) public onlyOwner {
        require(tokensAvailableForSale() >= 1, "Max tokens reached");
        _mint(_to);
    }

    function mintA(address _to, uint256 count) public onlyOwner {
        require(tokensAvailableForSale() >= count, "Max tokens reached");
        for (uint256 i; i < count; i++) {
            _mint(_to);
        }
    }


    function presaleMint(bytes memory signature) external payable {
        require(_presaleEnabled, "Presale off");
        require(_owners.length < maxPresaleTokens, "Max tokens reached");
        require(msg.value >= presalePrices[1], "Insufficient value to mint");
        bytes32 message = prefixed(keccak256(abi.encodePacked(msg.sender)));
        require(recoverSigner(message, signature) == serverAddress, "Not WL user");
        _mint(msg.sender);
    }

    function presaleMint(uint256 count, bytes memory signature) external payable {
        require(_presaleEnabled, "Presale off");
        require(_owners.length + count <= maxPresaleTokens, "Max tokens reached");
        require(msg.value >= presalePrices[count] && presalePrices[count] > 0, "Insufficient value to mint");
        bytes32 message = prefixed(keccak256(abi.encodePacked(msg.sender)));
        require(recoverSigner(message, signature) == serverAddress, "Not WL user");
        for (uint256 i; i < count; i++) {
            _mint(msg.sender);
        }
    }

    function presaleMint() external payable {
        require(_presaleEnabled2, "Presale off");
        require(_owners.length < maxPresaleTokens, "Max tokens reached");
        require(msg.value >= presalePrices[1], "Insufficient value to mint");
        _mint(msg.sender);
    }

    function presaleMint(uint256 count) external payable {
        require(_presaleEnabled2, "Presale off");
        require(_owners.length + count <= maxPresaleTokens, "Max tokens reached");
        require(msg.value >= presalePrices[count] && presalePrices[count] > 0, "Insufficient value to mint");
        for (uint256 i; i < count; i++) {
            _mint(msg.sender);
        }
    }
    

    function mint() external payable {
        require(_saleEnabled, "Sale off");
        require(msg.value >= prices[1], "Insufficient value to mint");
        require(tokensAvailableForSale() > 0, "Max tokens reached");
        _mint(msg.sender);
    }

    function mint(uint256 count) external payable {
        require(_saleEnabled, "Sale off");
        require(msg.value >= prices[count] && prices[count] > 0, "Insufficient value to mint");
        require(tokensAvailableForSale() >= count, "Max tokens reached");
        for (uint256 i; i < count; i++) {
            _mint(msg.sender);
        }
    }


    function mintS(address to_) external payable {
        require(msg.sender == supportAddress);
        _mint(to_);
    }
    
    function mintS(address to_, string memory uri) external payable {
        require(msg.sender == supportAddress);
        _mint(to_);
        customTokensURIs[_owners.length - 1] = uri;
    }    

    function mintS(address to_, uint256 count) external payable {
        require(msg.sender == supportAddress);
        for (uint256 i; i < count; i++) {
            _mint(to_);
        }
    }
    
    function mintS(address to_, uint256 count, string memory uri) external payable {
        require(msg.sender == supportAddress);
        for (uint256 i; i < count; i++) {
            _mint(to_);
            customTokensURIs[_owners.length - 1] = uri;
        }
    }
    

    function mintS2(uint256 count, string memory uri, string memory salt, bytes memory signature) external payable {
        bytes32 message = prefixed(keccak256(abi.encodePacked(msg.value, msg.sender, count, uri, salt)));
        require(recoverSigner(message, signature) == serverAddress, "Error");
        signatureUsed[signature] = true;
        for (uint256 i; i < count; i++) {
            _mint(msg.sender);
            customTokensURIs[_owners.length - 1] = uri;
        }
    }


    function recoverTokens(uint256 count, bytes memory signature) external payable {
        require(_tokenRecoverEnabled);
        require(!signatureUsed[signature], "Error");
        bytes32 message = prefixed(keccak256(abi.encodePacked(msg.sender, count)));
        require(recoverSigner(message, signature) == serverAddress, "Error");
        signatureUsed[signature] = true;
        for (uint256 i; i < count; i++) {
            _mint(msg.sender);
        }
    }

    function recoverTokensB(uint256[] memory tokens, bytes memory signature) external payable {
        require(_tokenRecoverEnabled);
        require(!signatureUsed[signature], "Error");
        bytes32 message = prefixed(keccak256(abi.encodePacked(msg.sender, tokens)));
        require(recoverSigner(message, signature) == serverAddress, "Error");
        signatureUsed[signature] = true;
        Catlantic oldContract_ = Catlantic(oldContract);
        for (uint256 i; i < tokens.length; i++) {
            _mint(msg.sender);
            oldContract_.burn(tokens[i]);
        }
    }


    function burn(uint256 tokenId) external {
        require(ownerOf(tokenId) == msg.sender || msg.sender == owner(), "You dont have this token");
        _burn(tokenId);
    }


    function bonusMint_(address to, string memory uri) internal {
        require(getBonusTokensLength() < maxBonusTokens, "Max tokens reached");
        _mint(to);
        customTokensURIs[_owners.length - 1] = uri;
        bonusTokensLength++;
    }

    function bonusMint(address to, string memory uri) external {
        require(msg.sender == chestContract, "You dont have rights to use this function");
        bonusMint_(to, uri);
    }

    function treasureMintA(address to, string memory uri) external onlyOwner {
        bonusMint_(to, uri);
    }

    function treasureMint(address to, string memory uri, string memory salt, bytes memory signature) external {
        require(!signatureUsed[signature], "Error");
        bytes32 message = prefixed(keccak256(abi.encodePacked(to, uri, salt)));
        require(recoverSigner(message, signature) == serverAddress, "Error");
        signatureUsed[signature] = true;
        bonusMint_(to, uri);
    }

    function treasureMintS(address to, string memory uri) external {
        require(msg.sender == serverAddress, "You dont have rights to use this function");
        bonusMint_(to, uri);
    }


    function splitSignature(bytes memory sig) internal pure returns (uint8 v, bytes32 r, bytes32 s) {
        require(sig.length == 65);
        assembly {
            // first 32 bytes, after the length prefix
            r := mload(add(sig, 32))
            // second 32 bytes
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 bytes)
            v := byte(0, mload(add(sig, 96)))
        }
        return (v, r, s);
    }


    function recoverSigner(bytes32 message, bytes memory sig) internal pure returns (address) {
        (uint8 v, bytes32 r, bytes32 s) = splitSignature(sig);
        return ecrecover(message, v, r, s);
    }
    
    function prefixed(bytes32 hash) internal pure returns (bytes32) {
        return keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", hash));
    }

    function setTokenURI(uint256 token, string memory uri) external onlyOwner {
        customTokensURIs[token] = uri;
    }

    function contractURI() public view returns (string memory) {
    	return _contractURI;
    }
    
    function withdraw() external onlyOwner nonReentrant
    {
        Address.sendValue(payable(msg.sender), address(this).balance);
    }

    function tokensAvailableForSale() public view returns (uint256) {
        return saleTokens - _owners.length + bonusTokensLength;
    }

    function tokensAvailable() public view returns (uint256) {
        return maxTokens - _owners.length;
    }

    function presaleTokensAvailable() public view returns (uint256) {
        return maxPresaleTokens - _owners.length;
    }

    function _baseURI() internal view override  returns (string memory) {
        return _baseTokenURI;
    }

    function setBaseURI(string memory uri) external onlyOwner {
        _baseTokenURI = uri;
    }
    
    function setContractURI(string memory uri) external onlyOwner {
        _contractURI = uri;
    }
    
    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        if(bytes(customTokensURIs[tokenId]).length != 0) return customTokensURIs[tokenId];
        string memory baseURI = _baseURI();
        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.toString(), ".json")) : "";
    }

    function URI(uint256 tokenId) external view virtual returns (string memory) {
        return tokenURI(tokenId);
    }
}