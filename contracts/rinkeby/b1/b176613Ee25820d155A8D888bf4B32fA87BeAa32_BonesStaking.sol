pragma solidity >=0.8.9 <0.9.0;

import '@openzeppelin/contracts/token/ERC20/ERC20.sol';
import '@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol';
import 'erc721a/contracts/extensions/ERC721AQueryable.sol';
import './BonesRewards.sol';

contract BonesStaking is Ownable, IERC721Receiver {

    struct Vault {
        ERC721AQueryable nft;
        BonesRewards token;
        ERC721AQueryable pair;
        uint multiplier; // @Dev, multiplier x10 so we can have decimals. ie: 2.5% needs to be entered here as 25
        uint[] specialTokens;
        uint specialMultiplier;
        string name;
    }

    struct Staked {
        uint tokenId;
        uint timestamp;
        uint multiplier;
        bool paired;
        uint pairedTokenId;
        string stakedVault;
        address owner;
    }

    uint public totalStaked;
    uint private baseEarnedPerDay = 5;

    mapping(bytes32 => Vault) public vault; // vaultKey => Vault
    mapping(bytes32 => mapping(uint => Staked)) public staked; // vaultKey => tokenId => Staked
    mapping(address => mapping(bytes32 => uint[])) public ownerStaked; // Owner wallet => vaultKey => [tokenIds]
    mapping(address => uint) public ownerStakedCount; // Owner wallet => staked count
    mapping(address => mapping(uint => bool)) public initialClaimed; // Contract address => tokenId => claimed
    
    event NFTStaked(address owner, uint tokenId, uint value);
    event NFTPairStaked(address owner, uint tokenId, uint pairedTokenId, uint value);
    event NFTUnstaked(address owner, uint tokenId, uint value);
    event NFTPairUnstaked(address owner, uint tokenId, uint pairedTokenId, uint value);
    event Claimed(address owner, uint amount);

    // ~~~~~~~~~~~~~~~~~~~~ Staking ~~~~~~~~~~~~~~~~~~~~
    /**
    * @dev Transaction limited to 5 tokens at a time. 
    */
    function stake(ERC721AQueryable _nft, uint[] calldata _tokenIds) external {
        require(_tokenIds.length <= 5, "Too many tokens for one transaction!");
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, address(0)));
        uint tokenId;
        Vault memory vaultInfo = vault[vaultKey];
        require(address(vaultInfo.pair) == address(0), "Cannot stake single token to paired vault!");
        require(address(vaultInfo.nft) != address(0), "This vault does not exist");
        for (uint i = 0; i < _tokenIds.length; i++) {
            tokenId = _tokenIds[i];
            require(vaultInfo.nft.ownerOf(tokenId) == _msgSender(), "You do not own this token");

            totalStaked++;
            ownerStakedCount[_msgSender()]++;
            vaultInfo.nft.transferFrom(_msgSender(), address(this), tokenId);
            emit NFTStaked(_msgSender(), tokenId, block.timestamp);

            uint amount;
            if (vaultInfo.specialTokens.length > 0) {
                for (uint j = 0; j < vaultInfo.specialTokens.length; j++) {
                    if (vaultInfo.specialTokens[j] == tokenId) {
                        amount = vaultInfo.specialMultiplier;
                    } else {
                        amount = vaultInfo.multiplier;
                    }
                }
            } else { 
                amount = vaultInfo.multiplier;
            }

            Staked memory newStaked = Staked(
                tokenId,
                block.timestamp,
                amount,
                false,
                0,
                vaultInfo.name,
                _msgSender()
            );

            staked[vaultKey][tokenId] = newStaked;
            ownerStaked[_msgSender()][vaultKey].push(tokenId);
        }
    }

    /**
    * @dev Transaction limited to 5 tokens at a time. 
    */
    function pairedStake(ERC721AQueryable _nft, ERC721AQueryable _pair, uint[] calldata _tokenIds, uint[] calldata _pairedTokenIds) external {
        require(_tokenIds.length <= 5, "Too many tokens for one transaction!");
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, _pair));
        uint tokenId;
        uint pairedTokenId;
        Vault memory vaultInfo = vault[vaultKey];
        require(address(vaultInfo.pair) != address(0), "This is not a paired vault!");
        require(address(vaultInfo.nft) != address(0), "This vault does not exist");
        require(_tokenIds.length == _pairedTokenIds.length, "You must submit the same number of tokens from both collections!");
        for (uint i = 0; i < _tokenIds.length; i++) {
            tokenId = _tokenIds[i];
            pairedTokenId = _pairedTokenIds[i];
            require(vaultInfo.nft.ownerOf(tokenId) == _msgSender(), "You do not own the primary token");
            require(vaultInfo.pair.ownerOf(pairedTokenId) == _msgSender(), "You do not own the paired token");
            
            totalStaked += 2;
            ownerStakedCount[_msgSender()] += 2;
            vaultInfo.nft.transferFrom(_msgSender(), address(this), tokenId);
            vaultInfo.pair.transferFrom(_msgSender(), address(this), pairedTokenId);
            emit NFTPairStaked(_msgSender(), tokenId, pairedTokenId, block.timestamp);

            uint amount;
            if (vaultInfo.specialTokens.length > 0) {
                for (uint j = 0; j < vaultInfo.specialTokens.length; j++) {
                    if (vaultInfo.specialTokens[j] == tokenId) {
                        amount = vaultInfo.specialMultiplier;
                    } else {
                        amount = vaultInfo.multiplier;
                    }
                }
            } else { 
                amount = vaultInfo.multiplier;
            }

            Staked memory newStaked = Staked(
                tokenId,
                block.timestamp,
                amount,
                true,
                pairedTokenId,
                vaultInfo.name,
                _msgSender()
            );
            staked[vaultKey][tokenId] = newStaked;

            ownerStaked[_msgSender()][vaultKey].push(tokenId);
        }
    }

    function _unstake(ERC721AQueryable _nft, address _account, uint[] calldata _tokenIds) internal {
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, address(0)));
        uint[] memory ownedTokens = ownerStaked[_msgSender()][vaultKey];
        uint tokenId;
        Vault memory vaultInfo = vault[vaultKey];
        for (uint i = 0; i < _tokenIds.length; i++) {
            tokenId = _tokenIds[i];
            Staked memory stakedInfo = staked[vaultKey][tokenId];
            require(stakedInfo.tokenId == tokenId, "This token is not staked in this Vault");
            require(stakedInfo.owner == _msgSender(), "You do not own this token");
            require(!stakedInfo.paired, "This token is staked in a paired vault, and must be unstaked with an accompanying paired token!");

            totalStaked--;
            ownerStakedCount[_msgSender()]--;
            delete staked[vaultKey][tokenId];
            delete ownerStaked[_msgSender()][vaultKey];
            vaultInfo.nft.transferFrom(address(this), _account, tokenId);
            emit NFTUnstaked(_account, tokenId, block.timestamp);

            for (uint j = 0; j < ownedTokens.length; j++) {
                if (ownedTokens[j] != tokenId) {
                    ownerStaked[_msgSender()][vaultKey].push(tokenId);
                }
            }
        }
    }

    function _pairedUnstake(ERC721AQueryable _nft, ERC721AQueryable _pair, address _account, uint[] calldata _tokenIds) internal {
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, _pair));
        uint[] memory ownedTokens = ownerStaked[_msgSender()][vaultKey];
        uint tokenId;
        uint pairedTokenId;
        Vault memory vaultInfo = vault[vaultKey];
        for (uint i = 0; i < _tokenIds.length; i++) {
            tokenId = _tokenIds[i];
            Staked memory stakedInfo = staked[vaultKey][tokenId];
            pairedTokenId = stakedInfo.pairedTokenId;
            require(stakedInfo.tokenId == tokenId, "This token is not staked in this Vault");
            require(stakedInfo.owner == _msgSender(), "You do not own this token");
            require(stakedInfo.paired, "This token is not staked in a paired vault!");

            totalStaked -= 2;
            ownerStakedCount[_msgSender()]-= 2;
            delete staked[vaultKey][tokenId];
            delete ownerStaked[_msgSender()][vaultKey];
            vaultInfo.nft.transferFrom(address(this), _account, tokenId);
            vaultInfo.pair.transferFrom(address(this), _account, pairedTokenId);
            emit NFTPairUnstaked(_account, tokenId, pairedTokenId, block.timestamp);

            for (uint j = 0; j < ownedTokens.length; j++) {
                if (ownedTokens[j] != tokenId) {
                    ownerStaked[_msgSender()][vaultKey].push(tokenId);
                }
            }
        }
    }

    // ~~~~~~~~~~~~~~~~~~~~ Claiming ~~~~~~~~~~~~~~~~~~~~
    function _claim(ERC721AQueryable _nft, ERC721AQueryable _pair, address _account, uint[] calldata _tokenIds, bool _claimAndUnstake) internal {
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, _pair));
        uint tokenId;
        uint earned = 0;
        Vault memory vaultInfo = vault[vaultKey];
        for (uint i = 0; i < _tokenIds.length; i++) {
            tokenId = _tokenIds[i];
            Staked memory stakedInfo = staked[vaultKey][tokenId];
            require(stakedInfo.owner == _msgSender(), "You do not own this token");
            uint stakedAt = stakedInfo.timestamp;
            uint earnedMod = baseEarnedPerDay * (stakedInfo.multiplier / 10);
            // @Dev, make sure you adjust distribution time back to days vs. minutes!!!!
            earned += earnedMod * (block.timestamp - stakedAt) / 1 minutes;
            staked[vaultKey][tokenId].timestamp = block.timestamp;
        } 

        if (earned > 0) {
            vaultInfo.token.mint(_account, earned * (10 ** 18));
        }
        if(_claimAndUnstake) {
            if (address(vaultInfo.pair) != address(0)) {
                _pairedUnstake(vaultInfo.nft, _pair, _account, _tokenIds);
            } else {
                _unstake(vaultInfo.nft, _account, _tokenIds);
            }
        }
        emit Claimed(_account, earned);
    }

    function claim(ERC721AQueryable _nft, ERC721AQueryable _pair, uint[] calldata _tokenIds) external {
        _claim(_nft, _pair, _msgSender(), _tokenIds, false);
    }

    function claimForAddress(ERC721AQueryable _nft, ERC721AQueryable _pair, address _account, uint[] calldata _tokenIds) external {
        _claim(_nft, _pair, _account, _tokenIds, false);
    }

    /**
    * @dev Transaction limited to 5 tokens at a time. 
    */
    function unstake(ERC721AQueryable _nft, ERC721AQueryable _pair, uint[] calldata _tokenIds) external {
        require(_tokenIds.length <= 5, "Too many tokens for one transaction!");
        _claim(_nft, _pair, _msgSender(), _tokenIds, true);
    }

    function initialClaim(ERC721AQueryable _nft, uint[] calldata _tokenIds) external {
        // @Dev, initial claims will simply use the multiplier values to pay out that number of rewards token. 
        // ie: multipler is 25 (for 2.5X). This simply pays out 25 bones ONCE per tokenId.  
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, address(0)));
        uint tokenId;
        uint allocation;
        Vault memory vaultInfo = vault[vaultKey];
        for (uint i = 0; i < _tokenIds.length; i++) {
            tokenId = _tokenIds[i];
            require(vaultInfo.nft.ownerOf(tokenId) == _msgSender(), "You do not own this token");
            require(!initialClaimed[address(_nft)][tokenId], "Allocated amount already claimed for this token!");
            uint amount = vaultInfo.specialMultiplier;
            if (vaultInfo.specialTokens.length > 0) {
                for (uint j = 0; j < vaultInfo.specialTokens.length; j++) {
                    if (vaultInfo.specialTokens[j] == tokenId) {
                        amount = vaultInfo.specialMultiplier;
                    } else {
                        amount = vaultInfo.multiplier;
                    }
                }
            } else {
                amount = vaultInfo.multiplier;
            }
            allocation += amount;
            initialClaimed[address(_nft)][tokenId] = true;
        }

        if (allocation > 0) {
            vaultInfo.token.mint(_msgSender(), allocation * (10 ** 18));
        }
    }


    // ~~~~~~~~~~~~~~~~~~~~ Various checks ~~~~~~~~~~~~~~~~~~~~
    function checkEarnings(ERC721AQueryable _nft, ERC721AQueryable _pair) external view returns(uint) {
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, _pair));
        uint[] memory tokenIds = ownerStaked[_msgSender()][vaultKey];
        uint tokenId;
        uint earned = 0;
        for (uint i = 0; i < tokenIds.length; i++) {
            tokenId = tokenIds[i];
            Staked memory stakedInfo = staked[vaultKey][tokenId];
            require(stakedInfo.owner == _msgSender(), "You do not own this token");
            uint stakedAt = stakedInfo.timestamp;
            uint earnedMod = baseEarnedPerDay * (stakedInfo.multiplier / 10);
            // @Dev, make sure you adjust distribution time back to days vs. minutes!!!!
            earned += earnedMod * (block.timestamp - stakedAt) / 1 minutes;
            stakedInfo.timestamp = block.timestamp;
        }

        return earned * (10 ** 18);
    }

    /**
    * @dev Generate vaultKey to check 'vault' and 'staked' info
    */
    function checkVaultKey(ERC721AQueryable _nft, ERC721AQueryable _pair) external pure returns(bytes32) {
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, _pair));

        return vaultKey;
    }

    // ~~~~~~~~~~~~~~~~~~~~ onlyOwner Functions ~~~~~~~~~~~~~~~~~~~~
    /**
    * @dev Define 'multiplier' values x10. ie: if you want the multiplier to be 2X, set multiplier to '20'
    */
    function createVault(ERC721AQueryable _nft, BonesRewards _token, ERC721AQueryable _pair, uint _multiplier, uint[] calldata _specialTokens, uint _specialMultiplier, string memory _name) public onlyOwner {
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, _pair));
        Vault memory newVault = Vault(
            _nft,
            _token,
            _pair,
            _multiplier, 
            _specialTokens,
            _specialMultiplier,
            _name
        );

        vault[vaultKey] = newVault;
    }

    function updateVaultMultipliers(ERC721AQueryable _nft, ERC721AQueryable _pair, uint _newMultiplier, uint _newSpecialMultiplier) public onlyOwner {
        bytes32 vaultKey = keccak256(abi.encodePacked(_nft, _pair));
        vault[vaultKey].multiplier = _newMultiplier;
        vault[vaultKey].specialMultiplier = _newSpecialMultiplier;
    }

    function updateBaseEarnedPerDay(uint _newBase) public onlyOwner {
        baseEarnedPerDay = _newBase;
    }

    // ~~~~~~~~~~~~~~~~~~~~ Misc ~~~~~~~~~~~~~~~~~~~~
    function onERC721Received(
        address, 
        address from, 
        uint, 
        bytes calldata
    ) external pure override returns (bytes4) {
        // @Dev address requirement
        require(from == address(0x0), 'You cannot send NFT directly to vault');
        return IERC721Receiver.onERC721Received.selector;
    }
