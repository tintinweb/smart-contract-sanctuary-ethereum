    mapping(string => bool) public _usedNonces; 
    
    address public burnPool = 0x0000000000000000000000000000000000000001;
    string baseUri = "https://www.encryptgenie.com/token/";
    string private baseExtension = ".json";
    address linkAddress;
    uint nftPrice = 0.1 * 1e18;
    mapping(uint => uint) public evolutionLevel; //  tokenId --->  evolutionLevel

    constructor() ERC4907("EncryptGenieNFT", "EGNFT") ConfirmedOwner(msg.sender){ 
        linkAddress = 0xF8A0BF9cF54Bb92F17374d9e9A321E6a111a51bD; //  erc20
        setChainlinkToken(0x404460C6A5EdE2D891e8297795264fDe62ADBB75); // erc677
        setChainlinkOracle(0x7cA367cA973c74e8E4Bf9FaF45bc5c2eb8010da0);
        jobId_string = '67f732a7b0c44d16832dcda31f47dfff';
        fee = (1 * LINK_DIVISIBILITY) / 10; // 0.1 * 10**18
    }

    function transferNft(address _to, uint _tokenId) public{
        require(ownerOf(_tokenId) == msg.sender, "Owner Error");
        _balances[msg.sender] = _balances[msg.sender] - 1 > 0 ? _balances[msg.sender] - 1 : 0;
        _balances[_to] += 1;
        _owners[_tokenId] = _to;
        emit Transfer(msg.sender, _to, _tokenId);
    }

    function transferNftMany(uint8 transferNum, address[] calldata _tos, uint[] calldata _tokenIds) external onlyOwner{
        for(uint8 i =0;i<transferNum;i++){
             transferNft(_tos[i],_tokenIds[i]);
        }
    }

    function burn(uint _tokenId) public{
        require(ownerOf(_tokenId) == msg.sender, "Owner Error");
        _burn(_tokenId);
    }
    
    function burnMany(uint8 burnNum, uint[] calldata _tokenIds) external{
        for(uint8 i =0;i<burnNum;i++){
             burn(_tokenIds[i]);
        }
    }

    //  ***********chainlink functions  **************//
    function MintBychainlinkWithSign(string memory _url,string memory nonce, bytes32 hash, bytes memory signature) public returns (bytes32 requestId) {
        // signature realted
        require(matchSigner(hash, signature), "Plz mint through website");
        require(!_usedNonces[nonce], "Hash reused");
        require(hashTransaction(msg.sender, _url, nonce) == hash,"Hash failed");
        IBEP20(linkAddress).transferFrom(msg.sender, address(this), nftPrice); 
        _usedNonces[nonce] = true;
        Chainlink.Request memory req = buildChainlinkRequest(jobId_string, address(this), this.fulfillMint.selector);
        req.add('get', _url);
        req.add('path', 'data,nftDetail,test'); 
        emit MintRequest(_url, true);
        return sendChainlinkRequest(req, fee);
    }

    function Mint10Times() public{
        IBEP20(linkAddress).transferFrom(msg.sender, address(this), 10*nftPrice); 
        emit Mint10(msg.sender);
    }

    function fulfillMint(bytes32 _requestId, string memory _mintInfo) public recordChainlinkFulfillment(_requestId) {
        // tokenId-evolutionLevel-userAddress
        strings.slice memory s;
        strings.slice memory delim;
        s = _mintInfo.toSlice();
        delim = "-".toSlice();
        string memory _tokenId = s.split(delim).toString();
        string memory _evolutionLevel = s.split(delim).toString();
        string memory _userAddress = s.split(delim).toString();
        _safeMint(toAddress(_userAddress), st2num(_tokenId));
        _setTokenURI(st2num(_tokenId), string(abi.encodePacked(baseUri, _tokenId, baseExtension)));
        evolutionLevel[st2num(_tokenId)] = st2num(_evolutionLevel); 
    }

    function updateEvolutionLevelWithSign(string memory _url, string memory nonce, bytes32 hash, bytes memory signature) public returns (bytes32 requestId) {
        // signature realted
        require(matchSigner(hash, signature), "Plz mint through website");
        require(!_usedNonces[nonce], "Hash reused");
        require(hashTransaction(msg.sender, _url, nonce) == hash,"Hash failed");
        IBEP20(linkAddress).transferFrom(msg.sender, address(this), nftPrice); 
        _usedNonces[nonce] = true;
        Chainlink.Request memory req = buildChainlinkRequest(jobId_string, address(this), this.fulfillEvolution.selector);
        req.add('get', _url);
        req.add('path', 'data,nftDetail,test');
        emit EvolutionLevelRequest(_url, true);
        return sendChainlinkRequest(req, fee);
    }

    function fulfillEvolution(bytes32 _requestId, string memory _updateInfo) public recordChainlinkFulfillment(_requestId) {
        // tokenId-evolutionLevel
        strings.slice memory s;
        strings.slice memory delim;
        s = _updateInfo.toSlice();
        delim = "-".toSlice();
        string memory _tokenId = s.split(delim).toString();
        string memory _evolutionLevel = s.split(delim).toString();
        evolutionLevel[st2num(_tokenId)] = st2num(_evolutionLevel);
        emit EvolutionLevelFulfill(_requestId, st2num(_tokenId), st2num(_evolutionLevel));
    }

    //  *********** sign **************//
    function matchSigner(bytes32 hash, bytes memory signature) public view returns (bool) {
        return _pubAddress == hash.toEthSignedMessageHash().recover(signature);
    }

    function hashTransaction(address sender, string memory _url, string memory nonce) public view returns (bytes32) {
        bytes32 hash = keccak256(abi.encodePacked(sender, _url, nonce, address(this)));
        return hash;
    }

    //  ***********view functions  **************//
    //get token's owner
    function getNftOwner(uint256 tokenId) external view returns (address) {
        return ownerOf(tokenId);
    }
    
    //   *********   system function  *******//
    // other variable may change in the future
    function setBaseUri(string memory _newUri) public onlyOwner {
        baseUri = _newUri;
    }

    // function to claim stucked ERC20 Tokens
    function withdraw(address _token, uint256 _amount) external onlyOwner{
        IBEP20(_token).transfer(owner(), _amount);
    }
    // function to claim stucked Bnb
    function withdrawBnb(uint _amount) external onlyOwner {
        payable(owner()).transfer(_amount);
    }

    function st2num(string memory numString) internal pure returns(uint) {
        uint  val=0;
        bytes   memory stringBytes = bytes(numString);
        for (uint  i =  0; i<stringBytes.length; i++) {
            uint exp = stringBytes.length - i;
            bytes1 ival = stringBytes[i];
            uint8 uval = uint8(ival);
           uint jval = uval - uint(0x30);
   
           val +=  (uint(jval) * (10**(exp-1))); 
        }
      return val;
    }

    function fromHexChar(uint8 c) internal pure returns (uint8) {
        if (bytes1(c) >= bytes1('0') && bytes1(c) <= bytes1('9')) {
            return c - uint8(bytes1('0'));
        }
        if (bytes1(c) >= bytes1('a') && bytes1(c) <= bytes1('f')) {
            return 10 + c - uint8(bytes1('a'));
        }
        if (bytes1(c) >= bytes1('A') && bytes1(c) <= bytes1('F')) {
            return 10 + c - uint8(bytes1('A'));
        }
        return 0;
    }
    
    function hexStringToAddress(string memory s) internal pure returns (bytes memory) {
        bytes memory ss = bytes(s);
        require(ss.length%2 == 0); // length must be even
        bytes memory r = new bytes(ss.length/2);
        for (uint i=0; i<ss.length/2; ++i) {
            r[i] = bytes1(fromHexChar(uint8(ss[2*i])) * 16 +
                        fromHexChar(uint8(ss[2*i+1])));
        }
        return r;
    }
    
    function toAddress(string memory s) internal pure returns (address) {
        bytes memory _bytes = hexStringToAddress(s);
        require(_bytes.length >= 1 + 20, "toAddress_outOfBounds");
        address tempAddress;
        assembly {
            tempAddress := div(mload(add(add(_bytes, 0x20), 1)), 0x1000000000000000000000000)
        }
        return tempAddress;
    }

