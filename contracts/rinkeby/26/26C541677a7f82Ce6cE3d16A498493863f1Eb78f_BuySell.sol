//============== DIRECT BUY / SELL ==============

/// @title Direct Buy/Sell Smart Contract
/// @author eludius18lab
/// @notice Direct Buy/Sell --> MarketPlace
/// @dev This Contract will be used to Buy/Sell NFTs with fixed Price

contract BuySell is Initializable, Ownable, whitelistedAddresses{

    function initialize() external initializer{
        DefaultfeePercentages = 400;
    }

    //============== MAPPINGS ==============

    mapping(address => mapping(uint256 => Sales)) public nftContractSales;
    mapping(address => uint256) failedTransferCredits;

    //Each Sale is unique to each NFT (contract + id pairing).

    //============== STRUCTS ==============

    struct Sales {
        uint64 salesEnd;
        uint128 minPrice;
        uint128 buyPrice;
        uint128 nftHighestPush;
        address nftHighestPusher;
        address nftSeller;
        address nftRecipient;
        address ERC20Token;
        address feeRecipients;
        uint32 feePercentages;
    }

    //============== VARIABLES ==============

    uint32 public DefaultfeePercentages;
    address private DefaultfeeRecipient;

    //============== ERRORS ==============

    error NotNFTOwner();
    error NotPriceMeetsSaleRequirements();

    //============== EVENTS ==============

    event SaleCreated(
        address nftContractAddress,
        uint256 tokenId,
        address nftSeller,
        address erc20Token,
        uint128 buyPrice
    );

    event BuyPriceUpdated(
        address nftContractAddress,
        uint256 tokenId,
        uint128 newBuyPrice
    );

    event PushMade(
        address nftContractAddress,
        uint256 tokenId,
        address Pusher,
        uint256 ethAmount,
        address erc20Token,
        uint256 tokenAmount
    );

    event NFTTransferredAndSellerPaid(
        address nftContractAddress,
        uint256 tokenId,
        address nftSeller,
        uint128 nftHighestPush,
        address nftHighestPusher,
        address nftRecipient
    );

    event SaleWithdrawn(
        address nftContractAddress,
        uint256 tokenId,
        address nftOwner
    );

    //============== MODIFIERS ==============

    modifier isSaleNotStartedByOwner(
        address _nftContractAddress,
        uint256 _tokenId
    ) {
        require(
            nftContractSales[_nftContractAddress][_tokenId].nftSeller !=
                msg.sender,
            "Sale already started by owner"
        );

        if (
            nftContractSales[_nftContractAddress][_tokenId].nftSeller !=
            address(0)
        ) {
            require(
                msg.sender == IERC721(_nftContractAddress).ownerOf(_tokenId),
                "Sender doesn't own NFT"
            );

            _resetSale(_nftContractAddress, _tokenId);
        }
        _;
    }

    modifier saleOngoing(address _nftContractAddress, uint256 _tokenId) {
        require(
            _isSaleOngoing(_nftContractAddress, _tokenId),
            "Sale has ended"
        );
        _;
    }

    modifier priceGreaterThanZero(uint256 _price) {
        require(_price > 0, "Price cannot be 0");
        _;
    }

    modifier notNftSeller(address _nftContractAddress, uint256 _tokenId) {
        require(
            msg.sender !=
                nftContractSales[_nftContractAddress][_tokenId].nftSeller,
            "Owner cannot push on own NFT"
        );
        _;
    }
    modifier onlyNftSeller(address _nftContractAddress, uint256 _tokenId) {
        require(
            msg.sender ==
                nftContractSales[_nftContractAddress][_tokenId].nftSeller,
            "Only nft seller"
        );
        _;
    }

    modifier isNFTOwner(
        address nftAddress,
        uint256 tokenId,
        address spender
    ) {
        IERC721 nft = IERC721(nftAddress);
        address owner = nft.ownerOf(tokenId);
        if (spender != owner) {
            revert NotNFTOwner();
        }
        _;
    }

    modifier pushAmountMeetsPushRequirements(
        address _nftContractAddress,
        uint256 _tokenId,
        uint128 _tokenAmount
    ) {
        require(
            _doesPriceMeetSaleRequirements(
                _nftContractAddress,
                _tokenId,
                _tokenAmount
            ),
            "Not enough funds to push on NFT"
        );
        _;
    }

    modifier paymentAccepted(
        address _nftContractAddress,
        uint256 _tokenId,
        address _erc20Token,
        uint128 _tokenAmount
    ) {
        require(
            _isPaymentAccepted(
                _nftContractAddress,
                _tokenId,
                _erc20Token,
                _tokenAmount
            ),
            "Push right ERC20"
        );
        _;
    }

    modifier notZeroAddress(address _address) {
        require(_address != address(0), "Cannot specify 0 address");
        _;
    }

    modifier isFeePercentagesLessThanMaximum(uint32[] memory _feePercentages) {
        uint32 totalPercent;
        for (uint256 i = 0; i < _feePercentages.length; i++) {
            totalPercent = totalPercent + _feePercentages[i];
        }
        require(totalPercent <= 10000, "Fee percentages exceed maximum");
        _;
    }

    //============== CONSTRUCTOR ==============

    constructor() {
        DefaultfeePercentages = 400;
        DefaultfeeRecipient = 0x75889c731Cb003F2E3B4170B4b050e177890302A;
    }

    //============== CHECK FUNCTIONS ==============

    function _isSaleOngoing(address _nftContractAddress, uint256 _tokenId)
        internal
        view
        returns (bool)
    {
        uint64 salesEndTimestamp = nftContractSales[_nftContractAddress][
            _tokenId
        ].salesEnd;
        return (salesEndTimestamp == 0 ||
            block.timestamp < salesEndTimestamp);
    }

    function _isAPushMade(address _nftContractAddress, uint256 _tokenId)
        internal
        view
        returns (bool)
    {
        return (nftContractSales[_nftContractAddress][_tokenId]
            .nftHighestPush > 0);
    }

    function _isMinimumSaleMade(address _nftContractAddress, uint256 _tokenId)
        internal
        view
        returns (bool)
    {
        uint128 minPrice = nftContractSales[_nftContractAddress][_tokenId]
            .minPrice;
        return
            minPrice > 0 &&
            (nftContractSales[_nftContractAddress][_tokenId].nftHighestPush >=
                minPrice);
    }

    function _isBuyPriceMet(address _nftContractAddress, uint256 _tokenId)
        internal
        view
        returns (bool)
    {
        uint128 buyPrice = nftContractSales[_nftContractAddress][_tokenId]
            .buyPrice;
        return
            buyPrice > 0 &&
            nftContractSales[_nftContractAddress][_tokenId].nftHighestPush >=
            buyPrice;
    }

    function _doesPriceMeetSaleRequirements(
        address _nftContractAddress,
        uint256 _tokenId,
        uint128 _tokenAmount
    ) internal view returns (bool) {
        uint128 buyPrice = nftContractSales[_nftContractAddress][_tokenId]
            .buyPrice;
        if (
            buyPrice > 0 &&
            (msg.value >= buyPrice || _tokenAmount >= buyPrice)
        ) {
            return true;
        }

        revert NotPriceMeetsSaleRequirements();
    }

    function _isPaymentAccepted(
        address _nftContractAddress,
        uint256 _tokenId,
        address _pushERC20Token,
        uint128 _tokenAmount
    ) internal view returns (bool) {
        address saleERC20Token = nftContractSales[_nftContractAddress][
            _tokenId
        ].ERC20Token;
        if (_isERC20Sale(saleERC20Token)) {
            return
                msg.value == 0 &&
                saleERC20Token == _pushERC20Token &&
                _tokenAmount > 0;
        } else {
            return
                msg.value != 0 &&
                _pushERC20Token == address(0) &&
                _tokenAmount == 0;
        }
    }

    function _isERC20Sale(address _saleERC20Token)
        internal
        pure
        returns (bool)
    {
        return _saleERC20Token != address(0);
    }

    function _getPortionOfPush(uint256 _AmountPaid, uint256 _percentage)
        internal
        pure
        returns (uint256)
    {
        return (_AmountPaid * (_percentage)) / 10000;
    }

    //============== GET FUNCTIONS ==============

    function _getSaleFeePercentatges(address _nftContractAddress, uint256 _tokenId)
        internal
        view
        returns (uint32)
    {
        uint32 feePercentages = nftContractSales[_nftContractAddress][
            _tokenId
        ].feePercentages;

        if (feePercentages == 0) {
            return DefaultfeePercentages;
        } else {
            return feePercentages;
        }
    }
    function _getNftRecipient(address _nftContractAddress, uint256 _tokenId)
        internal
        view
        returns (address)
    {
        address nftRecipient = nftContractSales[_nftContractAddress][
            _tokenId
        ].nftRecipient;

        if (nftRecipient == address(0)) {
            return
                nftContractSales[_nftContractAddress][_tokenId]
                    .nftHighestPusher;
        } else {
            return nftRecipient;
        }
    }

    function _getFeeRecipient(address _nftContractAddress, uint256 _tokenId)
        internal
        view
        returns (address)
    {
        address feeRecipients = nftContractSales[_nftContractAddress][
            _tokenId
        ].feeRecipients;
        if (feeRecipients == address(0)) {
            return DefaultfeeRecipient;
        } else {
            return DefaultfeeRecipient;
        }

    }

    //============== TRANSFER FUNCTION NFT TO CONTRACT ==============

    function _transferNftToSaleContract(
        address _nftContractAddress,
        uint256 _tokenId
    ) internal {
        address _nftSeller = nftContractSales[_nftContractAddress][_tokenId]
            .nftSeller;
        if (IERC721(_nftContractAddress).ownerOf(_tokenId) == _nftSeller) {
            IERC721(_nftContractAddress).transferFrom(
                _nftSeller,
                address(this),
                _tokenId
            );
            require(
                IERC721(_nftContractAddress).ownerOf(_tokenId) == address(this),
                "nft transfer failed"
            );
        } else {
            require(
                IERC721(_nftContractAddress).ownerOf(_tokenId) == address(this),
                "Seller doesn't own NFT"
            );
        }
    }

    //============== RESET FUNCTIONS ==============

    function _resetSale(address _nftContractAddress, uint256 _tokenId)
        internal
    {
        nftContractSales[_nftContractAddress][_tokenId].minPrice = 0;
        nftContractSales[_nftContractAddress][_tokenId].buyPrice = 0;
        nftContractSales[_nftContractAddress][_tokenId].salesEnd = 0;
        nftContractSales[_nftContractAddress][_tokenId].nftSeller = address(
            0
        );
        nftContractSales[_nftContractAddress][_tokenId].ERC20Token = address(
            0
        );
    }

    function _resetPush(address _nftContractAddress, uint256 _tokenId)
        internal
    {
        nftContractSales[_nftContractAddress][_tokenId]
            .nftHighestPusher = address(0);
        nftContractSales[_nftContractAddress][_tokenId].nftHighestPush = 0;
        nftContractSales[_nftContractAddress][_tokenId]
            .nftRecipient = address(0);
    }

    //============== PUSH ==============

    function _updateHighestPush(
        address _nftContractAddress,
        uint256 _tokenId,
        uint128 _tokenAmount
    ) internal {
        address saleERC20Token = nftContractSales[_nftContractAddress][
            _tokenId
        ].ERC20Token;
        if (_isERC20Sale(saleERC20Token)) {
            IERC20(saleERC20Token).transferFrom(
                msg.sender,
                address(this),
                _tokenAmount
            );
            nftContractSales[_nftContractAddress][_tokenId]
                .nftHighestPush = _tokenAmount;
        } else {
            nftContractSales[_nftContractAddress][_tokenId]
                .nftHighestPush = uint128(msg.value);
        }
        nftContractSales[_nftContractAddress][_tokenId]
            .nftHighestPusher = msg.sender;
    }

    function _reverseAndResetPreviousPush(
        address _nftContractAddress,
        uint256 _tokenId
    ) internal {
        address nftHighestPusher = nftContractSales[_nftContractAddress][
            _tokenId
        ].nftHighestPusher;

        uint128 nftHighestPush = nftContractSales[_nftContractAddress][
            _tokenId
        ].nftHighestPush;
        _resetPush(_nftContractAddress, _tokenId);

        _payout(_nftContractAddress, _tokenId, nftHighestPusher, nftHighestPush);
    }

    function _reversePreviousPushAndUpdateHighestPush(
        address _nftContractAddress,
        uint256 _tokenId,
        uint128 _tokenAmount
    ) internal {
        address prevNftHighestPusher = nftContractSales[_nftContractAddress][
            _tokenId
        ].nftHighestPusher;

        uint256 prevNftHighestPush = nftContractSales[_nftContractAddress][
            _tokenId
        ].nftHighestPush;
        _updateHighestPush(_nftContractAddress, _tokenId, _tokenAmount);

        if (prevNftHighestPusher != address(0)) {
            _payout(
                _nftContractAddress,
                _tokenId,
                prevNftHighestPusher,
                prevNftHighestPush
            );
        }
    }

    //============== SALES ==============

    function _setupSale(
        address _nftContractAddress,
        uint256 _tokenId,
        address _erc20Token,
        uint128 _buyPrice
    )
        internal
        isNFTOwner(_nftContractAddress, _tokenId, msg.sender)
        isWhitelisted(_erc20Token)

    {
        _getSaleFeePercentatges(_nftContractAddress, _tokenId);
        _getFeeRecipient(_nftContractAddress, _tokenId);
        if (_erc20Token != address(0)) {
            nftContractSales[_nftContractAddress][_tokenId]
                .ERC20Token = _erc20Token;
        }
        nftContractSales[_nftContractAddress][_tokenId]
            .feeRecipients = _getFeeRecipient(_nftContractAddress, _tokenId);
        nftContractSales[_nftContractAddress][_tokenId]
            .feePercentages = _getSaleFeePercentatges(_nftContractAddress, _tokenId);
        nftContractSales[_nftContractAddress][_tokenId]
            .buyPrice = _buyPrice;
        nftContractSales[_nftContractAddress][_tokenId].nftSeller = msg
            .sender;
    }

    function createSale(
        address _nftContractAddress,
        uint256 _tokenId,
        address _erc20Token,
        uint128 _buyPrice
    )
        external
        isSaleNotStartedByOwner(_nftContractAddress, _tokenId)
        priceGreaterThanZero(_buyPrice)
        isNFTOwner(_nftContractAddress, _tokenId, msg.sender)
    {
        nftContractSales[_nftContractAddress][_tokenId]
            .feeRecipients = _getFeeRecipient(_nftContractAddress, _tokenId);
        _setupSale(
            _nftContractAddress,
            _tokenId,
            _erc20Token,
            _buyPrice
        );

        emit SaleCreated(
            _nftContractAddress,
            _tokenId,
            msg.sender,
            _erc20Token,
            _buyPrice
        );
        if (_isAPushMade(_nftContractAddress, _tokenId)) {
                if (_isBuyPriceMet(_nftContractAddress, _tokenId)) {
                    _transferNftToSaleContract(
                        _nftContractAddress,
                        _tokenId
                    );
                    _transferNftAndPaySeller(_nftContractAddress, _tokenId);
                }
             else {
                _reverseAndResetPreviousPush(_nftContractAddress, _tokenId);
            }
        }
    }

    function updateBuyPrice(
        address _nftContractAddress,
        uint256 _tokenId,
        uint128 _newBuyPrice
    )
        external
        onlyNftSeller(_nftContractAddress, _tokenId)
        priceGreaterThanZero(_newBuyPrice)
    {
        nftContractSales[_nftContractAddress][_tokenId]
            .buyPrice = _newBuyPrice;
        emit BuyPriceUpdated(_nftContractAddress, _tokenId, _newBuyPrice);
        if (_isBuyPriceMet(_nftContractAddress, _tokenId)) {
            _transferNftToSaleContract(_nftContractAddress, _tokenId);
            _transferNftAndPaySeller(_nftContractAddress, _tokenId);
        }
    }

    //============== BUY ==============

    function _Buy(
        address _nftContractAddress,
        uint256 _tokenId,
        address _erc20Token,
        uint128 _tokenAmount
    )
        internal
        notNftSeller(_nftContractAddress, _tokenId)
        paymentAccepted(
            _nftContractAddress,
            _tokenId,
            _erc20Token,
            _tokenAmount
        )
        pushAmountMeetsPushRequirements(
            _nftContractAddress,
            _tokenId,
            _tokenAmount
        )
    {
        _reversePreviousPushAndUpdateHighestPush(
            _nftContractAddress,
            _tokenId,
            _tokenAmount
        );
        emit PushMade(
            _nftContractAddress,
            _tokenId,
            msg.sender,
            msg.value,
            _erc20Token,
            _tokenAmount
        );
        _updateOngoingSale(_nftContractAddress, _tokenId);
    }

    function Buy(
        address _nftContractAddress,
        uint256 _tokenId,
        address _erc20Token,
        uint128 _tokenAmount
    )
        external
        payable
        saleOngoing(_nftContractAddress, _tokenId)
        isWhitelisted(_erc20Token)
    {
        _Buy(_nftContractAddress, _tokenId, _erc20Token, _tokenAmount);
    }

    function CustomBuy(
        address _nftContractAddress,
        uint256 _tokenId,
        address _erc20Token,
        uint128 _tokenAmount,
        address _nftRecipient
    )
        external
        payable
        saleOngoing(_nftContractAddress, _tokenId)
        notZeroAddress(_nftRecipient)
        isWhitelisted(_erc20Token)
    {
        nftContractSales[_nftContractAddress][_tokenId]
            .nftRecipient = _nftRecipient;
        _Buy(_nftContractAddress, _tokenId, _erc20Token, _tokenAmount);
    }

    //============== UPDATE PRICE ==============

    function _updateOngoingSale(
        address _nftContractAddress,
        uint256 _tokenId
    ) internal {
        if (_isBuyPriceMet(_nftContractAddress, _tokenId)) {
            _transferNftToSaleContract(_nftContractAddress, _tokenId);
            _transferNftAndPaySeller(_nftContractAddress, _tokenId);
            return;
        }
        //min price not set, nft not up for sale yet
        if (_isMinimumSaleMade(_nftContractAddress, _tokenId)) {
            _transferNftToSaleContract(_nftContractAddress, _tokenId);
        }
    }

    //============== WITHDRAW ==============

    function withdrawSale(address _nftContractAddress, uint256 _tokenId)
        external
        isNFTOwner(_nftContractAddress, _tokenId, msg.sender)
    {
        _resetSale(_nftContractAddress, _tokenId);
        emit SaleWithdrawn(_nftContractAddress, _tokenId, msg.sender);
    }

    //============== PAY SELLER AND TRANSFER NFT ==============

    function _transferNftAndPaySeller(
        address _nftContractAddress,
        uint256 _tokenId
    ) internal {
        address _nftSeller = nftContractSales[_nftContractAddress][_tokenId]
            .nftSeller;
        address _nftHighestPusher = nftContractSales[_nftContractAddress][
            _tokenId
        ].nftHighestPusher;
        address _nftRecipient = _getNftRecipient(_nftContractAddress, _tokenId);
        uint128 _nftHighestPush = nftContractSales[_nftContractAddress][
            _tokenId
        ].nftHighestPush;
        _resetPush(_nftContractAddress, _tokenId);

        _payFeesAndSeller(
            _nftContractAddress,
            _tokenId,
            _nftSeller,
            _nftHighestPush
        );
        IERC721(_nftContractAddress).transferFrom(
            address(this),
            _nftRecipient,
            _tokenId
        );

        _resetSale(_nftContractAddress, _tokenId);
        emit NFTTransferredAndSellerPaid(
            _nftContractAddress,
            _tokenId,
            _nftSeller,
            _nftHighestPush,
            _nftHighestPusher,
            _nftRecipient
        );
    }

    function _payFeesAndSeller(
        address _nftContractAddress,
        uint256 _tokenId,
        address _nftSeller,
        uint256 _AmountPaid
    ) internal {
        uint256 feesPaid;
       
            nftContractSales[_nftContractAddress][_tokenId]
                .feeRecipients;
         {
            uint256 fee = _getPortionOfPush(
                _AmountPaid,
                nftContractSales[_nftContractAddress][_tokenId]
                    .feePercentages
            );
            feesPaid = feesPaid + fee;
            _payout(
                _nftContractAddress,
                _tokenId,
                nftContractSales[_nftContractAddress][_tokenId]
                    .feeRecipients,
                fee
            );
        }
        _payout(
            _nftContractAddress,
            _tokenId,
            _nftSeller,
            (_AmountPaid - feesPaid)
        );
    }

    function _payout(
        address _nftContractAddress,
        uint256 _tokenId,
        address _recipient,
        uint256 _AmountPaid
    ) internal {
        address saleERC20Token = nftContractSales[_nftContractAddress][
            _tokenId
        ].ERC20Token;
        if (_isERC20Sale(saleERC20Token)) {
            IERC20(saleERC20Token).transfer(_recipient, _AmountPaid);
        } else {
            // attempt to send the funds to the recipient
            (bool success, ) = payable(_recipient).call{
                value: _AmountPaid,
                gas: 20000
            }("");
            // if it failed, update their credit balance so they can pull it later
            if (!success) {
                failedTransferCredits[_recipient] =
                    failedTransferCredits[_recipient] +
                    _AmountPaid;
            }
        }
    }

    //============== OTHER FUNCTIONS ==============

    function ownerOfNFT(address _nftContractAddress, uint256 _tokenId)
        external
        view
        returns (address)
    {
        address nftSeller = nftContractSales[_nftContractAddress][_tokenId]
            .nftSeller;
        require(nftSeller != address(0), "NFT not deposited");

        return nftSeller;
    }

    function withdrawAllFailedCredits() external {
        uint256 amount = failedTransferCredits[msg.sender];

        require(amount != 0, "no credits to withdraw");

        failedTransferCredits[msg.sender] = 0;

        (bool successfulWithdraw, ) = msg.sender.call{
            value: amount,
            gas: 20000
        }("");
        require(successfulWithdraw, "withdraw failed");
    }
}

