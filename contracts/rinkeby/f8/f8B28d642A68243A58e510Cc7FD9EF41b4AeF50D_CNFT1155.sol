// contracts/CNFT.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "./ComptrollerInterface.sol";
import "./ErrorReporter.sol";
import "./CarefulMath.sol";
import "./CNFTInterfaces.sol";

//It wraps all the nfts erc721 and erc1155
contract CNFT1155 is ERC1155Supply, CNFTStorage, Ownable, ReentrancyGuard, TokenErrorReporter, CarefulMath{
    using Counters for Counters.Counter;
    Counters.Counter public totalTokenCollateralized;
    struct NFTData {
        address owner;
        bool isCollateralized; //true if surrendered to platform
        uint256 blocknumber; //surrendered or redeemed timestamp
        uint256 count; //for erc1155 contracts.
    }
    struct NftIdCount {
        uint tokenId;
        uint tokenCount;
    }
    //stores total nft id & its count for all the owners
    // it is used in comptroller to determine all the collateralized assets by the borrower.
    mapping(address => NftIdCount[]) private ownersNftIdCount;
    /**
    @notice update the tokenId being collateralized in platform
    */
    mapping(uint256 => NFTData[]) private nftTokensData;
    // Mapping from token ID to owner address
    mapping(uint256 => address) private _owners;

    // Mapping owner address to unique token count
    mapping(address => uint256) private _tokenBalances;

    //list of tokenIds of each user
    mapping(address => uint256[]) public ownerTokenIds;

    //total nfts collateralized

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _tokenURI, //"https://nft.webapp/api/item/{id}.json"
        address _underlyingAddress,
        uint _nftType //721 or 1155
        ) ERC1155(_tokenURI) {
            underlying = _underlyingAddress;
            name = _name;
            symbol = _symbol;
            nftType = _nftType;
    }
    // only admin (i.e. comptroller) modifier
    modifier onlyOwnerOrAdmin() {
        require(owner() == _msgSender() || admin == _msgSender(), "CNFT: caller is not the owner or admin");
        _;
    }

    //
    function setAdmin(address _comptroller) public onlyOwner {
        admin = payable(_comptroller);
        comptroller = ComptrollerInterface(_comptroller);
    }

    function getOwnersNftIdCount(address _account) public view returns (NftIdCount[] memory){
        return ownersNftIdCount[_account];
    }
    //checks if tokenId already exists or not.
    //if tokenId already exists, i.e. tokenId already collateralized.
    function isValidTokenToMint(address _account, uint _tokenId) public view returns (bool){
            if(balanceOf(_account, _tokenId) == 0)
                return true;
            else return false;
    }

    /// @notice mints cNFT token
    /// @dev _count is 1 for ERC721 and for unique tokens of ERC1155
    function mint(uint256 _tokenId, uint _count) public {
        //check is valid tokenId - from nftTokensData
        address _account = msg.sender;
        require(
            _count > 0 &&
            ((nftType==721 && _count == 1) || (nftType == 1155 && _count >= 1)),
            'CNFT1155: invalid token count supplied');

        require(isValidTokenToMint(_account, _tokenId), 'CNFT1155: tokenId already exists or not a valid token');

        if(nftType == 721){
            ERC721(underlying).transferFrom(_account, address(this), _tokenId);
        } else {
            ERC1155(underlying).safeTransferFrom(_account, address(this), _tokenId, _count, '');
        }
        // addNewCollateralTokenIdData(_account, _count, _tokenId);
        _mint(_account, _tokenId, _count, "");
    }

    function redeemUnderlyingNFT(uint256 _tokenId, uint256 _count)
     external {
         address _account = msg.sender;
         require(balanceOf(_account, _tokenId) >= _count, 'CNFT1155: user does not have enough tokens for the supplied tokenId');

        //call comptroller to check the balance after redeeming this token.
        uint256 res = comptroller.redeemNFTAllowed(_tokenId, _account, _count);
        require(res == 0, 'CNFT1155: Insufficient liquidity, user not allowed to redeem token');

        //burn cNFT from user to cNFT contract.
        _burn(_account, _tokenId, _count);

        //transfer underlying tokenId to user.
        if(nftType == 721){
            ERC721(underlying).transferFrom(address(this), _account, _tokenId);
        } else {
            ERC1155(underlying).safeTransferFrom(address(this), _account, _tokenId, _count, '');
        }
        // _tokenBalances[msg.sender] -= 1;
        // removeCollateralTokenIdData(_account, _count, _tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
        ) public virtual override {
            require(
                from == _msgSender() || isApprovedForAll(from, _msgSender()) || _msgSender() == admin,
                "ERC1155: caller is not owner nor approved"
            );
            _safeTransferFrom(from, to, id, amount, data);
            _tokenBalances[from] -= 1;
            _tokenBalances[to] += 1;
    }

    /**
     * @dev See {IERC1155-safeBatchTransferFrom}.
     */
    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
        ) public virtual override {
            require(
                from == _msgSender() || isApprovedForAll(from, _msgSender()),
                "ERC1155: transfer caller is not owner nor approved"
            );
            _safeBatchTransferFrom(from, to, ids, amounts, data);
    }

    function burn(
        uint256 id,
        uint256 amount
        ) external virtual onlyOwner{
            address from = _msgSender();
            _burn(from, id, amount);
            totalTokenCollateralized.decrement();
    }

    function balanceOf(address account, uint256 id) public view virtual override returns (uint256) {
        return super.balanceOf(account, id);
    }

    function uri(uint256 _tokenId) public view virtual override returns (string memory) {
        return super.uri(_tokenId);
    }

    /**
     * @dev See {ERC1155-_beforeTokenTransfer}.
     */
    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal virtual override {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);

        if (from == address(0)) {
            //it updates the list of tokenId and its count for the user.
            NftIdCount[] storage nftIdCountList = ownersNftIdCount[to];
            // bool[] memory isAvailable = new bool[](ids.length);
            for (uint256 i = 0; i < ids.length; ++i) {
                bool isAvailable = false;
                for (uint256 index = 0; index < nftIdCountList.length; index++) {
                    if(nftIdCountList[index].tokenId == ids[i]){
                        nftIdCountList[index].tokenCount += amounts[i];
                        isAvailable = true;
                        break;
                    }
                }
                if(!isAvailable){
                    nftIdCountList.push(NftIdCount(
                        {
                            tokenId: ids[i],
                            tokenCount: amounts[i]
                        }));
                }
            }
        }

        if (to == address(0)) {
            //it updates the list of tokenId and its count for the user.
            NftIdCount[] storage nftIdCountList = ownersNftIdCount[from];
            // bool[] memory isAvailable = new bool[](ids.length);
            for (uint256 i = 0; i < ids.length; ++i) {
                bool isAvailable = false;
                uint256 indexFound;
                for (uint256 index = 0; index < nftIdCountList.length; index++) {
                    if(nftIdCountList[index].tokenId == ids[i]){
                        nftIdCountList[index].tokenCount -= amounts[i];
                        isAvailable = true;
                        indexFound = index;
                        break;
                    }
                }
                if(isAvailable){
                    if(nftIdCountList[indexFound].tokenCount == 0){
                        nftIdCountList[indexFound] = nftIdCountList[nftIdCountList.length-1];
                        nftIdCountList.pop();
                    }
                }
            }
        }

        if( from != address(0) && to != address(0)){

            // it will remove tokens from the 'from' address
            NftIdCount[] storage nftIdCountList_from = ownersNftIdCount[from];
            for (uint256 i = 0; i < ids.length; ++i) {
                bool isAvailable = false;
                uint256 indexFound;
                for (uint256 index = 0; index < nftIdCountList_from.length; index++) {
                    if(nftIdCountList_from[index].tokenId == ids[i]){
                        nftIdCountList_from[index].tokenCount -= amounts[i];
                        isAvailable = true;
                        indexFound = index;
                        break;
                    }
                }
                if(isAvailable){
                    if(nftIdCountList_from[indexFound].tokenCount == 0){
                        nftIdCountList_from[indexFound] = nftIdCountList_from[nftIdCountList_from.length-1];
                        nftIdCountList_from.pop();
                    }
                }
            }

            //it will add tokens to the 'to' address
            NftIdCount[] storage nftIdCountList_to = ownersNftIdCount[to];
            for (uint256 i = 0; i < ids.length; ++i) {
                bool isAvailable = false;
                for (uint256 index = 0; index < nftIdCountList_to.length; index++) {
                    if(nftIdCountList_to[index].tokenId == ids[i]){
                        nftIdCountList_to[index].tokenCount += amounts[i];
                        isAvailable = true;
                        break;
                    }
                }
                if(!isAvailable){
                    nftIdCountList_to.push(NftIdCount(
                        {
                            tokenId: ids[i],
                            tokenCount: amounts[i]
                        }));
                }
            }
        }
    }


    function onERC721Received(address _operator, address _from, uint256 _tokenId, bytes memory _data)
        external view returns(bytes4){
            require(msg.sender == address(underlying), 'CNFT1155: Only collection contract is allowed to call it');
            require(_operator == address(this), 'CNFT1155: Operator should be CNFT1155 contract');
            return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    }

    function onERC1155Received(address _operator, address _from, uint256 _id, uint256 _value, bytes calldata _data)
     external view returns(bytes4){
            require(msg.sender == address(underlying), 'CNFT1155: Only collection contract is allowed to call it');
            require(_operator == address(this), 'CNFT1155: Operator should be CNFT1155 contract');
            return bytes4(keccak256("onERC1155Received(address,address,uint256,uint256,bytes)"));
    }
}

