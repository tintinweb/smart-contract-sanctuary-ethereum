pragma solidity 0.8.16;

import { IERC721AUpgradeable } from "erc721a-upgradeable/contracts/IERC721AUpgradeable.sol";
import { CatcoinStorage } from "./CatcoinStorage.sol";
import { ICatcoin } from "./ICatcoin.sol";
import { ICats } from "../cats/ICats.sol";
import { DiamondOwnable } from "../acl/DiamondOwnable.sol";
import { ReentrancyGuard } from "@solidstate/contracts/utils/ReentrancyGuard.sol";
import { IERC165 } from "@solidstate/contracts/introspection/IERC165.sol";
import { ERC165 } from "@solidstate/contracts/introspection/ERC165.sol";
import { ERC165Storage } from "@solidstate/contracts/introspection/ERC165Storage.sol";
import { IERC1155 } from "@solidstate/contracts/token/ERC1155/IERC1155.sol";
import { IERC1155Metadata } from "@solidstate/contracts/token/ERC1155/metadata/IERC1155Metadata.sol";
import { ERC1155MetadataStorage } from "@solidstate/contracts/token/ERC1155/metadata/ERC1155MetadataStorage.sol";
import { UintUtils } from "@solidstate/contracts/utils/UintUtils.sol";
import "@solidstate/contracts/token/ERC1155/SolidStateERC1155.sol";

contract Catcoin is SolidStateERC1155, DiamondOwnable, ReentrancyGuard, ICatcoin {
    using UintUtils for uint256;
    using ERC165Storage for ERC165Storage.Layout;

    uint256 constant CATCOIN_PER_CATS = 21;

    function initialize() public onlyOwner {
        ERC165Storage.layout().setSupportedInterface(type(IERC1155).interfaceId, true);
        ERC165Storage.layout().setSupportedInterface(type(IERC1155Metadata).interfaceId, true);
        ERC165Storage.layout().setSupportedInterface(type(IERC165).interfaceId, true);
    }

    function setCatsContract(IERC721AUpgradeable cats) external onlyOwner {
        CatcoinStorage.layout().cats = cats;
    }

    function setBaseURI(string calldata baseURI) external onlyOwner {
        _setBaseURI(baseURI);
    }

    function exchangeCat(uint256 catId) external override nonReentrant {
        IERC721AUpgradeable cats = CatcoinStorage.layout().cats;
        if (cats.ownerOf(catId) != msg.sender) revert WrongCatOwner();
        cats.transferFrom(msg.sender, address(this), catId);
        _mint(msg.sender, 0, CATCOIN_PER_CATS, "");
    }

    function daoMint(address recipient, uint256 amount) external onlyOwner {
        IERC721AUpgradeable cats = CatcoinStorage.layout().cats;
        ICats(address(cats)).mint(address(this), amount);
        _mint(recipient, 0, CATCOIN_PER_CATS * amount, "");
    }

    function moveCatTo(address recipient, uint256 catId) external override onlyOwner {
        IERC721AUpgradeable cats = CatcoinStorage.layout().cats;
        cats.transferFrom(address(this), recipient, catId);
    }

    // ============================= VIEWS =============================

    function uri(uint256) public view virtual override(ERC1155Metadata, IERC1155Metadata) returns (string memory) {
        string memory baseURI = ERC1155MetadataStorage.layout().baseURI;
        string memory result = string(abi.encodePacked(baseURI, "0.json"));
        return bytes(baseURI).length != 0 ? result : "";
    }
}

