pragma solidity ^0.8.4;
import "./pokoDao.sol";
import "./pokoToken.sol";
import "./daoStruct.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/proxy/beacon/UpgradeableBeacon.sol";
import "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";

import "@openzeppelin/contracts/access/Ownable.sol";


contract PokoFactory is Ownable, DaoStruct {
  address immutable daoBeacon;
  address immutable tokenBeacon;

  event NewDaoCreated(
    address owner,
    string uuid,
    address aDao,
    address aToken,
    address[] initialHolders,
    uint256[] initialToken
  );

  address[] public dao;

  constructor() {
    UpgradeableBeacon _daoBeacon = new UpgradeableBeacon(address(new PokoDao()));
    daoBeacon = address(_daoBeacon);
    UpgradeableBeacon _tokenBeacon = new UpgradeableBeacon(address(new PokoToken()));
    tokenBeacon = address(_tokenBeacon);
  }

  function setPokoDaoTemplate(address newAddress) public onlyOwner {
    UpgradeableBeacon(daoBeacon).upgradeTo(newAddress);
  }
  function setPokoTokenTemplate(address newAddress) public onlyOwner {
    UpgradeableBeacon(tokenBeacon).upgradeTo(newAddress);
  }

  function createDao(
    string memory id,
    string memory name,
    string memory symbol,
    uint256 totalSupply,
    uint256 tokenRaise,
    address[] memory initialHolders,
    uint256[] memory initialToken
  ) internal returns (address aPokoDao, address aDaoToken) {
    BeaconProxy tokenProxy = new BeaconProxy(
      tokenBeacon,
      abi.encodeWithSelector(PokoToken.initialize.selector, name, symbol)
    );
    aDaoToken = address(tokenProxy);
    BeaconProxy daoProxy = new BeaconProxy(
      daoBeacon,
      abi.encodeWithSelector(PokoDao.initialize.selector, name, address(tokenProxy), totalSupply, tokenRaise)
    );
    aPokoDao = address(daoProxy);

    for (uint256 i; i < initialHolders.length; i++) {
      PokoToken(aDaoToken).mint(initialHolders[i], initialToken[i]);
    }

    emit NewDaoCreated(msg.sender, id, aPokoDao, aDaoToken, initialHolders, initialToken);

    return (aPokoDao, aDaoToken);
  }

  function afterCreateDao(address aPokoDao, address aDaoToken) internal {
    dao.push(aPokoDao);
    PokoToken(aDaoToken).transferOwnership(aPokoDao);
    PokoDao(aPokoDao).transferOwnership(msg.sender);
  }

  function startDao(
    string memory id,
    string memory name,
    string memory symbol,
    uint256 totalSupply,
    uint256 tokenRaise,
    address[] memory initialHolders,
    uint256[] memory initialToken,
    uint256 totalRaise,
    uint256 duration,
    bool needApproved
  ) public virtual returns (address aPokoDao, address aDaoToken) {
    (aPokoDao, aDaoToken) = createDao(id, name, symbol, totalSupply, tokenRaise, initialHolders, initialToken);
    PokoDao(aPokoDao).startFundraising(totalRaise, duration, needApproved);
    afterCreateDao(aPokoDao, aDaoToken);
    return (aPokoDao, aDaoToken);
  }

  function draftDao(
    string memory id,
    string memory name,
    string memory symbol,
    uint256 totalSupply,
    uint256 tokenRaise,
    address[] memory initialHolders,
    uint256[] memory initialToken
  ) public virtual returns (address aPokoDao, address aDaoToken) {
    (aPokoDao, aDaoToken) = createDao(id, name, symbol, totalSupply, tokenRaise, initialHolders, initialToken);
    afterCreateDao(aPokoDao, aDaoToken);
    return (aPokoDao, aDaoToken);
  }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "./daoStruct.sol";

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./pokoToken.sol";

import "hardhat/console.sol";


contract PokoDao is Initializable, OwnableUpgradeable, DaoStruct {
  // ----- EVENT -----
  event NewPendingTx(address investor, uint256 amount);
  event TxApproved(address investor, uint256 amount);
  event TxRejected(address investor);
  event OwnerDeposit(address currency, address sender, uint256 amount);
  event OwnerWithdraw(address currency, address reciever, uint256 amount);

  mapping(address => uint256) public pendingDeposit;
  int public status;
  uint256 private raised;
  PokoDaoContract public daoInfo;
  PokoToken public daoToken;

  modifier validateAmount() {
      require(fundraiseLeft() >= msg.value, "Deposit: Too much");
      _;
  }

  modifier daoStart() {
      require(status == 1, "DAO: Fundraising has not start yet");
      _;
  }

  function fundraiseLeft() public view returns (uint256) {
    return daoInfo.config.totalRaise - raised;
  }

  function finish() internal {
    status = 2;
  }

  function startFundraising(uint256 totalRaise, uint256 duration, bool needApproved) public onlyOwner {
    console.log("startFundraising: totalRaise %s duration %s needApproved %s", totalRaise, duration, needApproved);
    require(status == 0, "Dao: Fundraising already been started");

    daoInfo.config.totalRaise = totalRaise;
    daoInfo.config.duration = duration;
    daoInfo.config.needApproved = needApproved;
    daoInfo.config.ethRate = daoInfo.config.tokenRaise / daoInfo.config.totalRaise;
    console.log("startFundraising: daoInfo.config.ethRate %s", daoInfo.config.ethRate);
    status = 1;
  }

  function initialize(
    string memory sName,
    address aToken,
    uint256 totalSupply,
    uint256 tokenRaise
  ) initializer public {
    __Ownable_init();
    daoInfo.id = address(this);
    daoInfo.name = sName;
    daoInfo.config.totalSupply = totalSupply;
    daoInfo.config.tokenRaise = tokenRaise;
    daoToken = PokoToken(aToken);
  }

  function deposit() public payable daoStart validateAmount {
    console.log("new deposit %s ETH from %s", msg.value, msg.sender);
    if (daoInfo.config.needApproved == false || msg.sender == owner()) {
      emit TxApproved(msg.sender, msg.value);
      raised += msg.value;
      mintToken(msg.sender, msg.value);
    } else {
      pendingDeposit[msg.sender] += msg.value;
      emit NewPendingTx(msg.sender, msg.value);
    }
  }

  function setApproval(bool needApproved) public onlyOwner {
    daoInfo.config.needApproved = needApproved;
  }

  function approveDeposit(address invester, uint256 amount) public onlyOwner {
    console.log("approveDeposit %s ETH from %s", amount, invester);

    require(pendingDeposit[invester] >= (amount), "Peding deposit too small");
    require(fundraiseLeft() >= amount, "Too much");
    pendingDeposit[invester] -= amount;
    raised += amount;
    emit TxApproved(invester, amount);
    mintToken(invester, amount);
  }

  function rejectDeposit(address investor) public onlyOwner {
    require(pendingDeposit[investor] > 0, "Investors has't pending transaction");
    payable(investor).transfer(pendingDeposit[investor]);
    pendingDeposit[investor] = 0;
    emit TxRejected(investor);
  }

  function withdrawToken(address _tokenContract, address reciever, uint256 amount) public onlyOwner {
    IERC20(_tokenContract).transfer(reciever, amount);
    emit OwnerWithdraw(_tokenContract, reciever, amount);
  }

  function withdraw(address reciever, uint256 amount) public onlyOwner {
    payable(reciever).transfer(amount);
    emit OwnerWithdraw(address(0), reciever, amount);
  }

  // Should have another way to implement the bellow function
  // Just use for emit event to subgraph.
  function investMoney() external payable {
    if (msg.value == 0) {
      revert("Amount can not be zero");
    }

    emit OwnerDeposit(address(0), msg.sender, msg.value);
  }

  function depositToken(address currency, uint256 amount) public {
    bool result = IERC20(currency).transferFrom(msg.sender, address(this), amount);
    if (result) {
      emit OwnerDeposit(currency, msg.sender, amount);
    }
  }

   function mintToken(
        address member,
        uint256 value
    ) internal virtual {
      console.log("mintToken %s token for %s", value, member);
      daoToken.mint(member, (value * daoInfo.config.ethRate * 10 ** daoToken.decimals())/ 1 ether );
    }
}

