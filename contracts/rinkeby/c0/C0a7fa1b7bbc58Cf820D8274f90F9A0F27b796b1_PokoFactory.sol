pragma solidity ^0.8.4;

contract DaoStruct {

  struct DaoConfig {
    uint256 totalRaise;
    uint256 totalSupply;
    uint256 tokenRaise;
    uint256 ethRate;
    uint256 duration;
    bool needApproved;
  }

  struct PokoDaoContract {
    address id;
    string name;
    DaoConfig config;
  }
}

