import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./ERC721A.sol";
import "hardhat/console.sol";

contract HayliCollection is Ownable, ERC721A {
    // Merged Provenance NFT Hash List (DNA)
    string public NFT_PROVENANCE = "";

    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdCounter;

    // NFT pricing
    uint256 public constant NFT_STARTING_PRICE = 0.05 ether; // Starting at 0.05 ether
    uint256 public constant NFT_ENDING_PRICE = 0.01 ether; // Ending at 0.01 ether
    uint256 public NFT_FINAL_PRICE; //The final NFT price
    uint256 public constant NFT_DECREMENT = 0.005 ether; // Decrease by 0.05 every frequency
    uint256 public constant NFT_DECREMENT_FREQUENCY = 5 minutes; // Decrement price every 5 minutes
    uint256 public NFT_STARTING_TIMESTAMP = block.timestamp + 30 minutes; // Starting after 10 minutes.
    uint256 public constant NFT_WITHDRAW_DURATION_TIMESTAP = 1 hours; // Withdraw can be successful 1 hour after contract is revealed
    uint256 public constant NFT_MAX_QUANTITY = 100; // The quantity for NFT.
    uint256 public constant NFT_MAX_PRESALE_QUANTITY = NFT_MAX_QUANTITY / 5; // The presale quantity for NFT
    uint256 public constant NFT_RESERVE = 10; // Reserved for owner/developer
    uint256 public constant INCREASED_MAX_TOKEN_PER_TX = 5; // One-time purchase limit
    uint256 public constant INCREASED_MAX_TOKEN_PER_ADDRESS_FOR_PRESALE = 10; // NFT limit on per wallet for presale

    // Stakeholders for withdraw
    address public NFT_OWNER_ADDRESS =
        0x190cc0978615a474092676aB740F486b7f528A5F;
    address public NFT_FOUNDER_ADDRESS =
        0x0E861ddDA17f7C20996dC0868cAcc200bc1985c0;
    address public NFT_BUSINESS_TEAM_ADDRESS =
        0x2082f1014C3e494eb5AaCE2F780A83d97a75dE22;
    address public NFT_DEV_TEAM_ADDRESS =
        0x469c6936Aaf050C96754E804586F293B44d9bb5a;
    address public NFT_CONTRACT_CREATOR_ADDRESS =
        0x0AF49237d27090B7dB6aAF6D312f345507a00686;

    // Mintedt NFT counts
    uint16 public NFT_MINTED_BY_PUBLIC = 0;
    uint16 public NFT_MINTED_BY_OWNER = 0;
    uint16 public NFT_MINTED_BY_WHITELIST = 0;

    // Whitelist for owner/developers and lucky persons on presale
    struct Whitelist {
        address addr;
        uint256 hasMinted;
    }

    mapping(address => Whitelist) public whitelist;

    address[] public WHITE_LIST_ADDRESSES;

    // Hayli Collection Contract URI
    string public CONTRACT_URI =
        "ipfs://QmVKAL7K4QEmcSmmJeR1H2qBuHKkF2fxSbq1UfG1YarSqr";

    // States
    bool public REVEALED = false;
    bool public PAUSED = false;
    bool public IS_PRESALE_ACTIVE = false;
    bool public NFT_FIXED_PRICE = false;

    // Hayli Collection Contract Unrevealed URI
    string public UNREVEALED_URI =
        "ipfs://QmSf7v1Qxy4VkgLQ4pcZYBGSsnaxbZ7P8TWfD3hKHhgfzf";
    string public BASE_URI;

    // Constructor
    constructor() ERC721A("HayliCollection", "HAYLI") {
        // Token index start from 1
        _tokenIdCounter.increment();
    }

    modifier callerIsUser() {
        require(
            tx.origin == msg.sender,
            "The caller is from different contract"
        );
        _;
    }

    // NFT reservation for owner/developer
    function reserveToken() public onlyOwner {
        require(REVEALED == false, "Contract has been releaved");
        require(
            block.timestamp >= NFT_STARTING_TIMESTAMP,
            "Contract has been started"
        );
        uint256 supply = totalSupply();
        for (uint256 i = 0; i < NFT_RESERVE; i++) {
            _safeMint(msg.sender, supply + i);
        }
    }

    // NFT price is dynamic or fixed minimum price
    function toggleNftFixedPrice() public onlyOwner returns (bool) {
        NFT_FIXED_PRICE = !NFT_FIXED_PRICE;
        return NFT_FIXED_PRICE;
    }

    // Calculating current NFT price
    function currentPrice() public view returns (uint256) {
        require(
            block.timestamp >= NFT_STARTING_TIMESTAMP,
            "Contract has not started!"
        );

        if (NFT_FIXED_PRICE) {
            return NFT_STARTING_PRICE;
        }

        if (NFT_FINAL_PRICE > 0) {
            return NFT_FINAL_PRICE;
        }

        uint256 timeSinceStart = block.timestamp - NFT_STARTING_TIMESTAMP;
        uint256 decrementsSinceStart = timeSinceStart / NFT_DECREMENT_FREQUENCY;
        uint256 totalDecrement = decrementsSinceStart * NFT_DECREMENT;
        if (totalDecrement >= NFT_STARTING_PRICE - NFT_ENDING_PRICE) {
            return NFT_ENDING_PRICE;
        }

        return NFT_STARTING_PRICE - totalDecrement;
    }

    // Withdraw for all stakeholders
    function withdraw() public onlyOwner {
        require(
            block.timestamp >=
                NFT_STARTING_TIMESTAMP + NFT_WITHDRAW_DURATION_TIMESTAP,
            "The defined duration time for withdrawing money has not expired."
        );

        uint256 finalFunds = address(this).balance;

        require(finalFunds > 0, "No funds yet");

        (bool succ, ) = payable(NFT_FOUNDER_ADDRESS).call{
            value: (finalFunds * 10) / 100
        }("");
        require(succ, "Founder transfer failed");

        (succ, ) = payable(NFT_BUSINESS_TEAM_ADDRESS).call{
            value: (finalFunds * 10) / 100
        }("");
        require(succ, "Business team transfer failed");

        (succ, ) = payable(NFT_DEV_TEAM_ADDRESS).call{
            value: (finalFunds * 10) / 100
        }("");
        require(succ, "Developer team transfer failed");

        (succ, ) = payable(NFT_CONTRACT_CREATOR_ADDRESS).call{
            value: (finalFunds * 5) / 100
        }("");
        require(succ, "Contract creator transfer failed");

        (succ, ) = payable(NFT_OWNER_ADDRESS).call{value: finalFunds}("");
        require(succ, "Owner transfer failed");
    }

    // Toggle revealed anytime
    function toggleRevealed() public onlyOwner returns (bool) {
        REVEALED = !REVEALED;
        return REVEALED;
    }

    // Set NFT starting time (miliseconds) revealed anytime
    function setNftStartingTimeStamp(uint256 nftStartingTimestamp)
        public
        onlyOwner
        returns (uint256)
    {
        require(REVEALED == false, "Contract has been releaved");
        require(
            block.timestamp >= NFT_STARTING_TIMESTAMP,
            "Cannot identify NftStartingTimeStamp (milliseconds) older than current date"
        );
        NFT_STARTING_TIMESTAMP = nftStartingTimestamp;
        return NFT_STARTING_TIMESTAMP;
    }

    // Get NFT starting time stamp (miliseconds) revealed anytime
    function nftStartingTimeStamp() public view returns (uint256) {
        return NFT_STARTING_TIMESTAMP;
    }

    // Get revealed status
    function revealed() public view returns (bool) {
        return REVEALED;
    }

    // Toggle contract paused status
    function togglePaused() public onlyOwner returns (bool) {
        PAUSED = !PAUSED;
        return PAUSED;
    }

    // Get Contract paused status
    function paused() public view returns (bool) {
        return PAUSED;
    }

    // Set baseURI
    function setBaseURI(string memory _baseURI) public onlyOwner {
        require(REVEALED == false, "Contract has been releaved");
        BASE_URI = _baseURI;
    }

    // Get contract URI
    function contractURI() public view returns (string memory) {
        return CONTRACT_URI;
    }

    // Set contract URI
    function setContractURI(string memory _contractURI) public onlyOwner {
        CONTRACT_URI = _contractURI;
    }

    // Get tokenURI by revealed status
    // If contract has not been revealed, baseURI goes to unrevealed (mysterious look) NFT display
    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(_exists(_tokenId), "Token not found");
        if (REVEALED) {
            return
                string(
                    abi.encodePacked(
                        BASE_URI,
                        Strings.toString(_tokenId),
                        ".json"
                    )
                );
        }
        return UNREVEALED_URI;
    }

    // Toggle presale status
    function togglePreSale() public onlyOwner returns (bool) {
        IS_PRESALE_ACTIVE = !IS_PRESALE_ACTIVE;
        return IS_PRESALE_ACTIVE;
    }

    // Add address to whitelist
    function addAddressToWhitelist(address _address)
        public
        onlyOwner
        returns (bool success)
    {
        require(!isWhitelisted(_address), "Address already whitelisted");
        whitelist[_address].addr = _address;
        whitelist[_address].hasMinted = 0;
        WHITE_LIST_ADDRESSES.push(_address);
        success = true;
    }

    // Add multi address (array) to whitelist
    function addAddressesToWhitelist(address[] memory addrs)
        public
        onlyOwner
        returns (bool success)
    {
        for (uint256 i = 0; i < addrs.length; i++) {
            addAddressToWhitelist(addrs[i]);
        }
        success = true;
    }

    // Checking address is exist on whitelist
    function isWhitelisted(address _address)
        public
        view
        returns (bool isWhiteListed)
    {
        return whitelist[_address].addr == _address;
    }

    // Get all whitelist
    function getWhiteListedAddresses() public view returns (address[] memory) {
        return WHITE_LIST_ADDRESSES;
    }

    // Get how many NFTs an address in the whitelist has
    function getWhitelistedData(address _address)
        public
        view
        returns (uint256)
    {
        return whitelist[_address].hasMinted;
    }

    // Set provenancehash.
    // Set before reveal operation and nft starting
    function setProvenanceHash(string memory provenanceHash) public onlyOwner {
        require(REVEALED == false, "Contract has not been releaved");
        require(
            block.timestamp >= NFT_STARTING_TIMESTAMP,
            "Cannot be changed after the reveal date has expired"
        );
        NFT_PROVENANCE = provenanceHash;
    }

    // Get tokens (array) of owner
    function tokensOfOwner(
        address addr,
        uint256 startId,
        uint256 endId
    ) external view returns (uint256[] memory) {
        uint256 tokenCount = balanceOf(addr);
        if (tokenCount == 0) {
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            uint256 index = 0;

            for (uint256 tokenId = startId; tokenId < endId; tokenId++) {
                if (index == tokenCount) break;

                if (ownerOf(tokenId) == addr) {
                    result[index] = tokenId;
                    index++;
                }
            }

            return result;
        }
    }

    // Get total supply
    function totalSupply() public view override returns (uint256) {
        return _tokenIdCounter.current() - 1;
    }

    // Owner NFT mint without payment
    function ownerMint(uint256 numberOfTokens) public onlyOwner {
        uint256 current = _tokenIdCounter.current();
        require(
            current + numberOfTokens < NFT_MAX_QUANTITY,
            "Will exceed the total limit"
        );
        for (uint256 i = 0; i < numberOfTokens; i++) {
            uint256 tokenId = _tokenIdCounter.current();
            _tokenIdCounter.increment();
            _safeMint(msg.sender, tokenId);
            NFT_MINTED_BY_OWNER++;
        }
    }

    // NFT minting on pre-sale and public-sale
    function mintNFT(uint256 numberOfTokens) public payable callerIsUser {
        require(PAUSED == false, "Contract has been paused");
        require(REVEALED == true, "Contract has not been releaved");
        require(
            numberOfTokens > 0 && numberOfTokens <= INCREASED_MAX_TOKEN_PER_TX,
            "One-time purchase nft limit"
        );

        uint256 _currentPrice = currentPrice();
        require(
            msg.value >= numberOfTokens * _currentPrice,
            "Insufficient fee"
        );

        uint256 current = _tokenIdCounter.current();

        if (IS_PRESALE_ACTIVE) {
            require(
                current + numberOfTokens < NFT_MAX_PRESALE_QUANTITY,
                "Will exceed the total presale nft limit"
            );
            require(isWhitelisted(msg.sender), "Address is not on whitelist");
            require(
                whitelist[msg.sender].hasMinted + numberOfTokens <=
                    INCREASED_MAX_TOKEN_PER_ADDRESS_FOR_PRESALE,
                "Exceeding the maximum sales limit for pre-sale"
            );
            for (uint256 i = 0; i < numberOfTokens; i++) {
                uint256 mintIndex = totalSupply();
                if (totalSupply() < NFT_MAX_QUANTITY) {
                    _safeMint(msg.sender, mintIndex);
                    NFT_MINTED_BY_WHITELIST++;
                }
            }
        } else {
            require(
                current + numberOfTokens < NFT_MAX_QUANTITY,
                "Will exceed the total nft limit"
            );
            for (uint256 i = 0; i < numberOfTokens; i++) {
                uint256 mintIndex = totalSupply();
                if (totalSupply() < NFT_MAX_QUANTITY) {
                    _safeMint(msg.sender, mintIndex);
                    NFT_MINTED_BY_PUBLIC++;
                }
            }
        }
    }
}

