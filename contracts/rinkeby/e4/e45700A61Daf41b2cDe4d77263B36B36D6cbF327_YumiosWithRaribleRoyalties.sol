pragma solidity ^0.8.0;

import "../base/ERC721PresetMinterPauserAutoId.sol";

contract Yumios is ERC721PresetMinterPauserAutoId {

    string private constant NAME = "Yumios";

    string private constant SYMBOL = "Yumios";

    uint256 private constant MAX_ITEMS = 10000;

    bool public revealed = false;

    string private _unrevealedURI;

    constructor(
        address platformSettings_,
        string memory contractURI_,
        string memory baseURI_,
        string memory unrevealedURI_
    ) ERC721PresetMinterPauserAutoId(platformSettings_, contractURI_, NAME, SYMBOL, baseURI_) {
        _unrevealedURI = unrevealedURI_;
    }

    function _totalItems() internal pure override returns (uint256) {
        return MAX_ITEMS;
    }

    function _minterRoleHash() internal view override returns (bytes32) {
        return _rolesManagerConsts().YUMIOS_MINTER_ROLE();
    }

    /**
     * @dev Hook that is called before a set of serially-ordered token ids are about to be transferred. This includes minting.
     * And also called before burning one token.
     *
     * startTokenId - the first token id to be transferred
     * quantity - the amount to be transferred
     *
     * Calling conditions:
     *
     * - When `from` and `to` are both non-zero, `from`'s `tokenId` will be
     * transferred to `to`.
     * - When `from` is zero, `tokenId` will be minted for `to`.
     * - When `to` is zero, `tokenId` will be burned by `from`.
     * - `from` and `to` are never both zero.
     *
     * See {ERC721A-_beforeTokenTransfers}
     */
    function _beforeTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity
    ) internal override {
        super._beforeTokenTransfers(from, to, startTokenId, quantity);
        require(
            _getPlatformSettingsValue(_settingsConsts().YUMIOS_PAUSED()) == 0,
            "YUMIOS_TOKEN_PAUSED"
        );
    }

    function setRevealed(bool newRevealed) public onlyOwner(msg.sender) {
        revealed = newRevealed;
    }

    function updateBaseURI(string memory baseURI_) public onlyOwner(msg.sender) {
        _setBaseURI(baseURI_);
    }

    function setUnrevealedURI(string calldata unrevealedURI_) external onlyOwner(msg.sender) {
        _unrevealedURI = unrevealedURI_;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        if (revealed) {
            return super.tokenURI(tokenId);
        }
        return _unrevealedURI;
    }

}

