                if (ownership.addr != address(0)) {
                    currentOwnershipAddr = ownership.addr;
                }

                if (currentOwnershipAddr == owner) {
                    if (tokenIdsIndex == index) {
                        tokenId = i;
                        break;
                    }
                    tokenIdsIndex++;
                }
            }
        }

        // returns 0 if no token could be found because 1 is the starting token id 
        return tokenId;
    }

}

