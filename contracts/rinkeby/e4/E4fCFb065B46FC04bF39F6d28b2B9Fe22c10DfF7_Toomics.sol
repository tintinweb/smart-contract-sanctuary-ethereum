contract Validator is EIP712Upgradeable, AccessControlUpgradeable {
    bytes32 public constant WITHDRAW_TOKEN =
        keccak256(
            "WithdrawToken(uint256 nonce,address userAddress,uint256 userId,uint256 withdrawRequestId,address tokenAddress,uint256 amount)"
        );
    bytes32 public constant WITHDRAW_NFT =
        keccak256(
            "WithdrawNFT(uint256 nonce,address userAddress,uint256 userId,uint256 withdrawRequestId,uint256[] localIds,address nftAddress,uint256[] tokenIds)"
        );

    address[] private validators;
    uint256 threshold;
    mapping(address => uint256) prevNonce;

    event AddValidator(address validator);
    event RemoveValidator(address validator);
    event RejectTransaction(address requester, uint256 nonce);

    function getValidators() external view returns (address[] memory) {
        return validators;
    }

    function getChainID() external view returns (uint256 id) {
        assembly {
            id := chainid()
        }
    }

    function changeThreshold(uint256 _threshold)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(
            _threshold > 0 && _threshold <= validators.length,
            "Validator: invalid threshold"
        );
        threshold = _threshold;
    }

    function addValidator(address _validator)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(_validator != address(0), "Validator: Null address");
        for (uint256 index = 0; index < validators.length; index++) {
            require(
                _validator != validators[index],
                "Validator: Already added"
            );
        }
        validators.push(_validator);
        emit AddValidator(_validator);
    }

    function removeValidator(address _validator)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        for (uint256 index = 0; index < validators.length; index++) {
            if (_validator == validators[index]) {
                validators[index] = validators[validators.length - 1];
                validators.pop();
                emit RemoveValidator(_validator);
                return;
            }
        }
        revert("Validator: Validator not found to remove");
    }

    function rejectTransaction(uint256 nonce) external {
        require(prevNonce[msg.sender] < nonce, "Validator: wrong nonce");
        prevNonce[msg.sender] = nonce;
        emit RejectTransaction(msg.sender, nonce);
    }

    function _checkWithdrawToken(
        bytes[] calldata signature,
        uint256 nonce,
        address userAddress,
        uint256 userId,
        uint256 withdrawRequestId,
        address tokenAddress,
        uint256 amount
    ) internal view returns (bool) {
        // digest the data to transactionHash
        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    WITHDRAW_TOKEN,
                    nonce,
                    userAddress,
                    userId,
                    withdrawRequestId,
                    tokenAddress,
                    amount
                )
            )
        );
        for (uint256 index = 0; index < signature.length; index++) {
            if (!_checkSignature(signature[index], digest)) return false;
        }
        return true;
    }

    function _checkWithdrawNFT(
        bytes[] calldata signature,
        uint256 nonce,
        address userAddress,
        uint256 userId,
        uint256 withdrawRequestId,
        uint256[] calldata localIds,
        address nftAddress,
        uint256[] calldata tokenIds
    ) internal view returns (bool) {
        // digest the data to transactionHash
        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    WITHDRAW_NFT,
                    nonce,
                    userAddress,
                    userId,
                    withdrawRequestId,
                    keccak256(abi.encodePacked(localIds)),
                    nftAddress,
                    keccak256(abi.encodePacked(tokenIds))
                )
            )
        );
        for (uint256 index = 0; index < signature.length; index++) {
            if (!_checkSignature(signature[index], digest)) return false;
        }
        return true;
    }

    function _checkSignature(bytes memory signature, bytes32 digest)
        private
        view
        returns (bool)
    {
        address checkAdress = ECDSAUpgradeable.recover(digest, signature);

        for (uint256 index = 0; index < validators.length; index++) {
            if (checkAdress == validators[index]) {
                return true;
            }
        }
        return false;
    }

    modifier validatorPrecheck(bytes[] memory signature, uint256 nonce) {
        //threshold is alway >0, from constructor, changeThreshold
        // require(signature.length > 0, "Validator: validator(s) is empty");

        require(
            signature.length >= threshold,
            "Validator: Threshold not reached"
        );
        require(
            prevNonce[msg.sender] < nonce,
            "Validator: wrong nonce/already excuted"
        );

        if (signature.length >= 2) {
            for (uint256 i = 0; i < signature.length; i++) {
                for (uint256 j = i + 1; j < signature.length; j++) {
                    require(
                        keccak256(abi.encodePacked(signature[i])) !=
                            keccak256(abi.encodePacked(signature[j])),
                        "Validator: Can not be the same signature"
                    );
                }
            }
        }
        _;
    }
}

