contract Toomics is
    Initializable,
    AccessControlUpgradeable,
    ERC721HolderUpgradeable,
    Validator
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    address tmgToken;
    address tmrToken;

    // check if the nft contract is allowed for toomics contract
    mapping(address => bool) public nftAddressToAuthorized;

    event WithdrawToken(
        address indexed requester,
        uint256 nonce,
        uint256 indexed userId,
        uint256 withdrawRequestId,
        address indexed tokenAddress,
        uint256 amount
    );
    event DepositToken(
        address indexed requester,
        uint256 indexed userId,
        address indexed tokenAddress,
        uint256 amount
    );
    event WithdrawNFT(
        address indexed requester,
        uint256 nonce,
        uint256 indexed userId,
        uint256 withdrawRequestId,
        address indexed nftAddress,
        uint256[] tokenIds
    );
    event DepositNFT(
        address indexed requester,
        uint256 indexed userId,
        address indexed nftAddress,
        uint256[] tokenIds
    );
    event MintNFT(
        address indexed requester,
        uint256 nonce,
        uint256 indexed userId,
        uint256 withdrawRequestId,
        uint256[] localIds,
        address indexed nftAddress,
        uint256 startTokenId
    );
    event BurnToken(uint256 tmgAmount, uint256 tmrAmount);
    event BurnNFT(address nftAddress, uint256[] tokenIds);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(
        address _tmgToken,
        address _tmrToken,
        address _nftCharacters,
        address _nftBadges,
        address _nftEquipments
    ) public initializer {
        __AccessControl_init();
        tmgToken = _tmgToken;
        tmrToken = _tmrToken;

        nftAddressToAuthorized[_nftCharacters] = true;
        nftAddressToAuthorized[_nftBadges] = true;
        nftAddressToAuthorized[_nftEquipments] = true;

        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        __EIP712_init("toomics", "1");

        threshold = 1;
        //   addValidator(msg.sender);
    }

    // Don't need proxy to call

    function depositToken(
        uint256 userId,
        address tokenAddress,
        uint256 amount
    ) external {
        require(
            (tokenAddress == tmgToken || tokenAddress == tmrToken) &&
                amount > 0,
            "depositToken: invalid address"
        );

        IERC20Upgradeable(tokenAddress).safeTransferFrom(
            msg.sender,
            address(this),
            amount
        );
        emit DepositToken(msg.sender, userId, tokenAddress, amount);
    }

    function depositNFT(
        uint256 userId,
        address nftAddress,
        uint256[] calldata tokenIds
    ) external isOurNFTaddress(nftAddress) isValidArray(tokenIds) {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            IERC721AUpgradeable(nftAddress).safeTransferFrom(
                msg.sender,
                address(this),
                tokenIds[i]
            );
        }
        emit DepositNFT(msg.sender, userId, nftAddress, tokenIds);
    }

    // Need proxy to call

    function withdrawToken(
        bytes[] calldata signature,
        uint256 nonce,
        uint256 userId,
        uint256 withdrawRequestId,
        address tokenAddress,
        uint256 amount
    ) external validatorPrecheck(signature, nonce) {
        require(
            (tokenAddress == tmgToken || tokenAddress == tmrToken) &&
                amount > 0,
            "withdrawToken: invalid address"
        );
        require(
            _checkWithdrawToken(
                signature,
                nonce,
                msg.sender,
                userId,
                withdrawRequestId,
                tokenAddress,
                amount
            ),
            "withdrawToken: invalid signature"
        );

        prevNonce[msg.sender] = nonce;

        if (tokenAddress == tmgToken) {
            IERC20Upgradeable(tokenAddress).safeTransfer(msg.sender, amount);
        } else {
            if (
                IERC20Upgradeable(tokenAddress).balanceOf(address(this)) <
                amount
            ) {
                TMRToken(tokenAddress).mint(msg.sender, amount);
            } else {
                IERC20Upgradeable(tokenAddress).safeTransfer(
                    msg.sender,
                    amount
                );
            }
        }
        // if (
        //     tokenAddress == tmrToken &&
        //     IERC20Upgradeable(tokenAddress).balanceOf(address(this)) < amount
        // ) {
        //     TMRToken(tokenAddress).mint(msg.sender, amount);
        // } else {
        //     IERC20Upgradeable(tokenAddress).safeTransfer(msg.sender, amount);
        // }

        emit WithdrawToken(
            msg.sender,
            nonce,
            userId,
            withdrawRequestId,
            tokenAddress,
            amount
        );
    }

    /// @notice Explain to an end user what this does
    /// @dev Explain to a developer any extra details
    /// @param signature data from backend, for validate
    /// @param nonce data from backend, for validate
    /// @param userId data from backend, for validate and emit event
    /// @param localIds data from backend, using for emit event after mint
    /// @param nftAddress nftAddress to interact with
    /// @param tokenIds list of tokenId needed to be withdrawn
    function withdrawNFT(
        bytes[] calldata signature,
        uint256 nonce,
        uint256 userId,
        uint256 withdrawRequestId,
        uint256[] calldata localIds,
        address nftAddress,
        uint256[] calldata tokenIds
    ) external isOurNFTaddress(nftAddress) validatorPrecheck(signature, nonce) {
        require(
            localIds.length + tokenIds.length > 0 &&
                localIds.length + tokenIds.length <= 5,
            "Toomics: Invalid array"
        );
        require(
            _checkWithdrawNFT(
                signature,
                nonce,
                msg.sender,
                userId,
                withdrawRequestId,
                localIds,
                nftAddress,
                tokenIds
            ),
            "withdrawNFT: invalid signature"
        );
        prevNonce[msg.sender] = nonce;

        if (localIds.length > 0) {
            uint256 startTokenId = INFT(nftAddress).nextTokenId();
            INFT(nftAddress).mint(msg.sender, localIds.length); //mint
            emit MintNFT(
                msg.sender,
                nonce,
                userId,
                withdrawRequestId,
                localIds,
                nftAddress,
                startTokenId
            );
        }
        if (tokenIds.length > 0) {
            for (uint256 i = 0; i < tokenIds.length; i++) {
                IERC721AUpgradeable(nftAddress).safeTransferFrom( //withdraw
                    address(this),
                    msg.sender,
                    tokenIds[i]
                );
            }
            emit WithdrawNFT(
                msg.sender,
                nonce,
                userId,
                withdrawRequestId,
                nftAddress,
                tokenIds
            );
        }
    }

    //admin role

    function burnToken(uint256 tmgAmount, uint256 tmrAmount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        ERC20BurnableUpgradeable(tmgToken).burn(tmgAmount);
        ERC20BurnableUpgradeable(tmrToken).burn(tmrAmount);

        emit BurnToken(tmgAmount, tmrAmount);
    }

    function burnNFT(address nftAddress, uint256[] calldata tokenIds)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        isOurNFTaddress(nftAddress)
    {
        require(tokenIds.length > 0, "Invalid array");

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                IERC721AUpgradeable(nftAddress).ownerOf(tokenIds[i]) ==
                    address(this),
                "Toomics/burnNFT: not the owner"
            );
            IERC721ABurnableUpgradeable(nftAddress).burn(tokenIds[i]);
        }
        emit BurnNFT(nftAddress, tokenIds);
    }

    modifier isOurNFTaddress(address nftAddress) {
        require(
            nftAddressToAuthorized[nftAddress],
            "Toomics: Not our NFT address"
        );
        _;
    }
    modifier isValidArray(uint256[] calldata array) {
        require(
            array.length > 0 && array.length <= 5,
            "Toomics: Invalid array"
        );
        _;
    }
}

