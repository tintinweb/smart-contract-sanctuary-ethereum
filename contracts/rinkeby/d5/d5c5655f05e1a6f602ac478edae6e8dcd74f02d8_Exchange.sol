struct Trade {
    string reqId; // deduplication id to prevent double posting of trades
    uint256 chainId; // to avoid cross-chain replay
    address long; // long option, assumed to also be the sender.
    address short; // short option
    uint256 premiumAmount; // amount in decimals of given token
    OptionSpec optionSpec; // details of the option
    uint256 quoteExpiry; // deadline by which tx must be mined
}

struct OptionSpec {
    address baseToken;
    address quoteToken;
    address notionalToken;
    address premiumToken;
    address settlementToken;
    uint256 notionalAmount;
    uint256 strike;
    uint256 expiryDateTime;
    uint256 settlementDateTime;
    OptionType optionType;
    ExerciseStyle exerciseStyle;
    SettlementStyle settlementStyle;
}

