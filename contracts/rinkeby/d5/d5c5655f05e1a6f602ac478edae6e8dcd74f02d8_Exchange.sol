pragma solidity =0.8.13;

import "solmate/tokens/ERC721.sol";
import "solmate/auth/owned.sol";
import { OptionType, ExerciseStyle, SettlementStyle } from "../config/enums.sol";
import { OptionSpec } from "../config/types.sol";

contract OptionERC721 is ERC721, Owned {
    uint256 public idCounter;
    mapping(uint256 => string) public idToURI;

    // OPTION SPEC
    mapping(uint256 => address) public baseToken;
    mapping(uint256 => address) public quoteToken;
    mapping(uint256 => address) public notionalToken;
    mapping(uint256 => address) public premiumToken;
    mapping(uint256 => address) public settlementToken;
    mapping(uint256 => uint256) public notionalAmount;
    mapping(uint256 => uint256) public strike;
    mapping(uint256 => uint256) public expiryDateTime;
    mapping(uint256 => uint256) public settlementDateTime;
    mapping(uint256 => OptionType) public optionType;
    mapping(uint256 => ExerciseStyle) public exerciseStyle;
    mapping(uint256 => SettlementStyle) public settlementStyle;

    // counterparty
    mapping(uint256 => address) public short;

    // solhint-disable-next-line no-empty-blocks
    constructor(string memory name, string memory symbol) ERC721(name, symbol) Owned(msg.sender) {}

    function tokenURI(uint256 id) public view override returns (string memory) {
        return idToURI[id];
    }

    function mint(
        address to,
        address _short,
        string calldata uri,
        OptionSpec memory optionSpec
    ) public onlyOwner returns (uint256 id) {
        // NFT
        id = idCounter;
        idCounter += 1;
        idToURI[id] = uri;

        // Option Spec
        baseToken[id] = optionSpec.baseToken;
        quoteToken[id] = optionSpec.quoteToken;
        notionalToken[id] = optionSpec.notionalToken;
        settlementToken[id] = optionSpec.settlementToken;
        short[id] = _short;
        notionalAmount[id] = optionSpec.notionalAmount; // UNIT_DECIMALS decimals
        strike[id] = optionSpec.strike;
        expiryDateTime[id] = optionSpec.expiryDateTime;
        settlementDateTime[id] = optionSpec.settlementDateTime;
        optionType[id] = optionSpec.optionType;
        exerciseStyle[id] = optionSpec.exerciseStyle;
        settlementStyle[id] = optionSpec.settlementStyle;

        _mint(to, id);
        return id;
    }

    function burn(uint256 id) public onlyOwner {
        _burn(id);
    }
}

