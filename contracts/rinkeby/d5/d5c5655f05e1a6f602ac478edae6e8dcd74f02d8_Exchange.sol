contract Exchange is IExchange {
    // solhint-disable-next-line var-name-mixedcase
    bytes32 internal DOMAIN_SEPERATOR;
    IOracle internal oracle;
    OptionERC721 public optionERC721;
    uint256 private _version;

    mapping(address => mapping(address => uint256)) public ownerToTokenToBalance; // uses decimals of respective tokens

    constructor(address _oracleAddr) {
        _version = 1; // domain seperator uses version value
        oracle = IOracle(_oracleAddr);
        optionERC721 = new OptionERC721("GreySwanOption", "GSO");
        DOMAIN_SEPERATOR = _computeDomainSeperator();
    }

    //
    // Exchange implementation
    //

    function version() public view returns (uint256) {
        return _version;
    }

    function postTrade(Trade calldata _trade, Signature calldata _signature) public returns (uint256 optionId) {
        validate(_trade, _signature);
        IERC20 erc20 = IERC20(_trade.optionSpec.premiumToken);
        erc20.transferFrom(_trade.long, _trade.short, _trade.premiumAmount);
        optionId = optionERC721.mint(_trade.long, _trade.short, "INSERT URI LATER", _trade.optionSpec);
    }

    /**
      @dev transfer erc20 tokens to the margin account of msg.sender
     */
    function postMarginERC20(address _tokenAddress, uint256 _amount) public {
        // TODO check margin token is in list of accepted margin tokens
        IERC20 erc20 = IERC20(_tokenAddress);
        erc20.transferFrom(msg.sender, address(this), _amount);
        ownerToTokenToBalance[msg.sender][_tokenAddress] += _amount;
    }

    /**
      @dev remove erc20 tokens from the margin account of msg.sender.
           reverts if the account goes underwater TODO
     */
    function removeMarginERC20(address _tokenAddress, uint256 _amount) public {
        IERC20 erc20 = IERC20(_tokenAddress);
        // TODO assert margin balance > margin required after transfer
        if (ownerToTokenToBalance[msg.sender][_tokenAddress] < _amount) {
            revert MARGIN_BALANCE_INSUFFICIENT();
        }
        ownerToTokenToBalance[msg.sender][_tokenAddress] -= _amount;
        erc20.transfer(msg.sender, _amount);
    }

    /**
      @dev returns the value of the option in terms of amount of quoteToken. Given to UNIT_DECIMALS.
           only supports call options.
     */
    function valueExpiredOption(uint256 _optionId) public view returns (uint256 amount, address settlementToken) {
        OptionType optionType = optionERC721.optionType(_optionId);
        if (optionType == OptionType.CALL) {
            revert NOT_IMPLEMENTED();
        }

        // ALL in UNIT_DECIMALS decimals.
        address baseToken = optionERC721.baseToken(_optionId);
        address quoteToken = optionERC721.quoteToken(_optionId);
        settlementToken = optionERC721.settlementToken(_optionId);

        if (settlementToken != quoteToken) {
            console.log("base, quote, settlement", baseToken, quoteToken, settlementToken);
            revert NOT_IMPLEMENTED();
        }

        uint256 expiryDateTime = optionERC721.expiryDateTime(_optionId);
        uint256 strike = optionERC721.strike(_optionId);
        uint256 notionalAmount = optionERC721.notionalAmount(_optionId);
        address notionalAddress = optionERC721.notionalToken(_optionId);

        uint256 price = oracle.getPriceAtExpiry(baseToken, settlementToken, expiryDateTime);

        console.log("price (UNIT DECIMALS)", price);
        console.log("strike (UNIT DECIMALS)", strike);
        if (optionType == OptionType.PUT) {
            if (price >= strike) {
                // OTM or ATM
                amount = 0;
                return (amount, settlementToken);
            }
            // ITM
            uint8 notionalDecimals = ERC20(notionalAddress).decimals();
            uint8 settlementDecimals = ERC20(settlementToken).decimals();
            console.log("notional decimals", notionalDecimals);
            // console.log("quote decimals", quoteDecimals);
            console.log("settlement decimals", settlementDecimals);
            amount = ((strike - price) * notionalAmount) / (10**notionalDecimals);
            console.log("expiry amount (UNIT DECIMALS): ", amount);
            return (amount, quoteToken);
        }
    }

    // solhint-disable-next-line no-empty-blocks
    function settleAll() public {}

    /**
      @dev computes the settlement and pays out the long position if necessary
     */
    function settleOption(uint256 _optionId) public {
        address owner = optionERC721.ownerOf(_optionId); // owner is long OptionType

        if (msg.sender != owner) {
            revert OPTION_NOT_OWNER();
        }

        // check that the option has expired
        uint256 expiry = optionERC721.expiryDateTime(_optionId);
        if (expiry > block.timestamp) {
            revert OPTION_NOT_EXPIRED();
        }

        optionERC721.burn(_optionId);

        // compute the value of the option
        // TODO change to settlement token
        (uint256 amount, address quoteToken) = valueExpiredOption(_optionId);

        if (amount == 0) {
            // No payout
            return;
        }

        // make any decimal adjustment
        // if quote is 18 decimals, and UNIT_DECIMALS is 6, then we need to multiply by 10e12
        uint256 quoteDecimals = ERC20(quoteToken).decimals();
        uint256 amountInQuoteDecimals = amount * 10**(UNIT_DECIMALS - quoteDecimals);
        console.log("amount in quote decimals: ", amountInQuoteDecimals);

        // transfer funds from their margin account
        address short = optionERC721.short(_optionId);
        console.log("short", short);
        console.log("margin balance of short ", ownerToTokenToBalance[short][quoteToken], short);
        // TODO should use terminology SETTLEMENT TOKEN and SETTLEMENT DECIMALS
        _payout(short, owner, amountInQuoteDecimals, quoteToken);
    }

    function getPortfolioMarginRequired(address _holder)
        external
        view
        returns (address[] memory tokenAddresses, uint256[] memory tokenAmounts)
    {} // solhint-disable-line no-empty-blocks

    /**
      @dev payout from the margin account
     */

    function _payout(
        address _from,
        address _to,
        uint256 _amount,
        address _token
    ) internal {
        IERC20 erc20 = IERC20(_token);
        console.log("from token balance", ownerToTokenToBalance[_from][_token]);
        console.log("to token balance", ownerToTokenToBalance[_to][_token]);

        if (ownerToTokenToBalance[_from][_token] < _amount) {
            revert MARGIN_BALANCE_INSUFFICIENT();
        }
        ownerToTokenToBalance[_from][_token] -= _amount;
        erc20.transfer(_to, _amount);
    }

    //
    // Validator implementation
    //

    function validate(Trade memory _trade, Signature memory _signature) public view {
        if (!(_trade.quoteExpiry >= block.timestamp)) revert TRADE_DEADLINE_EXPIRED();

        bytes32 optionSpecStructHash = keccak256(
            abi.encodePacked(
                // OPTION_SPEC_TYPEHASH,
                _trade.optionSpec.baseToken,
                _trade.optionSpec.quoteToken,
                _trade.optionSpec.notionalToken,
                _trade.optionSpec.premiumToken,
                _trade.optionSpec.settlementToken,
                _trade.optionSpec.notionalAmount,
                _trade.optionSpec.strike,
                _trade.optionSpec.expiryDateTime,
                _trade.optionSpec.settlementDateTime,
                _trade.optionSpec.optionType,
                _trade.optionSpec.exerciseStyle,
                _trade.optionSpec.settlementStyle
            )
        );
        bytes32 tradeStructHash = keccak256(
            abi.encodePacked(
                // TRADE_TYPEHASH,
                _trade.reqId,
                _trade.chainId,
                _trade.long,
                _trade.short,
                _trade.premiumAmount,
                optionSpecStructHash,
                _trade.quoteExpiry
            )
        );

        // bytes32 typedTradeHash = keccak256(abi.encodePacked("\x19\x01", DOMAIN_SEPERATOR, tradeStructHash));

        address recoveredAddress = ecrecover(tradeStructHash, _signature.v, _signature.r, _signature.s);
        if (!(recoveredAddress != address(0) && recoveredAddress == _trade.short)) revert TRADE_INVALID_SIGNER();
    }

    function _computeDomainSeperator() internal view returns (bytes32) {
        // common choice of domain seperator per https://eips.ethereum.org/EIPS/eip-2612
        return
            keccak256(
                abi.encode(
                    keccak256("EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)"),
                    keccak256(bytes(optionERC721.name())), // ERC-721 Name
                    keccak256(bytes(Strings.toString(_version))), // Version
                    block.chainid,
                    address(this)
                )
            );
    }

    // /**
    //  * creds to author antoncoding
    //  * @notice convert decimals
    //  *
    //  * @param  _amount      number to convert
    //  * @param _fromDecimals the decimals _amount has
    //  * @param _toDecimals   the target decimals
    //  *
    //  * @return _ number with _toDecimals decimals
    //  */
    // function _convertDecimals(
    //     uint256 _amount,
    //     uint8 _fromDecimals,
    //     uint8 _toDecimals
    // ) internal pure returns (uint256) {
    //     if (_fromDecimals == _toDecimals) return _amount;

    //     if (_fromDecimals > _toDecimals) {
    //         uint8 diff;
    //         unchecked {
    //             diff = _fromDecimals - _toDecimals;
    //         }
    //         return _amount / (10**diff);
    //     } else {
    //         uint8 diff;
    //         unchecked {
    //             diff = _toDecimals - _fromDecimals;
    //         }
    //         return _amount * (10**diff);
    //     }
    // }
}

