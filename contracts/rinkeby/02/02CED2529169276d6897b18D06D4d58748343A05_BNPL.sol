pragma solidity ^0.8.13;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

import "hardhat/console.sol";

contract BNPL is ReentrancyGuard, Ownable{

    address payable public immutable govAccount;
    address payable private immutable pool;
    uint public feePercent;

    uint public itemCount;

    string private key;

    enum ItemState{
        // Default zero value
        Inactive,
        // The Item has been Created on the list 
        Created,
        // The Item has been already sold
        Sold,
        // The Item has been canceled
        Canceled
    }

    enum OrderState{
        // Default zero value
        Inactive,
        // The Order is Created 
        Created,
        // The Order has already paid
        Paid
    }
    

    struct Item{
        uint itemId;
        IERC721 nft;
        uint tokenId;
        uint price;
        address payable seller;
        ItemState state;
    }

    struct Order{
        uint OrderId;
        address buyer;
        address custodioWallet;
        IERC721 nft;
        uint tokenId;
        uint price;
        uint downPayment;
        OrderState state;
    }

    mapping(uint => Item) public items;
    mapping(uint => Order) public orders;
    mapping(address => address) public wallets;


    event Deposit(uint amount);

    event Offered(
        uint itemId,
        address indexed nft,
        uint tokenId,
        uint price,
        address indexed seller
    );
    

    event OrderList(
        uint indexed OrderId,
        address indexed buyer,
        address custodioWallet,
        address nft,
        uint tokenId,
        uint price,
        uint downPayment,
        uint remain,
        OrderState state,
        address indexed seller
    );

    constructor(address _pool){
        govAccount = payable(msg.sender);
        pool = payable(_pool);
    }
    

    // manipulate wallet map
    function mapWallet(address _userWallet, address _custodioWallet) external onlyOwner {
        wallets[_userWallet] = _custodioWallet;
    }

    function getCustodioWallet(address _userWallet) view external returns(address) {
        return wallets[_userWallet];
    }
  
    /* @dev buy now
       List method TBD
    */ 
     function buyNow(uint _itemId, uint _orderId, uint _price, uint _downPayment, uint _processingFee, address _custodioWallet, bytes32 _sign) external payable nonReentrant{
        
        bytes32 sign = hashByShaToString(_orderId, _itemId, _price, _downPayment, key);
        require(sign == _sign, "Inequivalent sign");

        require(_itemId > 0 && _itemId <= itemCount,"item does not exist");

        // calculate total amount
        uint _totalPrice = _downPayment + _processingFee;
        require(msg.value >= _totalPrice, "not enough ether to cover item price and market fee");
        // calculate remain amount
        uint _remain = _price - _downPayment;

        // Get item
        Item storage item = items[_itemId];

        require(item.state == ItemState.Created, "item is unavailable");
        require(address(this).balance >= item.price, "Insufficient balance to lend");

        orders[_orderId] = Order(
            _orderId,
            msg.sender,
            _custodioWallet,
            item.nft,
            item.tokenId,
            _price,
            _downPayment,
            OrderState.Created
            );

        // pay seller 
        item.seller.transfer(item.price);
         // update item state
        item.state = ItemState.Sold;
        // transfer nft to pool
        item.nft.transferFrom(address(this), pool, item.tokenId);
        

        emit OrderList(
            _orderId,
            msg.sender,
            _custodioWallet,
            address(item.nft),
            item.tokenId,
            _price,
            _downPayment,
            _remain,
            OrderState.Created,
            item.seller
        );
     }

    // claim the item
     function sendNft(uint _orderId) external nonReentrant returns(bool){
            Order storage order = orders[_orderId];
            require(order.state == OrderState.Paid, "This order have debt to return");
            require(order.buyer == msg.sender, "No privilege to claim this order");
            order.nft.safeTransferFrom(address(this), msg.sender, order.tokenId);
            return true;
     }

    /* @dev List nft
       List method TBD
    */ 
    function listItem(IERC721 _nft, uint _tokenId, uint _price) external nonReentrant {
        require(_price > 0, "Price must be greater than zero");

        itemCount ++;

        _nft.transferFrom(msg.sender, address(this), _tokenId);

        items[itemCount] = Item(
            itemCount,
            _nft,
            _tokenId,
            _price,
            payable(msg.sender),
            ItemState.Created
        );

        // emit Offered event
        emit Offered(
            itemCount, 
            address(_nft), 
            _tokenId, 
            _price, 
            msg.sender);
    }

    // cancel item and get item back
    function cancelItem(uint _itemId) external nonReentrant {
        Item storage item = items[_itemId];
        require(msg.sender == item.seller, "No privilege to cancel this item");
        item.state = ItemState.Canceled;
        items[_itemId] = item;
       item.nft.transferFrom(address(this), msg.sender, item.tokenId);


    }

    // pool deposit 
    function deposit() external payable {
        require(msg.sender == pool, "Only pool can deposit");
        emit Deposit(msg.value);
    }

    // withdraw to pool - only deployer
    function withdraw(uint _amount) external onlyOwner returns(bool) {
        require(_amount<=address(this).balance, "Insufficient balance");
        return payable(pool).send(_amount);
    }

    // Get contracts balance
    function getBalance() external view returns(uint){
        return address(this).balance;
  }

    // set key 
    function setKey(string memory _key) external onlyOwner{
        key = _key;
    }

    function updatePrice(uint _itemId, uint _price) external onlyOwner{
        require(_itemId > 0 && _itemId <= itemCount,"item does not exist");
        require(_price > 0, "Price must be greater than zero");
        Item storage item = items[_itemId];
        item.price = _price;
    }

    function getPrice(uint _itemId) view external returns(uint){
        require(_itemId > 0 && _itemId <= itemCount,"item does not exist");
        Item storage item = items[_itemId];
        return item.price;
    }


    // Convert to string & Hash by sha256
    function hashByShaToString(uint _orderId, uint _itemId, uint _price, uint _downPayment,  string memory _key) public pure returns(bytes32) {
      string memory orderIdStr = Strings.toString(_orderId);
      string memory itemIdStr = Strings.toString(_itemId);
      string memory priceStr = Strings.toString(_price);
      string memory downPaymentStr = Strings.toString(_downPayment);
      return sha256(abi.encodePacked(orderIdStr, itemIdStr,priceStr,downPaymentStr,_key));
    }

    function checkSign(uint _orderId, uint _itemId, uint _price, uint _downPayment,  string memory _key, bytes32 _sign) external pure returns(bool){
        bytes32 sign = hashByShaToString(_orderId, _itemId, _price, _downPayment,_key);
        return sign == _sign;
    }

}

