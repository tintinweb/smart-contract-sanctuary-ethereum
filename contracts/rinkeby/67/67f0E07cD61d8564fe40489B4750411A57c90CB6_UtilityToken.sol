pragma solidity ^0.8.0;

interface IUNISWAPFACTORY {
    function getPool(
        address tokenA,
        address tokenB,
        uint24 fee
    ) external view returns (address pool);
}

import "@uniswap/v3-periphery/contracts/libraries/OracleLibrary.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol"; 
import "../interfaces/IUTILITYTOKENERC20.sol"; 
import "../interfaces/ICARDSERC1155.sol"; 


contract UtilityToken is IUTILITYTOKENERC20, ERC20, Pausable, Ownable { 

    //////////////////////////////////////////////////////////////////////////////////////////////////
    // State
    //////////////////////////////////////////////////////////////////////////////////////////////////

    address immutable private s_wethUniswapV3;

    address public s_stocksNFTsAddress;
    address public s_cardsNFTsAddress;
    address public s_buyAddress;

    address public s_liquidity;
    address public s_presaleAddress;

    mapping(address => uint256) private s_unlockTimeStamp;
    mapping(address => uint256) private s_amountVesting;
    mapping(address => uint256) private s_monthlyUnlock;

    uint256 private s_freeMinted;

    uint256 constant public s_maxTotalSupply = 100000000000000000000000000; // 100M Tokens

    //////////////////////////////////////////////////////////////////////////////////////////////////
    // Constructor
    //////////////////////////////////////////////////////////////////////////////////////////////////

    constructor(
        string memory p_name, 
        string memory p_symbol, 
        address p_wethUniswapV3, 
        address p_presaleAddress
    ) ERC20(p_name, p_symbol) {
        s_wethUniswapV3 = p_wethUniswapV3;

        s_liquidity = msg.sender;
        s_presaleAddress = p_presaleAddress;

        _mint(s_liquidity, 6000000 * 10 ** decimals());
        _mint(s_presaleAddress, 20000000 * 10 ** decimals());
    }

    //////////////////////////////////////////////////////////////////////////////////////////////////
    // Public functions
    ////////////////////////////////////////////////////////////////////////////////////////////////// 

    // => View functions

    function costMint(uint128 p_amount) public view override returns(uint256) {
        (int24 tick, ) = OracleLibrary.consult(IUNISWAPFACTORY(0x1F98431c8aD98523631AE4a59f267346ea31F984).getPool(s_wethUniswapV3, address(this), 3000), 60);
        uint256 cost = OracleLibrary.getQuoteAtTick(tick, p_amount, address(this), s_wethUniswapV3);

        uint256 mintPower = ICARDSERC1155(s_cardsNFTsAddress).mintPower(msg.sender);
        return cost - ((cost / 100) * mintPower);
    }

    function ethToTokens(uint256 p_amount) public view override returns(uint256) {
        uint256 mintPower = ICARDSERC1155(s_cardsNFTsAddress).mintPower(msg.sender);
        
        uint256 value = p_amount + ((p_amount / 100) * mintPower);
        (int24 tick, ) = OracleLibrary.consult(IUNISWAPFACTORY(0x1F98431c8aD98523631AE4a59f267346ea31F984).getPool(s_wethUniswapV3, address(this), 3000), 60);
        return OracleLibrary.getQuoteAtTick(tick, uint128(value), s_wethUniswapV3, address(this));
    }

    function maxMint() public view override returns(uint256) {
        uint256 mintPower = ICARDSERC1155(s_cardsNFTsAddress).mintPower(msg.sender);

        if (mintPower == 0) { return 0; }

        uint256 amount = (totalSupply() / 10000) * mintPower;
        if (totalSupply() + amount > s_maxTotalSupply) {
            return amount - ((totalSupply() + amount) - s_maxTotalSupply);
        }

        return amount;
    }

    function freeBalanceOf(address p_from) public view override returns(uint256) {
        if (s_amountVesting[p_from] > 0) {
            return balanceOf(p_from) - _freezes(p_from);
        }

        return balanceOf(p_from);
    }

    function monthlyUnlock(address p_owner) public view override returns(uint256) {
        return s_monthlyUnlock[p_owner];
    } 

    function poolUniswapV3() public view override returns(address) {
        return IUNISWAPFACTORY(0x1F98431c8aD98523631AE4a59f267346ea31F984).getPool(s_wethUniswapV3, address(this), 3000);
    } 

    // => Set functions

    function setAddressStocksNFTs(address p_stocksNFTsAddress) public onlyOwner override { 
        s_stocksNFTsAddress = p_stocksNFTsAddress;
    }

    function setAddressCardsNFTs(address p_cardsNFTsAddress) public onlyOwner override {  
        s_cardsNFTsAddress = p_cardsNFTsAddress;
    }

    function setAddressBuy(address p_buyAddress) public onlyOwner override {  
        s_buyAddress = p_buyAddress;
    }

    function setPause(bool p_pause) public onlyOwner override {  
        if (p_pause) { 
            _pause();
        } else {
            _unpause();
        }
    } 

    function freeMint(address p_owner, uint256 p_amount) public override {
        uint256 amount = p_amount * 10 ** decimals();

        require(msg.sender == s_stocksNFTsAddress, "Error origin");
        require(s_freeMinted + amount <= 6000000 * 10 ** decimals(), "Exceeds the amount of free minted");
        require(totalSupply() + amount <= s_maxTotalSupply, "Exceeds the limit of total supply");

        s_freeMinted += amount;

        if (s_unlockTimeStamp[p_owner] == 0) {
            s_amountVesting[p_owner] = amount;
        } else {
            s_amountVesting[p_owner] -= s_amountVesting[p_owner] - _freezes(p_owner);
            s_amountVesting[p_owner] += amount;
        }
        
        s_unlockTimeStamp[p_owner] = block.timestamp;
        s_monthlyUnlock[p_owner] += (amount / 100) * 10;

        _mint(p_owner, amount);
        
        emit FreeMint(p_owner, amount);
    }

    function mint() public payable override {
        require(
            IUNISWAPFACTORY(0x1F98431c8aD98523631AE4a59f267346ea31F984).getPool(s_wethUniswapV3, address(this), 3000) != address(0),
            "Pool does not exist"
        );

        uint256 mintPower = ICARDSERC1155(s_cardsNFTsAddress).mintPower(msg.sender);
        require(mintPower > 0, "Not power to mint");

        ICARDSERC1155(s_cardsNFTsAddress).deleteMintPower(msg.sender);

        uint256 value = msg.value + ((msg.value / 100) * mintPower);
        (int24 tick, ) = OracleLibrary.consult(IUNISWAPFACTORY(0x1F98431c8aD98523631AE4a59f267346ea31F984).getPool(s_wethUniswapV3, address(this), 3000), 60);
        uint256 amountTokens = OracleLibrary.getQuoteAtTick(tick, uint128(value), s_wethUniswapV3, address(this));

        if (mintPower == 1) {
            require(amountTokens <= totalSupply() / 10000 && amountTokens <= s_maxTotalSupply, "Exceeds the amount of 0.01% of the total supply");
            _mint(msg.sender, amountTokens);
        } else if (mintPower == 2) {
            require(amountTokens <= (totalSupply() / 10000) * 2 && amountTokens <= s_maxTotalSupply, "Exceeds the amount of 0.02% of the total supply");
            _mint(msg.sender, amountTokens);
        } else if (mintPower == 3) {
            require(amountTokens <= (totalSupply() / 10000) * 3 && amountTokens <= s_maxTotalSupply, "Exceeds the amount of 0.03% of the total supply");
            _mint(msg.sender, amountTokens);
        } else if (mintPower == 4) {
            require(amountTokens <= (totalSupply() / 10000) * 4 && amountTokens <= s_maxTotalSupply, "Exceeds the amount of 0.04% of the total supply");
            _mint(msg.sender, amountTokens);
        } else {
            require(amountTokens <= (totalSupply() / 10000) * 5 && amountTokens <= s_maxTotalSupply, "Exceeds the amount of 0.05% of the total supply");
            _mint(msg.sender, amountTokens);
        }

        payable(owner()).transfer((msg.value / 100) * 30); // 30% Company
        payable(s_cardsNFTsAddress).transfer(address(this).balance); // 70% Cards Contract
    }

    function burn(address p_owner, uint256 p_amount) public override {  
        require(
            msg.sender == s_cardsNFTsAddress || msg.sender == s_buyAddress,
            "Error origin"
        );

        _burn(p_owner, p_amount);
    }

    receive() external payable {}

    //////////////////////////////////////////////////////////////////////////////////////////////////
    // Internal functions
    //////////////////////////////////////////////////////////////////////////////////////////////////

    function _beforeTokenTransfer(address from, address to, uint256 amount)
        internal
        whenNotPaused
        override
    {
        if (from == s_presaleAddress && to != address(0)) {
            if (s_unlockTimeStamp[to] == 0) {
                s_amountVesting[to] = amount;
            } else {
                s_amountVesting[to] -= s_amountVesting[to] - _freezes(to);
                s_amountVesting[to] += amount;
            }
            
            s_unlockTimeStamp[to] = block.timestamp;
            s_monthlyUnlock[to] += (amount / 100) * 10;
        }

        if (from != address(0) && from != s_presaleAddress) {
            uint256 freeze = _freezes(from);

            require(balanceOf(from) - freeze >= amount, "Insufficient balance");

            if (freeze == 0 && s_amountVesting[from] > 0) {
                delete s_amountVesting[from];
                delete s_monthlyUnlock[from];
                delete s_unlockTimeStamp[from];
            }
        }

        super._beforeTokenTransfer(from, to, amount);
    }

    function _freezes(address p_from) internal view returns(uint256) {
        if (s_amountVesting[p_from] == 0) {
            return 0;
        }

        if (block.timestamp - s_unlockTimeStamp[p_from] < 30 days) {
            return s_amountVesting[p_from];
        }

        uint256 months = (block.timestamp - s_unlockTimeStamp[p_from]) / 30 days;
        if (months * s_monthlyUnlock[p_from] >= s_amountVesting[p_from]) {
            return 0;
        } else {
            return s_amountVesting[p_from] - (months * s_monthlyUnlock[p_from]);
        }
    }
}

