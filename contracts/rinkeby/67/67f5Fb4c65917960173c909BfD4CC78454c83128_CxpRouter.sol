        address tokenA,

        address tokenB,

        uint256 amountADesired,

        uint256 amountBDesired,

        uint256 amountAMin,

        uint256 amountBMin

    ) internal virtual returns (uint256 amountA, uint256 amountB) {

        // create the pair if it doesn't exist yet

        if (ICxpFactory(factory).getPair(tokenA, tokenB) == address(0)) {

            ICxpFactory(factory).createPair(tokenA, tokenB);

        }

        (uint256 reserveA, uint256 reserveB) = CxpLibrary.getReserves(

            factory,

            tokenA,

            tokenB

        );

        (amountA, amountB) = _liquidityFromReserves(reserveA, reserveB, amountADesired, amountBDesired, amountAMin, amountBMin);

