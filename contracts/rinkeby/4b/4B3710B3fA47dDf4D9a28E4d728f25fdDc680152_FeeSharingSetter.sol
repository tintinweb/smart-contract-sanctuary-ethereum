pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "./FeeSharingSetter.sol";
import "./utils/FullMath.sol";
import "./StakingRewardDistributor.sol";
import "./VotingFeeDistributor.sol";

import "hardhat/console.sol";

contract FeeSharingSetter is Ownable, ReentrancyGuard {
    uint128 public immutable FEE_RATIO = 90;
    uint128 public immutable VOTING_RATIO = 10;

    StakingRewardDistributor public stakingDistributor;
    VotingFeeDistributor public votingDistributor;

    event updateFeeRewards(uint256 indexed block, uint256 staking, uint256 voting);
    event Received(address indexed sender, uint256 amount);

    constructor(address payable _stakingDistributor, address payable _votingDistributor) {
        stakingDistributor = StakingRewardDistributor(_stakingDistributor);
        votingDistributor = VotingFeeDistributor(_votingDistributor);
    }

    /**
     * @notice split and transfer reward to Staking & voting
     */

    function updateRewards() external payable onlyOwner nonReentrant {
        require(_balance() > 0, "Setter: not sufficient funds");

        // split reward
        uint256 amountStaking = FullMath.mulDiv(_balance(), FEE_RATIO, 100);
        uint256 amountVoting = FullMath.mulDiv(_balance(), VOTING_RATIO, 100);

        // transfer staking reward
        _transferBNB(address(stakingDistributor), amountStaking);
        // transfer collection voting reward
        _transferBNB(address(votingDistributor), amountVoting);

        stakingDistributor.updateRewards(amountStaking);
        emit updateFeeRewards(block.number, amountStaking, amountVoting);
    }

    function _transferBNB(address to, uint256 amount) internal {
        (bool success, ) = to.call{ value: amount }("");
        require(success, "Setter: Transfer failed");
    }

    function _balance() internal view returns (uint256) {
        return address(this).balance;
    }

    /**
     * @notice Transfer fee back to address
     * @dev It is for emergency purposes
     * @param amount amount to withdraw
     */
    function withdrawAllFee(uint256 amount) external onlyOwner {
        _transferBNB(msg.sender, amount);
    }

    receive() external payable {}
}

