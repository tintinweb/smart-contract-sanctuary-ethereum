pragma solidity 0.8.10;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./Metatransaction.sol";

import "hardhat/console.sol";

/// @title Digital Cork
/// @notice ERC1155 contract for NFTs associated with bottles of wine, and associated proof of experience tokens
///     CoA - Certificate of Authenticity - Supply 1 - Transferable
///     Soldier - Empty bottle token - Supply 1. Non-transferable unless authorized by admin
///     Tasting token - Variable supply depending on bottle - Non-transferable unless authorized by admin
/// @dev Token ID namespace is used to track CoA & derived Proof of experience tokens
///
///     Metatransaction parent contract enables any transaction to be submitted by relayer
///
///     Before Token transfer hook is used to restrict transfers, and supplies owned by single addresses
contract DVinDigitalCork is
    ERC1155Supply,
    AccessControl,
    NativeMetaTransaction,
    ContextMixin
{
    bytes32 public constant COA_MINTER_ROLE = keccak256("COA_MINTER_ROLE"); /*CoA minter can create CoA tokens*/
    bytes32 public constant CORK_MINTER_ROLE = keccak256("CORK_MINTER_ROLE"); /*Cork minter can create proof of experience tokens on behalf of redeemer*/
    bytes32 public constant TOKEN_TRANSFER_ROLE =
        keccak256("TOKEN_TRANSFER_ROLE"); /*Token transfer role can move proof of experience tokens when approved by holder*/
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE"); /*Owner can change contract URI*/
    string public contractURI; /*contractURI contract metadata json*/

    struct Bottle {
        uint256 maxTastingTokens; /*Number of tasting tokens that can be distributed by CoA redeemer*/
        uint256 redemptionDate;
        address redeemer; /*Address of owner of CoA upon redemption*/
    }

    /*****************
    Token ID Namespace
    
    Example:
    Certificate of Authenticity token ID 921525
    When redeemed, generate soldier token 1e13 + 921525 = 10000000921525
    Also generate tasting tokens 2e13 921525 = 20000000921525
    *****************/
    uint256 constant MAX_COA_TOKENID = 1e9; // TODO how long will CoA bottle IDs be
    uint256 constant PREFIX_SOLDIER = 1e13;
    uint256 constant PREFIX_TASTING_TOKEN = 2e13;

    uint256 TASTING_TOKEN_REDEMPTION_PERIOD = 30 days; // TODO adjustable?

    mapping(uint256 => Bottle) public bottleData; /*Track max number of tasting tokens for each bottle*/

    /// @dev Construtor sets the token and contract URIs
    constructor(string memory uri_, string memory _contractURI)
        ERC1155(uri_)
        NativeMetaTransaction("DVinDigitalCork")
    {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(COA_MINTER_ROLE, msg.sender);
        _setupRole(CORK_MINTER_ROLE, msg.sender);
        _setupRole(TOKEN_TRANSFER_ROLE, msg.sender);
        _setupRole(OWNER_ROLE, msg.sender);
        contractURI = _contractURI;
    }

    /// @notice Admin only function to mint new Certificates of Authenticity
    /// @dev Requires COA_MINTER_ROLE
    /// @param _tokenId ID of new bottle
    /// @param _dst Recipient of CoA token
    /// @param _tastingTokens Number of tasting tokens that can be generated once redeemed
    function mintCoA(
        uint256 _tokenId,
        address _dst,
        uint256 _tastingTokens
    ) external {
        require(hasRole(COA_MINTER_ROLE, msgSender()), "!minter"); /*Only COA minter can create new tokens*/
        require(_tokenId < MAX_COA_TOKENID, "Token ID too high"); /*Validate namespace compliance*/
        bottleData[_tokenId] = Bottle(_tastingTokens, 0, address(0)); /*Todo input validation on this?*/
        _mint(_dst, _tokenId, 1, "");
    }

    /// @notice Function to mark a bottle as consumed and start the process of issuing tasting tokens
    /// @dev Can use metatx
    /// @param _tokenId ID of new bottle
    /// @param _soldierDst Recipient of soldier (empty bottle) token
    function redeemCoA(uint256 _tokenId, address _soldierDst) external {
        require(balanceOf(msgSender(), _tokenId) == 1, "!owner");
        bottleData[_tokenId].redemptionDate = block.timestamp;
        bottleData[_tokenId].redeemer = msgSender(); /*Track redeemer to set who is authorized to mint tasting tokens*/
        _burn(msgSender(), _tokenId, 1);
        _mint(_soldierDst, PREFIX_SOLDIER + _tokenId, 1, ""); /*Generate soldier token*/
    }

    /// @notice Function to mint tasting tokens during redemption period
    /// @dev Can use metatx
    ///      Can be done by redeemer or cork admin
    ///      An address can only own one tasting token from a specific CoA bottle
    /// @param _tokenId ID of new bottle
    /// @param _dsts Array of recipients
    function mintTastingTokens(uint256 _tokenId, address[] memory _dsts)
        external
    {
        Bottle memory _bottle = bottleData[_tokenId];
        require(
            _bottle.redeemer == msgSender() ||
                hasRole(CORK_MINTER_ROLE, msgSender()),
            "!redeemer or admin"
        ); /*Only redeemer or admin can mint*/
        require(
            block.timestamp <
                _bottle.redemptionDate + TASTING_TOKEN_REDEMPTION_PERIOD,
            "Redemption period over"
        ); /*Only allow during redemption period*/
        uint256 _mintedSupply = totalSupply(PREFIX_TASTING_TOKEN + _tokenId); /*Check how many have been redeemed already*/
        require(
            _bottle.maxTastingTokens - _mintedSupply >= _dsts.length,
            "Insufficient supply remaining"
        ); /*Only allow up to amount set on mint*/
        for (uint256 _i = 0; _i < _dsts.length; _i++) {
            _mint(_dsts[_i], PREFIX_TASTING_TOKEN + _tokenId, 1, ""); /*before token transfer hook ensures recipient can't already hold one*/
        }
    }

    /// @notice Set the base URI
    /// @param uri_ Base URI
    function setURI(string memory uri_) external {
        require(hasRole(OWNER_ROLE, msgSender()), "!owner");
        _setURI(uri_);
    }

    /// @notice Set new contract URI
    /// @param _contractURI Contract metadata json
    function setContractURI(string memory _contractURI) external {
        require(hasRole(OWNER_ROLE, msgSender()), "!owner");
        contractURI = _contractURI;
    }

    /*****************
    Hooks and internal utils
    *****************/
    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155Supply) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data); /*Trigger parent hook to track supplies*/
        for (uint256 _i = 0; _i < ids.length; _i++) {
            if (ids[_i] / PREFIX_TASTING_TOKEN == 1) {
                require(balanceOf(to, ids[_i]) == 0, "duplicate tasting token"); // Can't hold 2 of same tasting token
            }
            require(
                to == address(0) ||
                    from == address(0) ||
                    hasRole(TOKEN_TRANSFER_ROLE, operator) ||
                    ids[_i] / PREFIX_SOLDIER == 0, /*Truncate token ID and see if CoA token. If CoA, allow transfers*/
                "!burn or mint or not transferable"
            );
        }
    }
    
    ///@dev override _msgSender to use metatx friendly context
    function _msgSender() internal view override(Context) returns (address) {
        return msgSender();
    }

    /*****************
    Public interfaces
    *****************/
    ///@dev Support interfaces for ERC1155
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155, AccessControl)
        returns (bool)
    {
        return
            interfaceId == type(IERC1155).interfaceId ||
            interfaceId == type(IERC1155MetadataURI).interfaceId ||
            interfaceId == type(IAccessControl).interfaceId ||
            super.supportsInterface(interfaceId);
    }
}

