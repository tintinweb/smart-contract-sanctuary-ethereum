pragma solidity ^0.8.4;

import "./RobotsItems.sol";
import "../Common/Governable.sol";
import "../TestContracts/LiquidiftyCollection.sol";

contract RobotsItemsShop is Governable {
    LiquidiftyCollection public itemsCases;
    RobotsItems public items;

    uint public commonId = 0;
    uint public rareId = 1;

    uint private commonCount;
    uint private rareCount = 1500;

    constructor(LiquidiftyCollection _itemsCases, RobotsItems _items){
        itemsCases = _itemsCases;
        items = _items;
    }

    function changeIds(uint _commonId, uint _rareId) public onlyGovernance {
        commonId = _commonId;
        rareId = _rareId;
    }

    function openCase(uint amount, bool isRare) public {
        require(amount > 0, "can't open 0 cases");
        require(itemsCases.balanceOf(msg.sender, isRare? rareId: commonId) >= amount, "no case of this type");

        itemsCases.burn(msg.sender, commonId, amount);
        for(uint i = 0; i < amount; i++){
            items.mint(msg.sender, isRare? rareCount++: commonCount++);
        }
    }
}

