     *
     * - The contract must not be paused.
     */
    modifier whenNotPaused() {
        require(!paused(), "Pausable: paused");
        _;
    }

    /**
     * @dev Modifier to make a function callable only when the contract is paused.
     *
     * Requirements:
     *
     * - The contract must be paused.
     */
    modifier whenPaused() {
        require(paused(), "Pausable: not paused");
        _;
    }

    /**
     * @dev Triggers stopped state.
     *
     * Requirements:
     *
     * - The contract must not be paused.
     */
    function _pause() internal virtual whenNotPaused {
        _paused = true;
        emit Paused(_msgSender());
    }

    /**
     * @dev Returns to normal state.
     *
     * Requirements:
     *
     * - The contract must be paused.
     */
    function _unpause() internal virtual whenPaused {
        _paused = false;
        emit Unpaused(_msgSender());
    }
}

pragma solidity ^0.8.0;

contract MultiBudsStaking is
    ReentrancyGuard,
    Pausable,
    Ownable,
    IERC721Receiver
{
    using Counters for Counters.Counter;
    using Strings for uint256;
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeERC20 for IERC20;

    /* ------------------------ NFT Staking ------------------------- */
    uint256 public START_DATETIME = 1663259200; //  (GMT): Thursday, September 15, 2022 10:00:00 PM
    uint256 public END_DATETIME = 1978898400; //  (GMT): Thursday, September 15, 2032 10:00:00 PM

    //address public NFT_TOKEN_ADDRESS = address(0xC6766f4793899B75970089C1E9DC80E4e1ef0107); // MultiBuds Collection
    address public NFT_TOKEN_ADDRESS = address(0x2307F4f3407A097f99025e16d2312eEddc04DB64); // MultiBuds Collection
    //address public REWARD_TOKEN_ADDRESS = address(0xde19E8bfc16E7cbBc1900F0bc9510e37EeC5a9C0); // BUD Token.
    address public REWARD_TOKEN_ADDRESS = address(0xaE659D3dF508f224e4bf4de1571162075Caa7C22); // BUD Token.

    uint256 public TOKEN_REWARD_PER_DAY = 5096000000000000000000; // 5096BUD Per Day and Rarity

    struct UserInfo {
        uint256 amount; //The staked nft count per rarity and account
        uint256 reward;
        uint256 rewardDebt; // Reward debt. 
    }

    struct PoolInfo {
        uint256 amount; //The staked nft count per rarity
        uint256 accBudPerShare; // Accumulated BUD Tokens per share, times 1e12. See below.
        uint256 lastRewardTimestamp; //Last Timestamp that BUD Token distribution occurs.
    }

    mapping(address => EnumerableSet.UintSet) private userTokenIdList;

    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    mapping(uint256 => PoolInfo) public poolInfo;
    mapping(uint256 => uint256) public rarityOfTokenId;

    address[] public stakerList;

    constructor(){
        for (uint256 i = 0 ; i < 5 ; i++){
            poolInfo[i] = PoolInfo({amount:0, accBudPerShare: 0, lastRewardTimestamp: START_DATETIME});
        }
    }

    /* --------------------------------------------------------------------------------- */
    function setStakingDateTime(uint256 startDateTime, uint256 endDateTime)
        external
        onlyOwner
    {
        START_DATETIME = startDateTime;
        END_DATETIME = endDateTime;
        for (uint256 i = 0 ; i < 5 ; i++){
            PoolInfo storage pool = poolInfo[i];
            pool.lastRewardTimestamp = START_DATETIME;
        }
    }

    function withdraw() external onlyOwner {
        uint256 balance = address(this).balance;
        payable(_msgSender()).transfer(balance);
        IERC20(REWARD_TOKEN_ADDRESS).safeTransfer(_msgSender(), IERC20(REWARD_TOKEN_ADDRESS).balanceOf(address(this)));
    }

    function userHoldNFT(address _owner) public view returns (uint256[] memory) {
        uint256 tokenCount = IERC721(NFT_TOKEN_ADDRESS).balanceOf(_owner);
        if (tokenCount == 0) {
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            uint256 index;
            for (index = 0; index < tokenCount; index++) {
                result[index] = IERC721Enumerable(NFT_TOKEN_ADDRESS).tokenOfOwnerByIndex(_owner, index);
            }
            return result;
        }
    }

    /* --------------------------------------------------------------------- */
    function setNFTAddress(address _nftAddress) external onlyOwner {
        NFT_TOKEN_ADDRESS = _nftAddress;
    }

    function setTokenRewardAddress(address _tokenAddress) external onlyOwner {
        REWARD_TOKEN_ADDRESS = _tokenAddress;
    }

    function setTokenRewardAmountPerDay(uint256 rewardPerDay) external onlyOwner {
        TOKEN_REWARD_PER_DAY = rewardPerDay;
    }

    function addStakerList(address _user) internal {
        for (uint256 i = 0; i < stakerList.length; i++) {
            if (stakerList[i] == _user) return;
        }
        stakerList.push(_user);
    }

    function userStakeInfo(address _owner, uint256 rarity) external view returns (UserInfo memory) {
        return userInfo[rarity][_owner];
    }

    function userStakedNFT(address _owner) public view returns (uint256[] memory) {
        uint256 tokenCount = userTokenIdList[_owner].length();
        if (tokenCount == 0) {
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            uint256 index;
            for (index = 0; index < tokenCount; index++) {
                result[index] = userTokenIdList[_owner].at(index);
            }
            return result;
        }
    }

    function isStaked(address account, uint256 tokenId) public view returns (bool) {
        return userTokenIdList[account].contains(tokenId);
    }

    function earnedWithRarity(address account, uint256 rarity) internal view returns(uint256){
        PoolInfo memory pool = poolInfo[rarity];
        UserInfo memory user = userInfo[rarity][account];
        uint256 accBudPerShare = pool.accBudPerShare;
        if (block.timestamp >= pool.lastRewardTimestamp && pool.amount != 0){
            accBudPerShare = accBudPerShare.add((block.timestamp - pool.lastRewardTimestamp)
                            .mul(TOKEN_REWARD_PER_DAY)
                            .div(1 days)
                            .mul(1e12).div(pool.amount));
        }
        return user.reward.add(user.amount.mul(accBudPerShare).div(1e12).sub(user.rewardDebt));
    }

    // Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools() public{
        for (uint256 rarity = 0; rarity < 5; rarity++) {
            updatePool(rarity);
        }
    }

    function updatePool(uint256 rarity) public {
        PoolInfo storage pool = poolInfo[rarity];
        if (block.timestamp >= pool.lastRewardTimestamp && pool.amount != 0){
            pool.accBudPerShare = pool.accBudPerShare
                .add((block.timestamp.sub(pool.lastRewardTimestamp))
                .mul(TOKEN_REWARD_PER_DAY)
                .div(1 days)
                .mul(1e12)
                .div(pool.amount)); //12 = 30 -18
        }
        pool.lastRewardTimestamp = block.timestamp;
    }

    function getTotalEarned(address account) public view returns (uint256){
        uint256 totalEarning = 0;
        for (uint256 rarity = 0 ; rarity < 5; rarity++){
            totalEarning += earnedWithRarity(account, rarity);
        }
        return totalEarning;
    }

    function totalEarned() external view returns (uint256) {
        uint256 totalEarning = 0;
        for (uint256 i = 0; i < stakerList.length; i++) {
            totalEarning += getTotalEarned(stakerList[i]);
        }
        return totalEarning;
    }

    function totalStakedCount() public view returns (uint256) {
        uint256 totalCount = 0;
        for (uint256 i = 0; i < stakerList.length; i++) {
            totalCount += userTokenIdList[stakerList[i]].length();
        }
        return totalCount;
    }

    function totalStakedMembers() public view returns (uint256) {
        uint256 totalMembers = 0;
        for (uint256 i = 0; i < stakerList.length; i++) {
            if (userTokenIdList[stakerList[i]].length() > 0) totalMembers += 1;
        }
        return totalMembers;
    }

    function stake(uint256[] calldata _tokenIDList, uint256[] calldata rarityList, uint256[] calldata amountList)
        external
        nonReentrant
        whenNotPaused
    {
        require(block.timestamp >= START_DATETIME && block.timestamp <= END_DATETIME, "Staking is not activated");
        require(IERC721(NFT_TOKEN_ADDRESS).isApprovedForAll(_msgSender(),address(this)),"Not approve nft to staker address");

        addStakerList(_msgSender());
        for (uint256 j = 0 ; j < _tokenIDList.length; j++){
            rarityOfTokenId[_tokenIDList[j]] = rarityList[j];
        }

        for (uint256 rarity = 0 ; rarity < 5; rarity++){
            if (amountList[rarity] == 0)continue;
            updatePool(rarity);
            UserInfo storage user = userInfo[rarity][_msgSender()];
            user.reward = earnedWithRarity(_msgSender(), rarity);
        }
        
        for (uint256 i = 0; i < _tokenIDList.length; i++) {
            IERC721(NFT_TOKEN_ADDRESS).safeTransferFrom(_msgSender(),address(this),_tokenIDList[i]);
            userTokenIdList[_msgSender()].add(_tokenIDList[i]);
        }
        
        for (uint256 rarity = 0 ; rarity < 5; rarity++){
            if (amountList[rarity] == 0)continue;
            UserInfo storage user = userInfo[rarity][_msgSender()];
            PoolInfo storage pool = poolInfo[rarity];
            pool.amount = pool.amount.add(amountList[rarity]);
            user.amount = user.amount.add(amountList[rarity]);
            user.rewardDebt = user.amount.mul(pool.accBudPerShare).div(1e12);
        }
        
    }

    function unstake(uint256[] calldata tokenIdList, uint256[] calldata amountList) external nonReentrant {
        for (uint256 rarity = 0 ; rarity < 5; rarity++){
            if (amountList[rarity] == 0)continue;
            updatePool(rarity);
            UserInfo storage user = userInfo[rarity][msg.sender];
            user.reward = earnedWithRarity(msg.sender, rarity);
        }

        for (uint256 i = 0; i < tokenIdList.length; i++) {
            require(isStaked(msg.sender, tokenIdList[i]),"Not staked this nft");
            IERC721(NFT_TOKEN_ADDRESS).safeTransferFrom(address(this),msg.sender,tokenIdList[i],"");
            userTokenIdList[msg.sender].remove(tokenIdList[i]);
        }
        
        for (uint256 rarity = 0 ; rarity < 5; rarity++){
            if (amountList[rarity] == 0)continue;
            UserInfo storage user = userInfo[rarity][msg.sender];
            PoolInfo storage pool = poolInfo[rarity];
            user.amount = user.amount.sub(amountList[rarity]);
            pool.amount = pool.amount.sub(amountList[rarity]);
            user.rewardDebt = user.amount.mul(pool.accBudPerShare).div(1e12);
        }
    }

    function harvest() external nonReentrant{
        //require(block.timestamp >= END_DATETIME, "Harvest is not activated");

        uint256 userRewards = 0;
        for (uint256 rarity = 0 ; rarity < 5; rarity++){
            updatePool(rarity);
            userRewards += earnedWithRarity(msg.sender, rarity);
        }

        require( IERC20(REWARD_TOKEN_ADDRESS).balanceOf(address(this)) >= userRewards,"Reward token amount is small");

        if (userRewards > 0) {
            IERC20(REWARD_TOKEN_ADDRESS).safeTransfer(msg.sender,userRewards);
        }

        for (uint256 rarity = 0 ; rarity < 5; rarity++){
            UserInfo storage user = userInfo[rarity][msg.sender];
            PoolInfo memory pool = poolInfo[rarity];
            user.rewardDebt = user.amount.mul(pool.accBudPerShare).div(1e12);
            user.reward = 0;
        }
    }

    function emergencyWithdraw(uint256 rarity) external {
        updatePool(rarity);
        uint256 tokenCount = userTokenIdList[msg.sender].length();
        uint256 amount = 0;
        for (uint256 i = 0; i < tokenCount; i++) {
            if(!isStaked(msg.sender, userTokenIdList[msg.sender].at(i)))continue;
            uint256 tokenId = userTokenIdList[msg.sender].at(i);
            if (rarityOfTokenId[tokenId] != rarity)continue;
            IERC721(NFT_TOKEN_ADDRESS).safeTransferFrom(address(this),msg.sender, tokenId,"");
            amount++;
            userTokenIdList[msg.sender].remove(tokenId);
        }
        UserInfo storage user = userInfo[rarity][msg.sender];
        user.rewardDebt = 0;
        user.reward = 0;
        user.amount -= amount;
        PoolInfo storage pool = poolInfo[rarity];
        pool.amount -= amount; 
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    receive() external payable {}
}