contract HSPP is  ERC721Enumerable, Ownable, ReentrancyGuard{

    using Strings for uint256;
    address private MixContract;
    string _baseTokenURI;
    string public baseExtension = ".json";
    string public notRevealedUri;                    // Not Revealed URI
    uint256 public _maxperTX = 3;                    // Max per mint
    uint256 public _maxperWalletWaitList = 3;        // Waitlist max mint per wallet
    uint256 public _maxperWalletGPass = 3;           // GPass max Mint per wallet
    uint256 public _maxperWalletMint = 20;           // Mint max Mint per wallet
    uint256 public _limit = 10000;                   // Supply of token 
    uint256 public _price = 0.2 ether;               // Price 
    uint256 public _Gprice = 0 ether;                // G Price 
    uint256 public _reserved = 400;                  // Reserved for team and marketing 
    uint256 public _reservedG = 100;                 // Reserved for claim G
    uint256 public Gpass_count  =  0;                // Start count for mintpass
    uint256 public waitlist_count  =  0;             // Start count for waitlist
    address public fundWallet;                       // Fund Wallet
    uint256 internal _currentIndex;                  // index 
    bool public _presaleGPassPaused = true;          // G Pass Contract is paused 
    bool public _presaleWLPaused = true;             // WL Contract is paused 
    bool public _PublicPaused = true;                // Public Contract is paused 
    mapping (address => bool) public Gpass;          // Mapping per minpass
    mapping (address => uint256) public perWallet;   // Mapping per wallet
    mapping (address => bool) public waitlist;       // Mapping per waitlist
    SauceToken public TokenContract;                 // Token functions
    address payable public payments;                 // Payable 
    address public hsppgAddress;                     // HSSP Genisus Address  
    address public crossmintAddress;                 // CrossMint  Address
    uint256[40] claimedBitMap;                       // ClaimMap
    bool public isBurningActive = false;             // Paused for Burning Function

    IERC721Enumerable public immutable doodles;

    // Constructor for the token 
    constructor(string memory initbaseURI, address _fundWallet,address doodles_ ,string memory _initNotRevealedUri
    ) ERC721("HS Pepper Party ", "HSPP")  {
        require(_fundWallet != address(0), "Zero address error");
        setBaseURI(initbaseURI);
        setNotRevealedURI(_initNotRevealedUri);
        doodles = IERC721Enumerable(doodles_);
        sethsppgAddress(0xf3252f274074A3b486C874836d440a70E02680Ed); // HsppG address 
        setCrossmintAddress(0xdAb1a1854214684acE522439684a145E62505233); // CrossMint address 
        fundWallet = _fundWallet;
    }


    // Internal URI 
    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    // Token Public only owner functions 
    function setSauceToken (SauceToken _TokenContract) public onlyOwner{
    // require(_TokenContract != address(0), "Contract address can't be zero address");
        TokenContract = _TokenContract;
    }

    // Public view token 
    function getSauceToken(address __address) public view returns(uint256) {
        require(__address != address(0), "Contract address can't be zero address");
        return TokenContract.balanceOf(__address);
    }

    // Get Allowance  public view 
    function getSauceAllowance(address __address) public view returns(uint256) {
        require(__address != address(0), "Contract address can't be zero address");
        return TokenContract.allowance(__address, address(this));
    }
  
 
    // Mint Function public payable 
    function MINT(uint256 num) public payable {
        uint256 supply = totalSupply();
        require( !_PublicPaused,                                     "Sale paused" );
        require( num <= _maxperTX,                                   "You are exceeding limit of per transaction TLM" );
        require( perWallet[msg.sender] + num <= _maxperWalletMint,   "You are exceeding limit of per wallet TLM" );
        require( num > 0);
        require( supply + num <= _limit - _reserved - _reservedG,    "Exceeds maximum TLM supply" );
        require( msg.value >= _price * num,                          "Ether sent is not correct" );

        for(uint256 i = 1 ; i <= num; i++){
            _safeMint( msg.sender, supply + i );
        }
        perWallet[msg.sender] += num;

    }

    // Mint cross mint Function public payable 
    function Crossmint(address _to, uint256 num) public payable {
        uint256 supply = totalSupply();
        require(msg.sender == crossmintAddress,                      "This function is for Crossmint only.");
        require( num > 0);
        require( supply + num <= _limit - _reserved - _reservedG,    "Exceeds maximum TLM supply" );
        require( msg.value >= _price * num,                          "Ether sent is not correct" );
        for(uint256 i = 1 ; i <= num; i++){
            _safeMint( msg.sender, supply + i );
        }
        perWallet[msg.sender] += num;

    }

    // Mint Function external onlyOwner 
    function mintForAddress(address _to, uint256 _amount) external onlyOwner() {
        require( _to !=  address(0), "Zero address error");
        require( _amount <= _reserved, "Exceeds Limit  supply");
        uint256 supply = totalSupply();
        for(uint256 i; i < _amount; i++){
            _safeMint( _to, supply + i );
        }
        _limit -= _amount;  
        }

    // Internal memory with public view for the wallet of owner 
    function walletOfOwner(address _owner) public view returns(uint256[] memory) {
        uint256 tokenCount = balanceOf(_owner);

        uint256[] memory tokensId = new uint256[](tokenCount);
        for(uint256 i; i < tokenCount; i++){
            tokensId[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokensId;
    }

    // is Claim function
    function isClaimed(uint256 tokenId) public view returns (bool) {
        uint256 claimedWordIndex = tokenId / 256;
        uint256 claimedBitIndex = tokenId % 256;
        uint256 claimedWord = claimedBitMap[claimedWordIndex];
        uint256 mask = (1 << claimedBitIndex);
        return claimedWord & mask == mask;
    }
        // Set Claim function
       function _setClaimed(uint256 tokenId) private {
        uint256 claimedWordIndex = tokenId / 256;
        uint256 claimedBitIndex = tokenId % 256;
        claimedBitMap[claimedWordIndex] = claimedBitMap[claimedWordIndex] | (1 << claimedBitIndex);
    }

// add requre gpass
    // GPASS Mint public payable only for the Mintpass Holders 
    function Claim() external nonReentrant{
        uint256 numTokensClaimed;

         for (uint256 i; i < doodles.balanceOf(msg.sender); i++) {
            uint256 tokenId = doodles.tokenOfOwnerByIndex(msg.sender, i);
            if (!isClaimed(tokenId)) {
                _setClaimed(tokenId);
                numTokensClaimed++;
            }
        }
       _safeMint(msg.sender, numTokensClaimed);
    }

