pragma solidity ^0.8.9;

import "./ERC721Tradable.sol";
import "../interfaces/IMintable.sol";

error OnlyAuthorizedMinter();

/**
 * @title ERC721MintManagement
 * @dev Contract that adds management for custom minter on top of the ERC721Tradable
 */
abstract contract ERC721MintManagement is ERC721Tradable, IMintable {
    mapping(address => bool) private minters;

    /**
     * @dev Allow or disallow given accounts to mint new tokens
     */
    function _setMinters(address[] memory _minters, bool[] memory trusted) public {
        for(uint256 i = 0; i < _minters.length; i++) {
            minters[_minters[i]] = trusted[i];
        }
    }

    /**
     * @dev Checks if a given account is a minter
     */
    function isMinter(address account) public view returns (bool) {
        return minters[account];
    }

    /**
     * @dev Mints a token to a given address
     */
    function mintTo(address to) public override onlyMinters {
        _mintTo(to);
    }

    /**
     * @dev Mints a token with a given ID to a given address
     */
    function mintById(address to, uint256 tokenId) public override onlyMinters {
        _mintById(to, tokenId);
    }

    /**
     * @notice Mints a token to a given address and sets a tokenURI for that token
     */
    function mintToWithTokenURI(address to, string memory tokenURI) public override onlyMinters { 
        _mintToWithTokenURI(to, tokenURI);
    }

    /**
     * @notice Mints a token with a given ID to a given address and sets a tokenURI for that token
     */
    function mintByIdWithTokenURI(address to, uint256 tokenId, string memory tokenURI) public override onlyMinters {
        _mintByIdWithTokenURI(to, tokenId, tokenURI);
    }

    /**
     * @dev Mints a many tokens to many accounts
     * Can specify a different number of tokens for each account
     */
    function mintMany(address[] memory recipients, uint256[] memory tokenCounts) public override onlyMinters {
        for(uint256 i = 0; i < recipients.length; i++) {
            uint256 cnt = tokenCounts[i];

            for(uint256 j = 0; j < cnt; j++) {
                _mintTo(recipients[i]);
            }
        }
    }

    /**
     * @notice Mints a many tokens to many accounts
     * Can specify a different tokenIds for each account
     */
    function mintManyByIds(address[] memory recipients, uint256[][] memory tokenIds) public override onlyMinters {
        for(uint256 i = 0; i < recipients.length; i++) {
            uint256 cnt = tokenIds[i].length;

            for(uint256 j = 0; j < cnt; j++) {
                _mintById(recipients[i], tokenIds[i][j]);
            }
        }
    }

    /**
    * @dev Throws if called by any account other than an authorized minter.
    */
    modifier onlyMinters() {
        if(!minters[_msgSender()]) {
            revert OnlyAuthorizedMinter();
        }
        _;
    }
}

