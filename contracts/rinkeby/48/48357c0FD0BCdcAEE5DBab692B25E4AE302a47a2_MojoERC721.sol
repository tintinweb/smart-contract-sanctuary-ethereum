contract OwnableDelegateProxy {}

/**
 * @dev Used to delegate ownership of a contract to another address, to save on unneeded transactions to approve contract use for users
 */
contract ProxyRegistry {
    mapping(address => OwnableDelegateProxy) public proxies;
}

/**
 * @title ERC721Tradable
 * @dev ERC721 contract that whitelists a the OpenSea proxies, and has minting functionality.
 */
abstract contract ERC721Tradable is ERC721, RelayRecipientAndContextMixin, NativeMetaTransaction {
    /// @dev Used to track of the next token ID when minting by auto-increment
    uint256 public tokenIdCounter;
    /// @dev Total number of tokens minted
    uint256 public totalSupply;

    address openSeaProxyRegistryAddress;
    mapping(uint256 => string) public tokenURIs;

    constructor(
        string memory _name,
        string memory _symbol,
        address _openSeaProxyRegistryAddress
    ) ERC721(_name, _symbol) {
        openSeaProxyRegistryAddress = _openSeaProxyRegistryAddress;
        tokenIdCounter = 1;
        _initializeEIP712(_name);
    }

    /**
     * @dev Set the token ID counter to a specific value
     * @param _tokenIdCounter the number of the next token to mint when minting with auto-increment
     */
    function _setTokenIdCounter(uint256 _tokenIdCounter) internal virtual {
        tokenIdCounter = _tokenIdCounter;
    }

    /**
     * @dev Mints a token with a given ID to an address
     */
    function _mintTo(address _to) internal virtual returns(uint256 tokenId) {
        tokenId = tokenIdCounter;
        
        _safeMint(_to, tokenIdCounter);
        tokenIdCounter++;
        totalSupply++;

        return tokenId;
    }

    /**
     * @dev Mints a token to an address with a tokenURI.
     */
    function _mintToWithTokenURI(address _to, string memory _tokenURI) internal virtual {
        uint256 tokenId = _mintTo(_to);
        _setTokenURI(tokenId, _tokenURI);
    }

    /**
    * @dev Mints a token with a given ID to an address
    */
    function _mintById(address _to, uint256 _tokenId) internal virtual {
        _safeMint(_to, _tokenId);
        totalSupply++;
    }

    /**
     * @dev Mints a token with a given ID to an address with a tokenURI.
     */
    function _mintByIdWithTokenURI(address _to, uint256 _tokenId, string memory _tokenURI) internal virtual {
        _mintById(_to, _tokenId);
        _setTokenURI(_tokenId, _tokenURI);
    }

    /**
     * @dev Sets the token URI for a given token ID
     */
    function _setTokenURI(uint256 _tokenId, string memory _tokenURI) internal virtual {
        tokenURIs[_tokenId] = _tokenURI;
    }
    
    function baseTokenURI() virtual public view returns (string memory);

    function tokenURI(uint256 _tokenId) override public view returns (string memory) {
        return bytes(tokenURIs[_tokenId]).length != 0
            ? tokenURIs[_tokenId] 
            : string(abi.encodePacked(baseTokenURI(), Strings.toString(_tokenId)));
    }

    /**
     * Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-less listings.
     */
    function isApprovedForAll(address owner, address operator)
        override
        public
        view
        returns (bool)
    {
        // Whitelist OpenSea proxy contract for easy trading.
        ProxyRegistry proxyRegistry = ProxyRegistry(openSeaProxyRegistryAddress);
        if (address(proxyRegistry.proxies(owner)) == operator) {
            return true;
        }

        return super.isApprovedForAll(owner, operator);
    }
}

