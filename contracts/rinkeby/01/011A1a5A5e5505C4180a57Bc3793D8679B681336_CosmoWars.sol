pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";


enum ActionType {None, Attack, Defend, Collaborate}

    enum Faction {None, Jahjahrion, Breedorok, Foodrak, Pimpmyridian,
        Muskarion, Lamborgardoz, Schumarian, Creatron}


abstract contract CosmoGang is IERC721, Ownable
{    
    function main_address() public virtual returns (address);
    function isHolder(address addr) public virtual returns (bool);
    function tokenIdExists(uint256 tokenId) public virtual returns (bool);
    function totalSupply() public virtual returns (uint256);
}

abstract contract CosmoItemsInteractions is Ownable
{    

}

abstract contract CosmoGangItems is ERC1155Supply, Ownable
{    
    mapping(address => bool) public approvedAddresses;
    function burnNFT(address fromAddress, uint256 amount, Faction _faction) public virtual;
}

contract CosmoWars is Ownable
{
    struct Decision
    {
        address decider;
        Action action;
        Faction onFaction;
    }

    struct FinalDecision
    {
        address[] deciders;
        Action action;
        Faction onFaction;
        uint256 defense;
    }

    struct Action
    {
        ActionType actionType;
        bool success;
        uint256 attackPower;
    }

    struct Battle
    {
        bool isActive;
        Faction[] attackers;
        uint256 nAttackers;
        uint256 attackersPower;
        Faction defenders;
        uint256 nDefenders;
        uint256 defendersPower;
    }

    struct AttackCounter
    {
        Faction faction;
        int256 attackcounter;
    }

    event Played(uint256 timestamp, address player, Decision decision);
    event StaminaReduced(uint256 timestamp, address player, int256 value);
    event SpaceshipDamaged(uint256 timestamp, address player, int256 value);
    event SpaceshipUsured(uint256 timestamp, address player, int256 value);
    event StartedNewRound(uint256 timestamp, uint256 round);
    event RoundSettled(uint256 timestamp, uint256 round, Faction faction, FinalDecision decision);
    
    CosmoGang nftCollection;
    IERC20 rewardsToken;
    address stakingContract;
    CosmoGangItems cosmoItemsContract;

    uint256 gamePrice = 0;
    uint256 currentRound = 0;
    mapping(uint256 => mapping(address => Decision)) actions;
    mapping(uint256 => mapping(Faction => Decision[])) actionsByOnFaction;
    mapping(uint256 => mapping(Faction => FinalDecision)) finalDecisions;
    mapping(address => bool) public approvedAddresses;

    // 0 means not started, 1 means started, 2 means settled, 3 means applied
    mapping(uint256 => uint256) roundState;

    mapping(address => mapping(Faction => uint256)) public staminaPower;
    mapping(address => mapping(Faction => uint256)) public attackPower;
    mapping(address => mapping(Faction => uint256)) public spaceshipDamages;
    mapping(address => mapping(Faction => uint256)) public spaceshipFragility;
    mapping(address => mapping(Faction => uint256)) public drivingAbility;

    // Temp mapping for settling round, since mapping are not allowed to be created at runtime    
    mapping(Faction => int256) attackCounter;
    mapping(Faction => Battle) public battles;
    mapping(Faction => mapping(Faction => bool)) collaborations;
    mapping(uint256 => mapping(Faction => uint256)) factionBalances;
    mapping(uint256 => mapping(address => bool)) holderClaimed;

    uint256 defaultFragility = 30;
    uint256 defaultDrivingAbility = 50;

    // constructor(IERC721 _nftCollection, IERC20 _rewardsToken)
    constructor(address _nftCollection, IERC20 _rewardsToken, address _stakingContract, address _cosmoItemsContract)
    {
        // nftCollection = _nftCollection;
        nftCollection = CosmoGang(_nftCollection);
        rewardsToken = _rewardsToken;
        stakingContract = _stakingContract;
        cosmoItemsContract = CosmoGangItems(_cosmoItemsContract);

        roundState[currentRound] = 1;
        emit StartedNewRound(block.timestamp, currentRound);
    }
    
    // Setting holder stats
    /////////////////////////////////////////

    function setHolderAttackPower(address holder, Faction faction, uint256 newAttackPower)
        public onlyApproved
    {
        attackPower[holder][faction] = newAttackPower;
    }

    function setHolderStaminaPower(address holder, Faction faction, uint256 newStaminaPower)
        public onlyApproved
    {
        staminaPower[holder][faction] = newStaminaPower;
    }

    function setHolderSpaceshipDamages(address holder, Faction faction, uint256 newSpaceshipDamages)
        public onlyApproved
    {
        spaceshipDamages[holder][faction] = newSpaceshipDamages;
    }

    function setHolderSpaceshipFragility(address holder, Faction faction, uint256 newSpaceshipFragility)
        public onlyApproved
    {
        spaceshipDamages[holder][faction] = newSpaceshipFragility;
    }

    function setHolderDrivingAbility(address holder, Faction faction, uint256 newDrivingAbility)
        public onlyApproved
    {
        spaceshipDamages[holder][faction] = newDrivingAbility;
    }

    // Shift holder stats
    /////////////////////////////////////////

    function shiftHolderAttackPower(address holder, Faction faction, int256 _attackPower)
        public onlyApproved
    {
        if (_attackPower > 0)
        {
            attackPower[holder][faction] += uint256(_attackPower);
        }
        else
        {
            attackPower[holder][faction] -= uint256(_attackPower);
            if (attackPower[holder][faction] < 0)
            {
                attackPower[holder][faction] = 0;
            }
        }   
    }

    function shiftHolderStaminaPower(address holder, Faction faction, int256 stamina)
        public onlyApproved
    {
        if (stamina > 0)
        {
            staminaPower[holder][faction] += uint256(stamina);
        }
        else
        {
            staminaPower[holder][faction] -= uint256(stamina);
            if (staminaPower[holder][faction] < 0)
            {
                staminaPower[holder][faction] = 0;
            }
        }
    }

    function shiftHolderSpaceshipDamages(address holder, Faction faction, int256 _spaceshipDamages)
        public onlyApproved
    {
        if (_spaceshipDamages > 0)
        {
            spaceshipDamages[holder][faction] += uint256(_spaceshipDamages);
            if (spaceshipDamages[holder][faction] > 100)
            {
                spaceshipDamages[holder][faction] = 100;
            }
        }
        else
        {
            spaceshipDamages[holder][faction] -= uint256(_spaceshipDamages);
            if (spaceshipDamages[holder][faction] < 0)
            {
                spaceshipDamages[holder][faction] = 0;
            }
        }
    }

    function shiftHolderSpaceshipFragility(address holder, Faction faction, int256 _spaceshipFragility)
        public onlyApproved
    {
        if (_spaceshipFragility > 0)
        {
            spaceshipFragility[holder][faction] += uint256(_spaceshipFragility);
            if (spaceshipFragility[holder][faction] > 100)
            {
                spaceshipFragility[holder][faction] = 100;
            }
        }
        else
        {
            spaceshipFragility[holder][faction] -= uint256(_spaceshipFragility);
            if (spaceshipFragility[holder][faction] < 0)
            {
                spaceshipFragility[holder][faction] = 0;
            }
        }
    }

    function shiftHolderDrivingAbility(address holder, Faction faction, int256 _drivingAbility)
        public onlyApproved
    {
        if (_drivingAbility > 0)
        {
            drivingAbility[holder][faction] += uint256(_drivingAbility);
            if (drivingAbility[holder][faction] > 100)
            {
                drivingAbility[holder][faction] = 100;
            }
        }
        else
        {
            drivingAbility[holder][faction] -= uint256(_drivingAbility);
            if (drivingAbility[holder][faction] < 0)
            {
                drivingAbility[holder][faction] = 0;
            }
        }
    }

    // Getting holder stats
    /////////////////////////////////////////

    function getHolderAttackPower(address holder, Faction faction)
        public view
        returns (uint256)
    {
        return attackPower[holder][faction];
    }
    
    function getHolderStaminaPower(address holder, Faction faction)
        public view
        returns (uint256)
    {
        return staminaPower[holder][faction];
    }

    function getHolderSpaceshipDamages(address holder, Faction faction)
        public view
        returns (uint256)
    {
        return spaceshipDamages[holder][faction];
    }

    function getHolderSpaceshipFragility(address holder, Faction faction)
        public view
        returns (uint256)
    {
        return spaceshipFragility[holder][faction];
    }

    function getHolderDrivingAbility(address holder, Faction faction)
        public view
        returns (uint256)
    {
        
        uint256 ability = drivingAbility[holder][faction];
        if (ability == 0)
        {
            ability = defaultDrivingAbility;
        }
        return ability;
    }

    // Game functions
    ////////////////////////////////////

    modifier onlyApproved()
    {
        require(msg.sender == owner() || approvedAddresses[msg.sender], "caller is not approved");
        _;
    }

    function canPlay(address holder, Faction faction)
        public view
        returns (bool)
    {
        require(getHolderStaminaPower(holder, faction) > 0, "No more stamina");
        require(getHolderSpaceshipDamages(holder, faction) < 100, "Spaceship has been destroyed");
        return true;
    }

    function play(ActionType actionType, Faction fromFaction, Faction onFaction, uint256 regimentSize)
        public
    {
        require(nftCollection.isApprovedForAll(msg.sender, address(this)), "CosmoWars contract have to be approved for all on CosmoGang contract");
        require(roundState[currentRound] == 1, "No open round for now");
        require(canPlay(msg.sender, fromFaction), "You can't play anymore right now");

        uint256 maxRegimentSize = cosmoItemsContract.balanceOf(msg.sender, uint256(fromFaction));

        require(maxRegimentSize > 0, "You're not a holder of that faction");
        require(regimentSize <= maxRegimentSize, "Youe haven't enough NFT from this faction");
        require(rewardsToken.allowance(msg.sender, address(this)) >  gamePrice * regimentSize, "Not enough allowance for CosmoWars on your Cosmogs");
        require(rewardsToken.balanceOf(msg.sender) > gamePrice * regimentSize, "Not enough Cosmogs to go on war");
        rewardsToken.transferFrom(msg.sender, address(this), gamePrice * regimentSize);
        factionBalances[currentRound][fromFaction] = gamePrice * regimentSize;

        // Update stamina ... war is exhausting !
        shiftHolderStaminaPower(msg.sender, fromFaction, -int256(randomBetween(1, regimentSize)));

        // Will the driver avoid this asteroid ?
        uint256 ability = getHolderDrivingAbility(msg.sender, fromFaction);
        if (randomBetween(0, 100) > ability)
        {
            // How many damages will the collision cause ?
            uint256 fragility = getHolderSpaceshipFragility(msg.sender, fromFaction);
            shiftHolderSpaceshipDamages(msg.sender, fromFaction, int256(randomBetween(5, fragility)));
        }

        // Update spaceship fragility ... war is not safe !
        shiftHolderSpaceshipFragility(msg.sender, fromFaction, 10);

        // Defining the decision
        Action memory action;
        bool success = true;
        action = Action(actionType, success, regimentSize * getHolderAttackPower(msg.sender, fromFaction));

        // Registering the decision;
        Decision memory decision = Decision(msg.sender, action, onFaction);
        actions[currentRound][msg.sender] = decision;
        actionsByOnFaction[currentRound][fromFaction].push(decision);

        emit Played(block.timestamp, msg.sender, decision);
    }

    function startRound()
        public onlyApproved
    {
        if (roundState[currentRound] == 1)
        {
            endRound();
        }
        currentRound += 1;
        roundState[currentRound] = 1;
        
        emit StartedNewRound(block.timestamp, currentRound);
    }

    function endRound()
        public onlyApproved
    {
        require(roundState[currentRound] == 1, "current round already ended");
        settleRoundAll();
    }

    function settleRoundAll()
        public onlyApproved
    {
        require(roundState[currentRound] == 1, "current round already settled");
        settleRoundOne(Faction.Breedorok);
        settleRoundOne(Faction.Jahjahrion);
        settleRoundOne(Faction.Foodrak);
        settleRoundOne(Faction.Pimpmyridian);
        settleRoundOne(Faction.Muskarion);
        settleRoundOne(Faction.Lamborgardoz);
        settleRoundOne(Faction.Schumarian);
        settleRoundOne(Faction.Creatron);
        roundState[currentRound] = 2;
    }

    function settleRoundOne(Faction faction)
        public onlyApproved
    {
        Decision[] storage decisions = actionsByOnFaction[currentRound][faction];

        delete attackCounter[Faction.None];
        delete attackCounter[Faction.Jahjahrion];
        delete attackCounter[Faction.Breedorok];
        delete attackCounter[Faction.Foodrak];
        delete attackCounter[Faction.Pimpmyridian];
        delete attackCounter[Faction.Muskarion];
        delete attackCounter[Faction.Lamborgardoz];
        delete attackCounter[Faction.Schumarian];
        delete attackCounter[Faction.Creatron];

        address[] memory deciders = new address[](decisions.length);
        Faction currentFinalAttackFaction = Faction.None;
        int256 currentMaxAttackCounter = 0;
        Faction currentFinalCollabFaction = Faction.None;
        int256 currentMaxCollabCounter = 0;
        uint256 defense = 0;
        for (uint256 idx; idx < decisions.length; idx++)
        {
            Decision memory decision = decisions[idx];
            deciders[idx] = decision.decider;
            Faction onFaction = decision.onFaction;
            int256 _attackCounter = attackCounter[onFaction];
            if (decision.action.actionType == ActionType.Attack)
            {
                _attackCounter += int256(decision.action.attackPower);
            }
            else if (decision.action.actionType == ActionType.Collaborate)
            {
                _attackCounter -= int256(decision.action.attackPower);
            }
            else if (decision.action.actionType == ActionType.Defend)
            {
                defense += decision.action.attackPower;
            }

            if (_attackCounter > currentMaxAttackCounter)
            {
                currentMaxAttackCounter = _attackCounter;
                currentFinalAttackFaction = onFaction;
            }

            if (_attackCounter < currentMaxCollabCounter)
            {
                currentMaxCollabCounter = _attackCounter;
                currentFinalCollabFaction = onFaction;
            }
            attackCounter[onFaction] = _attackCounter;
        }

        Faction finalOnFaction;
        Action memory finalAction;
        
        if (currentMaxAttackCounter > (-currentMaxCollabCounter))
        {
            finalOnFaction = currentFinalAttackFaction;
            finalAction = Action(ActionType.Attack, true, uint256(currentMaxAttackCounter));
        }
        else
        {
            finalOnFaction = currentFinalCollabFaction;
            finalAction = Action(ActionType.Collaborate, true, uint256(-currentMaxCollabCounter));
        }
        FinalDecision memory finalDecision = FinalDecision(deciders, finalAction, finalOnFaction, defense);
        finalDecisions[currentRound][faction] = finalDecision;
        
        emit RoundSettled(block.timestamp, currentRound, faction, finalDecision);
    }

    function applyDecisions(uint256 round)
        public
    {
        delete battles[Faction.None];
        delete battles[Faction.Jahjahrion];
        delete battles[Faction.Breedorok];
        delete battles[Faction.Foodrak];   
        delete battles[Faction.Pimpmyridian];
        delete battles[Faction.Muskarion];
        delete battles[Faction.Lamborgardoz];
        delete battles[Faction.Schumarian];
        delete battles[Faction.Creatron];

        require(round <= currentRound, "round has not been played yet");
        
        // Getting Battles (attack) and Collaborations (collaborate)
        for (uint256 idx = 1; idx < uint(Faction.Creatron) + 1; idx++)
        {
            Faction faction = Faction(idx);
            FinalDecision memory finalDecision = finalDecisions[round][faction];
            if (finalDecision.action.actionType == ActionType.Attack)
            {
                Battle storage onFactionBattle = battles[finalDecision.onFaction];
                onFactionBattle.isActive = true;
                onFactionBattle.attackersPower += finalDecision.action.attackPower;
                onFactionBattle.defenders = finalDecision.onFaction;
                onFactionBattle.attackers.push(faction);
                onFactionBattle.nAttackers += finalDecision.deciders.length;

                collaborations[faction][finalDecision.onFaction] = false;
            }
            else
            {
                collaborations[faction][finalDecision.onFaction] = true;
            }
            
            Battle storage fromFactionBattle = battles[faction];
            fromFactionBattle.defendersPower = finalDecision.defense;
        }

        // Applying results for Battles
        for (uint256 idx = 1; idx < uint(Faction.Creatron) + 1; idx++)
        {
            Faction attackedFaction = Faction(idx);
            Battle storage battle = battles[attackedFaction];
            if (battle.isActive)
            {
                if (battle.attackersPower > battle.defendersPower)
                {
                    uint256 nAttackersFaction = battle.attackers.length;
                    uint256 attackedPool = factionBalances[round][attackedFaction];
                    factionBalances[round][attackedFaction] = 0;        
                    for (uint idx2 = 0; idx2 < battle.attackers.length; idx2++)
                    {
                        Faction attackerFaction = battle.attackers[idx2];
                        factionBalances[round][attackerFaction] += attackedPool / nAttackersFaction;
                    }
                }
                else
                {
                    uint256 attackersPool = 0;
                    for (uint idx2 = 0; idx2 < battle.attackers.length; idx2++)
                    {
                        Faction attackerFaction = battle.attackers[idx2];
                        attackersPool += factionBalances[round][attackerFaction];
                        factionBalances[round][attackerFaction] = 0;
                    }
                    factionBalances[round][attackedFaction] += attackersPool;
                }
            }
        }

        // Applying results for Collaborations
        // for (uint256 idx = 1; idx < uint(Faction.Creatron) + 1; idx++)
        // {
        //     Faction faction1 = Faction(idx);
        //     for (uint256 idx2 = 1; idx2 < uint(Faction.Creatron) + 1; idx2++)
        //     {
        //         Faction faction2 = Faction(idx2);
        //         if (collaborations[faction1][faction2])
        //         {
        //         }
        //         collaborations[faction1][faction2] = false;
        //         collaborations[faction2][faction1] = false;
        //     }  
        // }
        roundState[currentRound] = 3;
    }

    function claimRewards(uint256 round)
        public
    {
        require(roundState[round] == 1, "Round haven't been settled yet");
        require(!holderClaimed[round][msg.sender], "you already claimed your rewards for this round");
        
        for (uint256 idx = 1; idx < uint(Faction.Creatron) + 1; idx++)
        {
            Faction faction = Faction(idx);
            if (cosmoItemsContract.balanceOf(msg.sender, uint(faction)) > 0)
            {
                rewardsToken.transferFrom(address(this), msg.sender, factionBalances[round][faction] / 1000);
            }
        }
        holderClaimed[round][msg.sender] = true;
    }

    function randomBetween(uint256 min, uint256 max)
        public view
        returns (uint)
    {
        require (max > min, "max have to be > min");
        string memory difficulty = Strings.toString(block.difficulty);
        string memory timestamp = Strings.toString(block.timestamp);

        // abi.encodePacked is used to concatenate strings and get the result in bytes
        bytes memory key = abi.encodePacked(difficulty, timestamp, msg.sender);
        uint random = uint(keccak256(key)) % (max - min);
        random += min;
        return random;
    }

    function addApprovedAddress(address addr)
        external onlyOwner
    {
        approvedAddresses[addr] = true;
    }
}

