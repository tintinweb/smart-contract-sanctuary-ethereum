pragma solidity ^0.8.7;

import "https://github.com/RollaProject/solidity-datetime/blob/master/contracts/DateTimeContract.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "erc721a/contracts/ERC721A.sol";
import "base64-sol/base64.sol";

import "./potted_types.sol";

interface PPRenderer {
    function drawUnrevealPP(PottedTypes.Gene memory gene) external view returns (string memory);
    function drawRevealPP(PottedTypes.Gene memory gene) external view returns (string memory);
    function drawNoBgPP(PottedTypes.Gene memory gene) external view returns (string memory);
}

contract PottedPotted is Ownable, ERC721A, ReentrancyGuard{
    /*
    1. change pet contract
    2. check supply
    3. check all config
    4. check PPrenderer address
    5. check PPdata address
    */

    PPRenderer public renderer = PPRenderer(0xfE539f641d06D4b1279C2b538B155B10D6474355);
    
    struct Config {
        uint maxPotted;
        uint presalePrice;
        uint publicPrice;
        uint publicMaxMint;
        address petContract;
        bytes32 claimHash;
        bytes32 presaleHash;
        Phase phase;
    }

    Config public config;
    uint private _mintedId = 1;
    uint private _teamReserve = 50;
    mapping(address => bool) public claimed;
    mapping(address => bool) public got;
    mapping(address => uint) public buyed;
    mapping(uint => PottedTypes.Gene) private potteds;

    constructor() ERC721A("POTTED-POTTED", "POTTED-POTTED") {
        config.maxPotted = 5000;
        config.presalePrice = 0;
        config.publicPrice = 0;
        config.publicMaxMint = 1;
        config.petContract = 0xB74bf94049D2c01f8805B8b15Db0909168Cabf46;
        config.claimHash = 0x0;
        config.presaleHash = 0x0;
        config.phase = Phase.PUBLIC;
    }

    enum Phase {
        PAUSED,
        CLAIM,
        PRESALE,
        PUBLIC
    }

    /// ᴍᴏᴅɪꜰɪᴇʀ
    modifier validToken(uint tokenId) {
        require(tokenId >= _startTokenId() && tokenId <= _totalMinted(), "Not valid id");
        _;
    }

    modifier validOwner(uint tokenId) {
        require(ownerOf(tokenId) == msg.sender, "Not token's owner");
        _;
    }

    modifier mintable() {
        require(_totalMinted() < config.maxPotted, "No potted left");
        _;
    }

    /// ᴍᴀɪɴ
    function claimPotted(bytes32[] calldata _proof) external mintable {
        uint max = ERC721A(config.petContract).balanceOf(msg.sender);
        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));

        require(!claimed[msg.sender], "Claimed");
        require(config.phase == Phase.CLAIM, "Invalid phase");
        require(MerkleProof.verify(_proof, config.claimHash, leaf), "invalid proof");
        
        _mintPotted(max);
    }

    function getPotted(bytes32[] calldata _proof) external mintable payable {
        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));

        require(!got[msg.sender], "Got");
        require(config.phase == Phase.PRESALE, "Invalid phase");
        require(config.presalePrice <= msg.value, "No enough eth.");
        require(MerkleProof.verify(_proof, config.presaleHash, leaf), "invalid proof");
        
        _mintPotted(1);
    }

    function buyPotted(uint quantity) external mintable payable {
        require(config.phase == Phase.PUBLIC, "Invalid phase");
        require(buyed[msg.sender] <= config.maxPotted, "Exceed max buy");
        require(quantity * config.publicPrice <= msg.value, "No enough eth.");
        
        buyed[msg.sender] += quantity;
        _mintPotted(quantity);
    }

    function setPottedReveal(uint tokenId, uint revealVal) external validOwner(tokenId) {
        require(revealVal >= 1 && revealVal <= 8, "Out of range");
        require(potteds[tokenId].revealVal == 0, "Potted revealed");

        _setPottedReveal(tokenId, revealVal);
    }

    function _mintPotted(uint quantity) private {
        for (uint i = 0; i < quantity; i++) {
            uint tokenId = _mintedId;
            uint seed = _getRandom(tokenId);

            potteds[tokenId] = PottedTypes.Gene(seed, 0);
            _mintedId++;
        }

        _safeMint(msg.sender, quantity);
    }

    function _setPottedReveal(uint _tokenId, uint _revealVal) private {
        potteds[_tokenId] = PottedTypes.Gene(potteds[_tokenId].dna, _revealVal);
    }

    /// ᴍɪɴᴏʀ
    function _getUnreveal(PottedTypes.Gene memory gene) private view returns (string memory) {
        string memory json = 
                string(
                    abi.encodePacked(
                        '{"name": "#"',
                        '"description": "",', 
                        '"attributes": [',
                        '],'
                        '"image": "data:image/svg+xml;base64,', Base64.encode(bytes(renderer.drawRevealPP(gene))), '"}' 
                        )
                    );

        return Base64.encode(
            bytes(
                string(json)
                )
            );
    }

    function _getReveal(PottedTypes.Gene memory gene) private view returns (string memory) {
        string memory json = 
                string(
                    abi.encodePacked(
                        '{"name": "#"',
                        '"description": "",', 
                        '"attributes": [',
                        '],'
                        '"image": "data:image/svg+xml;base64,', Base64.encode(bytes(renderer.drawRevealPP(gene))), '"}' 
                        )
                    );

        return Base64.encode(
            bytes(
                string(json)
                )
            );
    }

    function _tokenURI(uint256 tokenId) private view  returns (string memory) {
        PottedTypes.Gene memory gene = potteds[tokenId];
        string memory json = gene.revealVal == 0 ? _getUnreveal(gene) : _getReveal(gene);

        return string(abi.encodePacked('data:application/json;base64,', json));
    }

    function tokenURI(uint256 tokenId) override (ERC721A) public view validToken(tokenId) returns (string memory) {
        return _tokenURI(tokenId);
    }

    function numberMinted(address _addr) public view returns(uint256){
        return _numberMinted(_addr);
    }

    function _startTokenId() override internal pure virtual returns (uint256) {
        return 1;
    }
    
    function _getRandom(uint tokenId) private view returns (uint) {
        uint randomlize = uint(keccak256(abi.encodePacked(blockhash(block.number - 1), tokenId, msg.sender)));
        return randomlize;
    }
    
    // ERC721AQueryable.sol
    function tokensOfOwner(address owner) public view virtual returns (uint256[] memory) {
        unchecked {
            uint256 tokenIdsIdx;
            address currOwnershipAddr;
            uint256 tokenIdsLength = balanceOf(owner);
            uint256[] memory tokenIds = new uint256[](tokenIdsLength);
            TokenOwnership memory ownership;
            for (uint256 i = _startTokenId(); tokenIdsIdx != tokenIdsLength; ++i) {
                ownership = _ownershipAt(i);
                if (ownership.burned) {
                    continue;
                }
                if (ownership.addr != address(0)) {
                    currOwnershipAddr = ownership.addr;
                }
                if (currOwnershipAddr == owner) {
                    tokenIds[tokenIdsIdx++] = i;
                }
            }
            return tokenIds;
        }
    }

    /// ᴀᴅᴍɪɴ
    function setPresalePrice(uint data) external onlyOwner {
        config.presalePrice = data;
    }
    function setPublicPrice(uint data) external onlyOwner {
        config.publicPrice = data;
    }
    function setPublicMaxMint(uint data) external onlyOwner {
        config.publicMaxMint = data;
    }
    function setClaimHash(bytes32 data) external onlyOwner {
        config.claimHash = data;
    }
    function setPresaleHash(bytes32 data) external onlyOwner {
        config.presaleHash = data;
    }
    function setPhase(Phase phase) external onlyOwner {
        config.phase = phase;
    }
    function setRenderContract(address _address) external onlyOwner {
        renderer = PPRenderer(_address);
    }
    function teamReserve() external mintable onlyOwner {
        _mintPotted(_teamReserve);
    }
    function withdraw() external onlyOwner nonReentrant {
        (bool success, ) = msg.sender.call{value: address(this).balance}("");
        require(success, ".");
    }
}

