pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155URIStorage.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "./INFT1155.sol";

contract NFT1155 is
    INFT1155,
    ERC1155Supply,
    ERC1155URIStorage,
    ERC2981,
    Pausable,
    ReentrancyGuard,
    AccessControl,
    Ownable
{
    using ECDSA for bytes32;
    using Strings for uint256;
    using SafeMath for uint256;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    string public _name;
    string public _symbol;
    string private baseURI_ = "";
    uint256 private _ethAmount;
    mapping (uint256 => bool) private _minted;

    constructor(
        string memory tokenName,
        string memory tokenSymbol,
        uint96 feeNumerator,
        string memory baseURI
    ) ERC1155(baseURI) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _setDefaultRoyalty(msg.sender, feeNumerator);
        _setName(tokenName);
        _setSymbol(tokenSymbol);
        _setBaseURI(baseURI);
    }

    function _setName(string memory tokenName) internal virtual {
        _name = tokenName;
    }

    function _setSymbol(string memory tokenSymbol) internal virtual {
        _symbol = tokenSymbol;
    }

    function _baseURI() internal view returns (string memory) {
        return baseURI_;
    }

    function pause() public override onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public override onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function _setApprovalForAll(
        address holder,
        address operator,
        bool approved
    ) internal virtual override {
        require(
            !paused() ||
                hasRole(DEFAULT_ADMIN_ROLE, msg.sender) ||
                hasRole(MINTER_ROLE, msg.sender),
            "Paused or Invalid Role"
        );
        super._setApprovalForAll(holder, operator, approved);
    }

    function _safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) internal virtual override {
        require(
            !paused() ||
                hasRole(DEFAULT_ADMIN_ROLE, msg.sender) ||
                hasRole(MINTER_ROLE, msg.sender),
            "Paused or Invalid Role"
        );
        super._safeTransferFrom(from, to, id, amount, data);
    }

    function mint(
        address to,
        uint256 tokenId,
        uint256 amount,
        string memory tokenUri
    ) public override onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        _mint(to, tokenId, amount, "");
        _setURI(tokenId, tokenUri);
    }

    function mintBatch(
        address[] memory accounts,
        uint256[] memory tokenIds,
        uint256[] memory amounts
    ) external override onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        require(
            accounts.length == tokenIds.length,
            "accounts and tokenIds length mismatch"
        );
        require(
            tokenIds.length == amounts.length,
            "tokenIds and amounts length mismatch"
        );

        for (uint256 i = 0; i < tokenIds.length; ++i) {
            address account = accounts[i];
            uint256 tokenId = tokenIds[i];
            uint256 amount = amounts[i];
            _mint(account, tokenId, amount, "");
        }
    }

    function burn(
        address to,
        uint256 tokenId,
        uint256 amount
    ) public override onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        _burn(to, tokenId, amount);
    }

    function burnBatch(
        address[] memory accounts,
        uint256[] memory tokenIds,
        uint256[] memory amounts
    ) external override onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        require(
            accounts.length == tokenIds.length,
            "accounts and ids length mismatch"
        );
        require(
            tokenIds.length == amounts.length,
            "ids and amounts length mismatch"
        );

        for (uint256 i = 0; i < tokenIds.length; ++i) {
            uint256 tokenId = tokenIds[i];
            address account = accounts[i];
            uint256 amount = amounts[i];
            _burn(account, tokenId, amount);
        }
    }

    function buy(
        uint256 tokenId,
        uint256 expires,
        bytes memory signature,
        address to,
        uint256 orderId,
        string memory tokenUri,
        uint256 orderAmount,
        uint256 totalSupplyLimit
    ) external payable override nonReentrant {
        require(expires >= block.timestamp, "Expired signature");
        require(
            orderAmount + totalSupply(tokenId) <= totalSupplyLimit,
            "TotalSupply Limit"
        );
        require(!_minted[orderId], "NFT is received.");
        uint256 value = msg.value;
        {
            // CompilerError: Stack too deep回避のため、ブロックを利用
            bytes32 messageHash = keccak256(
                abi.encodePacked(
                    address(this),
                    to,
                    tokenId,
                    value,
                    orderId,
                    expires,
                    orderAmount,
                    totalSupplyLimit,
                    tokenUri
                )
            );
            address signer = messageHash.toEthSignedMessageHash().recover(
                signature
            );
            require(hasRole(MINTER_ROLE, signer), "Invalid signature");
        }
        _mint(to, tokenId, orderAmount, "");
        _minted[orderId] = true;
        _setURI(tokenId, tokenUri);
        _ethAmount = _ethAmount.add(value);
        emit Buy(
            msg.sender,
            to,
            tokenId,
            value,
            orderId,
            expires,
            orderAmount,
            totalSupplyLimit
        );
    }

    function withdraw(address payable to)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
        nonReentrant
    {
        uint256 amount = _ethAmount;
        _ethAmount = 0;

        to.transfer(amount);
    }

    function balance()
        external
        view
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (uint256)
    {
        return _ethAmount;
    }

    // For etherscan

    function name() public view virtual override returns (string memory) {
        return _name;
    }

    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory tokenIds,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) {
        super._beforeTokenTransfer(operator, from, to, tokenIds, amounts, data);
    }

    function _burn(
        address to,
        uint256 tokenId,
        uint256 amount
    ) internal override(ERC1155) {
        super._burn(to, tokenId, amount);
        _resetTokenRoyalty(tokenId);
    }

    function uri(uint256 tokenId)
        public
        view
        override(INFT1155, ERC1155, ERC1155URIStorage)
        returns (string memory)
    {
        return super.uri(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155, ERC2981, AccessControl)
        returns (bool)
    {
        return
            interfaceId == type(INFT1155).interfaceId ||
            interfaceId == type(ERC1155Supply).interfaceId ||
            interfaceId == type(ERC1155URIStorage).interfaceId ||
            interfaceId == type(Pausable).interfaceId ||
            interfaceId == type(Ownable).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function _grantRole(bytes32 role, address account)
        internal
        virtual
        override(AccessControl)
    {
        super._grantRole(role, account);
    }

    function _revokeRole(bytes32 role, address account)
        internal
        virtual
        override(AccessControl)
    {
        super._revokeRole(role, account);
    }
}

