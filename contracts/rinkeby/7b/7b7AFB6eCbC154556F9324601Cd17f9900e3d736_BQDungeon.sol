pragma solidity ^0.8.4;

import "erc721a/contracts/ERC721A.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "base64-sol/base64.sol";
import "./BQToken.sol";

contract BQWeapon is ERC721A, Pausable, Ownable {
    event CreateWeapon(uint256 indexed tokenId);

    bytes32 internal entropySauce;
    uint16 public maxSupply;
    uint public cost;

    BQToken bqt;

    struct Weapon {
        // 0: strength
        // 1: agility
        // 2: intelligence
        uint8 weaponType;
        uint16 power;
    }

    mapping(uint16 => Weapon) weapons;
    mapping(address => bool) controllers;

    constructor(address _bqToken) ERC721A ("Weapon", "WP") {
        cost = 10;
        bqt = BQToken(_bqToken);
        maxSupply = 10000;
    }

    // weapon only have 1 statistics

    function mint(uint8 _mintAmount) public whenNotPaused safetyProcedure {
        uint16 supply = uint16(totalSupply());
        require(_mintAmount > 0, "must mint at least 1 weapon");
        require(supply + _mintAmount <= maxSupply, "minting exceed max supply");

        for (uint16 i = 0; i < _mintAmount; i++)
        {
            uint16 id = supply + i;

            uint256 rand = _randomize("rand", id);

            uint8 weaponType_ = uint8(rand) % 3;
            rand = rand >> 8;
            uint16 power_ = uint16(rand) % 1000;
            // rand = rand >> 16;
            // uint16 agility_ = uint16(rand);
            // rand = rand >> 16;
            // uint16 intelligence_ = uint16(rand);

            weapons[id] = Weapon({
                weaponType: weaponType_,
                power: power_
            });
            emit CreateWeapon(id);
        }
        bqt.burn(_msgSender(), cost * _mintAmount);
        _safeMint(_msgSender(), _mintAmount);
        
    }

    function mint(address _owner, uint8 _weaponType, uint16 _power) private whenNotPaused {
        uint16 supply = uint16(totalSupply());
        weapons[supply] = Weapon({
            weaponType: _weaponType,
            power: _power
        });
        _safeMint(_owner, 1);
    }

    function getPower(uint16 _tokenId) public view returns (uint256)
    {
        Weapon memory weapon = weapons[_tokenId];
        return (weapon.power);
    }

    function getWeaponStats(uint16 _tokenId) public view returns (uint8 weaponType, uint16 power) {
        Weapon memory weapon = weapons[_tokenId];
        weaponType = weapon.weaponType;
        power = weapon.power;
    }

    function updateWeaponPower(uint16 _tokenId, uint16 _power) public onlyController {
        Weapon storage weapon = weapons[_tokenId];
        weapon.power = _power;
    }

    function setController(address _controller, bool _value) external onlyOwner {
        controllers[_controller] = _value;
    }

    function fuseWeapons(uint8 _weaponType, uint16 _weapon1Id, uint16 _weapon2Id) external safetyProcedure {
        // pay bq token and burn
        Weapon memory weapon1 = weapons[_weapon1Id];
        Weapon memory weapon2 = weapons[_weapon2Id];
        require(_weaponType == weapon1.weaponType || _weaponType == weapon2.weaponType, "weapon type not valid");
        uint16 newPower = 0;
        if (weapon1.power > weapon2.power) {
            newPower = weapon1.power;
        } else {
            newPower = weapon2.power;
        }
        uint16 multiplier = uint16(_rand() % 10000);
        newPower = uint16(uint256(newPower) * uint256(multiplier)/10000);
        mint(_msgSender(), _weaponType, newPower);
        _burn(_weapon1Id);
        _burn(_weapon2Id);
        delete weapons[_weapon1Id];
        delete weapons[_weapon2Id];
    }

    /**
    * enables owner to pause / unpause minting
    */
    function setPaused(bool _paused) external onlyOwner {
        if (_paused) _pause();
        else _unpause();
    }

    function _rand() internal view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(msg.sender, block.timestamp, entropySauce)));
    }

    /// @dev Create a bit more of randomness
    function _randomize(string memory _val, uint256 _spicy) internal view returns (uint256) {
        return uint256(keccak256(abi.encode(entropySauce, _val, _spicy)));
    }

    // MODIFIERS

    modifier safetyProcedure() {
        uint256 size = 0;
        address acc = msg.sender;
        assembly { size := extcodesize(acc)}

        require(msg.sender == tx.origin , "sender not allowed");
        require(size == 0,                "cannot use contract");
        _;

        // We'll use the last caller hash to add entropy to next caller
        entropySauce = keccak256(abi.encodePacked(acc, block.coinbase));
    }

    modifier onlyController() {
        require(controllers[msg.sender], "not controller");
        _;
    }
}

