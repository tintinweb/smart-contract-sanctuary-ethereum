pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "base64-sol/base64.sol";
import "erc721a/contracts/ERC721A.sol";

interface MetadataHandler {
    function getTokenURI(uint16 _tokenId)
                         external view returns (string memory);
}

interface IBlockQuest {

}

contract Primus is ERC721A, Ownable {
    event CreatedPrimus(uint256 indexed tokenId);

    uint public cost;
    uint16 maxSupply;
    uint16 public publicMaxMint;
    uint16 public whitelistMaxMint;

    bytes32 internal entropySauce;

    uint8 mintStatus;

    uint8 numEffect = 13;
    uint8 numBody = 21;
    uint8 numHead = 37;
    uint8 numElbow = 22;
    uint8 numKnee = 29;

    struct Warrior {
        uint8 effect;
        uint8 body;
        uint8 head;
        uint8 elbow;
        uint8 knee;

        uint16 power;
        uint16 agility;
        uint16 intelligence;
        uint16 luck; // divided by 10000

        uint16 win;
        uint16 totalBattle;
        uint16 lastBattle;
    }

    // mapping(address => bool) whitelistAddresses;

    mapping(uint256 => Warrior) warriors;

    IBlockQuest blockQuest;

    bool revealed = false;
    string unrevealedString;

    // list of probabilities for each trait type
    uint8[][5] public rarities;
    // list of aliases for Walker's Alias algorithm
    uint8[][5] public aliases;

    MetadataHandler metaHandler;

    bytes32 public root;

    constructor() ERC721A ("Primus", "Primus") Ownable() {
        cost = 0.01 ether;
        maxSupply = 8888;
        whitelistMaxMint = 10;
        publicMaxMint = 10;

        // A.J. Walker's Alias Algorithm

        // effect: 13
        rarities[0] = [204, 153, 255, 255, 255, 255, 255, 153, 101, 204, 101, 50, 153];
        aliases[0] = [3, 4, 0, 0, 0, 0, 0, 6, 7, 5, 5, 8, 8];
        // body: 21
        rarities[1] = [148, 255, 212, 148, 248, 177, 198, 148, 255, 212, 148, 248, 177, 198, 148, 255, 212, 148, 248, 177, 198];
        aliases[1] = [2, 0, 1, 5, 5, 2, 5, 9, 0, 8, 12, 12, 9, 12, 16, 0, 15, 19, 19, 16, 19];
        // head: 37
        rarities[2] =  [255, 224, 192, 203, 140, 199, 205, 239, 115, 227, 239, 110, 73, 172, 161, 98, 143, 136, 72, 158, 190, 158, 239, 100, 159, 212, 63, 186, 79, 79, 79, 66, 66, 66, 52, 52, 52];
        aliases[2] = [0, 0, 1, 2, 3, 4, 5, 0, 6, 8, 0, 9, 11, 12, 13, 14, 15, 16, 17, 18, 19, 1, 1, 20, 2, 2, 23, 2, 4, 5, 8, 12, 15, 16, 18, 19, 26];
        // elbow: 22
        rarities[3] = [255, 232, 203, 235, 177, 148, 238, 251, 177, 187, 158, 177, 213, 249, 206, 213, 106, 142, 177, 70, 106, 142];
        aliases[3] = [ 0, 0, 1, 2, 3, 4,  5, 6, 0, 7, 9, 2, 2,  2, 10, 2, 3, 5, 5, 6, 10, 14];
        // knee: 29
        rarities[4] = [251, 255, 232, 187, 240, 213, 190, 199, 219, 212, 153, 219, 193, 166, 251, 156, 251, 211, 156, 219, 152, 188, 93, 188, 251, 93, 156, 219, 125];
        aliases[4] = [ 1, 0, 1, 2, 3, 4, 5, 6, 2, 7, 9, 2, 10, 12, 2, 3, 3, 13, 3, 3, 17, 5, 6, 7, 7, 10, 13, 13, 20];
    }

    function setMetadataHandler(address _metadataHandler) external onlyOwner {
        metaHandler = MetadataHandler(_metadataHandler);
    }

    function setRevealed(bool value) external onlyOwner {
        revealed = value;
    }

    function setUnrevealedString(string memory value) external onlyOwner {
        unrevealedString = value;
    }

    function setRoot(bytes32 _root) public onlyOwner {
        root = _root;
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
        _exists(_tokenId),
        "ERC721Metadata: URI query for nonexistent token"
        );

        if (revealed) {
            return metaHandler.getTokenURI(uint16(_tokenId));
        } else {
            return string(
                abi.encodePacked(
                    'data:application/json;base64,',
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"Primus #', 
                                Strings.toString(_tokenId),
                                unrevealedString
                            )
                        )
                    )
                )
            );
        }

    }

    function setBlockQuest(address _blockQuest) external onlyOwner {
        blockQuest = IBlockQuest(_blockQuest);
    }

    function setMintStatus(uint8 _status) external onlyOwner {
        mintStatus = _status;
    }

    function whitelistMint(uint8 _mintAmount, bytes32[] calldata _merkleProof) external payable safetyProcedure {
        require(mintStatus == 1, "mint unavailable");
        bytes32 _leaf = keccak256(abi.encodePacked(msg.sender));
        require(MerkleProof.verify(_merkleProof, root, _leaf), "invalid proof");
        require((_mintAmount + balanceOf(_msgSender())) <= whitelistMaxMint, "exceeded allowed mint");

        _mint(_mintAmount);
    }

    function publicMint(uint8 _mintAmount) external payable safetyProcedure {
        require(mintStatus == 2, "mint unavailable");
        require((_mintAmount + balanceOf(_msgSender())) <= publicMaxMint, "exceeded allowed mint");

        _mint(_mintAmount);
    }

    /**
    * mint
    */
    function _mint(uint8 _mintAmount) internal {
        require(_mintAmount > 0, "at least 1 token");
        require(_mintAmount * cost == msg.value, "Invalid amount.");
        uint256 supply = totalSupply();
        require(supply + _mintAmount <= maxSupply, "exceed max supply");

        // uint256 seed = _rand();

        for (uint256 i = 0; i < _mintAmount; i++)
        {
            uint256 id = supply + i;

            uint256 rand = _randomize("warrior", id);
            uint8 effect = selectTrait(uint8(rand), 0);
            rand = rand >> 16;
            uint8 body = selectTrait(uint8(rand), 1);
            rand = rand >> 16;
            uint8 head = selectTrait(uint8(rand), 2);
            rand = rand >> 16;
            uint8 elbow = selectTrait(uint8(rand), 3);
            rand = rand >> 16;
            uint8 knee = selectTrait(uint8(rand), 4);
            
            rand = rand >> 16;
            uint16 power = uint16(rand % 1000); // strength
            rand = rand >> 16;
            uint16 agility = uint16(rand % 1000);
            rand = rand >> 16;
            uint16 intelligence = uint16(rand % 1000);

            rand = rand >> 16;
            uint16 luck = uint16(rand % 4000 + 5000); // luck from 50% to 90%

            warriors[id] = Warrior({
                effect: effect,
                body: body,
                head: head,
                elbow: elbow,
                knee: knee,

                power: power,
                agility: agility,
                intelligence: intelligence,

                luck: luck, // divided by 10000

                win: 0,
                totalBattle: 0,
                lastBattle: 0
            });
            emit CreatedPrimus(id);
        }
        _safeMint(msg.sender, _mintAmount);
    }

    function selectTrait(uint16 seed, uint8 traitType) internal view returns (uint8) {
        uint8 trait = uint8(seed) % uint8(rarities[traitType].length);
        if (seed >> 8 < rarities[traitType][trait]) return trait;
        return aliases[traitType][trait];
    }

    /**
    * TODO: change to splitter if possible
    * allows owner to withdraw funds from minting
    */
    function withdraw() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    /// @dev Create a bit more of randomness
    function _randomize(string memory _val, uint256 _spicy) internal view returns (uint256) {
        return uint256(keccak256(abi.encode(entropySauce, _val, _spicy)));
    }

    function getWarriorTraits(uint16 _tokenId) external view returns (uint8 effect,
        uint8 body, uint8 head, uint8 elbow, uint8 knee) {
        Warrior memory warrior = warriors[_tokenId];

        effect = warrior.effect;
        body = warrior.body;
        head = warrior.head;
        elbow = warrior.elbow;
        knee = warrior.knee;
    }

    function getWarriorStats(uint16 _tokenId) external view returns (uint16 power, 
        uint16 agility, uint16 intelligence, uint16 luck, uint16 win,
        uint16 totalBattle) {
        Warrior memory warrior = warriors[_tokenId];

        power = warrior.power;
        agility = warrior.agility;
        intelligence = warrior.intelligence;
        luck = warrior.luck;
        win = warrior.win;
        totalBattle = warrior.totalBattle;
    }

    function updateWarriorStats(uint16 _tokenId, uint16 _power, uint16 _agility, uint16 _intelligence,
        uint16 _luck, uint16 _win, uint16 _totalBattle) external {
        require(msg.sender == address(blockQuest), "cannot update stats");
        Warrior storage warrior = warriors[_tokenId];
        warrior.power = _power;
        warrior.agility = _agility;
        warrior.intelligence = _intelligence;
        warrior.luck = _luck;
        warrior.win = _win;
        warrior.totalBattle = _totalBattle;
    }

    // MODIFIERS

    modifier safetyProcedure() {
        uint256 size = 0;
        address acc = msg.sender;
        assembly { size := extcodesize(acc)}

        require(msg.sender == tx.origin , "sender not allowed");
        require(size == 0,                "cannot use contract");
        _;

        // We'll use the last caller hash to add entropy to next caller
        entropySauce = keccak256(abi.encodePacked(acc, block.coinbase));
    }

    /**
     * @dev See {IERC721-isApprovedForAll}.
     */
    function isApprovedForAll(address owner, address operator) public view virtual override returns (bool) {
        // hardcode approval so user doesn't need to spend gas to approve
        if (operator == address(blockQuest)) {
            return true;
        } else {
            return super.isApprovedForAll(owner, operator);
        }
    }
}

