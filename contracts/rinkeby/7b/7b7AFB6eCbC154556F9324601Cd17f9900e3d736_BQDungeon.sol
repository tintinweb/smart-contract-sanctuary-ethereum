pragma solidity ^0.8.4;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./Primus.sol";
import "./BQWeapon.sol";
import "./Corrupted.sol";
import "./BlockQuest.sol";
import "hardhat/console.sol";

contract BQDungeon is Pausable, Ownable {
    Primus warrior;
    BQWeapon weapon;
    Corrupted monster;
    BlockQuest blockQuest;
    uint256 nextBattle = 0;

    bytes32 internal entropySauce;

    constructor(address _warrior, address _weapon, address _monster, address _blockQuest) {
        warrior = Primus(_warrior);
        weapon = BQWeapon(_weapon);
        monster = Corrupted(_monster);
        blockQuest = BlockQuest(_blockQuest);
    }

    function battle(uint16 _warriorId, uint16 _weaponId, uint16 _monsterId) external returns (bool)
    {
        require(blockQuest.isWarriorStakedByOwner(uint16(_warriorId), _msgSender()), "warrior not staked");
        require(weapon.ownerOf(_weaponId) == _msgSender(), "you don't own the weapon");
        (uint16 power, uint16 agility, uint16 intelligence, uint16 luck, uint16 win,
            uint16 totalBattle) = warrior.getWarriorStats(uint16(_warriorId));
        bool battleResult;
        {
            (uint256 weaponPower) = weapon.getPower(_weaponId);
            (uint16 monsterStrength, uint16 variant1, uint16 variant2) = monster.getMonsterStats(_monsterId);
            uint16 battlePower = uint16(power + agility + intelligence + weaponPower);
            battleResult = battle_mechanic(battlePower, luck, monsterStrength, variant1, variant2);
        }
        totalBattle = totalBattle + 1;
        if (battleResult) {
            win = win + 1;
            blockQuest.updateWarriorStats(_warriorId, power, agility, intelligence, luck, win, totalBattle);
        }
        return battleResult;
    }

    function battle_mechanic(uint16 _playerPower, 
                             uint16 _playerLuck, 
                             uint16 _monsterStrength,
                             uint16 _monsterVariant1,
                             uint16 _monsterVariant2) internal
    returns (bool)
    {
        uint256 calculated_player_strength = 0;
        if (_playerPower > _monsterStrength) 
        {
            calculated_player_strength = _monsterStrength;
        }
        else
        {
            calculated_player_strength = _playerPower;
        }


        uint256 calculated_monster_strength = 0;
        if (count_random(_playerLuck))
        {
            calculated_monster_strength = uint256(_monsterStrength) * uint256(_monsterVariant1) / 100;
        }
        else
        {
            calculated_monster_strength = uint256(_monsterStrength) * uint256(_monsterVariant2)/ 100;
        }
        uint16 win_rate = uint16(calculated_player_strength * 10000 / calculated_monster_strength);
        return count_random(win_rate);
    }

    function count_random(uint16 prob) internal returns (bool) {
        entropySauce = keccak256(abi.encodePacked(msg.sender, block.coinbase));
        uint16 rand = uint16(_rand() % 10000);
        console.log(rand);
        return rand < prob;
    }

    function _rand() internal view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(msg.sender, block.timestamp, entropySauce)));
    }

    modifier noCheaters() {
        uint256 size = 0;
        address acc = msg.sender;
        assembly { size := extcodesize(acc)}

        require(msg.sender == tx.origin , "you're trying to cheat!");
        require(size == 0,                "you're trying to cheat!");
        _;

        // We'll use the last caller hash to add entropy to next caller
        entropySauce = keccak256(abi.encodePacked(acc, block.coinbase));
    }
    
    // Stick with 24 hr battle
    // Reward will be decreasing based on time
    // function battle(BQMonster.MonsterAttr memory monsterAttr) view external {
    //     // TODO: implement battle logic
    //     uint256 var1 = block.timestamp % 10000000;
    //     bool canBattle = var1<1000;
    // }
}

