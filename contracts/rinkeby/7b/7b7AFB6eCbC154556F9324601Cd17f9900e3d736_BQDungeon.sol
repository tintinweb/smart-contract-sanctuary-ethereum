    event StakeWarrior(address player, uint16 tokenId);
    event UnstakeWarrior(address player, uint16 tokenId);

    BQToken bqt;
    Primus warrior;

    // struct to store a stake's token, owner, and earning values
    struct Stake {
        uint16 tokenId;
        uint80 lastUpdated;
        address owner;

    }
    mapping(uint16 => Stake) public stakes;

    // data structure to keep track of staked tokens
    mapping(address => uint16) _balance;
    mapping(address => mapping(uint16 => uint16)) private _stakedTokens;
    mapping(uint16 => uint16) private _stakedTokensIndex;

    mapping(address => bool) warriorControllers;

    uint256 private HOURLY_BQ_RATE = 20;

    
    constructor(address _warrior, address _bqToken) {
        warrior = Primus(_warrior);
        bqt = BQToken(_bqToken);
    }

    // STAKING

    function isWarriorStakedByOwner(uint16 _warriorId, address _owner) public view returns (bool) {
        return stakes[_warriorId].owner == _owner;
    }

    function _stakeWarrior(address _owner, uint16 _warriorId) private {
        uint16 length = _balance[_owner];
        _stakedTokens[_owner][length] = _warriorId;
        _stakedTokensIndex[_warriorId] = length;

        stakes[_warriorId] = Stake({tokenId:_warriorId, 
            lastUpdated: uint80(block.timestamp), owner: _owner});
        warrior.transferFrom(_owner, address(this), _warriorId);

        _balance[_owner] += 1;
        emit StakeWarrior(_owner, _warriorId);
    }

    function _unstakeWarrior(address _owner, uint16 _warriorId) private {
        uint16 lastStakedIndex = _balance[_owner] - 1;
        uint16 stakedIndex = _stakedTokensIndex[_warriorId];

        // if unstaked index is the last staked index, don't need to swap
        if (stakedIndex != lastStakedIndex) {
            uint16 lastWarriorId = _stakedTokens[_owner][lastStakedIndex];

            _stakedTokens[_owner][stakedIndex] = lastWarriorId;
            _stakedTokensIndex[lastWarriorId] = stakedIndex;
        }

        Stake memory stake = stakes[_warriorId];
        warrior.transferFrom(address(this), _owner, _warriorId);
        uint256 owed = (block.timestamp - stake.lastUpdated) * HOURLY_BQ_RATE / 1 hours;
        delete stakes[_warriorId];
        _transferReward(_msgSender(), owed);

        _balance[_owner] -= 1;
        emit UnstakeWarrior(_owner, _warriorId);
    }

    function stakeWarriors(uint16[] calldata _warriorIds) external {
        for (uint i = 0; i < _warriorIds.length; i++) {
            uint16 warriorId = _warriorIds[i];
            require(warrior.ownerOf(warriorId) == _msgSender(), "cannot stake token(s) not belong to you");
            
            _stakeWarrior(_msgSender(), warriorId);
        }
    }

    function unstakeWarriors(uint16[] calldata _warriorIds) external {
        
        for (uint i = 0; i < _warriorIds.length; i++) {
            uint16 warriorId = _warriorIds[i];
            require(stakes[warriorId].owner == _msgSender(), "Cannot unstake token not belong to you");
            
            _unstakeWarrior(_msgSender(), warriorId);
        }
    }

    function stakedWarriorsByOwner(address _owner) public view returns (uint16[] memory) {
        uint16 numStaked = _balance[_owner];
        uint16[] memory tokenIds = new uint16[](numStaked);
        for (uint16 i = 0; i < numStaked; i++) {
            tokenIds[i] = _stakedTokens[_owner][i];
        }
        return tokenIds;
    }

    // EARNINGS & REWARDS

    function viewEarnings(address owner) external view returns (uint256) {
        uint16[] memory warriorIds = stakedWarriorsByOwner(owner);
        uint256 earnings = 0;

        for (uint256 i = 0; i < warriorIds.length; i++) {
            Stake memory stake = stakes[warriorIds[i]];
            earnings += (block.timestamp - stake.lastUpdated) * HOURLY_BQ_RATE / 1 hours;
        }
        return earnings;
    }

    function claimEarnings() external {
        uint16[] memory warriorIds = stakedWarriorsByOwner(_msgSender());
        uint256 owed = 0;

        for (uint256 i = 0; i < warriorIds.length; i++) {
            Stake storage stake = stakes[warriorIds[i]];
            owed += (block.timestamp - stake.lastUpdated) * HOURLY_BQ_RATE / 1 hours;
            stake.lastUpdated = uint80(block.timestamp);
        }
        _transferReward(_msgSender(), owed);
    }

    function _transferReward(address recepient, uint256 reward) private {
        bqt.mint(recepient, reward);
    }

    function setWarriorController(address _controller, bool _value) external onlyOwner {
        warriorControllers[_controller] = _value;
    }

    function updateWarriorStats(uint16 _tokenId, uint16 _power, uint16 _agility, uint16 _intelligence,
        uint16 _luck, uint16 _win, uint16 _totalBattle) public {
        require(warriorControllers[_msgSender()], "not warrior controller");
        warrior.updateWarriorStats(_tokenId, _power, _agility, _intelligence, _luck, _win, _totalBattle);
    }

    // Not sure whether this is needed
    // function onERC721Received(
    //     address,
    //     address from,
    //     uint256,
    //     bytes calldata
    // ) external pure override returns (bytes4) {
    //     // require(from == address(0x0), "Cannot send tokens to BlockQuest directly");
    //     return IERC721Receiver.onERC721Received.selector;
    // }
}

