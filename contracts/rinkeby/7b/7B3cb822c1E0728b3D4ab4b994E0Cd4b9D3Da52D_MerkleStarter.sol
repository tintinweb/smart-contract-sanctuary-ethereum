pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";

contract MerkleStarter is Ownable {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    // claim事件
    event Claimed(address claimant,uint256 Month,uint256 balance);
    // 白名单默克尔树增加
    event TrancheAdded(
        uint256 tranche,
        bytes32 merkleRoot,
        uint256 totalAmount
    );

    event TrancheExpired(uint256 tranche);


    mapping(uint256 => bytes32) public merkleRoots;

    // claim项目的配置
    struct claimInfo {
      uint256 publicSaleTime;
      uint256 claimMaxBNB; //每个用户最大兑换token数量
      uint256 claimMaxToken; // 用户申领的最大的token数量 claimBnb*exchangerate
      uint256 exchangeRate; // 1bnb可以兑换多少代币
      uint256 initPercentage; // 每次购买初始分配百分比
      uint256 harvestTimes; // 领取次数
      uint256 totalAmount; // 代币的总供应量
      address token; // 分配token的地址
    }
    mapping(uint256 => claimInfo) public claimInfos;

    struct harvestedInfo{
        uint256 lastClaimedTime;
        uint256 receiveTimes;
        uint256 remainingAmount; // 剩余token
        address token; // 分配token的地址
    }
    // 领取时间
    mapping(uint256 => mapping(address => harvestedInfo)) public harvested;

    // claim的数量
    mapping(uint256 => mapping(address => uint256)) public claimed;

    uint256 public tranches;

    constructor() public {
    }
    event Received(address, uint);
    receive() external payable {
        emit Received(msg.sender, msg.value);
    }
    // 设置项目claim配置
    function setClaimConfig(uint256 trancheId,uint256
        claimMaxBNB,uint256 exchangeRate,uint256 initPercentage,uint256 harvestTimes,
        uint256 totalAmount,uint256 publicSaleTime,address token) public onlyOwner  {
        uint256 claimMaxToken = claimMaxBNB.mul(exchangeRate);
        claimInfos[trancheId] = claimInfo(
            publicSaleTime,
            claimMaxBNB,
            claimMaxToken,
            exchangeRate,
            initPercentage, // 50 初始50 拿到50token
            harvestTimes,   // 7, 50 / 7
            totalAmount,    // 一个项目发售多次 100  50 50
            token);
    }
    // 添加项目
    // 加白名单
    // 加池子
    function seedNewAllocations(address token,bytes32 _merkleRoot, uint256 _totalAllocation) public onlyOwner returns (uint256 trancheId){
        IERC20(token).safeTransferFrom(msg.sender, address(this), _totalAllocation);
        trancheId = tranches;
        merkleRoots[trancheId] = _merkleRoot;
        tranches = tranches.add(1);
        emit TrancheAdded(trancheId, _merkleRoot, _totalAllocation);
    }

    // 项目过期
    function expireTranche(uint256 _trancheId) public onlyOwner {
        merkleRoots[_trancheId] = bytes32(0);
        emit TrancheExpired(_trancheId);
    }


    // 用户claim
    function claim(
        address user,
        uint256 _tranche,
        bytes32[] memory _merkleProof
    ) public payable {
        // 判断是否有claimInfo的配置
        claimInfo memory currentClaimInfo = claimInfos[_tranche];
        require(currentClaimInfo.totalAmount > 0, "The current token cannot be claimed");

        // 当前代币的总数
        uint256 totalClaimAmount = msg.value.div(10**18).mul(currentClaimInfo.exchangeRate);
        // 申领一次可以初始化释放多少代币
        uint256 initClaimAmount = totalClaimAmount.mul(currentClaimInfo.initPercentage).div(100);
        // 用户申领的Token数量不能大于项目配置的最大Token数量
        require(claimed[_tranche][user].add(totalClaimAmount) <= currentClaimInfo.claimMaxToken, "Claims exceed the maximum number");

        _claim(user, _tranche,totalClaimAmount, _merkleProof,currentClaimInfo.publicSaleTime);
        // 初始化代币分配
        _disburse(currentClaimInfo.token,user,initClaimAmount);
        // 增加申领数量
        harvested[_tranche][user].remainingAmount = harvested[_tranche][user].remainingAmount.add(totalClaimAmount).sub(initClaimAmount);
        harvested[_tranche][user].token = currentClaimInfo.token;
    }

    // 领取剩余代币
    function harvest(uint256 _tranche) external {
        harvestedInfo memory currentHarvestInfo = harvested[_tranche][msg.sender];
        require(currentHarvestInfo.remainingAmount > 0,"No token left");

        uint256 amount = 0;
        uint256 timeDiff = 0;
        if(currentHarvestInfo.lastClaimedTime != 0) {
            timeDiff = block.timestamp.sub(currentHarvestInfo.lastClaimedTime);
            require(timeDiff >= 1 days, "Already Claimed");
        }

        if (currentHarvestInfo.receiveTimes >= 7) {
            amount = currentHarvestInfo.remainingAmount;
        }else {
            amount = currentHarvestInfo.remainingAmount.div(uint256(7).sub(currentHarvestInfo.receiveTimes));
        }
        harvested[_tranche][msg.sender].receiveTimes = harvested[_tranche][msg.sender].receiveTimes.add(1);
        harvested[_tranche][msg.sender].remainingAmount = harvested[_tranche][msg.sender].remainingAmount.sub(amount);
        harvested[_tranche][msg.sender].lastClaimedTime = block.timestamp;
        _disburse(harvested[_tranche][msg.sender].token,msg.sender,amount);
    }

    // 验证是否在白名单内
    function verifyClaim(
        address user,
        uint256 _tranche,
        bytes32[] memory _merkleProof
    ) public view returns (bool valid) {
        return _verifyClaim(user, _tranche, _merkleProof);
    }

    // 验证claim权限
    function _claim(address user,uint256 _tranche,uint256 _balance,bytes32[] memory _merkleProof,uint256 publicSaleTime) private {
        require(_tranche < tranches, "You cannot claim future token");

        // 白名单时间
        if (block.timestamp <= publicSaleTime) {
            require(
                _verifyClaim(user, _tranche, _merkleProof),
                "Incorrect merkle proof"
            );
        }

        // 增加申领数量
        claimed[_tranche][user] = claimed[_tranche][user].add(_balance);
        emit Claimed(user, _tranche, _balance);
    }

    // 白名单
    function _verifyClaim(
        address user,
        uint256 _tranche,
        bytes32[] memory _merkleProof
    ) private view returns (bool valid) {
        bytes32 leaf = keccak256(abi.encodePacked(user));
        return MerkleProof.verify(_merkleProof, merkleRoots[_tranche], leaf);
    }

    // 分配代币 私有方法
    function _disburse(address token,address user,uint256 _balance) private {
        if (_balance > 0 ) {
            // 非标准代币 不一定有
            IERC20(token).safeTransfer(user,_balance);
        }else{
            revert(
                "No balance would be transferred - not going to waste your gas"
            );
        }
    }
    /**提取ETH
    * @dev function withdraw ETH to account owner
     * @param _amount is amount withdraw
     */
    function withdrawBNB(uint256 _amount) external onlyOwner {
        require(_amount > 0, "_amount must be greater than 0");
        require(
            address(this).balance >= _amount,
            "_amount must be less than the BNB balance of the contract"
        );
        payable(msg.sender).transfer(_amount);
    }

    /** 提取合约内发售的代币的余额
    * @dev function withdraw token to account owner
     * @param _amount is amount withdraw
     */
//    function withdrawToken(uint256 _amount) external onlyOwner {
//        require(_amount > 0, "_amount must be greater than 0");
//        require(
//            token.balanceOf(address(this)) >= _amount,
//            "The balance is not enough!"
//        );
//        token.transfer(msg.sender, _amount);
//    }

    /** 提取合约内其他ERC20的代币
    * @dev function withdraw ERC20 token to account owner
     * @param _token is address of the token
     * @param _amount is amount withdraw
     */
    function withdrawERC20(address _token, uint256 _amount) external onlyOwner {
        require(_amount > 0, "_amount must be greater than 0");
        require(
            IERC20(_token).balanceOf(address(this)) >= _amount,
            "The balance is not enough!"
        );
        IERC20(_token).transfer(msg.sender, _amount);
    }

}

