import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";
import { IMintableERC1155 } from "../interfaces/IMintableERC1155.sol";
import { NativeMetaTransaction } from "../common/NativeMetaTransaction.sol";
import { ContextMixin } from "../common/ContextMixin.sol";
import { AccessControl } from "@openzeppelin/contracts/access/AccessControl.sol";
import { IERC165 } from "@openzeppelin/contracts/interfaces/IERC165.sol";
import { AccessControlMixin } from "../common/AccessControlMixin.sol";
import { Strings } from "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";



contract BCCGTrade is
	ERC1155,
	AccessControlMixin,
	NativeMetaTransaction,
	ContextMixin,
	IMintableERC1155,
	Ownable,
	ERC1155Holder
{

	using SafeERC20 for IERC20;
	using Strings for uint256;

    error MustTradeNoMoreThanFour();
    error ArrayLengthsMustBeEqual();
	error MustMintMoreThanZero();
	error AmountMustBeGreaterThanZero();
	error OnlyFirstEditionIds();
	error SaleHasntStartedYet();
	error TradeHasntStartedYet();
	error NFTAmountExceedsCampaignMaxPerTx();
	error MaxTierNumberExceeded();
	error ArrayLengthMismatch();

	event TradeIn(address indexed from, uint256 indexed tierRecieved);
	event Purchase(address indexed from, uint256 indexed tierRecieved, uint256 indexed amount, uint256 campaign);


    address public currentCampaignPaymentToken;
    uint256 public constant FARZIN_ID = 10;

	bool public saleStarted;
	bool public tradeStarted;

	//bccg1FirstAdditionIds = [1,2,5,6,7,8,9,11,13,14,15,16,17,18,19];
	//bccg2FirstAdditionIds = [1,2,3,4,5,10,12,13,15,19,20,21,24,25,27,28,29,33,34];

	mapping (uint256 => bool) public AcceptedIdsBccg1;
	mapping (uint256 => bool) public AcceptedIdsBccg2;

	struct Trader {
		uint256 amount;
		uint256 tier;
	}

	ERC1155 bccg1;
	ERC1155 bccg2;

	uint256 public maxTier;

    uint256 public currentCampaign;
    uint256 public currentCampaignPricePerTier;
	uint256 public currentCampaignMaxAmountPerUserTx;

	address public recipientAddress;

	bytes32 public constant PREDICATE_ROLE = keccak256("PREDICATE_ROLE");
	// Contract name
	string public constant name = "Launchpad Pass";
	// Contract symbol
	string public constant symbol = "PASS";
    string private constant _uri = ""; //"https://api.bccg.digital/api/polygonbccg/";

	constructor() ERC1155(_uri) {
		_setupContractId("PASS");
		_setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
		_setupRole(PREDICATE_ROLE, _msgSender());
		_initializeEIP712("PASS");
		bccg1 = ERC1155(address(0xa42EB75754904955d9de6e031dB7dFa90027562c));
		bccg2 = ERC1155(address(0xc799520C6eDfc662EF87C95917F768dB85982Aa4));
	}

	function setRecipientAddress(address _recipient) external onlyOwner {
		recipientAddress = _recipient;
	}

	function setMaxTier(uint256 _maxTier) external onlyOwner {
		maxTier = _maxTier;
	}

	function setSaleStart(bool _status) external onlyOwner {
		saleStarted = _status;
	}

	function setTradeStart(bool _status) external onlyOwner {
		tradeStarted = _status;
	}

	function setAcceptedIds(uint256 _contractNumber, uint256[] memory _ids) external onlyOwner {
		for (uint i = 0; i < _ids.length; i++) {
			if (_contractNumber == 1) {
				AcceptedIdsBccg1[_ids[i]] = true;
			} else if (_contractNumber == 2) {
				AcceptedIdsBccg2[_ids[i]] = true;
			}
		}
	}

	function _checkId(uint256 _id, uint256 _contractNumber) internal view returns (bool) {
		if (_contractNumber == 1) {
			return AcceptedIdsBccg1[_id];
		} else if (_contractNumber == 2) {
			return AcceptedIdsBccg2[_id];
		}
	}

	function _sumOfAmounts(uint256[] memory _tokenAmounts) internal pure returns (uint256){
		uint256 sum;

		for (uint256 i = 0; i < _tokenAmounts.length; i++) {
			sum += _tokenAmounts[i];
		}
		return sum;
	}

    function tradeUp(
		uint256[] calldata _contract1TokenIds, 
    	uint256[] calldata _contract1TokenAmounts,
		uint256[] calldata _contract2TokenIds, 
        uint256[] calldata _contract2TokenAmounts,
		bool _tradingFromBccg1,
		bool _tradingFromBccg2
    ) external payable {

		Trader memory trader;

		trader.amount = (_sumOfAmounts(_contract1TokenAmounts) + _sumOfAmounts(_contract2TokenAmounts));

		if (!tradeStarted) revert TradeHasntStartedYet();
		if (_contract1TokenIds.length != _contract1TokenAmounts.length || _contract2TokenIds.length != _contract2TokenAmounts.length) revert ArrayLengthMismatch();
		if (trader.amount > 4) revert MustTradeNoMoreThanFour();
		if (trader.amount == 0) revert AmountMustBeGreaterThanZero();

		for (uint i = 0; i < _contract2TokenIds.length; i++) {
			if (!_checkId(_contract1TokenIds[i], 1)) revert OnlyFirstEditionIds();
			if (!_checkId(_contract2TokenIds[i], 1)) revert OnlyFirstEditionIds();
			if (_contract2TokenIds[i] == FARZIN_ID) {
				trader.tier = 4;
			} else {
				trader.tier = trader.amount;
			}
		}

		if (_tradingFromBccg1 && _tradingFromBccg2) {
			bccg1.safeBatchTransferFrom(address(msg.sender), recipientAddress, _contract1TokenIds, _contract1TokenAmounts, "");
			bccg2.safeBatchTransferFrom(address(msg.sender), recipientAddress, _contract2TokenIds, _contract2TokenAmounts, "");
		} else if (_tradingFromBccg1 && !_tradingFromBccg2) {
			bccg1.safeBatchTransferFrom(address(msg.sender), recipientAddress, _contract1TokenIds, _contract1TokenAmounts, "");
		} else if (_tradingFromBccg2 && !_tradingFromBccg1) {
			bccg2.safeBatchTransferFrom(address(msg.sender), recipientAddress, _contract2TokenIds, _contract2TokenAmounts, "");
		}

    	_mint(address(msg.sender), trader.tier, 1, "");
		
		emit TradeIn(msg.sender, trader.tier);
    }

    function campaignMint(
		uint256 _tier, 
		uint256 _amount 
		) public payable {

		if (_amount > currentCampaignMaxAmountPerUserTx) revert NFTAmountExceedsCampaignMaxPerTx();
		if (!saleStarted) revert SaleHasntStartedYet();
		if (_tier > maxTier) revert MaxTierNumberExceeded();
		if (_amount <= 0) revert MustMintMoreThanZero();

		uint256 paymentAmountRequired = (currentCampaignPricePerTier * _tier) * _amount;

		IERC20(currentCampaignPaymentToken).safeTransferFrom(msg.sender, address(this), paymentAmountRequired);

        _mint(address(msg.sender), _tier, _amount, "");
		emit Purchase(address(msg.sender), _tier, _amount, currentCampaign);
    }

    function setCurrentCampaign(
        address _paymentToken, 
        uint256 _campaign, 
        uint256 _price,
		uint256 _maxAmountPerUserTx
        ) external only(DEFAULT_ADMIN_ROLE) {
		currentCampaignPaymentToken = _paymentToken;
		currentCampaign = _campaign;
        currentCampaignPricePerTier = _price;
		currentCampaignMaxAmountPerUserTx = _maxAmountPerUserTx;
    }

	function uri(uint256 _id) public view virtual override returns (string memory) {
        return string(abi.encodePacked(_uri, _id.toString()));
    }

	function collect(address _token) external onlyOwner {
		if (_token == address(0)) {
			payable(msg.sender).transfer(address(this).balance);
		} else {
			uint256 amount = IERC20(_token).balanceOf(address(this));
			IERC20(_token).transfer(msg.sender, amount);
		}
	}

	function collectNFTs(address _token, uint256 _tokenId) external onlyOwner {
		uint256 amount = IERC1155(_token).balanceOf(address(this), _tokenId);
		IERC1155(_token).safeTransferFrom(
			address(this),
			msg.sender,
			_tokenId,
			amount,
			""
		);
	}

	function mint(
		address account,
		uint256 id,
		uint256 amount,
		bytes calldata data
	) external override only(PREDICATE_ROLE) {
		_mint(account, id, amount, data);
	}

	function mintBatch(
		address to,
		uint256[] calldata ids,
		uint256[] calldata amounts,
		bytes calldata data
	) external override only(PREDICATE_ROLE) {
		_mintBatch(to, ids, amounts, data);
	}

	function burn(
		address account,
		uint256 id,
		uint256 value
	) public virtual {
		require(
			account == _msgSender() || isApprovedForAll(account, _msgSender()),
			"ERC1155: caller is not owner nor approved"
		);
		_burn(account, id, value);
	}

	function burnBatch(
		address account,
		uint256[] memory ids,
		uint256[] memory values
	) public virtual {
		require(
			account == _msgSender() || isApprovedForAll(account, _msgSender()),
			"ERC1155: caller is not owner nor approved"
		);
		_burnBatch(account, ids, values);
	}

	function _msgSender()
		internal
		view
		virtual
		override
		returns (address)
	{
		return ContextMixin.msgSender();
	}

	function supportsInterface(
		bytes4 interfaceId
		) public view virtual override(AccessControl, IERC165, ERC1155, ERC1155Receiver) returns (bool) {
		return interfaceId == type(IMintableERC1155).interfaceId || super.supportsInterface(interfaceId);
	}
}

