pragma solidity 0.8.4;

import "../../base/DungeonBase.sol";
import "../../interface/IOracle.sol";
import "../../interface/IChamberController.sol";

import "hardhat/console.sol";

contract DungeonDenOfEvil is DungeonBase {

  // ---- CONSTANTS ----

  /// @notice Version of the contract
  /// @dev Should be incremented when contract changed
  string public constant VERSION = "1.0.0";
  string public constant override DUNGEON_NAME = "Den Of Evil";
  uint public constant override STAGES = 3;
  uint private constant _DUNGEON_LEVEL = 1;

  // ---- INITIALIZER ----

  function init(
    address controller_,
    address heroToken_,
    uint heroTokenId_,
    address payToken_
  ) external initializer override {
    IChamber[] memory chambers = new IChamber[](STAGES);
    IChamberController cc = IChamberController(IController(controller_).chamberController());
    for (uint i; i < STAGES; i++) {
      (uint[] memory cTypes, uint[] memory chances) = _stageInfo(i);
      chambers[i] = IChamber(cc.getRandomChamber(cTypes, chances, _DUNGEON_LEVEL));
    }

    __DungeonBase_init(
      controller_,
      heroToken_,
      heroTokenId_,
      payToken_,
      chambers
    );
  }

  /// @dev Lower chance has higher priority - if random number will be lower than this chance will be chosen.
  ///      The same chances is forbidden.
  ///      100e18 means that if no lower chances eligible it will be used always.
  function _stageInfo(uint stage) internal pure returns (uint[] memory cTypes, uint[] memory chances) {
    cTypes = new uint[](STAGES);
    chances = new uint[](STAGES);
    if (stage == 0) {
      // ***************** 0 ******************
      cTypes[0] = uint(IChamberController.ChamberType.ENEMY_NPC);
      chances[0] = 100e18;

      cTypes[1] = uint(IChamberController.ChamberType.SHRINE);
      chances[1] = 10e18;

      cTypes[2] = uint(IChamberController.ChamberType.CHEST);
      chances[2] = 5e18;
    } else if (stage == 1) {
      // ***************** 1 ******************
      cTypes[0] = uint(IChamberController.ChamberType.ENEMY_NPC);
      chances[0] = 100e18;

      cTypes[1] = uint(IChamberController.ChamberType.SHRINE);
      chances[1] = 20e18;

      cTypes[2] = uint(IChamberController.ChamberType.CHEST);
      chances[2] = 10e18;
    } else if (stage == 2) {
      // ***************** 2 ******************
      cTypes[0] = uint(IChamberController.ChamberType.ENEMY_NPC_RARE);
      chances[0] = 100e18;
    } else {
      revert ("Wrong stage");
    }
  }

  /// @dev Should be static for contract registration process
  function dungeonLevel() external pure override returns (uint) {
    return _DUNGEON_LEVEL;
  }


}

