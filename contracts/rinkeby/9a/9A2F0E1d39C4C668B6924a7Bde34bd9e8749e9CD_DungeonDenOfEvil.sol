pragma solidity 0.8.4;

import "../openzeppelin/IERC20.sol";
import "./Controllable.sol";
import "../lib/SlotsLib.sol";
import "../interface/IHero.sol";
import "../interface/IDungeon.sol";
import "../interface/IChamber.sol";
import "../interface/IItem.sol";
import "../interface/IStatController.sol";
import "../interface/IDungeonFactory.sol";
import "../openzeppelin/IERC721.sol";
import "../openzeppelin/SafeERC20.sol";
import "../openzeppelin/EnumerableSet.sol";
import "../openzeppelin/ERC721Holder.sol";

abstract contract DungeonBase is Controllable, IDungeon, ERC721Holder {
  using SlotsLib for bytes32;
  using SafeERC20 for IERC20;
  using EnumerableSet for EnumerableSet.AddressSet;

  // ---- CONSTANTS ----

  /// @notice Version of the contract
  /// @dev Should be incremented when contract changed
  string public constant DUNGEON_BASE_VERSION = "1.0.0";
  bytes32 internal constant _BUSY_SLOT = bytes32(uint256(keccak256("eip1967.DungeonBase.Busy")) - 1);
  bytes32 internal constant _HERO_TOKEN_SLOT = bytes32(uint256(keccak256("eip1967.DungeonBase.HeroToken")) - 1);
  bytes32 internal constant _HERO_TOKEN_ID_SLOT = bytes32(uint256(keccak256("eip1967.DungeonBase.HeroTokenId")) - 1);
  bytes32 internal constant _CURRENT_CHAMBER_SLOT = bytes32(uint256(keccak256("eip1967.DungeonBase.CurrentChamber")) - 1);
  bytes32 internal constant _COMPLETED_SLOT = bytes32(uint256(keccak256("eip1967.DungeonBase.Completed")) - 1);
  bytes32 internal constant _DUNGEON_LEVEL_SLOT = bytes32(uint256(keccak256("eip1967.DungeonBase.DungeonLevel")) - 1);

  // ---- VARIABLES ----

  IChamber[] internal _chambers;
  EnumerableSet.AddressSet internal _treasuryTokens;
  IStatController.NftItem[] public treasuryItems;

  // ---- EVENTS ----

  event ChamberCompleted(IChamber.ChamberResult result);

  // ---- INITIALIZER ----

  function __DungeonBase_init(
    address controller_,
    address heroToken_,
    uint heroTokenId_,
    address payToken_,
    IChamber[] memory chambers_
  ) internal onlyInitializing {
    __Controllable_init(controller_);
    require(chambers_.length != 0, "Empty chambers");
    _chambers = chambers_;
    _treasuryTokens.add(payToken_);

    _enter(heroToken_, heroTokenId_);
  }

  function init(
    address controller_,
    address heroToken_,
    uint heroTokenId_,
    address payToken_
  ) external virtual override {}

  // ---- RESTRICTIONS ----

  function onlyHero() internal view {
    require(IController(_controller()).validHeroes(msg.sender), "Not hero");
  }

  function onlyCompleted() internal view {
    require(_COMPLETED_SLOT.getUint() == 1, "Not completed");
  }

  function onlyEnteredHeroOwner() internal view {
    require(IHero(_HERO_TOKEN_SLOT.getAddress())
    .isApprovedOrOwner(
        msg.sender,
        _HERO_TOKEN_ID_SLOT.getUint()
      ), "Not hero owner");
  }

  // ---- VIEWS ----

  function isDungeon() external pure override returns (bool) {
    return true;
  }

  function isBusy() external view override returns (bool) {
    return _BUSY_SLOT.getUint() == 1;
  }

  function isCompleted() external view override returns (bool) {
    return _COMPLETED_SLOT.getUint() == 1;
  }

  function enteredHero() external view override returns (address token, uint id) {
    return (_HERO_TOKEN_SLOT.getAddress(), _HERO_TOKEN_ID_SLOT.getUint());
  }

  function dungeonLevel() external view virtual override returns (uint);

  function chambersLength() external view override returns (uint) {
    return _chambers.length;
  }

  function currentChamber() external view override returns (IChamber) {
    return _chambers[_CURRENT_CHAMBER_SLOT.getUint()];
  }

  function currentChamberIndex() external view override returns (uint) {
    return _CURRENT_CHAMBER_SLOT.getUint();
  }

  function treasuryTokensLength() external view returns (uint) {
    return _treasuryTokens.length();
  }

  function treasuryTokens(uint index) external view returns (address) {
    return _treasuryTokens.at(index);
  }

  // ---- ACTIONS ----

  function enter(address heroToken_, uint heroTokenId_) external {
    onlyHero();
    _enter(heroToken_, heroTokenId_);
  }

  function _enter(address heroToken_, uint heroTokenId_) internal {
    require(IHero(heroToken_).currentDungeon(heroTokenId_) == address(0), "Already in dungeon");
    require(IHero(heroToken_).stats(heroTokenId_).life > 0, "Hero is dead");
    require(_COMPLETED_SLOT.getUint() == 0, "Dungeon completed");
    require(_BUSY_SLOT.getUint() == 0, "Dungeon busy");

    IDungeonFactory(IController(_controller()).dungeonFactory()).removeFreeDungeon(address(this));

    IHero(heroToken_).changeCurrentDungeon(heroTokenId_, address(this));
    _BUSY_SLOT.set(1);
    _CURRENT_CHAMBER_SLOT.set(uint(0));
    _HERO_TOKEN_SLOT.set(heroToken_);
    _HERO_TOKEN_ID_SLOT.set(heroTokenId_);
  }

  function openChamber(bytes calldata data) external override returns (IChamber.ChamberResult memory){
    address heroToken = _HERO_TOKEN_SLOT.getAddress();
    uint heroTokenId = _HERO_TOKEN_ID_SLOT.getUint();
    uint _currentChamber = _CURRENT_CHAMBER_SLOT.getUint();
    IStatController statController = IStatController(IController(_controller()).statController());
    IStatController.ChangeableStats memory stats = statController.heroStats(heroToken, heroTokenId);

    require(IHero(heroToken).isApprovedOrOwner(msg.sender, heroTokenId), "Not hero owner");
    require(_COMPLETED_SLOT.getUint() == 0, "Dungeon completed");
    require(IHero(heroToken).currentDungeon(heroTokenId) == address(this), "Hero not in dungeon");
    require(statController.isHeroAlive(heroToken, heroTokenId), "Hero is dead");
    require(_currentChamber < _chambers.length, "Wrong chamber");

    IChamber.ChamberResult memory result = _chambers[_currentChamber].open(
      heroToken,
      heroTokenId,
      data
    );

    emit ChamberCompleted(result);

    // KILL
    if (result.kill || stats.life <= result.damage) {
      IHero(heroToken).changeCurrentDungeon(heroTokenId, address(0));
      IHero(heroToken).kill(heroTokenId);
      address rewardToken = IHero(heroToken).payToken();
      if (!_treasuryTokens.contains(rewardToken)) {
        _treasuryTokens.add(rewardToken);
      }
      _clear();
      return result;
    }

    // earned experience, heal and mana regen
    if (result.heal != 0 || result.manaRegen != 0 || result.experience != 0) {
      if (result.experience != 0) {
        // assume that each fight has experience
        IHero(heroToken).refreshLastFight(heroTokenId);
      }
      statController.changeCurrentStats(
        heroToken,
        heroTokenId,
        IStatController.ChangeableStats({
      level : 0,
      experience : result.experience,
      life : result.heal,
      mana : result.manaRegen
      }),
        true
      );
    }

    // damage and mana consuming
    if (result.damage != 0 || result.manaConsumed != 0) {
      require(stats.mana >= result.manaConsumed, "Not enough mana");
      statController.changeCurrentStats(
        heroToken,
        heroTokenId,
        IStatController.ChangeableStats({
      level : 0,
      experience : 0,
      life : result.damage,
      mana : result.manaConsumed
      }),
        false
      );
    }

    // mint items

    for (uint i; i < result.mintItems.length; i++) {
      if (result.mintItems[i] == address(0)) {
        continue;
      }
      uint itemId = IItem(result.mintItems[i]).mint();
      treasuryItems.push(IStatController.NftItem(result.mintItems[i], itemId));
    }

    if (_chambers.length <= _currentChamber + 1) {
      _exit(heroToken, heroTokenId);
    } else {
      _CURRENT_CHAMBER_SLOT.set(_currentChamber + 1);
    }

    // clear temporally attributes
    statController.clearTemporallyAttributes(heroToken, heroTokenId);

    return result;
  }

  function claimToken(address token) external {
    onlyEnteredHeroOwner();
    onlyCompleted();
    _claimToken(token);
  }

  function claimItem(address token, uint tokenId) external {
    onlyEnteredHeroOwner();
    onlyCompleted();
    _claimItem(token, tokenId);
  }

  function claimAllTokens() external {
    onlyEnteredHeroOwner();
    onlyCompleted();
    for (uint i; i < _treasuryTokens.length(); i++) {
      _claimToken(_treasuryTokens.at(i));
    }

    for (uint i; i < treasuryItems.length; i++) {
      _claimItem(treasuryItems[i].token, treasuryItems[i].tokenId);
    }
  }

  function _claimToken(address token) internal {
    uint amount = IERC20(token).balanceOf(address(this));
    if (amount > 0) {
      IERC20(token).safeTransfer(msg.sender, amount);
    }
  }

  function _claimItem(address token, uint tokenId) internal {
    IERC721(token).safeTransferFrom(address(this), msg.sender, tokenId);
  }

  function _exit(address heroToken, uint heroTokenId) internal {
    _COMPLETED_SLOT.set(1);
    IHero(heroToken).changeCurrentDungeon(heroTokenId, address(0));
  }

  function _clear() internal {
    _BUSY_SLOT.set(uint(0));
    _HERO_TOKEN_SLOT.set(address(0));
    _HERO_TOKEN_ID_SLOT.set(uint(0));
    IDungeonFactory(IController(_controller()).dungeonFactory()).addFreeDungeon(address(this));
  }

  /**
* @dev This empty reserved space is put in place to allow future versions to add new
* variables without shifting down storage in the inheritance chain.
* See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
*/
  uint256[46] private __gap;

}

