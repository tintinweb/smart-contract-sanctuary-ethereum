pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";


contract Cannarillaz is ERC721EnumerableUpgradeable, OwnableUpgradeable {
    using SafeMath for uint256;
    using Strings for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;

    string public _baseTokenURI;
    uint256 public itemPrice;
    uint256 public totalCount;
    uint256 public soldCount;
    uint256[] commissionRate;
    uint256[] otherRate;
    address[] wallets;
    address[] _ownerWallets;
    address[] _partnerWallets;

    address private _starter;

    uint256 public constant RATE_FACTOR = 1000;
    uint256 public constant ACC_FACTOR = 10 ** 36;

    uint256 public totalRewardsEth;
    uint256 public rewardsEthPerNft;

    mapping (address => address) parents;
    mapping (address => uint256) commissions;
    mapping (address => mapping (uint256 => uint256)) childCounts;
    mapping (address => Reward) public rewards;
    mapping (address => EnumerableSet.AddressSet) levelOnes;
    mapping (address => string) public accountNames;
    mapping (uint256 => TokenType) public tokenTypes;

    uint256[] _packageDiscounts;

    address[] blackHolders;
    uint256[] blackIds;
    uint256 blackSoldCount;

    struct Reward {
        uint256 totalRealisedForNft;
        uint256 totalExcludedForNft;
    }

    event ItemPriceUpdated(
        uint256 oldPrice,
        uint256 newPrice
    );

    event NFTMinted(
        uint256 count,
        address to,
        uint256 totalMinted
    );

    event Withdrawn(
        uint256 balance,
        address to
    );

    enum TokenType {
        NORMAL,
        YELLOW,
        RED
    }

    function initialize(string memory _uri, uint256 nftPrice) public initializer{
        __ERC721_init("Cannarillaz", "Cannarillaz");
        __Ownable_init();

        _baseTokenURI = _uri;
        itemPrice = nftPrice;
        totalCount = 1000;
        soldCount = 0;
        blackSoldCount = 0;
        commissionRate = [200, 100, 50, 25, 15, 10, 10, 5, 5, 5, 5, 5, 5, 5, 5];
        otherRate = [
            300, // invest
            100, // owner
            75, // master partners
            75 // black edition
        ];
        wallets = [
            0xFBEb5Caf102566f04dE50b9a7bebF983a292ca97, // invest
            0x0D1179E14c7AB77AD6474da1CEfbF9BC93f79c87, // owner tempor
            0x3F6E45B3E3f871301F33051E18f4BA59E1Df676d, // master partners tempor
            0xEd1973F50129f3735Ffb6650c71474ae868ce6D8 // black edition
        ];

        _ownerWallets = [
            0x0D1179E14c7AB77AD6474da1CEfbF9BC93f79c87,
            0x99Bd2090a40dFdB079000Cd35c4a7F7dc861a772
        ];

        _partnerWallets = [
            0x3F6E45B3E3f871301F33051E18f4BA59E1Df676d,
            0x3F6E45B3E3f871301F33051E18f4BA59E1Df676d,
            0x3F6E45B3E3f871301F33051E18f4BA59E1Df676d,
            0xA19c4bda0352CB74436e7A6dfe8eb06c2DCb9154,
            0xA19c4bda0352CB74436e7A6dfe8eb06c2DCb9154,
            0xA19c4bda0352CB74436e7A6dfe8eb06c2DCb9154,
            0xCbb9248A9aE88Bc7EfD086aB2707117F7366b0B7,
            0xB3C4a06503C2F02E285fA7a5817cB5cC6d0E6226,
            0x70CF51d2429bc10A14B39709fe8A49FAb6d94c00,
            0x56940bB7c7C131371a5Bbe00196cb88497EA2871,
            0x9B5D93849B7d9f9479723591aDD04D16bB3E5213,
            0x8653bE25ec6Df1737273A9a73dCD1099eeE5cBF6,
            0x4aEc5D1E80236210ad79e06E42AA3F5073793783,
            0x6983acbb0ce662607Fe81d4D26627873814E9002,
            0x7301BB55244FD81CeB0A6Af52Afb1D705d26b1e8,
            0x1A9399cbE0e1362e80Fbf258FAAE7135370a47B6,
            0xB31C537a4B6ac9d4218e6CF1D5861b613d98754c,
            0xA73Af6DF6636c18A59671ED06375b7392d147696,
            0x448E1d8de28B7e4AeB7CEE241C7DbFdA46F41CEb,
            0x2b9C3411f37893e14bb2BeA4950Eb49CE85A6D3E,
            0x901C15F6cA747C77A4FE30beDfc72b77BC540716,
            0x98d142dF0D82c29dfe258d257F2cA1884932d194,
            0x379d05C983FaB06B0Be44F4477609Da64D03904d,
            0x1EBAba46B6401BeA8264C6e4b5Ee8C29408cd93A
        ];
    }

    function _createSingleNFT(address to) private {
        uint256 newTokenId = soldCount;
        _mint(to, newTokenId);
        soldCount++;
    }
    
    function mintNFTsWithParent(uint256 count, address parent, string memory _name) external payable {
        setAccountName(_msgSender(), _name);
        setParent(_msgSender(), parent);
        _mintNFTs(count);
    }

    function mintNFTs(uint256 count) external payable {
        address[] memory _parents = new address[](5);
        _parents = findParents(_msgSender());
        if(_parents[0] != address(0x0) || msg.sender == _starter)
            _mintNFTs(count);
    }

    function _mintNFTs(uint256 count) internal {
        uint256 amount = msg.value;

        require(soldCount + count <= totalCount, "Insufficient NFTs");
        require(count != 0 && amount >= itemPrice * count, "Insufficient funds");

        address sender = _msgSender();

        distributeRewardForNft(sender);
        for(uint256 i = 0;i < count;i++) {
            _createSingleNFT(sender);
        }

        address[] memory _parents = new address[](5);
        _parents = findParents(sender);

        for(uint i = 0;i<4;i++) {
            uint256 walletAmount = amount.mul(otherRate[i]).div(RATE_FACTOR);
            if(i == 1) {
                uint256 ownerAmount = walletAmount.div(2);
                for(uint j = 0;j<2;j++) {
                    (bool oSuccess, ) = payable(_ownerWallets[j]).call{value: ownerAmount}("");
                    require(oSuccess, "Sending MATIC failed");
                }
            }
            else if(i == 2) { // master partners
                uint256 partnerAmount = walletAmount.div(24);
                for(uint j = 0;j<24;j++) {
                    (bool pSuccess, ) = payable(_partnerWallets[j]).call{value: partnerAmount}("");
                    require(pSuccess, "Sending MATIC failed");
                }
            } else {
                (bool success, ) = payable(wallets[i]).call{value: walletAmount}("");
                require(success, "Sending MATIC failed");
            }
        }
        
        for(uint i = 0;i<5;i++) {
            if(_parents[i] == address(0x0)) break;
            uint256 commission = amount.mul(commissionRate[i]).div(RATE_FACTOR);
            (bool success, ) = payable(_parents[i]).call{value: commission}("");
            require(success, "Sending commission ETH failed");
            commissions[_parents[i]] = commissions[_parents[i]].add(commission);
        }

        rewards[sender].totalExcludedForNft = getCumulativeRewardsEth(balanceOf(sender));

        emit NFTMinted(count, sender, soldCount);
    }

    function setAccountName(address account, string memory _name) public {
        accountNames[account] = _name;
    }

    function getAccountName(address account) public view returns(string memory) {
        return accountNames[account];
    }

    function burn(uint256 tokenId) public {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "caller is not owner nor approved");
        _burn(tokenId);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function setBaseURI(string memory uri) external onlyOwner {
        _baseTokenURI = uri;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        string memory baseURI = _baseURI();
        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.add(1).toString(), ".json")) : "";
    }

    function tokensOfOwner(address _owner) external view returns(uint256[] memory) {
        uint tokenCount = balanceOf(_owner);
        uint256[] memory tokensIds = new uint256[](tokenCount);
        for(uint i = 0; i < tokenCount; i++){
          tokensIds[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokensIds;
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721EnumerableUpgradeable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function setItemPrice(uint256 newPrice) external onlyOwner {
        uint256 oldPrice = itemPrice;
        itemPrice = newPrice;

        emit ItemPriceUpdated(oldPrice, itemPrice);
    }

    function getItemPrice() external view returns (uint256) {
        return itemPrice;
    }

    function setTotalCount(uint256 _total) external onlyOwner {
        totalCount = _total;
    }

    function getTotalCount() external view returns (uint256) {
        return totalCount;
    }

    function getMintedCount() external view returns (uint256) {
        return soldCount;
    }

    function setStarter(address account) public onlyOwner {
        _starter = account;
        setAccountName(account, "Starter");
    }

    function getStarter() external view returns(address) {
        return _starter;
    }

    function setWallets(address[] memory accounts) public onlyOwner {
        require(accounts.length == 4, "Wrong parameter");
        for(uint i = 0;i<4;i++)
            wallets[i] = accounts[i];
    }

    function getWallets() external view returns(address[] memory) {
        return wallets;
    }

    function setOwnerWallets(address[] memory accounts) public onlyOwner {
        require(accounts.length == 2, "Wrong parameter");
        for(uint i = 0;i<2;i++)
            _ownerWallets[i] = accounts[i];
    }

    function getOwnerWallets() external view returns(address[] memory) {
        return _ownerWallets;
    }

    function setPartnerWallet(address _wallet, uint256 idx) public onlyOwner {
        require(idx < 24, "Out of index");
        _partnerWallets[idx] = _wallet;
    }

    function getPartnerWalletByIndex(uint256 idx) external view returns(address) {
        return _partnerWallets[idx];
    }

    function setOtherRate(uint256[] memory _rates) external onlyOwner {
        require(_rates.length == 4, "Wrong parameter");
        for(uint i = 0;i<4;i++) 
            otherRate[i] = _rates[i];
    }

    function getOtherRate() external view returns(uint256[] memory) {
        return otherRate;
    }

    function setTokenTypes(uint256[] memory ids, TokenType _type) public {
        for(uint i = 0;i<ids.length;i++) {
            if(ids[i] < totalCount) {
                tokenTypes[i] = _type;
            }
        }
    }

    function getTokenType(uint256 id) public view returns(TokenType) {
        return tokenTypes[id];
    }

    // Function to receive Ether. msg.data must be empty
    receive() external payable {}

    // Fallback function is called when msg.data is not empty
    fallback() external payable {}

    function withdraw(uint256 amount) external onlyOwner {
        uint256 balance = address(this).balance;
        require(amount > 0 && amount <= balance, "Cannot withdraw");
        (bool success, ) = (_msgSender()).call{value: amount}("");
        require(success, "Withdraw failed!");

        emit Withdrawn(amount, _msgSender());
    }

    function setParent(address child, address parent) internal {
        require(child != parent && balanceOf(parent) > 0, "Cannot set as parent");
        if(parents[child] != parent) {
            parents[child] = parent;
            levelOnes[parent].add(child);
            address _cur = child;
            uint256 index = 0;
            while(index < 5 && parents[_cur] != address(0x0)) {
                _cur = parents[_cur];
                childCounts[_cur][index] ++;
                index ++;
            }
        }
    }

    function findParents(address sender) public view returns(address[] memory) {
        address[] memory _par = new address[](5);
        address _cur = sender;
        uint256 index = 0;
        while(index < 5 && parents[_cur] != address(0x0)) {
            _cur = parents[_cur];
            _par[index] = _cur;
            index ++;
        }
        return _par;
    }

    function getLevelOnes(address sender) public view returns(address[] memory) {
        uint256 count = levelOnes[sender].length();
        address[] memory _ones = new address[](count);
        for(uint i = 0;i<count;i++) {
            _ones[i] = levelOnes[sender].at(i);
        }
        return _ones;
    }

    function getChildCounts(address parent) external view returns(uint256[] memory) {
        uint256[] memory _counts = new uint256[](5);
        for(uint i = 0;i<5;i++){
            _counts[i] = childCounts[parent][i];
        }
        return _counts;
    }

    function setCommissionRate(uint256[] memory _rates) external onlyOwner {
        require(_rates.length == 15, "Wrong parameter");
        for(uint i = 0;i<15;i++) 
            commissionRate[i] = _rates[i];
    }

    function getCommissionRate() external view returns(uint256[] memory) {
        return commissionRate;
    }

    function getTotalCommission(address shareholder) external view returns(uint256) {
        return commissions[shareholder];
    }

    function depositRewardsEth() external payable onlyOwner {
        require(msg.value > 0, "Value must be greater than 0");
        require(soldCount > 0, "Users have to hold nfts");
        uint256 amount = msg.value;
        totalRewardsEth = totalRewardsEth.add(amount);
        rewardsEthPerNft = rewardsEthPerNft.add(ACC_FACTOR.mul(amount).div(soldCount));
    }
    
    function getTotalRewardsEth() external view returns(uint256) {
        return totalRewardsEth;
    }

    function claimRewardForNft() external {
        distributeRewardForNft(msg.sender);
    }

    function distributeRewardForNft(address shareholder) internal {
        uint256 earnedRewards = getUnpaidEth(shareholder);
        if(earnedRewards == 0 )
            return;
        rewards[shareholder].totalRealisedForNft = rewards[shareholder].totalRealisedForNft.add(earnedRewards);
        rewards[shareholder].totalExcludedForNft = getCumulativeRewardsEth(balanceOf(shareholder));

        uint256 balanceBefore = address(this).balance;
        (bool success, ) = payable(shareholder).call{value: earnedRewards}("");
        require(success, "ETH was not successfully sent1");
        require(address(this).balance >= balanceBefore - earnedRewards, "Only take proper amount from contract");
    }

    function getUnpaidEth(address shareholder) public view returns(uint256) {
        uint256 count = balanceOf(shareholder);
        if(count == 0) return 0;

        uint256 earnedRewards = getCumulativeRewardsEth(count);
        uint256 rewardsExcluded = rewards[shareholder].totalExcludedForNft;
        if(earnedRewards <= rewardsExcluded) {
            return 0;
        }

        return earnedRewards.sub(rewardsExcluded);
    }

    function getCumulativeRewardsEth(uint256 share) internal view returns(uint256) {
        return share.mul(rewardsEthPerNft).div(ACC_FACTOR);
    }

    // Black edition
    function addBlackTokenIds(uint256[] memory _ids) public {
        for(uint256 i = 0;i < _ids.length;i++) {
            blackIds.push(_ids[i]);
        }
    }

    function removeBlackTokenId(uint256 _ids) public {
        uint256 length = blackIds.length;
        for(uint256 i = 0;i < length; i++) {
            if(blackIds[i] == _ids) {
                for(uint256 j = i;j<length - 1;j++) {
                    blackIds[j] = blackIds[j+1];
                }
                blackIds.pop();
                break;
            }
        }
    }
    function getBlackTokenIds() public view returns(uint256[] memory ) {
        return blackIds;
    }
}

