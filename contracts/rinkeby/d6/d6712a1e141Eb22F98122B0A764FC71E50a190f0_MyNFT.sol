pragma solidity ^0.8.3;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract MyNFT is ERC721URIStorage, Ownable {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    constructor() ERC721("MyNFT", "NFT") {}

    //salt string
    string private _salt = "NFT";
    bool private isRevealed = true;

    string[] private opacity = [
        "1",
        "0.9",
        "0.8",
        "0.7",
        "0.6",
        "0.5",
        "0.4",
        "0.3"
    ];

    function setReveal(string memory _newSalt) public onlyOwner {
        _salt = _newSalt;
        isRevealed = true;
    }

    function mintNFT(address recipient, uint256 amount)
        public
        returns (uint256[] memory)
    {
        uint256[] memory tokenIDs = new uint256[](amount);

        for (uint256 i = 0; i < amount; i++) {
            _tokenIds.increment();

            uint256 newItemId = _tokenIds.current();
            _mint(recipient, newItemId);

            tokenIDs[i] = newItemId;
        }

        return tokenIDs;
    }

    function rgba(
        string memory r,
        string memory g,
        string memory b,
        string memory a
    ) private pure returns (bytes memory) {
        return abi.encodePacked("rgba(", r, ",", g, ",", b, ",", a, ")");
    }

    function rgbToString(
        string memory c1,
        string memory c2,
        string memory c3,
        uint256 seed
    ) private view returns (string memory) {
        return
            string(
                abi.encodePacked(
                    rgba(
                        c1,
                        c2,
                        c3,
                        opacity[uint256(seed) % uint256(opacity.length)]
                    )
                )
            );
    }

    function randomColor(uint256 seed) private view returns (string[5] memory) {
        string memory randomRGB1 = Strings.toString(
            uint256(seed) % uint256(255)
        );
        string memory randomRGB2 = Strings.toString(
            uint256(seed + 128) % uint256(255)
        );
        string memory randomRGB3 = Strings.toString(
            uint256(seed - 32) % uint256(255)
        );
        string memory randomRGB4 = Strings.toString(
            uint256(seed + 32) % uint256(255)
        );
        string memory randomRGB5 = Strings.toString(
            uint256(seed - 64) % uint256(255)
        );

        string[5] memory parts;
        parts[0] = rgbToString(
            randomRGB1,
            randomRGB2,
            randomRGB3,
            uint256(seed)
        );

        parts[1] = rgbToString(
            randomRGB4,
            randomRGB5,
            randomRGB1,
            uint256(seed + 1)
        );

        parts[2] = rgbToString(
            randomRGB4,
            randomRGB1,
            randomRGB3,
            uint256(seed + 2)
        );
        parts[3] = rgbToString(
            randomRGB4,
            randomRGB3,
            randomRGB5,
            uint256(seed - 2)
        );
        parts[4] = rgbToString(
            randomRGB2,
            randomRGB5,
            randomRGB1,
            uint256(seed + 3)
        );

        return parts;
    }

    function getSaltIndex(uint256 tokenId, string memory salt)
        private
        view
        returns (string[5] memory)
    {
        uint256 seed = uint256(keccak256(abi.encodePacked(salt, tokenId)));
        return randomColor(seed);
    }

    function createSvgRect(
        string memory color,
        uint256 x,
        uint256 y
    ) private pure returns (bytes memory) {
        return
            abi.encodePacked(
                "<rect x='",
                Strings.toString(x),
                "' y='",
                Strings.toString(y),
                "' width='12' height='12' style='fill:",
                color,
                ";' />"
            );
    }

    function getSvg(uint256 tokenId) private view returns (bytes memory) {
        //does token exist? and is minted to contract

        string[5] memory selectedColors = getSaltIndex(tokenId, _salt);
        return
            abi.encodePacked(
                "<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 450 450' class='main'> <style> .main {background:",
                selectedColors[0],
                " } .Rrrrr { font: italic 40px serif; fill:",
                selectedColors[2],
                "; } </style> <rect x='25' y='25' width='400' height='400' style='fill:",
                selectedColors[1],
                "; stroke:",
                selectedColors[3],
                "; stroke-width:5; stroke-opacity:0.9;' /> <rect x='50' y='50' width='12' height='12' style='fill:",
                selectedColors[0],
                ";'  />",
                createSvgRect(selectedColors[2], 250, 325),
                createSvgRect(selectedColors[4], 250, 338),
                createSvgRect(selectedColors[2], 250, 351),
                createSvgRect(selectedColors[2], 237, 351),
                createSvgRect(selectedColors[2], 224, 351),
                ";' /> <text x='105' y='115' class='heavy'>Token</text> <text x='150' y='150' class='Rrrrr'>",
                Strings.toString(tokenId),
                "</text></svg>"
            );
    }

    //get random rgb color

    function getHtml(uint256 tokenId, string memory salt)
        public
        view
        returns (string memory)
    {
        string[5] memory selectedColors = getSaltIndex(tokenId, salt);

        string memory base64 = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        "<html><style> .float { display: flex; justify-content: center; padding-top: 35px; padding-bottom: 35px; } @media (min-width: 1000px) {.float {height: 75% !important;display: flex;align-items: stretch;justify-content: center;overflow: hidden;padding-top: 75px } }</style><body class='float' style='background:",
                        selectedColors[0],
                        "'",
                        "onload=\"e = document.getElementById('e');z=v.height*=4;v.width*=2;X=0;a=h=136;s=[h];k=240;v=v.getContext('2d');setInterval(j=>{Y=X&k;s.unshift(h=h+X&15|h+Y&k);onkeydown=e=>X=[Y|j,X%j||15,Y||k,X%j|1][e.which&3]; G=(c,l=z/j)=>P=>{v.fillStyle=c;v.fillRect(P%j*l,(P>>4)*l,l,l)}; h-a?s[q](h,1)?s=[h]:s.pop():(Q=_=>e.play()&&s[q='includes'](a=Math.random()*256|0)&&Q())();G('",
                        selectedColors[1],
                        "',z)(0);",
                        "G('",
                        selectedColors[0],
                        "')(a);s.map(G('",
                        selectedColors[2],
                        "'))},a,16)\">",
                        "<canvas id=v style='border: 6px solid ",
                        selectedColors[3],
                        "'/><audio id='e' src='https://gateway.pinata.cloud/ipfs/QmSceLvEinSpTe5QqabrWAdr5S464y1xUCW2cGbz2pUMTv'/></html>"
                    )
                )
            )
        );

        return string(abi.encodePacked("data:text/html;base64,", base64));
        // return
        //     string(
        //         abi.encodePacked(
        //             "<body class='float' style='background:",
        //             selectedColors[0],
        //             "'",
        //             "onload=\"e = document.getElementById('e');z=v.height*=4;v.width*=2;X=0;a=h=136;s=[h];k=240;v=v.getContext('2d');setInterval(j=>{Y=X&k;s.unshift(h=h+X&15|h+Y&k);onkeydown=e=>X=[Y|j,X%j||15,Y||k,X%j|1][e.which&3]; G=(c,l=z/j)=>P=>{v.fillStyle=c;v.fillRect(P%j*l,(P>>4)*l,l,l)}; h-a?s[q](h,1)?s=[h]:s.pop():(Q=_=>e.play()&&s[q='includes'](a=Math.random()*256|0)&&Q())();G('",
        //             selectedColors[1],
        //             "',z)(0);",
        //             "G('",
        //             selectedColors[0],
        //             "')(a);s.map(G('",
        //             selectedColors[2],
        //             "'))},a,16)\">",
        //             "<canvas id=v style='border: 6px solid ",
        //             selectedColors[3],
        //             "'/><audio id='e' src='",
        //             ipfsURI("QmSceLvEinSpTe5QqabrWAdr5S464y1xUCW2cGbz2pUMTv"),
        //             "'/>"
        //         )
        //     );
    }

    function image_data(uint256 tokenId) private view returns (bytes memory) {
        bytes memory svgData = getSvg(tokenId);
        if (isRevealed)
            return
                abi.encodePacked(
                    getHtml(tokenId, _salt),
                    '","image_data": "',
                    svgData
                );
        else
            return
                abi.encodePacked(
                    "https://gateway.pinata.cloud/ipfs/QmZ9FB2Mg3tkyDhk2FF7rJ62X4T3arauiNZLBU2yG8SpcH"
                );
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{"name": "MY NFT", "description": "test", "animation_url": "',
                        image_data(tokenId),
                        '"}'
                    )
                )
            )
        );
        return string(abi.encodePacked("data:application/json;base64,", json));
    }
}

