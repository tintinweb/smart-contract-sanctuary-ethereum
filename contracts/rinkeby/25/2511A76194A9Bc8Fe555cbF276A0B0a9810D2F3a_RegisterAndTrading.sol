pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "hardhat/console.sol";

contract RegisterAndTrading is Ownable {

    /******************************************
    //for register
    *******************************************/
    mapping(address => bool) public registerAdd;
    mapping(address => address) public sponsorAdd;
    mapping(address => address[]) public childAdds;
    mapping(address => uint256[4]) public purchasedNFT;
    mapping(address => uint256[4]) public earnedBNB;

    bool public _pause;
    
    constructor(address _nftAddress, address _teamWallet) {
        nftAddress = _nftAddress;
        teamWallet = _teamWallet;
    }

    function registerWithoutSponsor() external payable {
        require(!_pause, "Register Paused!");
        require(msg.value >= 0.18 ether, "Insufficient funds.");
        require(!registerAdd[msg.sender], "Duplicated Address.");
        registerAdd[msg.sender] = true;
    }

    function registerWithSponsor(address _sponsorAdd) external payable {
        require(!_pause, "Register Paused!");
        require(msg.value >= 0.06 ether, "Insufficient funds.");
        require(!registerAdd[msg.sender], "Duplicated Address.");

        registerAdd[msg.sender] = true;
        sponsorAdd[msg.sender] = _sponsorAdd;
        childAdds[_sponsorAdd].push(msg.sender);
    }

    function getDownline(address _add) public view returns(
                                        uint256 first_level, 
                                        uint256 second_level, 
                                        uint256 third_level, 
                                        uint256 forth_level){

        first_level = 0;
        second_level = 0;
        third_level = 0;
        forth_level = 0;

        address _ownAdd = _add;
        address[] memory allFirstLevels = childAdds[_ownAdd];
        first_level = allFirstLevels.length;
        for(uint256 i = 0; i < allFirstLevels.length; i++) {
            address[] memory secondChilds = childAdds[allFirstLevels[i]];
            second_level += secondChilds.length;
            for(uint256 j = 0; j < secondChilds.length; j++) {
                address[] memory thirdChilds = childAdds[secondChilds[j]];
                third_level += thirdChilds.length;
                for(uint256 k = 0; k < thirdChilds.length; k++) {
                    address[] memory forthChilds = childAdds[thirdChilds[k]];
                    forth_level += forthChilds.length;
                }
            }
        }
    }

    function getGenealogicalTree(address _add) public view returns(
                                        address[] memory first_level, 
                                        address[] memory second_level, 
                                        address[] memory third_level, 
                                        address[] memory forth_level){

        address _ownAdd = _add;
        (, uint256 level2Cnt, uint256 level3Cnt, uint256 level4Cnt) = getDownline(_ownAdd);
        first_level = childAdds[_ownAdd];
        second_level = new address[](level2Cnt);
        second_level = new address[](level2Cnt);
        third_level = new address[](level3Cnt);
        forth_level = new address[](level4Cnt);
        
        for(uint256 i = 0; i < first_level.length; i++) {
            address[] memory secondChilds = childAdds[first_level[i]];
            for(uint256 j = 0; j < secondChilds.length; j++) {
                second_level[i * secondChilds.length + j] = secondChilds[j];
                address[] memory thirdChilds = childAdds[secondChilds[j]];
                for(uint256 k = 0; k < thirdChilds.length; k++) {
                    third_level[j * thirdChilds.length + k] = thirdChilds[k];
                    address[] memory forthChilds = childAdds[thirdChilds[k]];
                    for(uint256 l = 0; l < forthChilds.length; l++) {
                        forth_level[k * forthChilds.length + l] = forthChilds[l];
                    }
                }
            }
        }
    }

    function pause() public onlyOwner {
        _pause = true;
    }

    function unPause() public onlyOwner {
        _pause = false;
    }

    function withdraw() public onlyOwner() {
        payable(msg.sender).transfer(address(this).balance);
    }

    
    /******************************************
    //for trading
    *******************************************/
    
    address public nftAddress;
    address public teamWallet;

    struct SellInfo {
        uint256 price;
        uint256 totalVolume;
    }

    mapping(uint256 => SellInfo) public sellInfo;
    uint256 public totalVolume = 0;
    
    function changeTeamWalletAddress(address _teamWallet) public onlyOwner {
        teamWallet = _teamWallet;
    }

    function changeNFTAddress(address _nftAddress) public onlyOwner {
        nftAddress = _nftAddress;
    }


    function sell(uint256 _tokenId, uint256 _price) public {
        require(IERC721(nftAddress).ownerOf(_tokenId) == msg.sender, "This NFT is not your's.");
        require(registerAdd[msg.sender], "You are not a registered.");
        sellInfo[_tokenId].price = _price;
    }

    function batchSell(uint256[] memory _tokenIds, uint256[] memory _prices) public onlyOwner() {
        require(_tokenIds.length == _prices.length);
        for(uint i = 0; i < _tokenIds.length; i++) {
            sellInfo[_tokenIds[i]].price = _prices[i];
        }
    }

    function buy(uint256 _tokenId) public payable {        
        require(msg.value >= sellInfo[_tokenId].price, "Insufficient funds.");
        require(registerAdd[msg.sender], "You are not a registered.");

        distributeFunds(msg.sender, IERC721(nftAddress).ownerOf(_tokenId), msg.value);

        IERC721(nftAddress).transferFrom(IERC721(nftAddress).ownerOf(_tokenId), address(msg.sender), _tokenId);
        sellInfo[_tokenId].price = 0;
        sellInfo[_tokenId].totalVolume = sellInfo[_tokenId].totalVolume + msg.value;
        totalVolume += msg.value;
    }

    function edit(uint256 _tokenId, uint256 _price) public {
        require(IERC721(nftAddress).ownerOf(_tokenId) == msg.sender, "This NFT is not your's.");
        require(registerAdd[msg.sender], "You are not a registered.");
        sellInfo[_tokenId].price = _price;
    }

    function cancelSell(uint256 _tokenId) public {
        require(IERC721(nftAddress).ownerOf(_tokenId) == msg.sender, "This NFT is not your's.");
        require(registerAdd[msg.sender], "You are not a registered.");
        sellInfo[_tokenId].price = 0;
    }

    function distributeFunds(address _buyer, address _seller, uint256 _amount) internal {
        uint256 _amountForSeller = _amount * 80 / 100;
        uint256 _amountForLevel1 = _amount * 7 / 100;
        uint256 _amountForLevel2 = _amount * 4 / 100;
        uint256 _amountForLevel3 = _amount * 2 / 100;
        uint256 _amountForLevel4 = _amount * 2 / 100;
        payable(_seller).transfer(_amountForSeller);

        uint256 _amountForTeam = _amount - _amountForSeller;
        address first_level = sponsorAdd[_buyer];
        if(first_level != address(0)) {
            payable(first_level).transfer(_amountForLevel1);
            purchasedNFT[first_level][0] += 1;
            earnedBNB[first_level][0] += _amountForLevel1;
            _amountForTeam -= _amountForLevel1;
        }
        address second_level = sponsorAdd[first_level];
        if(second_level != address(0)) {
            payable(second_level).transfer(_amountForLevel2);
            purchasedNFT[first_level][1] += 1;
            earnedBNB[first_level][1] += _amountForLevel2;
            _amountForTeam -= _amountForLevel2;
        }
        address third_level = sponsorAdd[second_level];
        if(third_level != address(0)) {
            payable(third_level).transfer(_amountForLevel3);
            purchasedNFT[first_level][2] += 1;
            earnedBNB[first_level][2] += _amountForLevel3;
            _amountForTeam -= _amountForLevel3;
        }
        address forth_level = sponsorAdd[third_level];
        if(forth_level != address(0)) {
            payable(forth_level).transfer(_amountForLevel4);
            purchasedNFT[first_level][3] += 1;
            earnedBNB[first_level][3] += _amountForLevel4;
            _amountForTeam -= _amountForLevel4;
        }

        payable(teamWallet).transfer(_amountForTeam);
    }
}

