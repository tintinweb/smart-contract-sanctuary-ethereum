        require(_totalOriginalTokens < MaxOriginalTokens, "Mint: Max tokens reached");
        _mint(to, tokenId);
        _totalOriginalTokens+=1;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        return  string(abi.encodePacked(super.tokenURI(tokenId), ".json" ));
    }

    function _baseURI() internal view override virtual returns (string memory) {
        return  _baseTokenURI;
    }
    function setBaseURI(string memory uri) external onlyOwner {
        _baseTokenURI=uri;
    }
    
    function breedingPermit(uint256 nft_1,
                            uint256 nft_2,
                            address token1,
                            uint256 cost1,
                            address token2, 
                            uint256 cost2, 
                            uint256 deadline, 
                            uint256 price, 
                            uint8 v, bytes32 r, bytes32 s) external payable{
        require(block.timestamp <= deadline, "Breeding: expired deadline");
        require(msg.value >= price, "Not enough money");
        bytes32 hashStruct = keccak256(
             abi.encode(
                 _BREEDING_TYPEHASH,
                 msg.sender,
                 nft_1,
                 nft_2,
                 token1,
                 cost1,
                 token2,
                 cost2,
                 nonces(msg.sender),
                 deadline,
                 price
