    string private constant NAME = "GAIA NFT";
    string private constant SYMBOL = "GAIA";
    string private constant VERSION = "1";
    string  private _baseTokenURI;
    uint256 private _totalOriginalTokens;
    uint256 private _totalNonOriginalTokens;
