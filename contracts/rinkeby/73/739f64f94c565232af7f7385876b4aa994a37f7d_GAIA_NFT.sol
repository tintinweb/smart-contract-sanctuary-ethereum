        ERC20BurnableUpgradeable(token1).burnFrom(msg.sender, cost1);
        ERC20BurnableUpgradeable(token2).burnFrom(msg.sender, cost2);
        _mint(msg.sender,newTokenId);
        _totalNonOriginalTokens+=1;
        emit Breeded(msg.sender, newTokenId);
    }

    function mintPermit(uint256 tokenId,
                        uint256 deadline, 
                        uint256 price,
                        uint8 v, bytes32 r, bytes32 s) external payable{
        require(_totalOriginalTokens < MaxOriginalTokens, "Mint: Max tokens reached");
        require(msg.value >= price, "Not enough money");
        require(block.timestamp <= deadline, "Mint NFT: expired deadline");
        bytes32 hashStruct = keccak256(
             abi.encode(
                 _MINT_TYPEHASH,
                 msg.sender,
                 tokenId,
                 nonces(msg.sender),
                 deadline,
                 price
             )
         );
        bytes32 hash = _hashTypedDataV4(hashStruct);
        address signer = ECDSAUpgradeable.recover(hash, v, r, s);
        require(signer == _signer, "Mint NFT: invalid signature");
        _useNonce(msg.sender);
        _mint(msg.sender,tokenId);
        _totalOriginalTokens+=1;
    }


    function takeOn(address accessories, uint256 nft, uint256 token, uint256 amount ) external nonReentrant{
        IERC1155Upgradeable(accessories).safeTransferFrom(msg.sender, address(this), token, amount,"");
        require( IERC1155Upgradeable(accessories).balanceOf(msg.sender, token) >= amount, "TakeOn: no enough accessories");
        _accessories[msg.sender][nft][token]+=amount;
        emit takenOn(msg.sender, nft, token, amount);
    }
    function takeOff(address accessories, uint256 nft, uint256 token, uint256 amount ) external nonReentrant{
        require(_accessories[msg.sender][nft][token] >= amount, "Illegal accessories amount");
        IERC1155Upgradeable(accessories).safeTransferFrom(address(this), msg.sender,token, amount,"");
        _accessories[msg.sender][nft][token]-=amount;
        emit takenOff(msg.sender, nft, token, amount);
    }
    function nonces(address owner) public view virtual returns (uint256) {
        return _nonces[owner];
    }
    function _useNonce(address owner) internal virtual returns (uint256 current) {
        current = _nonces[owner];
        _nonces[owner]++;
    }
    function totalSupply() public view virtual returns (uint256) {
        return _totalOriginalTokens + _totalNonOriginalTokens;
    }
    function withdraw(uint256 amount, address receiver) external onlyOwner{
        AddressUpgradeable.sendValue(payable(receiver), amount);
    }
}