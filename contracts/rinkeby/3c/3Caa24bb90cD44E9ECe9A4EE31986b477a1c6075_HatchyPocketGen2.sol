    function initialize5() external onlyOwner {
        require(availableSharedCommon.values().length == 0, "init.");

        uint8[37] memory _sharedCommonPool = [43, 44, 47, 48, 50, 51, 52, 53, 57, 58, 59, 60, 63, 64, 65, 66, 67, 69, 70, 76, 79, 80, 81, 83, 87, 89, 90, 91, 92, 93, 96, 97, 99, 101, 103, 104, 106];
