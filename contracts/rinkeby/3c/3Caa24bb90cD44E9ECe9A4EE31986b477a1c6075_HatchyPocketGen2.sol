    function initialize6() external onlyOwner {
        require(availableSharedShiny.values().length == 0, "init.");

        uint24[43] memory _sharedShinyPool = [10011001, 10101001, 43888, 44888, 47888, 48888, 50888, 51888, 52888, 53888, 57888, 58888, 59888, 60888, 63888, 64888, 65888, 66888, 67888, 69888, 70888, 76888, 79888, 80888, 81888, 83888, 87888, 89888, 90888, 91888, 92888, 93888, 96888, 97888, 99888, 101888, 103888, 104888, 106888, 134888, 135888, 136888, 139888];
        for (uint i = 0; i < _sharedShinyPool.length;) {
            availableSharedShiny.add(_sharedShinyPool[i]);
        unchecked {i++;}
        }
