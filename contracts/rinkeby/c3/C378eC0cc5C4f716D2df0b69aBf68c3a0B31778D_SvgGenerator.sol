    function generateTokenUri(uint256 tokenId, IChameleon.Chameleon memory chameleon)
        external
        pure
        override
        returns (string memory)
    {
        // Generate style tags
        string memory styleTag = _generateStyleTag(chameleon);
        string memory attributes = _generateAttributeTag(chameleon);
        string memory name = string(
            abi.encodePacked(
                'Photonic Fungible Species',
                ' - ',
                Strings.toString(tokenId)
            )
        );

        // Combine with base SVG
        string memory combined = string(
            abi.encodePacked(_svgBeforeStyleTags, styleTag, _svgAfterStyleTags)
        );

        return _formatTokenUri(_svgToImageUri(combined), name, attributes);
    }

    function _valueToHSL(uint24 value) private pure returns (string memory) {
        string memory hue = UintStrings.decimalString(value % 360, 0, false);
        string memory lightness = "65";

        // Default
        if (value == 0) {
            lightness = "100";
        }

        return
            string(
                abi.encodePacked(
                    "hsl(",
                    hue,
                    ",85%,",
                    lightness,
                    "%)"
                )
            );
    }

    function coloringTypeString(uint8 coloringType)
        public
        pure
        returns (string memory)
    {
        return (
            coloringType == 1
                ? "Metachrosis"
                : (coloringType == 2 ? "Camouflage" : "Veiled")
        );
    }

    function environmentTypeString(uint8 environment)
        public
        pure
        returns (string memory)
    {
        return (environment == 1 ? "Nomadic" : "Sedentary");
    }

    function _generateAttributeTag(IChameleon.Chameleon memory chameleon)
        private
        pure
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    "[{",
                    '"trait_type": "Coloring",',
                    '"value":"',
                    coloringTypeString(chameleon.coloring),
                    '"}',
                    ", {",
                    '"trait_type": "Environment",',
                    '"value":"',
                    environmentTypeString(chameleon.environment),
                    '"}',
                    "]"
                )
            );
    }

    function _generateStyleTag(IChameleon.Chameleon memory chameleon)
        private
        pure
        returns (string memory)
    {
        string memory eye = string(
            abi.encodePacked(".eye{fill:", _valueToHSL(chameleon.eyes), ";}")
        );

        string memory body;
        string memory environment;

        if (chameleon.coloring == 1) {
            // The ability to only change the eye
            string memory bodyColor = _valueToHSL(chameleon.body);
            if(chameleon.eyes > 0) {
                bodyColor = "transparent";
            }

            body = string(
                abi.encodePacked(
                    ".body{fill:",
                    bodyColor,
                    ";}"
                )
            );
        }
        if (chameleon.coloring == 2) {
            body = string(
                abi.encodePacked(
                    ".body{fill: rgba(0,0,0,0);animation: transparent 3s ease-in-out;animation-iteration-count: unset;}"
                )
            );
        }

        if (chameleon.environment == 1) {
            environment = string(
                abi.encodePacked('svg { background: ', _valueToHSL(chameleon.environment), ';}')
            );
        }

        string memory styleTag = string(
            abi.encodePacked(
                "<defs><style>", environment,
                "@keyframes transparent { 0% { fill: ",
                _valueToHSL(chameleon.body),
                " } 100% { fill: rgba(0,0,0,0); } }",
                ".tongue{fill:brown;}",
                ".leaf{fill:#A8C686;}",
                ".bark{fill:#564D4A;}",
                body,
                eye,
                "</style> </defs>"
            )
        );
        return styleTag;
    }

    function _svgToImageUri(string memory svg)
        private
        pure
        returns (string memory)
    {
        string memory baseURL = "data:image/svg+xml;base64,";
        string memory svgBase64Encoded = Base64.encode(
            bytes(string(abi.encodePacked(svg)))
        );
        return string(abi.encodePacked(baseURL, svgBase64Encoded));
    }

    function _formatTokenUri(
        string memory imageURI,
        string memory name,
        string memory attributes
    ) private pure returns (string memory) {
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                name,
                                '", "description":"An Chameleon NFT based on SVG!", '
                                '"attributes": ',
                                attributes,
                                ',',
                                '"image":"',
                                imageURI,
                                '"}'
                            )
                        )
                    )
                )
            );
    }
