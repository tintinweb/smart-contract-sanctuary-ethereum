pragma solidity ^0.8.0;

pragma abicoder v2;
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";


abstract contract AuctionBase is OwnableUpgradeable{
    
    /// @dev default minimal auction duration and also the time for that auction is extended when it's about to end (endTime - now < EXTENSION_DURATION)
    uint96 internal constant EXTENSION_DURATION = 15 minutes;
    /// @dev royalties
    address royalties;
    /// @dev maximum auction duration
    uint128 internal constant MAX_DURATION = 1000 days;
    /// @dev balance failed bid
    //mapping(address => uint) balanceBid;
    /// @dev service fee percentage
    uint32 serviceFeePercent;
    /// @dev count the total fees collected from successful auction
    uint256 totalServiceFee;
    /// @dev latest auctionId
    uint256 public auctionId;
    /// @dev min percent for next bid
    uint32 public percentNextBid;
    /// @dev minimal auction duration
    uint96 public minimalDuration;
    /// @dev event that emits when auction is created
    event AuctionCreated(uint indexed auctionId, address buyer, uint128 endTime);
    /// @dev event that emits when bid is placed
    event BidPlaced(uint indexed auctionId, address buyer, uint endTime);
    /// @dev event that emits when auction is finished
    event AuctionFinished(uint indexed auctionId);
    /// @dev event that emits when auction is canceled
    event AuctionCancelled(uint indexed auctionId);
    /// @dev event that's emitted when minimal auction duration changes
    event MinimalDurationChanged(uint oldValue, uint newValue);
    /// @dev event that's emitted when next percent next amount changes
    event PercentNextBidChanged(uint32 oldValue, uint32 newValue);
    /// @dev event that's emitted when next percent next amount changes
    event ServiceFeePercentChanged(uint32 oldValue, uint32 newValue);
    /// @dev event that's emitted when royalties address changes
    event RoyaltiesChange(address oldValue, address newValue);

    function __AuctionBase_init() internal initializer {
        __Context_init_unchained();
        __Ownable_init_unchained();

        auctionId = 1;
        minimalDuration = EXTENSION_DURATION;
        percentNextBid = 500; // default 5%
        serviceFeePercent = 200; // default 2%
    }

    /// @dev increments auctionId and returns new value
    function getNextAndIncrementAuctionId() internal returns (uint256) {
        return auctionId++;
    }

    function changeMinimalDuration(uint96 newValue) external onlyOwner {
        emit MinimalDurationChanged(minimalDuration, newValue);
        minimalDuration = newValue;
    }

    function changePercentNextBid(uint32 newValue) external onlyOwner{
        emit PercentNextBidChanged(percentNextBid,newValue);
        percentNextBid = newValue;
    }

    function changeServiceFeePercent(uint32 newValue) external onlyOwner{
        emit ServiceFeePercentChanged(serviceFeePercent,newValue);
        serviceFeePercent = newValue;
    }

    function changeRoyalties(address _royalties) external onlyOwner{
        emit RoyaltiesChange(royalties,_royalties);
        royalties = _royalties;
    }

    uint256[50] private ______gap;
}

