pragma solidity ^0.8.0;

pragma abicoder v2;

import "./AuctionStruct721.sol";
import "../AuctionBase.sol";
import "../../libs/LibAucDataV1.sol";
import "../../libs/TokenToAuction.sol";
import "../../royalties/Royalties.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract Auction721 is AuctionStruct721,AuctionBase,TokenToAuction,ReentrancyGuard {
    
    constructor(address _royalties) {
        royalties = _royalties;
        __AuctionBase_init();
    }

    /// @dev creates an auction and locks sell asset
    function startAuction(
        address _sellToken,
        uint _sellTokenId,
        uint96 minimalPrice,
        bytes4 dataType,
        bytes memory data) external nonReentrant{
        LibAucDataV1.DataV1 memory aucData = LibAucDataV1.parse(data, dataType);
        address payable sender = payable(msg.sender);
        
        require(aucData.duration >= minimalDuration && aucData.duration <= MAX_DURATION, "incorrect duration");
        require(checkApproveNFT(_sellToken,sender),"collections has not been approved");

        uint96 startTime = uint96(aucData.startTime);
        uint96 endTime = 0;
        if (aucData.startTime > 0){
            require (aucData.startTime >= block.timestamp, "incorrect start time");
            endTime = uint96(startTime + aucData.duration);
        }
                
        uint currentAuctionId = getNextAndIncrementAuctionId();
        
        Auction memory auc = Auction(
            _sellToken,
            _sellTokenId,
            startTime,
            endTime,
            Bid(0, "", ""),
            sender,
            aucData.royaltiesId,
            minimalPrice,
            payable(address(0)),
            dataType,
            data
        );
        auctions[currentAuctionId] = auc;
        //transfer nft to this address auction
        _transferNFT(_sellToken, sender, address(this), _sellTokenId);
        setAuctionForToken(_sellToken, _sellTokenId, currentAuctionId);
        emit AuctionCreated(currentAuctionId, sender, endTime);
    }

    function putBid(uint _auctionId) payable public nonReentrant{
        Auction memory currentAuction = auctions[_auctionId];
        address payable newBuyer = payable(msg.sender);
        require(currentAuction.seller != newBuyer,"seller can't bid");
        uint newAmount = msg.value;
        uint96 endTime = currentAuction.endTime;
        checkAuctionInProgress(currentAuction);
        uint minBid = getMinimalNextBid(_auctionId);
        require(newAmount>=minBid,"bid too low");
        if(currentAuction.buyer != address(0x0)){
            _refundBid(currentAuction.buyer,currentAuction.lastBid.amount);
        }
        auctions[_auctionId].buyer = newBuyer;
        auctions[_auctionId].lastBid.amount = newAmount;
        emit BidPlaced(_auctionId, newBuyer, endTime);
    }

    /// @dev returns the minimal amount of the next bid (without fees)
    function getMinimalNextBid(uint _auctionId) public view returns (uint minBid){
        Auction memory currentAuction = auctions[_auctionId];
        if(currentAuction.buyer == address(0x0)){
            //first bid
            minBid = currentAuction.minimalPrice; 
        }else{
            // have old bid
            minBid = currentAuction.lastBid.amount + ((currentAuction.minimalPrice * percentNextBid) / 10000);
        }
    }

    /// @dev cancels existing auction without bid
    function cancel(uint _auctionId) external nonReentrant {
        Auction memory currentAuction = auctions[_auctionId];
        require(_checkAuctionExistence(currentAuction), "there is no auction with this id");
        address seller = currentAuction.seller;
        require(seller == msg.sender, "auction owner not detected");
        require(
            _checkAuctionRangeTime(currentAuction.endTime, currentAuction.startTime) ||
            currentAuction.buyer == address(0),
            "Canceled only when the auction period has not ended"
        );
        if(currentAuction.buyer != address(0x0)){
            //refunds for last bid buyer
            address payable receiver = payable(currentAuction.buyer);
            receiver.transfer(currentAuction.lastBid.amount);
        }
        //refunds the NFT for seller
        _transferNFT(currentAuction.sellToken,address(this),seller,currentAuction.sellTokenId);
        deactivateAuction(_auctionId, currentAuction.sellToken, currentAuction.sellTokenId);
        emit AuctionCancelled(_auctionId);
    }

    /// @dev Auction ends by seller or buyer
    function finishAuction(uint _auctionId) external nonReentrant {
        Auction memory currentAuction = auctions[_auctionId];
        require(_checkAuctionExistence(currentAuction), "there is no auction with this id");
        require(
            !_checkAuctionRangeTime(currentAuction.endTime, currentAuction.startTime) &&
            currentAuction.buyer != address(0),
            "only ended auction with bid can be finished"
        );
        address payable seller = payable(currentAuction.seller);
        require(seller == msg.sender || currentAuction.buyer == msg.sender,"only Seller & last Bidder are allowed");
        //transfer NFT for buyer
        _transferNFT(currentAuction.sellToken, address(this), currentAuction.buyer, currentAuction.sellTokenId);
        //fee deduction
        uint fee = (serviceFeePercent * currentAuction.lastBid.amount)/10000;
        totalServiceFee += fee;
        Royalties _royalties = Royalties(royalties);
        uint royaltiesPercent = _royalties.getRoyalties(currentAuction.royaltiesId).percent;
        uint amount = currentAuction.lastBid.amount - fee;
        if(royaltiesPercent>0){
            address payable _creator = payable(_royalties.getRoyalties(currentAuction.royaltiesId).owner);
            uint feeRoyalties = (royaltiesPercent * currentAuction.lastBid.amount)/10000;
            uint _amount = amount - feeRoyalties;
            //funds transfer for seller
            seller.transfer(_amount);
            //pay royalties
            _creator.transfer(feeRoyalties);
        }else{
            //funds transfer for seller
            seller.transfer(amount);
        }
        deactivateAuction(_auctionId, currentAuction.sellToken, currentAuction.sellTokenId);
    }

    /// @dev record the failed bid balance
    function _refundBid(address _oldBuyer,uint _amount) internal {
        //balanceBid[_oldBuyer] += _amount;
        address payable receiver = payable(_oldBuyer);
        receiver.transfer(_amount);
    }

    /// @dev transfer NFT ERC-721
    function _transferNFT(address _collection, address _from, address _to, uint _tokenId) internal{
        IERC721Upgradeable(_collection).transferFrom(_from, _to, _tokenId);
    }

    /// @dev returns current highest bidder for an auction
    function getSeller(uint _auctionId) public view returns(address) {
        return auctions[_auctionId].seller;
    }

    /// @dev returns current highest bidder for an auction
    function getCurrentBuyer(uint _auctionId) public view returns(address) {
        return auctions[_auctionId].buyer;
    }

    /// @dev returns current highest bidder for an auction
    function getCurrentBidAmount(uint _auctionId) public view returns(uint) {
        return auctions[_auctionId].lastBid.amount;
    }

    function auctionProgress(uint _auctionId) public view returns (bool){
        Auction memory currentAuction = auctions[_auctionId];
        if(_checkAuctionExistence(currentAuction) && _checkAuctionRangeTime(currentAuction.endTime, currentAuction.startTime)){
            return true;
        }
        return false;
    }

    function getStartTimeAuction(uint _auctionId) public view returns(uint){
        return auctions[_auctionId].startTime;
    }

    function getEndTimeAuction(uint _auctionId) public view returns(uint){
        return auctions[_auctionId].endTime;
    }

    /// @dev returns true if auction in progress, false otherwise
    function checkAuctionInProgress(Auction memory currentAuction) internal view{
        require(_checkAuctionExistence(currentAuction) && _checkAuctionRangeTime(currentAuction.endTime, currentAuction.startTime), "auction is inactive");
    }

    /// @dev returns true if auction exists, false otherwise
    function _checkAuctionExistence(Auction memory currentAuction) internal pure returns (bool){
        return currentAuction.seller != address(0);
    }

    function _checkAuctionRangeTime(uint endTime, uint startTime) internal view returns (bool){
        uint currentTime = block.timestamp;
        if (startTime > 0 && startTime > currentTime) {
            return false;
        }
        if (endTime > 0 && endTime <= currentTime){
            return false;
        }
        return true;
    }

    function checkApproveNFT(address _collection, address _seller) public view returns(bool){
        return IERC721Upgradeable(_collection).isApprovedForAll(_seller,address(this));
    }

    /*
    function getBalanceBid(address _address) public view returns(uint){
        return balanceBid[_address];
    }*/

    function totalFee() public view returns(uint){
        return totalServiceFee;
    }

    function serviceFee() public view returns(uint){
        return serviceFeePercent;
    }

    /// @dev deletes auction after finalizing
    function deactivateAuction(uint _auctionId, address _collection, uint tokenId) internal {
        emit AuctionFinished(_auctionId);
        deleteAuctionForToken(_collection, tokenId);
        delete auctions[_auctionId];
    }

    function withdrawFee(uint _amount) external onlyOwner{
        address payable receiver = payable(msg.sender);
        require(_amount>0,"withdrawal amount greater than 0");
        receiver.transfer(_amount);
    }

    /*
    function withdrawFailedBid() public nonReentrant{
        address payable receiver = payable(msg.sender);
        require(balanceBid[receiver]>0,"No balance to withdraw");
        receiver.transfer(balanceBid[receiver]);
        balanceBid[receiver]=0;
    }*/
}

