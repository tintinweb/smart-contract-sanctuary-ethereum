import "hardhat/console.sol";

import "@gnosis.pm/safe-contracts/contracts/base/GuardManager.sol";
import "@gnosis.pm/safe-contracts/contracts/common/Enum.sol";
import "@openzeppelin/contracts/utils/Context.sol";

/// Action not allowed
/// @param userGroup the group `origin` belongs to
/// @param destination the `to` of the transaction
/// @param selector the first 4 bytes of the calldata
error CallNotAllowed(bytes32 userGroup, address destination, bytes4 selector);

/// DelegateCall is attempted
error DelegateCallNotAllowed();

/// Caller is not the safe
error CallerIsNotTheSafe();

/// Guard is already initialized
error GuardAlreadySetup();

contract AccessControlGuard is Guard, Context {
    address managedSafe;
    mapping(bytes32 => bool) private actionAllowed;
    mapping(address => bytes32) private userToGroup;

    event AllowedActionChanged(bytes32 indexed action, bool allowed, bool prevState);
    event UserGroupChanged(address indexed user, bytes32 indexed group, bytes32 previousGroup);

    constructor() {
        /// Makes the implementation unusable, but can be used as a proxy target.
        managedSafe = address(1);
    }

    function setup(
        address _managedSafe,
        bytes32[] memory actions,
        bool[] memory allowed,
        address[] memory users,
        bytes32[] memory groups
    ) external {
        if (managedSafe != address(0)) {
            revert GuardAlreadySetup();
        }

        managedSafe = _managedSafe;
        changeAllowedActions(actions, allowed);
        changeUserGroups(users, groups);
    }

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlySafe() {
        if (managedSafe != _msgSender()) {
            revert CallerIsNotTheSafe();
        }
        _;
    }

    // solhint-disable-next-line payable-fallback
    fallback() external {
        // We don't revert on fallback to avoid issues in case of a Safe upgrade
        // E.g. The expected check method might change and then the Safe would be locked.
    }

    function checkTransaction(
        address to,
        uint256,
        bytes memory data,
        Enum.Operation operation,
        uint256,
        uint256,
        uint256,
        address,
        // solhint-disable-next-line no-unused-vars
        address payable,
        bytes memory,
        address sender
    ) external view override {
        if (operation != Enum.Operation.Call) {
            revert DelegateCallNotAllowed();
        }

        bytes32 userGroup = userToGroup[sender];
        if (isNotAllowed(userGroup, bytes4(data), to))
            revert CallNotAllowed(userGroup, to, bytes4(data));
    }

    function updatePermissions(
        bytes32[] memory actions,
        bool[] memory allowed,
        address[] memory users,
        bytes32[] memory groups
    ) external onlySafe {
        changeAllowedActions(actions, allowed);
        changeUserGroups(users, groups);
    }

    function checkAfterExecution(bytes32, bool) external view override {}

    function changeAllowedActions(
        bytes32[] memory actions,
        bool[] memory allowed
    ) private {
        uint256 i;
        for (i = 0; i < actions.length; ++i) {
            bool prevState = actionAllowed[actions[i]];
            actionAllowed[actions[i]] = allowed[i];

            emit AllowedActionChanged(actions[i], allowed[i], prevState);
        }
    }

    function changeUserGroups(
        address[] memory users,
        bytes32[] memory groups
    ) private {
        uint256 i;
        for (i = 0; i < users.length; ++i) {
            bytes32 previousGroup = userToGroup[users[i]];
            userToGroup[users[i]] = groups[i];

            emit UserGroupChanged(users[i], groups[i], previousGroup);
        }
    }

    function isNotAllowed(bytes32 userGroup, bytes4 selector, address to) private view returns (bool) {
        bytes32 catchAllActionHash = keccak256(abi.encodePacked(userGroup));
        bytes32 selectorActionHash = keccak256(abi.encodePacked(userGroup, selector));
        bytes32 contractActionHash = keccak256(abi.encodePacked(selectorActionHash, to));
        return (
            actionAllowed[catchAllActionHash] == false && 
            actionAllowed[selectorActionHash] == false && 
            actionAllowed[contractActionHash] == false
        );
    }
}

