pragma solidity 0.8.7;






contract Beatsu is ERC721A, Ownable, Withdrawable {
    //++++++++
    //Public Variables
    //++++++++
    uint256 public totalSupplyLeft;
    bool public saleIsActive;
    bool public revealEnabled;

    uint256 public mintPrice;

    uint256 public maximumMintPerWallet;
    mapping (uint256 => string) _tokenURIs;
    // Tracks how many tokens a wallet has minted
    mapping (address => uint256) public walletMintedCount;
    // Whitelisted addresses for early mint access
    //mapping (address => bool) public whitelistedAddresses;
    bool public whitelistOnlyEnabled;
    bytes32 public root = 0x74f4666169faccda89a45d47ab1997a62f24c3cd534a01539db8f0e40d3eb8b1;

    //++++++++
    //Internal Variables
    //++++++++
    uint256 internal tokenCounter;

    // The unRevealedUri that new mints use
    string internal unRevealUri;
    // The reveal uri that is set for new mints
    string internal revealUri;
    // Each token has a URI that it will reveal with set on minting, this allows for new drops even if all havent been revealed from previous
    mapping (uint256 => string) internal tokenRevealUri;

    // New mints will set this to their minReveal
    uint256 internal currentMinRevealId;
    // Track what the reveal id range should be for a particular token, this allows for new drops even if all haven't been revealed from previous
    mapping (uint256 => uint256[2]) internal tokenIdRevealRange;
    // Tracks which metadata has been used
    mapping (uint256 => bool) internal usedMeta;
    // Tracks which tokens have been revealed
    mapping (uint256 => bool) internal revealedTokens;

    //++++++++
    // Events
    //++++++++
    // Revealed event is triggered whenever a user reveals a token, address is indexed to make it filterable
    event Revealed(address indexed user, uint256 tokenId, uint256 timestamp);

    constructor() ERC721A("Beatsu", "BEAT")
    {
        //Defaults
        tokenCounter = 0;
        totalSupplyLeft = 9001; //the initial supply
        saleIsActive = false;
        revealEnabled = false;
        mintPrice = 50000000000000000;         //0.05 ETH
        maximumMintPerWallet = 2;
        whitelistOnlyEnabled = true;
    }

    modifier whenSaleIsActive() {
        require(saleIsActive, "Sale is not active");
        _;
    }
    modifier whenRevealIsEnabled() {
        require(revealEnabled, "Reveal is not yet enabled");
        _;
    }
    // Check if the whitelist is enabled and the address is part of the whitelist
    modifier isWhitelisted(address _address, bytes32[] calldata proof){
        //require(!whitelistOnlyEnabled || whitelistedAddresses[_address], "You need to be whitelisted");
        require(!whitelistOnlyEnabled || _verify(_leaf(_address), proof), "This address is not whitelisted");
        _;
    }
    //++++++++
    // Public functions
    //++++++++

    // Payable mint function for unrevealed NFTs
    function mint(uint256 amount, bytes32[] calldata proof) external payable whenSaleIsActive isWhitelisted(msg.sender, proof) {
        require(amount <= totalSupplyLeft, "Minting would exceed cap");
        require(walletMintedCount[msg.sender] + amount < maximumMintPerWallet, "Minting would exceed the per wallet limit");
        require(mintPrice * amount <= msg.value, "Value sent is not correct");
        _safeMint(msg.sender, amount);
        for (uint256 i = 0; i < amount; i++) {
            uint256 itemId = tokenCounter;
            --totalSupplyLeft;
            tokenCounter++;
            //Increment the amount this wallet has minted
            walletMintedCount[msg.sender]++;
            //Set the reveal id range to limit the reveal function
            tokenIdRevealRange[itemId][0] = currentMinRevealId;
            tokenIdRevealRange[itemId][1] = totalSupplyLeft + totalSupply();

            //Store the reveal URI for when its revealed
            tokenRevealUri[itemId] = revealUri;
            //Mint and set to unrevealed metadata uri
            _setTokenURI(itemId, unRevealUri);
        }
    }

    // Reveal the NFT by token owner
    function reveal(uint256 itemId, uint256 seed) public whenRevealIsEnabled {
        require(ownerOf(itemId) == msg.sender, "Cannot reveal an NFT that you don't own");
        require(revealedTokens[itemId] == false, "Cannot reveal an NFT that has already been revealed");

        //Reveal with pseudo randomness
        uint256 minId = tokenIdRevealRange[itemId][0];
        uint256 maxId = tokenIdRevealRange[itemId][1];
        // A random number between min and max
        uint256 randomResult = _getRandomNumber(seed) % (maxId - minId + 1) + minId;
        uint256 result = 0;
        // Find an unused meta at this index or above
        for (uint256 i = randomResult; i < maxId; i++) {
            if(usedMeta[i] == false){
                result = i;
                break;
            }
        }
        // Could not find an available index
        if(result == 0){
            // Find an unused meta at this index or below
            for (uint256 i = randomResult; i > minId; i--) {
                if(usedMeta[i] == false){
                    result = i;
                    break;
                }
            }
        }

        // Found an available index
        if(result > 0){
            usedMeta[result] = true;
            _setTokenURI(itemId, string(abi.encodePacked(tokenRevealUri[itemId], Strings.toString(result), ".json")));
            revealedTokens[itemId] = true;
            // Reveal event
            uint256 timestamp = block.timestamp;
            emit Revealed(ownerOf(itemId), itemId, timestamp);
        }
    }

    // Create a pseudo random number
    function _getRandomNumber(uint256 seed) internal view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(blockhash(block.number - 1), msg.sender, seed, totalSupplyLeft)));
    }

    //++++++++
    // Owner functions
    //++++++++
    function setRoot(bytes32 _root) external onlyOwner {
        root = _root;
    }
    // Sale functions
    function toggleSaleState() external onlyOwner {
        saleIsActive = !saleIsActive;
    }
    function setMintPrice(uint256 _mintPrice) external onlyOwner {
        mintPrice = _mintPrice;
    }
    // Increase the total supply to add new NFTs to the contract
    function increaseTotalSupply(uint256 amount) external onlyOwner {
        totalSupplyLeft += amount;
    }

    // Reveal functions
    function toggleRevealState() external onlyOwner {
        revealEnabled = !revealEnabled;
    }

    // Change the current minimum reveal id for new mints. 
    // This should only be changed after all are minted from previous drop.
    function setCurrentMinId(uint256 minId) external onlyOwner {
        currentMinRevealId = minId;
    }

    // Change the reveal URI used for new mints. 
    // This should only be changed after all are minted from previous drop
    function setRevealUri(string calldata uri) external onlyOwner {
        revealUri = uri;
    }

    // Change the unreveled URI that will be set for new mints, this should be a uri pointing to the unrevealed metadata json
    function setUnRevealUri(string calldata uri) external onlyOwner {
        unRevealUri = uri;
    }

    // Change the maximum mint that a single wallet can do, this should be increased before a new drop is enabled
    function setMaximumMint(uint256 amount) external onlyOwner {
        maximumMintPerWallet = amount;
    }

    // Un-paid mint function for community giveaways
    function mintForCommunity(address to) external onlyOwner {
        require(totalSupplyLeft > 0, "Minting would exceed cap");
        uint256 itemId = tokenCounter;
        --totalSupplyLeft;
        tokenCounter++;
        _safeMint(to, 1);
        _setTokenURI(itemId, unRevealUri);     
    }

    // Chance a single tokens uri in the case of a reveal error during VRF
    function changeTokenUri(uint256 tokenId, string calldata _uri) external onlyOwner {
        _setTokenURI(tokenId, _uri);
    }

    // Whitelist functions
    function toggleWhitelistState() external onlyOwner {
        whitelistOnlyEnabled = !whitelistOnlyEnabled;
    }
    
    // Add and remove user addresses to whitelist
    /*function addUserToWhitelist(address _addressToWhitelist) public onlyOwner {
        whitelistedAddresses[_addressToWhitelist] = true;
    }
    function removeUserFromWhitelist(address _addressToWhitelist) public onlyOwner {
        whitelistedAddresses[_addressToWhitelist] = false;
    }*/
    function _leaf(address account) internal pure returns (bytes32)
    {
        return keccak256(abi.encodePacked(account));
    }
    function _verify(bytes32 leaf, bytes32[] memory proof) internal view returns (bool)
    {
        return MerkleProof.verify(proof, root, leaf);
    }

    function _setTokenURI(uint256 tokenId, string memory _tokenURI) internal virtual {
        require(_exists(tokenId), "ERC721Metadata: URI set of nonexistent token");
        _tokenURIs[tokenId] = _tokenURI;
    }
    
    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        string memory _tokenURI = _tokenURIs[tokenId];
        string memory base = _baseURI();
        
        // If there is no base URI, return the token URI.
        if (bytes(base).length == 0) {
            return _tokenURI;
        }
        // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked).
        if (bytes(_tokenURI).length > 0) {
            return string(abi.encodePacked(base, _tokenURI));
        }
        // If there is a baseURI but no tokenURI, concatenate the tokenID to the baseURI.
        return string(abi.encodePacked(base, Strings.toString(tokenId)));
    }
}