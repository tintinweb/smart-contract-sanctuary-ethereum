interface IPresale {
  struct PresaleStatus {
    address token;
    bool whitelist;
    uint presaleRate;
    uint softCap;
    uint hardCap;
    uint minBuyAmount;
    uint maxBuyAmount;
    uint liquidityPercentage;
    uint listingRate;
    bool isRefund;
    uint startTime;
    uint endTime;
    uint liquidityToLockTime;
  }

  function initialize(PresaleStatus memory presaleStatus) external;
  function setAdmin(address _admin) external;
  function setLPTokenReceiver(address _addre) external;
  function setRouter(address _addre) external;
  function setContributorVestingFlag(bool _isSet) external;
  function setTeamVestingFlag(bool _isSet) external;
  
}

