pragma solidity ^0.8.0;

import "./interfaces/IPresale.sol";
import "./Presale.sol";

contract Factory {

    address public feeTo;
    address public feeToSetter;

    mapping(address => address) public getPad;
    address[] public allPads;

    event PadCreated(address indexed token,address pad, uint);

    constructor(address _feeToSetter) {
        feeToSetter = _feeToSetter;
    }

    fallback() external { 
    }

    function allPadLength() external view returns (uint) {
        return allPads.length;
    }

    function createPad(
      IPresale.PresaleStatus memory presaleStatus,
      address router,
      address presaleAdmin,
      address lpTokenReceiver,
      bool isContributorVesting,
      bool isTeamVesting
    ) external {
        require(presaleStatus.token != address(0), "Presale: ZERO_ADDRESS");
        bytes memory bytecode = type(Presale).creationCode;
        bytes32 salt = keccak256(abi.encodePacked(presaleStatus.token));
        address pad;

        assembly {
            pad := create2(0, add(bytecode, 32), mload(bytecode), salt)
        }
        
        getPad[presaleStatus.token] = pad;
        allPads.push(pad);
        
        IPresale(pad).initialize(presaleStatus);
        IPresale(pad).setAdmin(presaleAdmin);
        IPresale(pad).setLPTokenReceiver(lpTokenReceiver);
        IPresale(pad).setRouter(router);
        IPresale(pad).setContributorVestingFlag(isContributorVesting);
        IPresale(pad).setTeamVestingFlag(isTeamVesting);

        emit PadCreated(presaleStatus.token, pad, allPads.length);
    }

    function getAllPads() external view returns (address[] memory) {
        return allPads;
    }

    function setFeeTo(address _feeTo) external {
        require(msg.sender == feeToSetter, "Presale: FORBIDDEN");
        feeTo = _feeTo;
    }

    function setFeeToSetter(address _feeToSetter) external {
        require(msg.sender == feeToSetter, "Presale: FORBIDDEN");
        feeToSetter = _feeToSetter;
    }

    function withdraw() external {
        require(msg.sender == feeTo, "Not fee address");
        payable(feeTo).transfer(address(this).balance);
    }

    receive() external payable {}
}

