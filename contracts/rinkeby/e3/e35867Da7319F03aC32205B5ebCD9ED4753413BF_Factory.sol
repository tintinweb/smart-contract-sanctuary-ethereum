contract Presale is Ownable, IPresale, ReentrancyGuard {
    using SafeMath for uint;

    // address private factory;

    enum PoolStatus {
        Upworkcoming,   // 0
        Live,           // 1
        Success,          // 2
        Ended,        // 3
        Canceled        // 4
    }

    struct SocialInfo {
        bytes32  logoURL;    
        bytes32  websiteLink;    
        bytes32  github;    
        bytes32  telegram;    
        bytes32  instagram;    
        bytes32  reddit;    
        bytes32  twitter;
        bytes32  medium;
        bytes32  description;
    }

    struct ContributorVesting {
        uint firstPercentage;
        uint vestingPeriod;
        uint percentagePerCycle;
    }

    struct TeamVesting {
        uint totalVestingTokens;
        uint firstTokenRelease;
        uint firstPercentage;
        uint percentagePerCycle;
    }

    //depends on the decimals, e.g if token has 18 decimals the calculation can be done directly
    struct BuyersData {
        uint contribution;
        uint amountOfToken;
    }

    address public router;
    address public presaleAdmin;
    bool public isContributorVesting;
    bool public isTeamVesting;

    mapping(address=>bool) private isWhitelisted;
    mapping (address => BuyersData) public buyers;

    bool private marketOn;
    address public lpTokenReceiver;
    uint public collectedFunds;
    uint public numberOfUsers;

    IPresale.PresaleStatus public presaleStatus;
    ContributorVesting public contributorVesting;
    TeamVesting public teamVesting;
    SocialInfo public socialInfo;
    PoolStatus public _phase;

    uint private _currentWhitelistUsers;

    modifier isReally(bool _flag) {
        require(_flag, "Not enough condition");
        _;
    }

    modifier onlyAdmin() {
        require(msg.sender == presaleAdmin, "Only admin");
        _;
    }

    constructor() {}

    fallback() external { 
    }

    function initialize(PresaleStatus memory _presaleStatus) external override onlyOwner {
        require(_presaleStatus.listingRate < _presaleStatus.presaleRate, "Initialize: Rate is wrong");
        require(_presaleStatus.hardCap > _presaleStatus.softCap, "Initialize: Capitalization is wrong");
        require(_presaleStatus.maxBuyAmount > _presaleStatus.minBuyAmount, "Initialize: BuyAmount is wrong");
        require(_presaleStatus.endTime > _presaleStatus.startTime, "Initialize: Tim is wrong");
        require(_presaleStatus.liquidityPercentage < 96, "Initialize: LiquidityPercentage is wrong");

        presaleStatus = _presaleStatus;
    }

    function currentWhitelistUsers() external view returns (uint) {
        return _currentWhitelistUsers;
    }

    function buyerInfo(address addr) public view returns (BuyersData memory) {
        return buyers[addr];
    }

    function isWhitelist(address addr) public view returns (bool) {
        return isWhitelisted[addr];
    }

    function setAdmin(address _admin) external override onlyOwner {
        presaleAdmin = _admin;
    }

    function setRouter(address _router) external override onlyOwner {
        router = _router;
    }

    function setContributorVestingFlag(bool _flag) external override onlyOwner {
        isContributorVesting = _flag;
    }
    
    function setTeamVestingFlag(bool _flag) external override onlyOwner {
        isTeamVesting = _flag;
    }

    function setLPTokenReceiver(address _lpTokenReceiver) external override onlyOwner {
        lpTokenReceiver = _lpTokenReceiver;
    }

    function setContributorVesting(ContributorVesting memory _contributorVesting) external isReally(isContributorVesting) onlyAdmin {
        contributorVesting = _contributorVesting;
    }

    function setTeamVesting(TeamVesting memory _teamVesting) external isReally(isTeamVesting) onlyAdmin {
        teamVesting = _teamVesting;
    }
    
    function setPhase() external onlyAdmin {
        if (block.timestamp < presaleStatus.startTime)
            _phase = PoolStatus.Upworkcoming;
        else if (block.timestamp < presaleStatus.endTime)
            _phase = PoolStatus.Live;
        else _phase = PoolStatus.Ended;
    }
    
    function EnablePresaleLive() external onlyAdmin {
        require(_phase == PoolStatus.Canceled, "Must be canceled by admin");
        _phase = PoolStatus.Live;
    }   

    function addToWhitelist (address newUser) public onlyAdmin {
        isWhitelisted[newUser]=true;
        _currentWhitelistUsers += 1;
    }

    function userDepositsWhitelist() public payable nonReentrant{
        require(_phase == PoolStatus.Live, "presale not open yet");
        require(presaleStatus.whitelist == true, "Not True");
        require(isWhitelisted[msg.sender],"Not whitelisted");

        _userDeposit(msg.sender, msg.value);
    }

 
    function userDepositsPublicPhase() public payable nonReentrant {
        require(_phase == PoolStatus.Live,"Not on public _phase yet");
        require(presaleStatus.whitelist == false, "Not True");
        _userDeposit(msg.sender, msg.value);
    }

    function returnRemainingTokensInContract() public view returns (uint){
        return IBEP20(presaleStatus.token).balanceOf(address(this));
    }

    function startMarket() public onlyAdmin returns (bool) {
        require(address(this).balance >= presaleStatus.softCap, "Softcap not reached");
        require(_phase == PoolStatus.Success || _phase == PoolStatus.Live, "Marketing: Not enough");
        uint _remainTokenAmount;
        uint amountForLiquidity = (address(this).balance).mul(presaleStatus.liquidityPercentage).div(100);

        marketOn = true;
        addLiquidity(amountForLiquidity);
        _phase = PoolStatus.Ended;

        // payable(owner()).transfer(address(this).balance.mul(4).div(100));
        // payable(presaleAdmin).transfer(address(this).balance);

        // _remainTokenAmount = returnRemainingTokensInContract().sub(presaleStatus.presaleRate.mul(collectedFunds));
        // _refundToken(_remainTokenAmount);

        return marketOn;

    }

    function withdrawBaseToken() public nonReentrant {
        require(_phase == PoolStatus.Canceled,"not a refund phase");
        require(buyerInfo(msg.sender).contribution > 0,"Not contributed");
        payable(msg.sender).transfer(buyerInfo(msg.sender).contribution);
        collectedFunds = collectedFunds.sub(buyerInfo(msg.sender).contribution);
        buyers[msg.sender].contribution = 0;
        buyers[msg.sender].amountOfToken = 0;
    }

    function withdrawAdminWithTokens() public nonReentrant onlyAdmin {
        require(_phase == PoolStatus.Canceled, "not a refund phase");
        if (presaleStatus.isRefund) {
            IBEP20(presaleStatus.token).transfer(presaleAdmin, returnRemainingTokensInContract());
        }
        else IBEP20(presaleStatus.token).transfer(address(0), returnRemainingTokensInContract());
    }

    function withdrawTokens() public nonReentrant {
        require(_phase == PoolStatus.Ended && marketOn == true, "not ready to claim");

        uint amountToken = buyers[msg.sender].amountOfToken;
        require(
            amountToken > 0 && returnRemainingTokensInContract() > 0,
            "Must be bigger than zero"
        );
        IBEP20(presaleStatus.token).transfer(tx.origin, amountToken);
        buyers[msg.sender].amountOfToken = 0;
    }

    function cancelSale() public onlyAdmin {
        _phase = PoolStatus.Canceled;
    }

    function _userDeposit(address _buyer, uint _funds) internal {
        require(_funds >= presaleStatus.minBuyAmount, "At least min amount");
        require(address(this).balance.add(_funds) <= presaleStatus.hardCap, "Not be big HardCap");
        require(buyerInfo(_buyer).contribution.add(_funds) <= presaleStatus.maxBuyAmount,"Cant contribute anymore");

        buyers[_buyer].contribution = buyers[_buyer].contribution.add(_funds);
        buyers[_buyer].amountOfToken = buyers[_buyer].amountOfToken.add(_funds.mul(presaleStatus.presaleRate));

        collectedFunds = collectedFunds.add(_funds);
        numberOfUsers = numberOfUsers.add(1);
    }

    function _refundToken(uint256 _amount) private {
        if (presaleStatus.isRefund)
            IBEP20(presaleStatus.token).transfer(presaleAdmin, _amount);
        else  IBEP20(presaleStatus.token).transfer(address(0), _amount);
    }

    function addLiquidity(uint bnbAmount) private {
        uint256 tokenAmount = returnRemainingTokensInContract();
        IBEP20(presaleStatus.token).approve(router, tokenAmount);
        IUniswapV2Router02(router).addLiquidityETH{value: bnbAmount}
            (
                address(presaleStatus.token),
                bnbAmount.mul(presaleStatus.listingRate),
                0,
                0,
                lpTokenReceiver,
                block.timestamp + 1200
            );
    }
    //Contract shouldnt accept bnb/eth/etc thru fallback functions, pending implementation if its the opposite
    receive() external payable {}
}

