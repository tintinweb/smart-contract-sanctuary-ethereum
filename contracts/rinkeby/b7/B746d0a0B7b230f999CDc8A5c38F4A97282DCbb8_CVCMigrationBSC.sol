interface IUniswapV2Router {
    function factory() external pure returns (address);
    function WETH() external pure returns (address);

    function addLiquidityETH(
        address token,
        uint amountTokenDesired,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) external payable returns (uint amountToken, uint amountETH, uint liquidity);
    function getAmountsOut(uint amountIn, address[] calldata path) external view returns (uint[] memory amounts);
}

contract CVCMigrationBSC is Ownable {

    IERC20 public shibnobi = IERC20(0xab167E816E4d76089119900e941BEfdfA37d6b32);

    mapping(address => uint) claims;
    
    constructor() {
    }

    IUniswapV2Router public router = IUniswapV2Router(0x10ED43C718714eb63d5aA57B78B54704E256024E);

    function getClaims(address a) external view returns(uint) {
        return claims[a];
    }

    function claim() external {
        address sender = msg.sender;
        uint v = claims[sender];
        require(v > 0, "Nothing to claim");
        shibnobi.transfer(sender, getValue(v));
        claims[sender] = 0;
    }

    function addClaims(address[] calldata a, uint[] calldata b) external onlyOwner {
        require(a.length == b.length);
        for (uint i;i<a.length;i++){
            claims[a[i]] = b[i];
        }
    }

    function getValue(uint256 tokenAmount) public view returns (uint256 ethValue) {
        address[] memory path = new address[](2);
        path[0] = router.WETH();
        path[1] = address(shibnobi);
        ethValue = router.getAmountsOut(tokenAmount, path)[1];
    }

    function setShibnobi(IERC20 n) external onlyOwner {
        shibnobi = n;
    }

    function getTokens(IERC20 tokenAddress) external onlyOwner {
        SafeERC20.safeTransfer(tokenAddress, owner(), tokenAddress.balanceOf(address(this)));
    }

    function getShibnobi() external onlyOwner {
        SafeERC20.safeTransfer(shibnobi, owner(), shibnobi.balanceOf(address(this)));
    }

    function withdrawETH() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }
}