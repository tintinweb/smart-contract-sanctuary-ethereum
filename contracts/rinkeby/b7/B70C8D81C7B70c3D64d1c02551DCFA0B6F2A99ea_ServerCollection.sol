    }

    function getCreator () public view returns (address payable) {
       return creator;
    }

    function getServerids () public view returns (bytes32 [] memory) {
        return _serverids._inner._values;
    }

    function getSoldids () public view returns (bytes32 [] memory) {
        return _soldids._inner._values;
    }

    function mint(address to,string memory _uri, string memory _name,uint256 _pledge,address _pledgetoken,uint256 _price,address _pricetoken,bytes memory data) public {
        IERC20 token = IERC20(_pledgetoken);
        require(token.transferFrom(msg.sender, address(this), _pledge),"server: wrong token transfer");
        _mint(to, lastId, 1, data);
        _uris[lastId] = _uri;
        _names[lastId] = _name;
        _pledges[lastId] = _pledge;
        _pledgetokens[lastId] = _pledgetoken;
        _prices[lastId] = _price;
        _pricetokens[lastId] = _pricetoken;
        _workers[lastId] = msg.sender;
        _customstatuses[lastId] = false;
        _workerstatuses[lastId] = false;
        _serverids.add(lastId);
        emit Transfer(address(0), to, lastId);       
        lastId++;
    }


    function burn(address owner, uint256 id, uint256 value) public onlyAdmins{
        _burn(owner, id, value);
        _uris[id] = "";
        _names[id] = "";
        _pledges[id] = 0;
        _prices[id] = 0;
        _workerstatuses[id] = false;
        _customstatuses[id] = false;
        _pledgetokens[id] = address(0x0);
        _pricetokens[id] = address(0x0);
        _workers[id] = address(0x0);
        if(_serverids.contains(id)){
            _serverids.remove(id);
        }
        if(_soldids.contains(id)){
            _soldids.remove(id);
        }        
    }

    function burnbyworker(uint256 id) public{
        require(_exists(id), "ERC721Metadata: URI query for nonexistent token");
        require(ownerOf(id) == _workers[id], "server: only worker==owner");
        require(ownerOf(id) == msg.sender, "server: only burn yourself");
        _burn(msg.sender, id, 1);
        _uris[id] = "";
        _names[id] = "";
        _pledges[id] = 0;
        _prices[id] = 0;
        _workerstatuses[id] = false;
        _customstatuses[id] = false;
        _pledgetokens[id] = address(0x0);
        _pricetokens[id] = address(0x0);
        _workers[id] = address(0x0);
        _serverids.remove(id);
    }

}