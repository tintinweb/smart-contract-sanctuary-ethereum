contract SeaGoldStaking is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    bool allowStaking = false;
    uint256 dayDistribution = 200000000 * 1e18;

    struct User {
        uint256 amount;
        uint256 date;
    }

    mapping(address => User) public userStake;
    mapping(uint256 => uint256) public tradePerDay;
    uint256 public lastday;

    uint256 public totalamountStaked;
    uint256 public minAmount;
    uint256 public balanceStaked;
    address public tradeReward;

    IERC20 public SeaGoldoken;

    constructor(IERC20 _tokenAddress, uint256 startDate) {
        IERC20 tokenContract = IERC20(_tokenAddress);
        lastday = startDate;
        tradePerDay[lastday] = 0;
        SeaGoldoken = tokenContract;
    }

    function getApprovalStatus() public view returns (bool) {
        return allowStaking;
    }

    function updateApprovalStatus(bool _allowStaking) public onlyOwner {
        allowStaking = _allowStaking;
    }

    function getTradeRewardAddress() public view returns (address) {
        return tradeReward;
    }

    function updateTradeRewardAddress(address _tradeReward) public onlyOwner {
        tradeReward = _tradeReward;
    }

    function getDayDistribution() public view returns (uint256) {
        return dayDistribution;
    }

    function updateDayDistribution(uint256 _dayDistribution) public onlyOwner {
        dayDistribution = _dayDistribution * 1e18;
    }

    function stake(
        uint256 amount,
        uint256 _date,
        address ownerAddress
    ) public nonReentrant {
        require(allowStaking, "Not allowed to stake");
        require(amount >= 1000 * 1e18, "Not allowed to stake");

        // require(
        //     (block.timestamp - _date) / 60 / 60 / 24 == 0,
        //     "Enter valid date"
        // );
        address userAddress = msg.sender;
        if (tradeReward == msg.sender) {
            userAddress = ownerAddress;
        }

        uint256 daysDiff = ((_date - lastday) / 60 / 60 / 24);
        if (daysDiff > 0) {
            daysDiff = daysDiff - 1;
        }

        if (daysDiff > 0) {
            for (uint256 i = 0; i < daysDiff; i++) {
                lastday = lastday + 1 days;
                tradePerDay[lastday] = balanceStaked;
            }
        }

        if (userStake[userAddress].amount > 0) {
            harvest(_date);
        }

        lastday = _date;
        balanceStaked = balanceStaked + amount;
        tradePerDay[_date] = balanceStaked;

        userStake[userAddress].amount = userStake[userAddress].amount + amount;
        userStake[userAddress].date = _date;
        totalamountStaked = totalamountStaked + amount;
        uint256 allowance = SeaGoldoken.allowance(userAddress, address(this));
        uint256 balance = SeaGoldoken.balanceOf(userAddress);
        require(allowance >= amount, "Amount not approved");
        require(balance >= amount, "Insufficient balance user");
        if (tradeReward == msg.sender) {
        SeaGoldoken.safeTransferFrom(tradeReward, address(this), amount);
        } else {
        SeaGoldoken.safeTransferFrom(userAddress, address(this), amount);
        }
    }

    function harvest(uint256 _date) internal {
        uint256 amountRewards;
        uint256 daysDiff = (_date - userStake[msg.sender].date) / 60 / 60 / 24;
        uint256 daysStaked = userStake[msg.sender].date;
        if (daysDiff > 0) {
            for (uint256 i = 0; i < daysDiff; i++) {
                uint256 amountPercentage = (userStake[msg.sender].amount *
                    1e20) / tradePerDay[daysStaked];

                uint256 amountRewardPerDay = (amountPercentage *
                    dayDistribution) / 1e20;

                amountRewards = amountRewards + amountRewardPerDay;
                daysStaked = userStake[msg.sender].date + 1 days;
            }

            uint256 balance = SeaGoldoken.balanceOf(address(this));
            require(
                balance >= amountRewards,
                "Insufficent balance for harvest"
            );
            SeaGoldoken.transfer(msg.sender, amountRewards);
        }
    }

    function receiveamount(uint256 _date, address userAddress)
        public
        view
        returns (uint256)
    {
        uint256 amountRewards;
        uint256 daysDiff = (_date - userStake[userAddress].date) / 60 / 60 / 24;
        uint256 daysStaked = userStake[userAddress].date;
        if (daysDiff > 0) {
            for (uint256 i = 0; i < daysDiff; i++) {
                uint256 amountPercentage = (userStake[userAddress].amount *
                    1e20) / tradePerDay[daysStaked];

                uint256 amountRewardPerDay = (amountPercentage *
                    dayDistribution) / 1e20;
                amountRewards = amountRewards + amountRewardPerDay;
                daysStaked = userStake[userAddress].date + 1 days;
            }
        }
        return amountRewards;
    }

    function unstake(uint256 _date) public nonReentrant {
        // require(
        //     (block.timestamp - _date) / 60 / 60 / 24 == 0,
        //     "Enter valid date"
        // );

        uint256 daysDiff = ((_date - lastday) / 60 / 60 / 24);
        if (daysDiff > 0) {
            daysDiff = daysDiff - 1;
        }

        if (daysDiff > 0) {
            for (uint256 i = 0; i < daysDiff; i++) {
                lastday = lastday + 1 days;
                tradePerDay[lastday] = balanceStaked;
            }
        }

        if (userStake[msg.sender].amount > 0) {
            harvest(_date);
            balanceStaked = balanceStaked - userStake[msg.sender].amount;
            uint256 balance = SeaGoldoken.balanceOf(address(this));
            require(
                balance >= userStake[msg.sender].amount,
                "Insufficent balance in contract"
            );

            SeaGoldoken.transfer(msg.sender, userStake[msg.sender].amount);
            delete userStake[msg.sender];
        }
    }
}