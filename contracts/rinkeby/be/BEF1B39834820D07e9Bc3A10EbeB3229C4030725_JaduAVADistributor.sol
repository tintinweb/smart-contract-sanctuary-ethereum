pragma solidity ^0.8.7;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

import "./IJaduAVA.sol";
import "hardhat/console.sol";

error ContractUpgraded();

contract JaduAVADistributor is AccessControl {
    struct MintLimitInfo {
        uint256 totalMinted;
        uint256 tokenId;
        address contractAddress;
        uint256 maxLimit;
    }

    IJaduAVA public jaduAVAToken;

    address public JADU_TREASURY_WALLET; // JADU VAULT ADDRESS TO RECEIVE ETHEREUM

    uint256 public MINT_PRICE = 0.0001 ether; //0.08 ETH

    mapping(address => uint256) public mintCountUser;

    mapping(bytes32 => MintLimitInfo) public mintlimitInfo;

    mapping(bytes => bool) public isValidSignature;

    mapping(address => uint256) public walletMintingSpots;

    bytes32 private constant SIGNER_ROLE = keccak256("SIGNER_ROLE");

    bool public _mintingPaused = false;

    modifier onlyAdminWallet() {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()),
            "must call by signer wallet"
        );
        _;
    }

    fallback() external payable {}

    receive() external payable {}

    event UpdateLimit(uint256, address, uint256);

    constructor(
        IJaduAVA _jaduAVAToken,
        address signer,
        address treasuryWallet
    ) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(SIGNER_ROLE, signer);
        jaduAVAToken = _jaduAVAToken;
        JADU_TREASURY_WALLET = treasuryWallet;
    }

    function setJaduTreasuryWallet(address _jaduTreasuryWallet)
        external
        payable
        onlyAdminWallet
    {
        require(_jaduTreasuryWallet != address(0), "ZERO ADDRESS");
        JADU_TREASURY_WALLET = _jaduTreasuryWallet;
    }

    function _mintChecks(
        address to,
        uint256 size,
        uint256 nonce,
        bytes calldata signature,
        MintLimitInfo[] calldata mintLimitInfos
    ) internal returns (uint256) {
        require(size <= 10, "cannot premint/mint more than 10");
        require(!isValidSignature[signature], "Signature Invalid");
        address wallet = _msgSender();
        require(wallet == to, "caller and wallet mismatch");
        require(!_mintingPaused, "JADU: Minting paused");
        bytes32 mintData = keccak256(abi.encode(mintLimitInfos));
        address signerOwner = verifySignature(
            wallet,
            size,
            nonce,
            mintData,
            signature
        );
        require(
            hasRole(SIGNER_ROLE, signerOwner),
            "JADU: Not authorized to mint"
        );
        require(
            msg.value >= (MINT_PRICE * size),
            "INSUF Amt provided for minting"
        );
        uint256 tokenId = jaduAVAToken.getCurrentTokenId();
        require(
            tokenId + size <= jaduAVAToken.getMaxSupply(),
            "JADU: Maximum cap reached"
        );
        uint256 wantToMintCount;
        if (walletMintingSpots[to] > 0) {
            wantToMintCount = wantToMintCount + walletMintingSpots[to];
            if (size >= walletMintingSpots[to]) {
                walletMintingSpots[to] = 0;
            } else {
                walletMintingSpots[to] = walletMintingSpots[to] - size;
            }
        }
        if (mintLimitInfos[0].contractAddress != address(0)) {
            wantToMintCount =
                wantToMintCount +
                verifyAndGetMintCount(mintLimitInfos);
        }
        isValidSignature[signature] = true;
        payable(JADU_TREASURY_WALLET).transfer(msg.value);
        return wantToMintCount;
    }

    function mintNft(
        address to,
        uint256 size,
        uint256 nonce,
        bytes calldata signature,
        MintLimitInfo[] calldata mintLimitInfos
    ) external payable returns (bool) {
        uint256 sizeToMint = _mintChecks(
            to,
            size,
            nonce,
            signature,
            mintLimitInfos
        );
        jaduAVAToken.mint(_msgSender(), sizeToMint);
        return true;
    }

    function airDropJaduAVA(
        address[] calldata _users,
        uint256[] calldata _quantity
    ) external payable onlyAdminWallet returns (bool) {
        require(
            _users.length == _quantity.length,
            "len of input arr should be same"
        );
        uint256 usersLen = _users.length;
        for (uint256 i; i < usersLen; i++) {
            jaduAVAToken.mint(_users[i], _quantity[i]);
        }
        return true;
    }

    function walletWhitelist(
        address[] calldata _users,
        uint256[] calldata _quantity
    ) external payable onlyAdminWallet returns (bool) {
        require(
            _users.length == _quantity.length,
            "len of input arr should be same"
        );
        uint256 usersLen = _users.length;
        for (uint256 i; i < usersLen; i++) {
            // to do put in mapping
            walletMintingSpots[_users[i]] = _quantity[i];
        }
        return true;
    }

    function getWalletSpots(address _walletAddress)
        public
        view
        returns (uint256)
    {
        return walletMintingSpots[_walletAddress];
    }

    function jaduVaultAirdrop(uint256 _quantity, address walletAddress)
        external
        payable
        onlyAdminWallet
        returns (bool)
    {
        jaduAVAToken.mint(walletAddress, _quantity);
        return true;
    }

    function togglePause(bool _pause) external payable onlyAdminWallet {
        require(_mintingPaused != _pause, "Already in desired pause state");
        _mintingPaused = _pause;
    }

    function updatePrice(uint256 _newPrice) external payable onlyAdminWallet {
        MINT_PRICE = _newPrice;
    }

    function verifySignature(
        address wallet,
        uint256 _size,
        uint256 _nonce,
        bytes32 _mintData,
        bytes memory _signature
    ) internal pure returns (address) {
        return
            ECDSA.recover(
                keccak256(abi.encode(wallet, _size, _nonce, _mintData)),
                _signature
            );
    }

    // function uncheckedInc(uint256 x) internal pure returns (uint256) {
    //     unchecked {
    //         return x + 1;
    //     }
    // }

    function verifyAndGetMintCount(MintLimitInfo[] memory mintLimitInfos)
        internal
        returns (uint256 wantToMint)
    {
       
        uint256 mintLimitInfosLen = mintLimitInfos.length;
         console.log(mintLimitInfosLen,"array length");
        for (uint256 index; index < mintLimitInfosLen; index++) {
            bytes32 tokenHash = keccak256(
                abi.encodePacked(
                    mintLimitInfos[index].tokenId,
                    mintLimitInfos[index].contractAddress
                )
            );
            MintLimitInfo memory currentTokenLimit = mintlimitInfo[tokenHash];
            console.log(currentTokenLimit.totalMinted,"THis is currnet totalMinted");
            console.log(mintLimitInfos[index].totalMinted,"THis is mintinfo totalMinted");
            console.log(  mintLimitInfos[index].maxLimit,"THis is maxLimit mintlimit");
            require(
                currentTokenLimit.totalMinted +
                    mintLimitInfos[index].totalMinted <=
                    mintLimitInfos[index].maxLimit,
                "JADU AVA: Invalid minting limit"
            );
            wantToMint = wantToMint + mintLimitInfos[index].totalMinted;
            mintlimitInfo[tokenHash].totalMinted =
                mintlimitInfo[tokenHash].totalMinted +
                mintLimitInfos[index].totalMinted;
            emit UpdateLimit(
                mintLimitInfos[index].tokenId,
                mintLimitInfos[index].contractAddress,
                mintlimitInfo[tokenHash].totalMinted
            );
        }
    }
}

