pragma solidity ^0.8.0;

import {NFTExchange} from "./NFTExchange.sol";
import {SignatureUtils} from "../utils/SignatureUtils.sol";
import "openzeppelin-solidity/contracts/token/ERC20/IERC20.sol";
import "openzeppelin-solidity/contracts/token/ERC20/utils/SafeERC20.sol";

contract NFTExchangeV1 is NFTExchange {
    using SafeERC20 for IERC20;

    function setFeaturePrice(
        string memory _featureId,
        address _tokenAddress,
        uint256 _price
    ) public onlyAdmins {
        featurePrice[_featureId][_tokenAddress] = _price;
        emit FeaturePriceEvent(_tokenAddress, _featureId, _price);
    }

    function getFeaturePrice(string memory _featureId, address _tokenAddress)
        public
        view
        returns (uint256)
    {
        return featurePrice[_featureId][_tokenAddress];
    }

    function setTokenFee(
        string memory _currencyId,
        address _tokenAddress,
        uint256 _feeRate
    ) public onlyAdminsAndSubAdmins(2) {
        tokensFee[_tokenAddress] = _feeRate;
        emit ServiceFeeEvent(_tokenAddress, _currencyId, _feeRate);
    }

    function getTokenFee(address _tokenAddress) public view returns (uint256) {
        return tokensFee[_tokenAddress];
    }

    function addAcceptedToken(string memory _currencyId, address _tokenAddress)
        public
        onlyAdminsAndSubAdmins(1)
    {
        acceptedTokenAddress[_tokenAddress] = true;
        emit PaymentTokenEvent(_tokenAddress, _currencyId);
    }

    function removeAcceptedToken(address _tokenAddress) public onlyAdmins {
        acceptedTokenAddress[_tokenAddress] = false;
    }

    function isAcceptedToken(address tokenAddress) public view returns (bool) {
        return acceptedTokenAddress[tokenAddress];
    }

    function getSoldQuantityByTokenID(uint256 tokenID)
        public
        view
        returns (uint256)
    {
        return soldQuantity[tokenID];
    }

    function getSoldQuantityBySaleOrder(bytes memory signature)
        public
        view
        returns (uint256)
    {
        return soldQuantityBySaleOrder[signature];
    }

    function getFeatureStatus(string memory _storeID, string memory _feature)
        public
        view
        returns (bool)
    {
        return storeFeatures[_storeID][_feature];
    }

    function withdraw(
        address _to,
        address tokenAddress,
        uint256 _amount
    ) public onlyAdmins {
        require(tokenAddress != address(0), "Token address is zero");
        IERC20(tokenAddress).safeTransfer(_to, _amount);
    }
}

