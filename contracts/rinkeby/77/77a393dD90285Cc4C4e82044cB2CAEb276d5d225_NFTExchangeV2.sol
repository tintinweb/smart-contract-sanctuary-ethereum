pragma solidity ^0.8.0;

import {NFTExchangeV1} from "./NFTExchangeV1.sol";

contract NFTExchangeV2 is NFTExchangeV1 {

    // data: tokenID(0), quantity(1), sellOrderSupply(2), sellOrderPrice(3), enableMakeOffer(4), buyingAmount(5), tokenType(6), sellOrderID(7)
    // addr: creator(0), tokenAddress(1), collectionAddress(2), signer(3)
    // signatures: nftBuyRequestSignature(0), sellOrderSignature(1)
    // strs: internalTxID(0)
    function buyNowNative(uint[] memory /*data*/, address[] memory /*addr*/, string[] memory /*strs*/, bytes[] memory /*signatures*/) public payable {
        _delegatecall(buyHandlerNativeAddress);
    }

    // data: tokenID(0), quantity(1), sellOrderSupply(2), enableMakeOffer(3), sellOrderPrice(4), offerAmount(5)
    // offerPrice(6), listingTime(7), expirationTime(8), tokenType(9)
    // addr: creator(0), contractAddress(1), tokenAddress(2), reciever(3), signer(4)
    // strs: internalTxID(0)
    // signatures: nftAcceptOfferSignature(0)
    function acceptOffer(uint[] memory /*data*/, address[] memory /*addr*/, string[] memory /*strs*/, bytes[] memory /*signatures*/) public {
        _delegatecall(offerHandlerAddress);
    }

    // data: tokenID(0), royaltyRatio(1), sellOrderSupply(2), sellOrderPrice(3), enableMakeOffer(4), amount(5), tokenType(6)
    // addr: creator(0), contractAddress(1), tokenAddress(2), seller(3), signer(4)
    // signatures: NftResellSignature(0), sellOrderSignature(1)
    // strs: storeID(0), internalTxID(1)
    function sellNowNative(uint[] memory /*data*/, address[] memory /*addr*/, string[] memory /*strs*/, bytes[] memory /*signatures*/) public payable {
        _delegatecall(sellHandlerNativeAddress);
    }
    
    // addr: storeAddress(0), tokenAddress(1)
    // data: amount(0), type(1)
    // strs: storeID(0), featureID(1), internalTxID(2)
    function activateFeature(address[] memory /*addr*/, uint256[] memory /*data*/, string[] memory /*strs*/) public { 
        _delegatecall(featureHandlerAddress);
    }

    // addr: storeAddress(0), tokenAddress(1)
    // data: amount(0), salt(1), type(2)
    // strs: storeID(0), featureID(1), internalTxID(2)
    function deactivateFeature(address[] memory /*addr*/, uint256[] memory /*data*/, string[] memory /*strs*/) public {
        _delegatecall(featureHandlerAddress);
    }

    // data: tokenID(0), quantity(1), sellOrderSupply(2), sellOrderPrice(3), enableMakeOffer(4), tokenType(5), type(6), royaltyRatio(7)
    // addr: creator(0), tokenAddress(1), collectionAddress(2), signer(3), seller(4)
    // strs: internalTxID(0)
    // signatures: sellOrderSignature(0)
    function cancelSaleOrder(uint[] memory /*data*/, address[] memory /*addr*/, string[] memory /*strs*/, bytes[] memory /*signatures*/) public {
        _delegatecall(cancelHandlerAddress);
    } 
     
    // data: tokenID(0), quantity(1), sellOrderSupply(2), enableMakeOffer(3), sellOrderPrice(4), offerAmount(5)
    // offerPrice(6), listingTime(7), expirationTime(8), tokenType(9), type(10)
    // addr: creator(0), contractAddress(1), tokenAddress(2), reciever(3), signer(4)
    // strs: internalTxID(0)
    // signatures: offerSignature(0)
    function cancelOffer(uint[] memory /*data*/, address[] memory /*addr*/, string[] memory /*strs*/, bytes[] memory /*signatures*/) public { 
        _delegatecall(cancelHandlerAddress);
    }
    
    function setSigner(address _address, bool _value) external onlyAdmins{
        signers[_address] = _value;
    }

    function isSigner(address _address) external view returns (bool) {
        return signers[_address];
    }
    
    function _delegatecall(address _impl) internal {
        require(_impl != address(0), "Impl address is 0");
        assembly {
            let ptr := mload(0x40)
            calldatacopy(ptr, 0, calldatasize())
            let result := delegatecall(sub(gas(), 10000), _impl, ptr, calldatasize(), 0, 0)
            let size := returndatasize()
            returndatacopy(ptr, 0, size)
            switch result
            case 0 { 
                revert(ptr, size)
            }
            default {
                return(ptr, size)
            }
        }
    } 
}

