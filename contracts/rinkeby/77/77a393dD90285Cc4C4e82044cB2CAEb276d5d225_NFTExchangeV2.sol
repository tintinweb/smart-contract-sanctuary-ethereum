pragma solidity ^0.8.0;

import {ReentrancyGuarded} from "./ReentrancyGuarded.sol";
import {NFTify1155} from "../NFTify1155.sol";
import {NFTify721} from "../NFTify721.sol";

contract Upgradable is ReentrancyGuarded {
    address nftAddress;
    address recipientAddress;
    address signatureUtilsAddress;

    address buyHandlerAddress;
    address offerHandlerAddress;
    address configAddress;

    uint256 public ratioAdminFee;

    mapping(address => bool) adminList;
    mapping(address => bool) acceptedTokenAddress;
    mapping(uint256 => uint256) soldQuantity;
    mapping(bytes => bool) invalidSaleOrder;
    mapping(bytes => bool) invalidOffers;
    mapping(bytes => bool) acceptedOffers;
    mapping(bytes => uint256) soldQuantityBySaleOrder;
    mapping(uint256 => bytes) currentSaleOrderByTokenID;
    mapping(address => uint256) tokensFee;
    mapping(address => uint256) totalFeeAmount;

    address erc721NftAddress;
    address erc721OfferHandlerAddress;
    address erc721BuyHandlerAddress;
    address erc721OfferHandlerNativeAddress;
    address erc721BuyHandlerNativeAddress;
    address offerHandlerNativeAddress;
    address buyHandlerNativeAddress;
    address sellHandlerAddress;
    address erc721SellHandlerAddress;
    address featureHandlerAddress;
    address sellHandlerNativeAddress;
    address erc721SellHandlerNativeAddress;

    // storeID -> featureID -> activatedStatus
    mapping(string => mapping(string => bool)) storeFeatures;
    // storeID -> creator -> amount
    mapping(string => mapping(address => uint256)) royaltyFeeAmount;

    address cancelHandlerAddress;
    mapping(string => mapping(address => uint256)) featurePrice; // featureId => tokenAddress => price
    mapping(string => mapping(string => mapping(address => uint256))) featureStakedAmount; // storeID => featureID => address => amount
    mapping(address => bool) signers;

    /**
        The following variables are added on Sprint 16
     */
    mapping(address => mapping(uint8 => bool)) subAdminList;
    address public N1Address;   // no need
    mapping(string => uint256) feeForN1;    // no need
    mapping(string => uint256) feeForOtherTokens;   // no need
    mapping(uint8 => uint256) serviceFeeByPlan; // no need

    event AcceptOfferEvent(
        uint256 _tokenID,
        uint256 _amount,
        uint256 _pricePerItem,
        uint256 _listingTime,
        uint256 _expirationTime,
        address indexed _receiver,
        address indexed _contractAddress,
        uint8 _type, // 0: ProcessedOffer, 1: AcceptedOffer, 2: CanceledOffer
        string internalTxID
    );

    event BuyNFTEvent(
        uint256 _tokenID,
        uint256 _amount,
        address indexed _receiver,
        address indexed _contractAddress,
        uint8 _type, // 0: ProcessingBuyRequest, 1: AcceptedBuyRequest, 2: CanceledBuyRequest
        string internalTxID
    );

    event SellNFTEvent(
        uint256 _tokenID,
        uint256 _amount,
        address indexed _seller,
        address indexed _receiver,
        address indexed _collectionAddr,
        uint8 _type, // 0: ProcessingSellRequest, 1: AcceptedSellRequest, 2: CanceledSellRequest
        string internalTxID
    );

    event CancelEvent (
        address indexed _creator,
        uint256 _type, //  1:CancelOffer, 2: CancelSellOrder, 3: CancelSellOrderResell 
        string internalTxID
    );

    event ServiceFeeEvent(
        address indexed _tokenAddress,
        string _currencyId,
        uint256 _feeRate
    );

    event FeaturePriceEvent(
        address indexed _tokenAddress,
        string _featureId,
        uint256 _price
    );

    event FeatureEvent(
        string _storeID,
        address indexed _storeAddress,
        address indexed _tokenAddress,
        string _featureID,
        uint256 _amount,
        uint256 _type,
        string internalTxID
    );

    event PaymentTokenEvent(address indexed _tokenAddress, string _currencyId);

    modifier onlyAdmins() {
        require(adminList[msg.sender], "Need admin role");
        _;    
    }

    modifier onlyAdminsAndSubAdmins(uint8 _role) {
        require(adminList[msg.sender] || subAdminList[msg.sender][_role], "Only admins or sub-admin with this role");
        _;
    }

    function mint(
        address collection,
        address account,
        uint256 id,
        uint256 amount,
        uint256 tokenType,
        bytes memory data 
    ) internal {
        if (tokenType == 0) {
            NFTify1155(collection).mint(account, id, amount, data);
        } else {
            NFTify721(collection).mint(account, id, data);
        }
    }

    function transferNFT(
        address collection,
        address from,
        address to,
        uint256 id,
        uint256 amount,
        uint256 tokenType, 
        bytes memory data
    ) internal {
        if (tokenType == 0) {
            NFTify1155(collection).safeTransferFrom(from, to, id, amount, data);
        } else {
            NFTify721(collection).safeTransferFrom(from, to, id, data);
        }
    }

    function setAdminList(address _address, bool value) public {
        adminList[_address] = value;
    }

    function isAdmin(address _address) public view returns (bool) {
        return adminList[_address];
    }

    function setSubAdminList(address _address, uint8 _role, bool value) public onlyAdmins {
        subAdminList[_address][_role] = value;
    }

    function isSubAdmin(address _address, uint8 _role) public view returns (bool) {
        return subAdminList[_address][_role] || adminList[_address];
    }
}

