interface IRoyaltyRegistry {
    function setRoyalty(address _erc721address, address payable _payoutAddress, uint256 _payoutPerMille) external;

    function getRoyaltyPayoutAddress(address _erc721address) external view returns (address payable);

    function getRoyaltyPayoutRate(address _erc721address) external view returns (uint256);
}

interface IL1ERC721Bridge {
    function depositERC721To(
      address _l1Token,
      address _l2Token,
      address _to,
      uint256 _tokenId,
      uint32 _l2Gas,
      bytes calldata _data
    ) external;
}


contract L1BridgeSellOrderExchange is Pausable, ReentrancyGuard, NonblockingLzApp {
    event SellOrderFinalized(
        address indexed seller,
        address buyer,
        address l1ContractAddress,
        address l2ContractAddress,
        uint256 indexed tokenId,
        uint256 price
    );
    
    uint public counter;
    uint16 constant ETH_CHAIN_ID = 1;
    uint16 constant OPT_CHAIN_ID = 10;
    bytes4 constant INTERFACE_ID_ERC721 = 0x80ac58cd;  // The ERC-165 identifier for ERC-721
    uint256 constant MARKETPLACE_FEE_PER_MILLE = 25;
    uint256 constant MAX_ROYALTY_PER_MILLE = 150;

    address zroTokenHolder;
    uint256 public globalGasFee; // Global gas fee that is subtracted from finalized sell orders

    IRoyaltyRegistry royaltyRegistry;
    address l1Bridge;

    // Maps a collection's L1 NFT address to its gas fee.
    mapping(address => uint256) public collectionGasFees;

    constructor(address _lzEndpoint) NonblockingLzApp(_lzEndpoint) {}

    /* Sets the global gas fee that is subtracted from the price of finalized sell orders. */ 
    function setGlobalGasFee(uint256 _globalGasFee) external onlyOwner {
        globalGasFee = _globalGasFee;
    }

    /* Sets the gas fee for individual collections, which overrides the global gas fee. */ 
    function setCollectionGasFee(address _l1ContractAddress, uint256 _gasFee) external onlyOwner {
        collectionGasFees[_l1ContractAddress] = _gasFee;
    }

    function setRoyaltyRegistry(address _royaltyRegistry) external onlyOwner {
        royaltyRegistry = IRoyaltyRegistry(_royaltyRegistry);
    }

    function setL1ERC721Bridge(address _l1Bridge) external onlyOwner {
        l1Bridge = _l1Bridge;
    }

    /* Receives the cross-chain message and finalizes the sell order. */
    function _nonblockingLzReceive(
        uint16,
        bytes memory,
        uint64,
        bytes memory _payload
    ) internal override {
        // decode the variables in the payload
        (
            address seller,
            address l1ContractAddress,
            address l2ContractAddress,
            uint256 tokenId,
            uint256 price,
            address buyer,
            bytes memory signature
        ) = abi.decode(_payload, (address, address, address, uint256, uint256, address, bytes));

        // attempt to finalize sell order
        try this.finalizeSellOrder(
            payable(seller),
            l1ContractAddress,
            l2ContractAddress,
            tokenId,
            price,
            buyer
        ) {
            // do nothing
        } catch {
            bytes memory trustedRemote = trustedRemoteLookup[OPT_CHAIN_ID];

            (uint messageFee, ) = lzEndpoint.estimateFees(OPT_CHAIN_ID, address(this), signature, false, '');
            require(address(this).balance >= messageFee, "L1 Exchange has insufficient funds to.");

            lzEndpoint.send{value: messageFee}(
                OPT_CHAIN_ID,
                trustedRemote,
                signature, // payload
                payable(address(this)), // refund address
                zroTokenHolder,
                '' // adapter params
            );
        }
    }

    /*
    * @dev External trade function. This accepts the details of the sell order and signed sell
    * order (the signature) as a meta-transaction.
    *
    * Emits a {SellOrderFinalized} event via `_finalizeSellOrder`.
    */
    function finalizeSellOrder(
        address payable seller,
        address l1ContractAddress,
        address l2ContractAddress,
        uint256 tokenId,
        uint256 price,
        address buyer
    ) external whenNotPaused nonReentrant {
        require(msg.sender == address(this), 'Caller must be L1 Exchange.');
        require(
            ERC165Checker.supportsInterface(l1ContractAddress, INTERFACE_ID_ERC721),
            "L1 NFT contract does not support the ERC721 interface."
        );

        emit SellOrderFinalized(
            seller,
            buyer,
            l1ContractAddress,
            l2ContractAddress,
            tokenId,
            price
        );

        // Transfers NFT ownership from the seller to the L1 Exchange contract so that the
        // NFT can be transferred to the L1 Bridge.
        IERC721(l1ContractAddress).transferFrom(seller, address(this), tokenId);

        // Approves the L1 ERC721 Bridge to operate the NFT.
        IERC721(l1ContractAddress).approve(l1Bridge, tokenId);

        // Deposits the NFT from the L1 Exchange contract into the L1 Bridge, which will
        // cause an NFT to be minted on L2 and sent to the buyer.
        IL1ERC721Bridge(l1Bridge).depositERC721To(
            l1ContractAddress,
            l2ContractAddress,
            buyer,
            tokenId,
            1200000,
            ''
        );

        // Send payments
        _sendETHPaymentsWithRoyalties(l1ContractAddress, seller);
    }

    /*
    * @dev Sends out ETH payments to marketplace, royalty, and the final recipients
    */
    function _sendETHPaymentsWithRoyalties(address l1ContractAddress, address payable seller) internal {
        uint256 balance = address(this).balance;
        uint256 royaltyPayout = (royaltyRegistry.getRoyaltyPayoutRate(l1ContractAddress) * balance) / 1000;
        uint256 marketplacePayout = (MARKETPLACE_FEE_PER_MILLE * balance) / 1000;

        uint256 sellerPayout;
        uint256 collectionGasFee = collectionGasFees[l1ContractAddress];
        // Checks if a gas fee has been set for the collection. If so, the global gas fee is overridden.
        if (collectionGasFee > 0) {
            sellerPayout = balance - royaltyPayout - marketplacePayout - collectionGasFee;
        }
        // If a collection does not have a gas fee, we use the global gas fee instead.
        else {
            sellerPayout = balance - royaltyPayout - marketplacePayout - globalGasFee;
        }

        if (royaltyPayout > 0) {
            Address.sendValue(royaltyRegistry.getRoyaltyPayoutAddress(l1ContractAddress), royaltyPayout);
        }

        Address.sendValue(payable(owner()), marketplacePayout);
        Address.sendValue(seller, sellerPayout);
    }

    /*
    * @dev Sets the royalty as a int out of 1000 that the creator should receive and the address to pay.
    */
    function setRoyalty(address l1ContractAddress, address payable _payoutAddress, uint256 _payoutPerMille) external {
        require(_payoutPerMille <= MAX_ROYALTY_PER_MILLE, "Royalty must be between 0 and 15%");
        require(
            ERC165Checker.supportsInterface(l1ContractAddress, 0x1d1d8b63),
            "L1 contract does not support the EIP-165 ERC721 interface"
        );
        require(
            msg.sender == Ownable(l1ContractAddress).owner(),
            "Royalty must be set by owner of ERC721 contract."
        );

        royaltyRegistry.setRoyalty(l1ContractAddress, _payoutAddress, _payoutPerMille);
    }

    /*
    * @dev Gets the royalty payout address.
    */
    function getRoyaltyPayoutAddress(address l1ContractAddress) public view returns (address) {
        return royaltyRegistry.getRoyaltyPayoutAddress(l1ContractAddress);
    }

    /*
    * @dev Gets the royalty as a int out of 1000 that the creator should receive.
    */
    function getRoyaltyPayoutRate(address l1ContractAddress) public view returns (uint256) {
        return royaltyRegistry.getRoyaltyPayoutRate(l1ContractAddress);
    }

    /*
    * @dev Pauses trading on the exchange. To be used for emergencies.
    */
    function pause() external onlyOwner {
        _pause();
    }

    /*
    * @dev Resumes trading on the exchange. To be used for emergencies.
    */
    function unpause() external onlyOwner {
        _unpause();
    }

    /*
    * Withdraw just in case Ether is accidentally sent to this contract.
    */
    function withdraw() public onlyOwner {
        uint balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

    receive() external payable {}
}

