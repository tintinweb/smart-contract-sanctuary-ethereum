pragma solidity ^0.8.0;

import {GenShardsOpenStoreERC721} from "./asset/GenShardsOpenStoreERC721.sol";
import {GenShardsOpenStoreERC1155} from "./asset/GenShardsOpenStoreERC1155.sol";

import "@openzeppelin/contracts-upgradeable/interfaces/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC1155ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

import "./asset/IAssetCommon.sol";
import "./asset/IAssetERC721.sol";
import "./asset/IAssetERC1155.sol";

//later implement initializable properly and proxy stuff
contract GenNFTStore is
    IAssetCommon,
    IERC721ReceiverUpgradeable,
    IERC1155ReceiverUpgradeable,
    OwnableUpgradeable
{
    event nftCollectionCreated(uint256 indexed _nftId, address indexed _nft);

    mapping(uint => address) public nftContracts;
    uint public nftId;
    /**
     * @dev Constructor for upgradable contract.
     */
    function initialize() external initializer onlyOwner {
        __Ownable_init();
    }

    //Anyone can create NFT collection - This is the creation of NFT address
    function createCollection(AssetType assetType, string memory _name, string memory _symbol, string memory _uri) external returns (address _nft) {
        
        if (assetType == AssetType.ERC721) {
            GenShardsOpenStoreERC721 gso721 = new GenShardsOpenStoreERC721();
            gso721.initialize(_name, _symbol, msg.sender);
            _nft = address(gso721);
        } else if (assetType == AssetType.ERC1155) {
            GenShardsOpenStoreERC1155 gso1155 = new GenShardsOpenStoreERC1155();
            gso1155.initialize(_name, _uri, msg.sender);
            _nft = address(gso1155);
        } else{
            revert("Asset type undefined");
        }

        uint _nftId = nftId;
        emit nftCollectionCreated(_nftId, _nft);

        nftContracts[_nftId++] = _nft;
        nftId = _nftId;

    }


    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) external override returns (bytes4) {}

    function supportsInterface(bytes4 interfaceId)
        external
        view
        override
        returns (bool)
    {}

    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes calldata data
    ) external override returns (bytes4) {}

    function onERC1155BatchReceived(
        address operator,
        address from,
        uint256[] calldata ids,
        uint256[] calldata values,
        bytes calldata data
    ) external override returns (bytes4) {}
}

