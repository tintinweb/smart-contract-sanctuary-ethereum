// File: test.sol


pragma solidity ^0.8.4;

// import "@openzeppelin/contracts/token/ERC721/ERC721.sol";









contract ThroneVault is EIP712, Pausable, Ownable, ReentrancyGuard {

    event NewRegister(uint256 registerId, string indexed referral, address indexed user, address indexed inviter, address pool, address topInviter, uint256 totalPay);
    event PoolClaimed(uint256 claimId, string indexed claimCode, address indexed winner, uint256 amount);

    using Address for address;
    using SafeMath for uint256;
    using Strings for uint256;
    using Counters for Counters.Counter;


    uint256 public inviteCost = 30 ether;
    uint256 public poolCost = 45 ether;
    uint256 public topInviterReward = 15 ether;
    uint256 public maxReferralInvites = 1;

    Counters.Counter private _registerIds;
    Counters.Counter private _poolClaimed;
    Counters.Counter private _topInviterClaimed;
    mapping(string => uint8) private _referralCountMap;
    mapping(string => bool) private _claimCodeMap;
    mapping(address => string) private _userToReferralMap;

    
    string private constant SIGNING_DOMAIN = "THRONE";
    string private constant SIGNATURE_VERSION = "1";

    struct RegisterRecord {
        uint registerId;
        string referral;
        address user;
        uint256 inviterPay;
        uint256 poolPay;
        uint256 topInviterPay;
        // uint256 totalPay;
        address inviter;
        address pool;
        address topInviter;
    }

    struct PoolClaimRecord {
        uint claimId;
        string claimCode;
        address winner;
        uint256 amount;
    }
    mapping(uint256 => RegisterRecord) private idToRegisterRecord;
    mapping(uint256 => PoolClaimRecord) private idToPoolClaimRecord;


    constructor(uint256 _inviteCost, uint256 _poolCost, uint256 _topInviterReward) EIP712(SIGNING_DOMAIN, SIGNATURE_VERSION)  {
        inviteCost = _inviteCost;
        poolCost = _poolCost;
        topInviterReward = _topInviterReward;
    }

    function register(string memory _referral, address _inviter, address _pool, address _topInviter, bytes memory _signature) public payable whenNotPaused nonReentrant {
        //requirements
        uint _registerCost = registerCost();
        require(_inviter != _msgSender(), "you cannot invite yourself");
        require(_referralCountMap[_referral] < maxReferralInvites, "referral reached the maximum number of invites");
        require(msg.value >= _registerCost, "please topup to register");
        require(checkRegister(_referral, _msgSender(), _inviter, _pool, _topInviter, _signature) == owner(), "invalid voucher");
        require(bytes(_userToReferralMap[_msgSender()]).length == 0, "you have already registered");
        require(_inviter != address(this), "contract cannot invite");

        _referralCountMap[_referral] += 1;
        _registerIds.increment();
        uint256 registerId = _registerIds.current();

        if(_pool == address(0) || _pool == address(this)) {
            _pool = address(this);
        }

        if(_topInviter == address(0) || _topInviter == address(this)) {
            _topInviter = address(this);
        }

        _userToReferralMap[_msgSender()] = _referral;        
        idToRegisterRecord[registerId] = RegisterRecord(registerId, _referral, _msgSender(), inviteCost, poolCost, topInviterReward, _inviter, _pool, _topInviter);

        //pay
        Address.sendValue(payable(_inviter), inviteCost);
        Address.sendValue(payable(_pool), poolCost);
        Address.sendValue(payable(_topInviter), topInviterReward);

        if (msg.value > _registerCost) {
            Address.sendValue(payable(msg.sender), msg.value - _registerCost);
        }
        //events
        emit NewRegister(registerId, _referral, _msgSender(), _inviter, _pool, _topInviter, _registerCost);
    }

    function referralCount(string memory _referral) public view returns (uint8) {
        return _referralCountMap[_referral];
    }
    function registerRecordById(uint256 _registerId) public view returns (RegisterRecord memory) {
        return idToRegisterRecord[_registerId];
    }

    function referralInviteByUser(address _user) public view returns (string memory) {
        return _userToReferralMap[_user];
    }

    function totalRegisters() public view returns (uint256) {
        return _registerIds.current();
    }

    function registerCost() public view returns (uint256) {
        return inviteCost + poolCost + topInviterReward;
    }

    
    function checkRegister(string memory _referral, address _user, address _inviter, address _pool, address _topInviter, bytes memory _signature) public view returns (address) {
        return _verifyRegister(_referral, _user, _inviter, _pool, _topInviter, _signature);
    }

    function _verifyRegister(string memory _referral, address _user, address _inviter, address _pool, address _topInviter, bytes memory _signature) internal view returns (address) {
        bytes32 digest = _hashRegister(_referral, _user, _inviter, _pool, _topInviter);
        return ECDSA.recover(digest, _signature);
    }

    function _hashRegister(string memory _referral, address _user, address _inviter, address _pool, address _topInviter) internal view returns (bytes32) {
        return _hashTypedDataV4(keccak256(
            abi.encode(keccak256("Web3Struct(string referral,address user,address inviter,address pool,address topInviter)"), keccak256(bytes(_referral)), _user, _inviter, _pool, _topInviter)
        ));
    }

    function poolClaim(string memory claimCode, address winner, uint256 amount) public whenNotPaused nonReentrant onlyOwner {
        require(amount > 0, "amount must be greater than 0");
        require(address(this).balance >= amount, "pool has no balance");
        require(!_claimCodeMap[claimCode], "claim code already used");
        // require(!_poolClaimed.contains(_pool), "pool has already been claimed");

        _poolClaimed.increment();
        uint poolClaimId = _poolClaimed.current();
        _claimCodeMap[claimCode] = true;

        idToPoolClaimRecord[poolClaimId] = PoolClaimRecord(poolClaimId, claimCode, winner, amount);

        Address.sendValue(payable(winner), amount);

        emit PoolClaimed(poolClaimId, claimCode, winner, amount);
    }

    function poolClaimRecordById(uint256 _poolClaimId) public view returns (PoolClaimRecord memory) {
        return idToPoolClaimRecord[_poolClaimId];
    }

    function totalPoolClaims() public view returns (uint256) {
        return _poolClaimed.current();
    }

    function withdraw() public onlyOwner {
        require(address(this).balance > 0, "no funds to withdraw");
        (bool success, ) = payable(owner()).call{value: address(this).balance}("");
        require(success, "withdraw failed");
    }

    fallback() external payable {}
    receive() external payable {}

}