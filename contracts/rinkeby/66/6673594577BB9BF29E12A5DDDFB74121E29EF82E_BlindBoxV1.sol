pragma solidity ^0.8.8;

import {BatchMintV1} from "../../batchMint/v1/BatchMintV1.sol";
import {ERC721TransferWithAuthorization} from "../../../extension/ERC721TransferWithAuthorization.sol";
import {IBlindBoxV1} from "./IBlindBoxV1.sol";
import {ERC2771ContextUpgradeable, ContextUpgradeable} from "../../../utils/ERC2771ContextUpgradeable.sol";

error BurnFromWrongOwner(address owner, address from, uint256 tokenId);

contract BlindBoxV1 is 
    BatchMintV1,
    ERC721TransferWithAuthorization,
    IBlindBoxV1
{

    constructor(address trustedForwarder) BatchMintV1(trustedForwarder) {
    }

    function initialize(
        string calldata name_,
        string calldata symbol_,
        string calldata baseURI_,
        address admin_,
        address owner_
    ) public virtual override initializer {
        __BlindBoxV1_init(name_, symbol_, baseURI_, admin_, owner_);
    }

    function __BlindBoxV1_init(
        string calldata name_,
        string calldata symbol_,
        string calldata baseURI_,
        address admin_,
        address owner_
    ) internal onlyInitializing {
        __BatchMintV1_init(name_, symbol_, baseURI_, admin_, owner_);
        // do not change these variable
        __ERC721TransferWithAuthorization__init("BatchMintBlindBox", "1");
    }

    function transferWithAuthorization(
        address from,
        address to,
        uint256 value,
        uint256 validAfter,
        uint256 validBefore,
        bytes32 nonce,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public virtual {
        _transferWithAuthorization(
            from,
            to,
            value,
            validAfter,
            validBefore,
            nonce,
            v,
            r,
            s
        );
    }

    function _transferAfterAuthorization(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        _uncheckedTransfer(from, to, tokenId);
    }

    function _burnAfterAuthorization(
        address from,
        uint256 tokenId
    ) internal virtual override {
        address owner = ownerOf(tokenId);
        if(owner != from) revert BurnFromWrongOwner(owner, from, tokenId);
        _burn(tokenId);
    }

    function _msgSender() internal view virtual override(ContextUpgradeable, BatchMintV1) returns (address sender) {
        return ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData() internal view virtual override(ContextUpgradeable, BatchMintV1) returns (bytes calldata) {
        return ERC2771ContextUpgradeable._msgData();
    }

    uint256[50] private __gap;

}

