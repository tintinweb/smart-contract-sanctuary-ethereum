error TokenNotExist(uint256 tokenId);

abstract contract ERC721ABase is
    UUPSUpgradeable,
    AccessControlBase,
    ERC721AUpgradeable,
    IERC721ABase,
    ERC2771ContextUpgradeable,
    ERC721ABurnableUpgradeable
{
    using URIUtils for string;
    using StringsUpgradeable for uint256;

    string internal _baseTokenURI;
    string internal _contractURI;
    address internal _owner;
    bool    internal _newTokenURIFlag;

    constructor(address trustedForwarder) ERC2771ContextUpgradeable(trustedForwarder) {
        emit ContractDeployed(address(this));
    }

    function __ERC721ABase_init(
        string calldata name_,
        string calldata symbol_,
        string calldata baseURI_,
        address admin_,
        address owner_
    ) internal virtual onlyInitializing  {
        __AccessControlBase_init(admin_);
        __ERC721A_init(name_, symbol_);
        __ERC721ABase__init_unchained(baseURI_, owner_);
    }

    function __ERC721ABase__init_unchained(string calldata baseTokenURI_, address owner_)
        internal
        onlyInitializing
    {
        _baseTokenURI = baseTokenURI_.concatURI();
        _contractURI = baseTokenURI_.concatURI("contract");
        _newTokenURIFlag = true;
        if (owner_ == address(0)) {
            _owner = _msgSender();
        } else {
            _owner = owner_;
        }
    }

    function setBaseURI(string calldata baseURI_) external virtual onlyAdmin(){
        _newTokenURIFlag = true;
        _baseTokenURI = baseURI_;
    }

    function setContractURI(string calldata contractURI_) external virtual onlyAdmin() {
        _contractURI = contractURI_;
    }

    function contractURI() external view virtual override returns (string memory) {
        return _contractURI;
    }

    function owner() external view virtual override returns (address) {
        if (_owner == address(0) ){
            return getRoleMember(DEFAULT_ADMIN_ROLE, 0);
        }
        return _owner;    
    }
    
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override (
            ERC721AUpgradeable,
            IERC721MetadataUpgradeable
        )
        returns (string memory)
    {
        if(!_exists(tokenId)) revert TokenNotExist(tokenId);
        if (_newTokenURIFlag == true) {
            return bytes(_baseURI()).length > 0
                ? string(abi.encodePacked(_baseURI(), tokenId.toString()))
                : "";
        }
        return _baseURI().concatURI(tokenId.toString());
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function _authorizeUpgrade(address) internal view virtual override onlyAdmin() {
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(
            ERC721AUpgradeable,
            AccessControlEnumerableUpgradeable,
            IERC165Upgradeable
        )
        returns (bool)
    {
        return
            interfaceId == type(IERC721ABase).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function _msgSender() internal view virtual override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (address sender) {
        return ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData() internal view virtual override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (bytes calldata) {
        return ERC2771ContextUpgradeable._msgData();
    }


    // we leave some space for inheritance flexibility
    uint256[50] private __gap;

}

