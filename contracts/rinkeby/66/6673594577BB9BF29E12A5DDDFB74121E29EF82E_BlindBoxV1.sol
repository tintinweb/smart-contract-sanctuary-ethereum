import {StringsUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import {IERC721Base} from "./IERC721Base.sol";
import {ContextUpgradeable, ERC2771ContextUpgradeable} from "../utils/ERC2771ContextUpgradeable.sol";
import {ERC721BurnableUpgradeable} from "../extension/ERC721Burnable.sol";

error TokenNotExist(uint256 tokenId);

abstract contract ERC721Base is
    UUPSUpgradeable,
