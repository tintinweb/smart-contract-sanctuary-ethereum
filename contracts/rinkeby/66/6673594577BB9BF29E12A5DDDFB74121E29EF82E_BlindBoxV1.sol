import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

library URIUtils {
    using StringsUpgradeable for uint256;

    function concatURI(string memory baseUri, string memory id)
        internal
        view
        returns (string memory)
    {
        return
            bytes(baseUri).length > 0
                ? string(
                    abi.encodePacked(
                        concatURI(baseUri),
                        id
                    )
                )
                : "";
    }

    function concatURI(string memory baseUri)
        internal
        view
        returns (string memory) 
    {
        string memory contractAddress = uint256(uint160(address(this))).toHexString();
        return 
            bytes(baseUri).length > 0
                ? string(
                    abi.encodePacked(
                        baseUri,
                        "/",
                        block.chainid.toString(),
                         "/",
                        contractAddress,
                        "/"
                    )
                )
                : "";
    }
}

