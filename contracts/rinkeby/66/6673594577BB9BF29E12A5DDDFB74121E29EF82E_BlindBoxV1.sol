pragma solidity ^0.8.4;

library BuyOrder {
    address internal constant _ETH_CURRENCY = 0x0000000000000000000000000000000000455448;

    struct Order {
        address contractAddress;
        address to;
        address currency;
        address payable [] recipients;
        uint256 amount;
        uint256 expiredAt;
        uint256 price;
        uint256 orderID;
        uint256[] values;
    }

    struct OrderWithTokenType {
        Order order;
        uint256 tokenType;
    }


    function _hashOrder(
        Order calldata order
    ) internal pure returns (bytes32) {
        bytes32 hash = keccak256(
            abi.encodePacked(
                "\x19Ethereum Signed Message:\n32",
                keccak256(
                    abi.encodePacked(
                        order.contractAddress,
                        order.to,
                        order.currency,
                        order.recipients,
                        order.amount,
                        order.expiredAt,
                        order.price,
                        order.orderID,
                        order.values
                    )
                )
            )
        );

        return hash;
    }

    function _hashOrderWithTokenType(
        OrderWithTokenType calldata order
    ) internal pure returns (bytes32) {
        bytes32 hash = keccak256(
            abi.encodePacked(
                "\x19Ethereum Signed Message:\n32",
                keccak256(
                    abi.encodePacked(
                        order.order.contractAddress,
                        order.order.to,
                        order.order.currency,
                        order.order.recipients,
                        order.order.amount,
                        order.order.expiredAt,
                        order.order.price,
                        order.order.orderID,
                        order.order.values,
                        order.tokenType
                    )
                )
            )
        );

        return hash;
    }

    
}

