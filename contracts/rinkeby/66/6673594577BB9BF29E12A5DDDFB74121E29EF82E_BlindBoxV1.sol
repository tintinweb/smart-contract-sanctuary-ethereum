pragma solidity ^0.8.4;

import {ERC721Base} from "../base/ERC721Base.sol";
import {ReentrancyGuardUpgradeable} from "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import {AccessControlBase} from "../base/AccessControlBase.sol";
import {NotAdminRole} from "../base/AccessControlBase.sol";
import {Signature} from "../lib/Signature.sol";
import {BuyOrder} from "../lib/BuyOrder.sol";

error WrongContractAddress(address contractAddress);
error WrongCurrency(address currency);
error TokenMinted(uint256 tokenId);
error MessageExpired(uint256 expiredAt);
error PriceNotEqualSendAmount(uint256 price);
error ProfitSharingLengthNotEqual(uint256 recipientsLength, uint256 valuesLength);
error PriceNotEqualValues(uint256 price, uint256 values);
error EmptyRecipientAddress();
error OrderIDMarked(uint256 orderID);

abstract contract ERC721CryptoBuy is
    AccessControlBase,
    ReentrancyGuardUpgradeable
{

    mapping(uint256 => bool) internal _markedOrderID;

    function _buy(
        BuyOrder.Order calldata order,
        Signature.Sig calldata sig
    ) internal virtual nonReentrant() {
        address signer = _recover(
            order,
            sig
        );
        _verifyOrder(order, sig, signer);
        _afterBuy(order, sig);
    }

    function _buy(
        BuyOrder.OrderWithTokenType calldata order,
        Signature.Sig calldata sig
    ) internal virtual nonReentrant() {
        address signer = _recover(
            order,
            sig
        );
        _verifyOrder(order.order, sig, signer);
        _afterBuy(order, sig);
    }

    function _verifyOrder(
        BuyOrder.Order calldata order,
        Signature.Sig calldata sig,
        address signer
    ) internal virtual {
        uint256 recLen = order.recipients.length;
        uint256 valLen = order.values.length;
        uint256 totalValues = 0;
        if (!hasRole(DEFAULT_ADMIN_ROLE, signer)) revert NotAdminRole(signer);
        if (order.contractAddress != address(this)) revert WrongContractAddress(address(this));
        if (order.currency != BuyOrder._ETH_CURRENCY) revert WrongCurrency(order.currency);
        if (order.expiredAt < block.timestamp) revert MessageExpired(order.expiredAt);
        if (order.price != msg.value) revert PriceNotEqualSendAmount(order.price);
        if (recLen != valLen) revert ProfitSharingLengthNotEqual(recLen, valLen);
        for (uint256 i = 0; i < recLen; i++) {
            totalValues += order.values[i];
        } 
        if (order.price != totalValues) revert PriceNotEqualValues(order.price, totalValues);
        for (uint256 i = 0; i < recLen; i++) {
            if (order.recipients[i] == address(0)){
                revert EmptyRecipientAddress();
            }
            order.recipients[i].transfer(order.values[i]);
        }
        _markOrderIDAsUsed(order.orderID);
    } 

    function _markOrderIDAsUsed(uint256 orderID) internal virtual
    {
        if (_markedOrderID[orderID] == true) revert OrderIDMarked(orderID);
        _markedOrderID[orderID] = true;
    }

    function _afterBuy(
        BuyOrder.Order calldata order,
        Signature.Sig calldata sig
    ) internal virtual {}

    function _afterBuy(
        BuyOrder.OrderWithTokenType calldata order,
        Signature.Sig calldata sig
    ) internal virtual {}

    function _recover(
        BuyOrder.Order calldata order,
        Signature.Sig calldata sig
    ) internal pure virtual returns (address) {
        bytes32 digest = BuyOrder._hashOrder(
            order
        );
        return Signature._safeRecoverAddress(digest, sig);
    }

    function _recover(
        BuyOrder.OrderWithTokenType calldata order,
        Signature.Sig calldata sig
    ) internal pure virtual returns (address) {
        bytes32 digest = BuyOrder._hashOrderWithTokenType(order);
        return Signature._safeRecoverAddress(digest, sig);
    }

    uint256[50] private __gap;
}

