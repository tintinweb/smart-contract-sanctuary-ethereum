pragma solidity ^0.8.4;

import {ERC721ABase, TokenNotExist} from "../base/ERC721ABase.sol";
import {URIUtils} from "../utils/URIUtils.sol";
import {StringsUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

abstract contract ERC721ATokenType is
    ERC721ABase
{
    using URIUtils for string;
    using StringsUpgradeable for uint256;

    struct TokenType {
        uint64 tokenType;
        bool isZero;
    }

    mapping(uint256 => TokenType) internal _tokenTypes;
    bool    internal _useTokenIdFlag;

    function setUseTokenIdFlag(bool flag) external virtual onlyAdmin(){
        _useTokenIdFlag = flag;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {        
        if(!_exists(tokenId)) revert TokenNotExist(tokenId);
        uint256 uri = _useTokenIdFlag
            ? tokenId
            : _tokenTypeOf(tokenId);
        if (_newTokenURIFlag) {
            return bytes(_baseURI()).length > 0
                ? string(abi.encodePacked(_baseURI(), uri.toString()))
                : "";
        }
        return _baseURI().concatURI(uri.toString());
    }

    function _safeMint(
        address to,
        uint256 tokenType,
        uint256 numberOfTokens,
        uint256 orderID
    ) internal virtual {
        // Gas optimization
        uint256 currentIndex = _currentIndex;
        _safeMint(to, numberOfTokens);
        emit CompleteOrder(orderID);
        if (tokenType == 0) {
            _tokenTypes[currentIndex].isZero = true;
