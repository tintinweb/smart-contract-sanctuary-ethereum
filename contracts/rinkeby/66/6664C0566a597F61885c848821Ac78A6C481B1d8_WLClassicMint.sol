pragma solidity ^0.8.10;

import "erc721a-upgradeable/contracts/ERC721AUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/MerkleProofUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "contracts/utils/oz-presets/ERC2771ContextUpgradeable.sol";

import "../extensions/TokenMetadata.sol";

abstract contract BaseWhitelist is
    ERC721AUpgradeable,
    ERC2771ContextUpgradeable,
    PausableUpgradeable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable,
    TokenMetadata
{
    // total quantity of NFTs available  for whitelisted wallets to mint
    uint256 public WL_Quantity;

    // tracks the number of NFTs minted by the public
    uint256 public WL_TotalSupply;

    // whitelist minting price
    uint256 public WL_MintPrice;

    // maximum mints per whitelisted wallet
    uint256 public WL_MaxMintPerWallet;

    // maximum mints per transaction
    uint256 public WL_MaxMintPerTx;

    // time minting begins for whitelisted addresses
    uint256 public WL_StartingTimestamp;

    // time minting ends for whitelisted addresses
    uint256 public WL_EndingTimestamp;

    // reserve tokens for promotional or reward purposes
    uint256 public reservedTokens;

    // checks if reserved tokens have been minted
    bool public isReservedTokensMinted;

    // merklee root
    bytes32 public merkleRoot;

    // tracks number of times a whitelisted address has minted
    mapping(address => uint256) public WL_MintCountPerWallet;

    struct WhitelistSaleConfig {
        uint256 quantity;
        uint256 maxMint;
        uint256 maxMintTx;
        uint256 price;
        uint256 startTime;
        uint256 endTime;
    }

    function __BaseWhitelist_init(
        uint256 _reservedTokens,
        WhitelistSaleConfig memory whitelistSale
    ) internal onlyInitializing {
        __BaseWhitelist_init_unchained(_reservedTokens, whitelistSale);
    }

    function __BaseWhitelist_init_unchained(
        uint256 _reservedTokens,
        WhitelistSaleConfig memory whitelistSale
    ) internal onlyInitializing {
        reservedTokens = _reservedTokens;
        WL_Quantity = whitelistSale.quantity;
        WL_MaxMintPerWallet = whitelistSale.maxMint;
        WL_MaxMintPerTx = whitelistSale.maxMintTx;
        WL_MintPrice = whitelistSale.price;
        WL_StartingTimestamp = whitelistSale.startTime;
        WL_EndingTimestamp = whitelistSale.endTime;
    }

    /***********************************************************************
                                    MODIFIERS
     *************************************************************************/

    modifier callerIsUser() {
        require(tx.origin == _msgSender(), "The caller is another contract");
        _;
    }

    /***********************************************************************
                                PUBLIC FUNCTIONS
     *************************************************************************/

    function hasWLMintStarted() public view returns (bool) {
        return
            block.timestamp >= WL_StartingTimestamp &&
            block.timestamp <= WL_EndingTimestamp;
    }

    function privateMint(bytes32[] calldata _merkleProof, uint8 quantity)
        external
        payable
        whenNotPaused
        callerIsUser
        nonReentrant
    {
        require(
            block.timestamp >= WL_StartingTimestamp,
            "Premint has not started!"
        );
        require(block.timestamp <= WL_EndingTimestamp, "Premint has ended!");

        bytes32 sender = keccak256(abi.encodePacked(_msgSender()));
        require(
            MerkleProofUpgradeable.verify(_merkleProof, merkleRoot, sender),
            "Address has not been whitelisted"
        );

        require(WL_TotalSupply < WL_Quantity, "WL assets have sold out!");
        require(quantity > 0, "Quantity can not be 0");
        require(WL_TotalSupply + quantity <= WL_Quantity, "Exceeds WL assets");

        if (WL_MaxMintPerTx > 0) {
            require(
                quantity <= WL_MaxMintPerTx,
                "Exceeds mints allowed per transaction"
            );
        }

        if (WL_MaxMintPerWallet > 0) {
            require(
                WL_MintCountPerWallet[_msgSender()] + quantity <=
                    WL_MaxMintPerWallet,
                "Mints allowed exceeded"
            );
        }

        require(
            msg.value == WL_MintPrice * quantity,
            "Ether value sent is incorrect"
        );

        WL_MintCountPerWallet[_msgSender()] += quantity;
        WL_TotalSupply += quantity;
        _safeMint(_msgSender(), quantity);
    }

    /***********************************************************************
                                ONLY OWNER FUNCTIONS
     *************************************************************************/

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function mintReservedTokens() public onlyOwner nonReentrant {
        require(reservedTokens > 0, "No tokens were reserved");
        require(!isReservedTokensMinted, "Tokens have already been minted!");
        isReservedTokensMinted = true;

        while (reservedTokens > 10) {
            reservedTokens -= 10;
            _safeMint(owner(), 10);
        }
        _safeMint(_msgSender(), reservedTokens);
    }

    /**===========================================================================
                                    Setter Functions
    ============================================================================== */
    function _setOwnership(address owner) internal onlyInitializing {
        _transferOwnership(owner);
    }

    function setName(string memory name) external onlyOwner {
        ERC721AStorage.layout()._name = name;
    }

    function setSymbol(string memory symbol) external onlyOwner {
        ERC721AStorage.layout()._symbol = symbol;
    }

    function setMerkleRoot(bytes32 _merkleRoot) external onlyOwner {
        merkleRoot = _merkleRoot;
    }

    function setReservedTokens(uint256 tokens) external onlyOwner {
        reservedTokens = tokens;
    }

    function setWLQuantity(uint256 quantity) external onlyOwner {
        WL_Quantity = quantity;
    }

    function setWLMaxMint(uint256 maxMint) external onlyOwner {
        WL_MaxMintPerWallet = maxMint;
    }

    function setWLMaxMintTx(uint256 maxMintTx) external onlyOwner {
        WL_MaxMintPerTx = maxMintTx;
    }

    function setWLMintPrice(uint256 price) external onlyOwner {
        WL_MintPrice = price;
    }

    function setWLStartTime(uint256 startTime) external onlyOwner {
        WL_StartingTimestamp = startTime;
    }

    function setWLEndTime(uint256 endTime) external onlyOwner {
        WL_EndingTimestamp = endTime;
    }

    /***********************************************************************
                                OVERRIDE FUNCTIONS
     *************************************************************************/

    function _startTokenId() internal pure override returns (uint256) {
        return 1;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        if (!_exists(tokenId)) revert URIQueryForNonexistentToken();

        if (delayReveal && isRevealed == false) {
            return previewURI;
        }
        return _actualTokenURI(tokenId);
    }

    function _msgSender()
        internal
        view
        virtual
        override(ContextUpgradeable, ERC2771ContextUpgradeable)
        returns (address sender)
    {
        return ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData()
        internal
        view
        virtual
        override(ContextUpgradeable, ERC2771ContextUpgradeable)
        returns (bytes calldata)
    {
        return ERC2771ContextUpgradeable._msgData();
    }

    /***********************************************************************
                                INTERNAL FUNCTIONS
     *************************************************************************/

    function _actualTokenURI(uint256 tokenId)
        internal
        view
        returns (string memory)
    {
        string memory currentBaseURI = _baseURI();
        return
            bytes(currentBaseURI).length != 0
                ? string(
                    abi.encodePacked(
                        currentBaseURI,
                        "/",
                        _toString(tokenId),
                        baseExtension
                    )
                )
                : "";
    }
}

