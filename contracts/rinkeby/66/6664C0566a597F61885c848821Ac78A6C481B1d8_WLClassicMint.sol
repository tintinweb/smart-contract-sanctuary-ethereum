pragma solidity ^0.8.10;

import "contracts/extensions/BaseWhitelist.sol";
import "contracts/interfaces/IMiNFTContract.sol";

contract WLClassicMint is BaseWhitelist {
    // template type and version
    bytes32 private constant TEMPLATE_TYPE = bytes32("WLClassicMint");
    uint256 private constant TEMPLATE_VERSION = 1;

    // total quantity of NFTs available for the public to mint
    uint256 public CM_Quantity;

    // tracks the number of NFTs minted by the public
    uint256 public CM_TotalSupply;

    // maximum mint per public address
    uint256 public CM_MaxMintPerWallet;

    // maximum mint per each transaction
    uint256 public CM_MaxMintPerTx;

    // public mint price
    uint256 public CM_MintPrice;

    // time minting starts for the public
    uint256 public CM_StartingTimestamp;

    // time minting ends for the public
    uint256 public CM_EndingTimestamp;

    // tracks the number of times a public address has minted
    mapping(address => uint256) public CM_MintCountPerWallet;

    struct PublicSaleConfig {
        uint256 quantity;
        uint256 maxMint;
        uint256 maxMintTx;
        uint256 price;
        uint256 startTime;
        uint256 endTime;
    }

    function initialize(
        address owner,
        address trustedForwarder,
        string memory name,
        string memory symbol,
        uint256 _reservedTokens,
        WhitelistSaleConfig memory whitelistSale,
        PublicSaleConfig memory publicSale
    ) external initializerERC721A initializer {
        __ReentrancyGuard_init();
        __ERC2771Context_init(trustedForwarder);
        __ERC721A_init(name, symbol);
        __BaseWhitelist_init(_reservedTokens, whitelistSale);
        __Pausable_init();
        _setOwnership(owner);

        CM_Quantity = publicSale.quantity;
        CM_MaxMintPerWallet = publicSale.maxMint;
        CM_MaxMintPerTx = publicSale.maxMintTx;
        CM_MintPrice = publicSale.price;
        CM_StartingTimestamp = publicSale.startTime;
        CM_EndingTimestamp = publicSale.endTime;
    }

    /***********************************************************************
                                    CONTRACT METADATA
     *************************************************************************/

    // Returns the module type of the template.
    function contractType() external pure returns (bytes32) {
        return TEMPLATE_TYPE;
    }

    // Returns the version of the template.
    function contractVersion() external pure returns (uint8) {
        return uint8(TEMPLATE_VERSION);
    }

    /***********************************************************************
                                    MODIFIERS
     *************************************************************************/

    modifier whenMintHasEnded() {
        require(
            block.timestamp >= WL_EndingTimestamp,
            "WL Minting has not ended"
        );
        require(block.timestamp >= CM_EndingTimestamp, "Minting has not ended");
        _;
    }

    /***********************************************************************
                                 PUBLIC FUNCTIONS
     *************************************************************************/

    // total number of NFTs in the collection
    function totalQuantity() public view returns (uint256) {
        return WL_Quantity + CM_Quantity + reservedTokens;
    }

    function hasCMMintStarted() public view returns (bool) {
        return
            block.timestamp >= CM_StartingTimestamp &&
            block.timestamp <= CM_EndingTimestamp;
    }

    function publicMint(uint8 quantity)
        external
        payable
        whenNotPaused
        callerIsUser
        nonReentrant
    {
        require(
            block.timestamp >= CM_StartingTimestamp,
            "Minting has not started!"
        );
        require(block.timestamp <= CM_EndingTimestamp, "Minting has ended!");
        require(quantity > 0, "Quantity can not be 0");
        require(CM_TotalSupply < CM_Quantity, "Assets have sold out!");
        require(
            CM_TotalSupply + quantity <= CM_Quantity,
            "Exceeds total assets!"
        );

        if (CM_MaxMintPerTx > 0) {
            require(
                quantity <= CM_MaxMintPerTx,
                "Exceeds mints allowed per transaction"
            );
        }

        require(
            CM_MintCountPerWallet[_msgSender()] + quantity <=
                CM_MaxMintPerWallet,
            "Mints allowed exceeded!"
        );
        require(
            msg.value == CM_MintPrice * quantity,
            "Ether value sent is incorrect"
        );

        CM_MintCountPerWallet[_msgSender()] += quantity;
        CM_TotalSupply += quantity;
        _safeMint(_msgSender(), quantity);
    }

    /***********************************************************************
                                ONLY OWNER FUNCTIONS
     *************************************************************************/

    function withdrawUnmintedTokens()
        external
        onlyOwner
        whenMintHasEnded
        nonReentrant
    {
        uint256 unmintedTokens = totalQuantity() - totalSupply();
        require(unmintedTokens > 0, "No NFTs were left");

        while (unmintedTokens > 10) {
            unmintedTokens -= 10;
            _safeMint(owner(), 10);
        }

        _safeMint(owner(), unmintedTokens);
    }

    function withdrawFunds() external onlyOwner whenMintHasEnded nonReentrant {
        (bool success, ) = payable(owner()).call{value: address(this).balance}(
            ""
        );
        require(success, "Withdrawal failed");
    }

    /**===========================================================================
                                    Setter Functions
    ============================================================================== */

    function setCMQuantity(uint256 quantity) public onlyOwner {
        CM_Quantity = quantity;
    }

    function setCMMaxMint(uint256 maxMint) public onlyOwner {
        CM_MaxMintPerWallet = maxMint;
    }

    function setCMMaxMintTx(uint256 maxMintTx) public onlyOwner {
        CM_MaxMintPerTx = maxMintTx;
    }

    function setCMMintPrice(uint256 price) public onlyOwner {
        CM_MintPrice = price;
    }

    function setCMStartTime(uint256 startTime) public onlyOwner {
        CM_StartingTimestamp = startTime;
    }

    function setCMEndTime(uint256 endTime) public onlyOwner {
        CM_EndingTimestamp = endTime;
    }
}

