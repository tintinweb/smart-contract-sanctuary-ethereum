pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./BiletsClub.sol";
import "./BiletsClubCharStatus.sol";
import "./BiletsClubToken.sol";

/**
 * @title BiletsClubPotions Contract
 */

contract BiletsClubPotions is ERC1155, Ownable, ReentrancyGuard {
    string public name = "Bilets Club Potions";
    string public symbol = "BLT-Potions";
    bool public pause = false;

    uint256 public constant HP00 = 0;
    uint256 public constant HP01 = 1;
    uint256 public constant HP02 = 2;
    uint256 public constant HP03 = 3;

    uint256 public constant MP00 = 4;
    uint256 public constant MP01 = 5;
    uint256 public constant MP02 = 6;
    uint256 public constant MP03 = 7;

    string public contractURI =
        "https://bilets.club/api/opensea_contracts_meta/contractHeals";

    mapping(address => bool) public admins;

    modifier onlyAdmins() {
        require(admins[msg.sender] == true, "Caller is not a admin.");
        _;
    }
    modifier isPaused() {
        require(pause == false, "The contract is paused.");
        _;
    }

    constructor() ERC1155("https://bilets.club/api/opensea_meta_heals/{id}") {
        admins[msg.sender] = true;
    }

    /******************** ADMINS ********************/
    function mint(
        address account,
        uint256 id,
        uint256 amount
    ) external payable onlyAdmins nonReentrant isPaused {
        _mint(account, id, amount, "0x");
    }

    function burn(
        address account,
        uint256 id,
        uint256 amount
    ) external payable onlyAdmins nonReentrant isPaused {
        _burn(account, id, amount);
    }

    /******************** OWNER ********************/
    function flipAdmin(address _admin) public onlyOwner {
        admins[_admin] = !admins[_admin];
    }

    function flipPause() public onlyOwner {
        pause = !pause;
    }

    function setContractURI(string memory _contractURI) public onlyOwner {
        contractURI = _contractURI;
    }

    function withdraw() external payable onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }
}

