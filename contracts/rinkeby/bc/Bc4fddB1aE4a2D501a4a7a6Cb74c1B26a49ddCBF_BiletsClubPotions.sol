pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

/**
 * @title BiletsClubToken Contract
 * @dev Extends ERC20 Standard basic implementation
 */

contract BiletsClubToken is ERC20, Ownable, ReentrancyGuard {
    bool public pause = false;

    mapping(address => bool) public admins;

    modifier onlyAdmins() {
        require(admins[msg.sender] == true, "Caller is not a admin.");
        _;
    }
    modifier isPaused() {
        require(pause == false, "The contract is paused.");
        _;
    }

    constructor() ERC20("Bilets Club Token", "BLT-Token") {
        admins[msg.sender] = true;
    }

    /******************** SETTINGS ********************/
    function decimals() public view virtual override returns (uint8) {
        return 0;
    }

    /******************** ADMINS ********************/
    function mint(address account, uint256 amount)
        external
        payable
        onlyAdmins
        nonReentrant
        isPaused
    {
        _mint(account, amount);
    }

    function burn(address account, uint256 amount)
        external
        payable
        onlyAdmins
        nonReentrant
        isPaused
    {
        _spendAllowance(account, _msgSender(), amount);
        _burn(account, amount);
    }

    /******************** OWNER ********************/
    function flipAdmin(address _admin) public onlyOwner {
        admins[_admin] = !admins[_admin];
    }

    function flipPause() public onlyOwner {
        pause = !pause;
    }

    function withdraw() external payable onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }
}

