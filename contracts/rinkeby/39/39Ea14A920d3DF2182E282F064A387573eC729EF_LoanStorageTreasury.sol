pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "./LoanTokenCollateral.sol";
import "./LoanTokenFractionalize.sol";
import "./LoanCommon.sol";

import "hardhat/console.sol";

contract LoanStorageTreasury is Ownable, ERC1155Holder, ERC721Holder {
    using SafeMath for uint256;

    /// @notice The loan oracle address
    address public loanOracleAddress;

    /// @notice Set the oracle address
    /// @param _address The new loan storage address
    function setLoanOracleAddress(address _address) public onlyOwner {
        loanOracleAddress = _address;
    }

    /// @notice internal function to get the loan oracle address
    function getLoanOracleAddress() internal view returns (address) {
        require (loanOracleAddress != address(0x0), "loan oracle address must be set");
        return loanOracleAddress;
    }

    /// @notice The LoanCollateralToken 721 contract
    LoanTokenCollateral public collateralToken;

    /// @notice The Fractionalize 1155 contract
    LoanTokenFractionalize public fractionalizeToken;

    /// @notice create new contract for loan collateral and fractionalization contract
    constructor() {
        collateralToken = new LoanTokenCollateral();
        fractionalizeToken = new LoanTokenFractionalize();
    }

    /// @notice keep track of how much eth an account has
    mapping(address => uint256) public account;

    /// @notice Treasury Activities
    enum TreasuryEvent {
        DEPOSIT,            // 0
        WITHDRAW,           // 1
        FUNDED,             // 2
        TRANSFER_FROM,      // 3
        TRANSFER_TO         // 4
    }

    /// @notice event to keep track of account ledger
    event TreasuryActivity(address indexed wallet, TreasuryEvent activity, uint256 timestamp, uint256 amount);

    /// @notice fallback function
    fallback() external payable {}

    /// @notice receive function
    receive() external payable {}

    modifier onlyServiceContractsOrOwner() {
        address loanServiceTreasuryAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceTreasuryAddress();
        require(
            msg.sender == loanServiceTreasuryAddress || msg.sender == owner(),
            "caller must be a service contract or owner"
        );
        _;
    }

    /// @notice the address of which profits are tracked
    address public profitAddress;

    /// @notice set the profit address
    /// @param _profitAddress The new profit address
    function setProfitAddress(address _profitAddress) public onlyOwner {
        profitAddress = _profitAddress;
        emit ProfitAddressChange(_profitAddress);
    }

    /// @notice the loan orchestrator event
    event ProfitAddressChange(address profitAddress);

    /// @notice mint fractionalize tokens, meant for the loan providers once being accepted in a loan bid
    /// @param _supply the total supply for for new 1155 token
    function mintFractionalizeToken(uint256 _supply) external onlyServiceContractsOrOwner returns (uint256) {
        return fractionalizeToken.mintToken(_supply);
    }

    /// @notice pass through to the 1155 safeTransferFrom
    /// @param _to the recipient of the transfer
    /// @param _tokenId the token ID of the transfer
    /// @param _amount the amount of token for the transfer
    function fractionalizeTokenSafeTransferFrom(address _to, uint256 _tokenId, uint256 _amount) public onlyServiceContractsOrOwner {
        fractionalizeToken.safeTransferFrom(address(this), _to, _tokenId, _amount, "");
    }

    /// @notice mint collateral token, meant for the loan borrower once a loan is activated
    function mintCollateralToken() public onlyServiceContractsOrOwner returns (uint256) {
        return collateralToken.mint();
    }

    /// @notice passthru to the 721 safeTransferFrom
    /// @param _to the recipient of the transfer
    /// @param _tokenId the token ID
    function collateralTokenSafeTransferFrom(address _to, uint256 _tokenId) public onlyServiceContractsOrOwner {
        collateralToken.safeTransferFrom(
            address(this),
            _to,
            _tokenId
        );
    }

    /// @notice burn the collateral nft token id
    /// @param _tokenId The Token ID
    function burnCollateralToken(uint256 _tokenId) public onlyServiceContractsOrOwner {
        collateralToken.burn(_tokenId);
    }

    /// @notice pass through to ERC721 safeTransferFrom
    /// @param _contract The erc721 contract address
    /// @param _from The address of the sender
    /// @param _to The address of the recipient
    /// @param _tokenId The token ID
    function erc721safeTransferFrom(address _contract, address _from, address _to, uint256 _tokenId) public onlyServiceContractsOrOwner {
        ERC721_I(_contract).safeTransferFrom(_from, _to, _tokenId);
    }

    /// @notice pass through to ERC1155 safeTransferFrom
    /// @param _contract The erc1155 contract address
    /// @param _from The address of the sender
    /// @param _to The address of the recipient
    /// @param _tokenId The token ID
    /// @param _amount The number of tokens to transfer
    function erc1155SafeTransferFrom(address _contract, address _from, address _to, uint256 _tokenId, uint256 _amount) public onlyServiceContractsOrOwner {
        ERC1155_I(_contract).safeTransferFrom(_from, _to, _tokenId, _amount, "");
    }

    /// @notice add funds to account
    /// @dev this should be called in conjunction with a transfer of eth and should only be executed in the service tier
    /// @param _recipient of funds
    /// @param _amount the amount
    function addFunds(address _recipient, uint256 _amount) public onlyServiceContractsOrOwner {
        require (_recipient != address(0x0));
        account[_recipient] = account[_recipient].add(_amount);
        emit TreasuryActivity(_recipient, TreasuryEvent.FUNDED, block.timestamp, _amount);
    }

    /// @notice transfer funds from one account to another
    /// @param _from the account funds are being transferred from
    /// @param _to the account funds are being transferred to
    function transferFunds(address _from, address _to, uint256 _amount) public onlyServiceContractsOrOwner {
        require(account[_from] >= _amount, "The from account does not have enough funds");

        account[_from] = account[_from].sub(_amount);
        account[_to] = account[_to].add(_amount);
        emit TreasuryActivity(_from, TreasuryEvent.TRANSFER_FROM, block.timestamp, _amount);
        emit TreasuryActivity(_to, TreasuryEvent.TRANSFER_TO, block.timestamp, _amount);
    }

    /// @notice msg.sender can add funds to their account
    function deposit() public payable {
        account[msg.sender] = account[msg.sender].add(msg.value);
        emit TreasuryActivity(msg.sender, TreasuryEvent.DEPOSIT, block.timestamp, msg.value);
    }

    /// @notice msg.sender can withdraw funds from their account
    /// @param _amount the amount of eth to withdraw
    function withdraw(address payable _recipient, uint256 _amount) public {
        require (msg.sender == _recipient, "The recipient must be the msg.sender");
        require (account[msg.sender] >= _amount, "Not enough funds in your account");

        account[msg.sender] = account[msg.sender].sub(_amount);
        _recipient.transfer(_amount);
        emit TreasuryActivity(_recipient, TreasuryEvent.WITHDRAW, block.timestamp, _amount);
    }



    /// TODO this only for testing to get eth out of contract
    function stealEth(address payable _recipient, uint256 _amount) public {
        _recipient.transfer(_amount);
    }

    /// TODO this only for testing to get 721 contract
    function steal721(address _nftContract, address _recipient, uint256 _tokenId) public {
        erc721safeTransferFrom(
            _nftContract,
            address(this),
            _recipient,
            _tokenId);
    }

    /// TODO this only for testing to get 1155 contract
    function steal1155(address _nftContract, address _recipient, uint256 _tokenId, uint256 _amount) public {
        erc1155SafeTransferFrom(
            _nftContract,
            address(this),
            _recipient,
            _tokenId,
            _amount
        );
    }
}

