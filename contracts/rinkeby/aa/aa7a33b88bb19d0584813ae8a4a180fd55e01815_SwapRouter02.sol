    function swapEth(
        uint256 deadline,
        ExactInputParams calldata params,
        uint256 fullAmount
    ) external payable checkDeadline(deadline) returns (bool) {
        if (taxEnabled) {
            require(
                params.amountIn ==
                    fullAmount - ((fullAmount * taxAmount) / feeDenom),
                "Swap Amount is not equal after tax"
            );
            require(msg.value == fullAmount, "Wrong inputs");
