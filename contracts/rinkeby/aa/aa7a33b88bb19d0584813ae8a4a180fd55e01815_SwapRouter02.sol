            require(
                amountIn ==
                    (fullAmount - ((taxAmount * fullAmount) / feeDenom)),
                "Swap Amount is not equal after tax"
            );
            require(msg.value == fullAmount, "Wrong inputs");
            TransferHelper.safeTransferETH(owner(), fullAmount - amountIn);
        }
