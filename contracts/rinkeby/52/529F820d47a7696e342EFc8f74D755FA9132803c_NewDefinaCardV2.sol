interface IDefinaCard {
    function safeTransferFrom(address from, address to, uint256 tokenId) external;

    function ownerOf(uint256 tokenId) external view returns (address owner);

    function cardInfoes(uint cardId_) external returns (uint cardId, uint camp, uint rarity, string memory name, uint currentAmount, uint maxAmount, string memory cardURI);

    function cardIdMap(uint tokenId_) external returns (uint);

}

contract NewDefinaCardV2 is Ownable, ERC721Enumerable, NonblockingReceiver, ReentrancyGuard, Initializable {
    // for inherit
    function _burn(uint256 tokenId) internal override(ERC721) {
        ERC721._burn(tokenId);
    }

    using SafeMath for uint;
    using CommonUtils for *;
    using Address for address;
    using Strings for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    address public Admin;
    address public mergerOperator;

    event SetAdmin(address _new);
    event SetMergerOperator(address _new);
    event SetCurrentToken(IERC20 _currentToken, uint _nftPrice);
    event SetMaxRarity(uint _maxRarity);
    event Mint(address owner, uint tokenId_);
    event MintMulti(address owner, uint _amount);
    event Burn(uint indexed tokenId_);
    event PullNFTs(address tokenAddress_, address receivedAddress_, uint amount_);

    event ClaimableStateChanged(bool indexed claimableActive);

    event AddMerge(uint256 indexed tokenIdA, uint256 indexed tokenIdB, address owner);
    event MergeSuccess(uint256 indexed tokenIdA, uint256 indexed tokenIdB, address owner, uint _currentRarity, uint _currentHero, uint _currentPoint, bool success, bytes32 randTransactionHash);

    modifier onlyAdmin () {
        require(_msgSender() == Admin, "not Admin's calling");
        _;
    }
    modifier whenClaimableActive() {
        require(claimableActive, "Claimable state is not active");
        _;
    }

    modifier onlyEOA() {
        require(msg.sender == tx.origin, "DefinaCard: not eoa");
        _;
    }
    modifier onlyOperator() {
        require(mergerOperator == msg.sender, "Only operator can call this method");
        _;
    }
    modifier whenPublicSaleActive() {
        require(publicSaleActive, "Public sale is not active");
        _;
    }
    modifier whenMergerActive() {
        require(mergeActive, "Merge is not active");
        _;
    }
    function setAdmin(address newAdmin_) external onlyAdmin {
        require(newAdmin_ != address(0), "The address is null");
        Admin = newAdmin_;
        emit SetAdmin(newAdmin_);
    }


    function setMergerOperator(address newMergerOperator_) external onlyAdmin {
        require(newMergerOperator_ != address(0), "The address is null");
        mergerOperator = newMergerOperator_;
        emit SetMergerOperator(newMergerOperator_);
    }

    struct TokenInfo {
        uint tokenId;
        uint heroId;
        uint rarity;
        uint genesisPoint;
    }
    // old rarity => new rarity
    mapping(uint => uint) public rarityRarityMap;
    // token => genesis point
    mapping(uint => uint) public genesisPointMap;
    // old rarity => genesis point
    mapping(uint => uint) public rarityPointMap;
    // Okex heroId => genesis point
    mapping(uint => uint) public okexPointMap;
    // Okex heroId => is Okex card
    mapping(uint => bool) public iSOkexMap;
    // token => heroId
    mapping(uint => uint) public heroIdMap;
    // token => rarity
    mapping(uint => uint) public rarityMap;
    // rarity => heroIds
    mapping(uint => uint[]) public rarityHeroIdsMap;
    //[B,A,S,S+,SS,SS+,SSS,SSS+,X]
    uint public maxRarity = 9;
    string public myBaseURI;

    //MINT
    uint gasForDestinationLzReceive = 350000;
    uint256 public MAX_MINT = 100000;
    bool public publicSaleActive;
    IERC20  public  currToken;
    uint public nftPrice;
    uint256 public mintIndexPublicSale;

    //CLAIM
    bool public claimableActive;
    IDefinaCard public nftA;
    mapping(uint256 => bool) public claimed;
    uint256 public maxClaimedAmount;

    //MERGE
    struct MergeInfo {
        uint tokenIdA;
        uint tokenIdB;
        address user;
        uint price;
    }

    bool public mergeActive;
    mapping(uint256 => bool) public forMerge;
    mapping(uint256 => MergeInfo) public mergeMap;
    //rarity=>scales
    mapping(uint => uint) public rarityScalesMap;
    //rarity=>base merge price
    mapping(uint => uint) public rarityMergePriceMap;
    //rarity=>directional merge price
    mapping(uint => uint) public rarityDirectionalPriceMap;
    IERC20  public mergeToken;


    constructor() ERC721("Defina Card", "DEFINACARD") {
        Admin = _msgSender();
        mergerOperator = _msgSender();
    }

    function initialize(IDefinaCard nftA_, uint nftAmount_, address _layerZeroEndpoint, uint[][] calldata _rarityHeroIds, uint[][] calldata _oldCardSetting) onlyAdmin external initializer {
        nftA = nftA_;
        maxClaimedAmount = nftAmount_;
        mintIndexPublicSale = nftAmount_;
        endpoint = ILayerZeroEndpoint(_layerZeroEndpoint);
        setRarityHeroIds(_rarityHeroIds);
        setOldCardSetting(_oldCardSetting);
    }

    function setRarityHeroIds(uint[][] calldata _rarityHeroIds) onlyAdmin public {
        require(_rarityHeroIds.length == maxRarity, "Array length should be equal to Rarity length");
        //remove
        for (uint i = 0; i < maxRarity; i++) {
            uint rarity = i + 1;
            uint[] memory _heroIds = _rarityHeroIds[i];
            delete rarityHeroIdsMap[rarity];
            rarityHeroIdsMap[rarity] = _heroIds;
        }
    }

    function setOldCardSetting(uint[][] calldata _oldCardSetting) onlyAdmin public {
        require(_oldCardSetting.length == 4, "Array length should be equal 4");
        uint[] calldata _rarityRarity = _oldCardSetting[0];
        uint[] calldata _rarityPoints = _oldCardSetting[1];
        uint[] calldata _okexHeroIds = _oldCardSetting[2];
        uint[] calldata _okexPoints = _oldCardSetting[3];
        require(_rarityRarity.length > 0, "Array length > 0");
        require(_rarityPoints.length > 0, "Array length > 0");
        require(_okexHeroIds.length == _okexPoints.length, "Array length should be equal to Okex length");
        //remove
        for (uint i = 0; i < _rarityRarity.length; i++) {
            uint oldRarity = i + 1;
            uint newRarity = _rarityRarity[i];
            delete rarityRarityMap[oldRarity];
            rarityRarityMap[oldRarity] = newRarity;
        }
        //remove
        for (uint i = 0; i < _rarityPoints.length; i++) {
            uint oldRarity = i + 1;
            uint point = _rarityPoints[i];
            delete rarityPointMap[oldRarity];
            rarityPointMap[oldRarity] = point;
        }
        for (uint i = 0; i < _okexHeroIds.length; i++) {
            delete okexPointMap[_okexHeroIds[i]];
            okexPointMap[_okexHeroIds[i]] = _okexPoints[i];
            iSOkexMap[_okexHeroIds[i]] = true;
        }
    }

    function setMaxRarity(uint _maxRarity) onlyAdmin public {
        require(_maxRarity > 0, "The Max Rarity of Nft is null");
        maxRarity = _maxRarity;
        emit SetMaxRarity(_maxRarity);
    }

    function setMyBaseURI(string memory uri_) onlyAdmin external {
        myBaseURI = uri_;
    }

    function getClaimableCards(uint256[] calldata tokenIds) public view returns (uint256[] memory){
        uint256[] memory claimableCards;
        uint256 n=0;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint tokenId = tokenIds[i];
            if (claimed[tokenId] != true)
            {
                claimableCards[n]=tokenId;
                n++;
            }
        }
        return claimableCards;

    }

    function getRandHero(uint rarity, uint index) private returns (uint){
        uint[] memory heroIds = rarityHeroIdsMap[rarity];
        uint index = CommonUtils._randModulus(_msgSender(), heroIds.length, index);
        uint heroId = heroIds[index];
        return heroId;
    }

    // Public Sale Methods
    function startPublicSale(
        IERC20 _currToken,
        uint256 _nftPrice
    ) onlyAdmin external {
        require(!publicSaleActive, "Public sale has already begun");
        require(_nftPrice > 0, "The Nft Price of Nft is null");
        require(_currToken != IERC20(address(0)), "The address of IERC20 token is null");

        currToken = _currToken;
        nftPrice = _nftPrice;
        publicSaleActive = true;
    }

    function stopPublicSale() onlyAdmin whenPublicSaleActive external {
        publicSaleActive = false;
    }

//    function mintMulti(uint amount_) whenPublicSaleActive nonReentrant onlyEOA external {
//        require(amount_ > 0, "ERC721: missing amount");
//        require(mintIndexPublicSale + amount_ <= MAX_MINT, "DefianeCard: Mint exceeds supply");
//        currToken.transferFrom(_msgSender(), address(this), nftPrice * amount_);
//        for (uint i = 0; i < amount_; ++i) {
//            uint rarity = 1;
//            uint tokenId = mintIndexPublicSale++;
//            uint heroId = getRandHero(rarity, i);
//            rarityMap[tokenId] = rarity;
//            heroIdMap[tokenId] = heroId;
//            _safeMint(_msgSender(), tokenId);
//        }
//        emit MintMulti(_msgSender(), amount_);
//    }

//created temperarily for testing
    function mintMulti(uint amount_,uint rarity_) nonReentrant onlyEOA external {
        currToken.transferFrom(_msgSender(), address(this), nftPrice * amount_);
        for (uint i = 0; i < amount_; ++i) {
            uint rarity = rarity_;
            uint tokenId = mintIndexPublicSale++;
            uint heroId = getRandHero(rarity, i);
            rarityMap[tokenId] = rarity;
            heroIdMap[tokenId] = heroId;
            _safeMint(_msgSender(), tokenId);
        }
        emit MintMulti(_msgSender(), amount_);
    }


    //Claim Methods
    function changeClaimableState() onlyAdmin external {
        claimableActive = !claimableActive;
        emit ClaimableStateChanged(claimableActive);
    }

    function nftOwnerClaimCards(uint256[] calldata tokenIds) whenClaimableActive onlyEOA external {
        require(tokenIds.length > 0, "Should claim at least one nft");
        for (uint256 i; i < tokenIds.length; ++i) {
            uint256 tokenId = tokenIds[i];
            require(!claimed[tokenId], "NFT already claimed");
            require(nftA.ownerOf(tokenId) == _msgSender(), "Must own all of the nft defined by tokenIds");
            require(tokenId <= maxClaimedAmount, "NFT not allowed to claim");
            claimLandByTokenId(tokenId);
        }
    }

    function claimLandByTokenId(uint256 tokenId) private {
        claimed[tokenId] = true;
        _safeMint(_msgSender(), tokenId);
        uint cardId_ = nftA.cardIdMap(tokenId);
        (uint cardId, uint camp, uint rarity, string memory name, uint currentAmount, uint maxAmount, string memory cardURI) = nftA.cardInfoes(cardId_);
        rarityMap[tokenId] = rarityRarityMap[rarity];
        heroIdMap[tokenId] = CommonUtils.stringToUint(cardURI);
        if (iSOkexMap[heroIdMap[tokenId]] == true) {
            genesisPointMap[tokenId] = okexPointMap[heroIdMap[tokenId]];
        } else {
            genesisPointMap[tokenId] = rarityPointMap[rarity];
        }
    }

    // Merge Methods
    function startMerge(IERC20 _mergeToken, uint[] calldata _mergePrices, uint[] calldata _directionalPrices, uint[] calldata rarityScales
    ) onlyAdmin external {
        require(!mergeActive, "Merge has already begun");
        require(_mergePrices.length == (maxRarity - 1), "The Merge Price array length is error");
        require(_directionalPrices.length == (maxRarity - 1), "The Merge Price array length is error");
        require(_mergeToken != IERC20(address(0)), "The address of IERC20 token is null");
        require(rarityScales.length == (maxRarity - 1), "DefianeCard: The rarity scales array length is error");

        mergeToken = _mergeToken;
        mergeActive = true;
        //remove previous quota first
        for (uint i = 0; i < _mergePrices.length; ++i) {
            rarityMergePriceMap[i + 1] = _mergePrices[i];
        }
        //remove previous quota first
        for (uint i = 0; i < _directionalPrices.length; ++i) {
            rarityDirectionalPriceMap[i + 1] = _directionalPrices[i];
        }
        //remove previous quota first
        for (uint i = 0; i < rarityScales.length; ++i) {
            rarityScalesMap[i + 1] = rarityScales[i];
        }
    }

    function stopMerge() onlyAdmin whenMergerActive external {
        mergeActive = false;
    }


    function addMerge(uint tokenIdA, uint tokenIdB, uint price) payable onlyEOA whenMergerActive external {
        require(tokenIdA != tokenIdB, "DefinaCard: tokenIdA can not eq tokenIdB");
        require(rarityMap[tokenIdA] == rarityMap[tokenIdB], "DefinaCard: Two cards are of the same rarity");
        require(rarityMap[tokenIdA] < maxRarity, "ERC721Metadata:NFTA is already max level");
        require(_exists(tokenIdA) && _exists(tokenIdB), "ERC721Metadata: URI query for nonexistent token");
        require(_isApprovedOrOwner(_msgSender(), tokenIdA) && _isApprovedOrOwner(_msgSender(), tokenIdB), "DefinaCard: caller is not owner nor approved");
        require(!forMerge[tokenIdA] && !forMerge[tokenIdB], "NFT already for merging");
        if (iSOkexMap[heroIdMap[tokenIdA]] == true) {
            uint directionalPrice = rarityDirectionalPriceMap[rarityMap[tokenIdA]];
            require(price >= directionalPrice, "DefinaCard: This card only supports directional synthesis");
        }
        uint basePrice = rarityMergePriceMap[rarityMap[tokenIdA]];
        require(price >= basePrice, "DefinaCard: missing amount");
        mergeToken.transferFrom(_msgSender(), address(this), price);

        forMerge[tokenIdA] = true;
        forMerge[tokenIdB] = true;
        mergeMap[tokenIdA] = MergeInfo({
        tokenIdA : tokenIdA,
        tokenIdB : tokenIdB,
        user : _msgSender(),
        price : price
        });
        if (address(mergerOperator) != _msgSender()) {
            approve(address(mergerOperator), tokenIdA);
            approve(address(mergerOperator), tokenIdB);
        }
        emit AddMerge(tokenIdA, tokenIdB, _msgSender());
    }


    function toMerge(address user, uint tokenIdA, uint tokenIdB, bytes32 randomness_, bytes32 transactionHash) onlyOperator external {
        require(tokenIdA != tokenIdB, "DefinaCard: tokenIdA can not eq tokenIdB");
        require(rarityMap[tokenIdA] == rarityMap[tokenIdB], "DefinaCard: Two cards are of the same rarity");
        require(rarityMap[tokenIdA] < maxRarity, "ERC721Metadata:NFTA is already max level");
        require(_exists(tokenIdA) && _exists(tokenIdB), "ERC721Metadata: URI query for nonexistent token");
        require(_isApprovedOrOwner(_msgSender(), tokenIdA) && _isApprovedOrOwner(_msgSender(), tokenIdB), "DefinaCard: caller is not owner nor approved");
        require(forMerge[tokenIdA] && forMerge[tokenIdB], "NFT has not been used for mergers");

        MergeInfo memory mergeInfo = mergeMap[tokenIdA];
        require(mergeInfo.tokenIdB == tokenIdB, "NFTA merge object is not NFTB");

        uint rand = CommonUtils._randSeedModulus(user, randomness_, transactionHash, 100);
        uint scale = rarityScalesMap[rarityMap[tokenIdA]];
        bool success = false;
        if (rand <= scale) {
            success = true;
            uint directionalPrice = rarityDirectionalPriceMap[rarityMap[tokenIdA]];
            uint newRarity = rarityMap[tokenIdA] + 1;
            rarityMap[tokenIdA] = newRarity;
            genesisPointMap[tokenIdA] = genesisPointMap[tokenIdA] + genesisPointMap[tokenIdB];
            if (mergeInfo.price < directionalPrice) {
                //ModeL ONE
                uint[] memory heroIds = rarityHeroIdsMap[newRarity];
                uint index = CommonUtils._randSeedModulus(user, randomness_, transactionHash, heroIds.length);
                uint heroId = heroIds[index];
                heroIdMap[tokenIdA] = heroId;
            }
        }
        burn(tokenIdB);
        //delete Merge record
        delete forMerge[tokenIdA];
        delete forMerge[tokenIdB];
        delete mergeMap[tokenIdA];
        emit MergeSuccess(tokenIdA, tokenIdB, user, rarityMap[tokenIdA], heroIdMap[tokenIdA], genesisPointMap[tokenIdA], success, transactionHash);
    }


    function burn(uint tokenId_) public returns (bool){
        require(_isApprovedOrOwner(_msgSender(), tokenId_), "ERC721: burn caller is not owner nor approved");
        delete rarityMap[tokenId_];
        delete heroIdMap[tokenId_];
        delete genesisPointMap[tokenId_];
        _burn(tokenId_);
        emit Burn(tokenId_);
        return true;
    }

    function tokenURI(uint256 tokenId_) override(ERC721) public view returns (string memory) {
        require(_exists(tokenId_), "ERC721Metadata: URI query for nonexistent token");
        string memory baseURI = _baseURI();
        return baseURI;
    }

    function getTokenInfosByAddress(address who) view external returns (TokenInfo[] memory) {
        require(who != address(0));
        uint length = balanceOf(who);
        TokenInfo[] memory tmp = new TokenInfo[](length);
        for (uint i = 0; i < length; i++) {
            uint tokenId = tokenOfOwnerByIndex(who, length - i - 1);
            tmp[i] = TokenInfo({
            tokenId : tokenId,
            heroId : heroIdMap[tokenId],
            rarity : rarityMap[tokenId],
            genesisPoint : genesisPointMap[tokenId]
            });
        }
        return tmp;
    }
    // This function transfers the nft from your address on the
    // source chain to the same address on the destination chain
    function traverseChains(uint16 _chainId, uint tokenId) public payable {
        require(_msgSender() == ownerOf(tokenId), "You must own the token to traverse");
        require(trustedRemoteLookup[_chainId].length > 0, "This chain is currently unavailable for travel");

        // burn NFT, eliminating it from circulation on src chain
        _burn(tokenId);

        // abi.encode() the payload with the values to send
        bytes memory payload = abi.encode(_msgSender(), tokenId);

        // encode adapterParams to specify more gas for the destination
        uint16 version = 1;
        bytes memory adapterParams = abi.encodePacked(version, gasForDestinationLzReceive);

        // get the fees we need to pay to LayerZero + Relayer to cover message delivery
        // you will be refunded for extra gas paid
        (uint messageFee,) = endpoint.estimateFees(_chainId, address(this), payload, false, adapterParams);

        require(msg.value >= messageFee, "LZNFT: msg.value not enough to cover messageFee. Send gas for message fees");

        endpoint.send{value : msg.value}(
            _chainId, // destination chainId
            trustedRemoteLookup[_chainId], // destination address of nft contract
            payload, // abi.encoded()'ed bytes
            payable(msg.sender), // refund address
            address(0x0), // 'zroPaymentAddress' unused for this
            adapterParams                       // txParameters
        );
    }

    // just in case this fixed variable limits us from future integrations
    function setGasForDestinationLzReceive(uint newVal) onlyAdmin external {
        gasForDestinationLzReceive = newVal;
    }

    // ------------------
    // Internal Functions
    // ------------------

    function _LzReceive(uint16 _srcChainId, bytes memory _srcAddress, uint64 _nonce, bytes memory _payload) override internal {
        // decode
        (address toAddr, uint tokenId) = abi.decode(_payload, (address, uint));

        // mint the tokens back into existence on destination chain
        _safeMint(toAddr, tokenId);
    }

}

