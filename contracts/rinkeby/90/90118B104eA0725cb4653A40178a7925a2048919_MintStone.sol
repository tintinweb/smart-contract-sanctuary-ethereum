import "hardhat/console.sol";


import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./interfaces/ISpaceHunter.sol";
import "./libraries/Lib.sol";

contract MintStone is ReentrancyGuardUpgradeable, OwnableUpgradeable, PausableUpgradeable{
    using AddressUpgradeable for address;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;

    struct Round{
        uint256 startTime;
        uint256 endTime;
        uint256 endPublicTime;
        uint256 limitAmount;
        uint256 currentAmount;
        uint256 price;
        address paymentToken;
    }

    event MintStone(address indexed buyer, uint256 tokenId);
    event AddWhiteListed(address[] addresses);

    ISpaceHunterNFTs public stoneContract;

    mapping(uint256 => Round) public rounds;
    mapping(address => uint256) public users;     
    EnumerableSetUpgradeable.AddressSet private whitelisted;

    uint256 public currentRoundIndex;
    uint256 public maxStoneUserCanMint;

    // Function to receive Ether. msg.data must be empty
    receive() external payable {}

    // Fallback function is called when msg.data is not empty
    fallback() external payable {}

    function initialize(ISpaceHunterNFTs _stone) public initializer{
        __ReentrancyGuard_init_unchained();
        __Context_init_unchained();
        __Ownable_init_unchained();
        __Pausable_init_unchained();

        stoneContract = _stone;

        currentRoundIndex = 0;
        maxStoneUserCanMint = 2;

        rounds[0].startTime = block.timestamp;
        rounds[0].endTime = block.timestamp + 15 * 60 * 60 * 24;
        rounds[0].endPublicTime = block.timestamp + 30 * 60 * 60 * 24;
        rounds[0].limitAmount = 5000;
        rounds[0].price = 0.001 ether;
    }

    function setupRound(
        uint256 index, 
        uint256 _startTime, 
        uint256 _endTime,
        uint256 _endPublicTime,
        uint256 _limit, 
        uint256 _price, 
        address _paymentToken
    ) external onlyOwner{
        rounds[index].startTime = _startTime;
        rounds[index].endTime = _endTime;
        rounds[index].endPublicTime = _endPublicTime;
        rounds[index].limitAmount = _limit;
        rounds[index].price = _price;
        rounds[index].paymentToken = _paymentToken; 
    }

    function addWhitelisted(address[] memory addresses) external onlyOwner{
        for (uint8 i = 0; i < addresses.length; i++) {
            whitelisted.add(addresses[i]);
        }
        emit AddWhiteListed(addresses);
    }

    function addWhitelistedPublic(address[] memory addresses) external {
        for (uint8 i = 0; i < addresses.length; i++) {
            whitelisted.add(addresses[i]);
        }
        emit AddWhiteListed(addresses);

    }

    function isWhitelisted(address userAddress) public view returns(bool) {
        return whitelisted.contains(userAddress);
    }

    function setCurrentRoundIndex(uint256 index) external onlyOwner{
        currentRoundIndex = index;
    }

    function setupRoundTime(
        uint256 index,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _endPublicTime
    ) external onlyOwner{
        rounds[index].startTime = _startTime;
        rounds[index].endTime = _endTime;
        rounds[index].endPublicTime = _endPublicTime;
    }

    function setNFTContract(ISpaceHunterNFTs _stone) external onlyOwner{
        stoneContract = _stone;
    }

    function setMaxStoneCanMint(uint256 value) external onlyOwner{
        maxStoneUserCanMint = value;
    }

    function executeFundsTransfer(
        address paymentToken,
        address buyer,
        address seller,
        uint256 price
    ) internal virtual returns(uint256){
        /* Only payable in the special case of unwrapped Native coin. */
        if (paymentToken != address(0)) {
            // If buy with bnb -> need msg.value
            require(msg.value == 0, "No need to transfer native coin");
        }

        require(price > 0, "Price must be greater than 0");

        //Buyer make an order
        if(paymentToken != address(0)){
            uint256 balance = IERC20Upgradeable(paymentToken).balanceOf(buyer);
            require(balance >= price, "Not enough coin");
            transferTokens(paymentToken, buyer, seller, price);
        } else{
            require(msg.value >= price, "Not enough coint");

            if(seller != address(this)){
                (bool sendToSeller, bytes memory data) = payable(seller).call{
                    value: price
                }("");
                require(sendToSeller, "Failed to send BNB to seller");

            }
        }

        return price;
    }

    function mintStone(uint256 amount) external payable nonReentrant whenNotPaused{
        require(
            whitelisted.contains(msg.sender),
            "not whitelisted"
            );
        require(
            block.timestamp >= rounds[currentRoundIndex].startTime,
            "round not yet start"
            );
        require(
            block.timestamp <= rounds[currentRoundIndex].endTime,
            "round already ended"
            );
        require(
            users[msg.sender] + amount <= maxStoneUserCanMint,
            "you have bought the allowed boxes"
            );
        require(
            rounds[currentRoundIndex].currentAmount + amount <=
                rounds[currentRoundIndex].limitAmount,
            "reach a limit"
            );

        executeFundsTransfer(
            rounds[currentRoundIndex].paymentToken,
            _msgSender(),
            address(this),
            rounds[currentRoundIndex].price * amount
        );
        
        for(uint8 i=0; i < amount; i++){
            uint256 id = stoneContract.safeMint(msg.sender);
            emit MintStone(_msgSender(),id);
        }

        rounds[currentRoundIndex].currentAmount += amount;
        users[msg.sender] +=  amount;
    }

    function mintStonePublic(uint256 amount) external payable nonReentrant whenNotPaused{
        require(
            block.timestamp >= rounds[currentRoundIndex].endTime,
            "round not yet start"
            );
        require(
            block.timestamp <= rounds[currentRoundIndex].endPublicTime,
            "round already ended"
            );
        require(
            users[msg.sender] + amount <= maxStoneUserCanMint,
            "you have bought the allowed boxes"
            );
        require(
            rounds[currentRoundIndex].currentAmount + amount <=
                rounds[currentRoundIndex].limitAmount,
            "reach a limit"
            );

        executeFundsTransfer(
            rounds[currentRoundIndex].paymentToken,
            _msgSender(),
            address(this),
            rounds[currentRoundIndex].price * amount
        );
        
        for(uint8 i=0; i < amount; i++){
            uint256 id = stoneContract.safeMint(msg.sender);
            emit MintStone(_msgSender(),id);
        }

        rounds[currentRoundIndex].currentAmount += amount;
        users[msg.sender] +=  amount;
    }

    /**
     * @dev Transfer tokens
     * @param token Token to transfer
     * @param from Address to charge fees
     * @param to Address to receive fees
     * @param amount Amount of protocol tokens to charge
     */
    function transferTokens(
        address token,
        address from,
        address to,
        uint256 amount
    ) internal virtual{
        if(amount > 0){
            IERC20Upgradeable(token).transferFrom(from, to, amount);
        }
    }

    function transferToken(
        IERC20Upgradeable token,
        uint256 balance,
        address to
    ) external onlyOwner{
        if(address(token) == address(0)){
            payable(to).call{value: balance}("");
        } else{
            require(
                token.balanceOf(address(this)) >= balance,
                "Not enough balance"
            );
            token.transfer(to, balance);
        }
    }
}

