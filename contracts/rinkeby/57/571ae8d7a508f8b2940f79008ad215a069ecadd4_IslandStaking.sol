/// @title The Polychain Monsters Land Token including it's vesting
/// @author Stefan Miertschink
/// @notice This ERC20 Token will release/mint a specific amount of tokens each interval and decrease the amount each interval
/// @dev This is still an ERC20 Token ;)
contract IslandStaking is Initializable, AccessControlUpgradeable, PausableUpgradeable, ERC721HolderUpgradeable {
	using SafeERC20Upgradeable for IERC20Upgradeable;
	using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

	bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
	bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

	/// @notice the fixed duration of an interval in seconds. e.g. 2592000 for 30days
	/// @dev note that it is not possible to define intervals like "each calender month"
	uint256 public intervalLength;

	/// @notice the percentage that the amount of released tokens shall decrease each interval compared to the last month
	/// @dev this is passed in the init function. It's a simple integer like 7 for a decrease rate of 7%
	uint256 public periodicallyDecreaseRate;

	/// @notice the minimum amount of tokens that should be released in an interval
	/// if the decreased amount for the nex interval gets below this value, the vesting can be considered as ended
	/// @dev depending on the decrease rate there will be some tokens left in the contract that can only be minted by the owner
	uint256 public minimumReleaseInInterval;
	
	uint256 public initialReleaseForInterval;

	/// @notice the (almost) total supply of the token has been minted - no more tokens to distribute
	/// @dev this will be set by the closeLastInterval function once the amount for an interval gets below the threshold
	bool public ended;

	/// @notice tells if the contract has been initialized with all the needed values
	bool public initialized;

	/// @notice the contract address of the corresponding NFT contract for the islands
	IERC721Upgradeable public islandContract;

	/// @notice the ERC20 Token that is rewarded to the island owners
	address public pmlToken;

	/// @notice the wallet address of the dev-fund
	address public devFundWallet;

	/// @notice the percentage that goes to the dev-fund on each mint
	uint256 public devFundShare;

	/// @notice store that keeps the information about how many tokens have already been claimed for an island
	/// @dev since islands can be transferred between wallets, the already claimed amount is not mapped to wallet
	mapping(uint256 => StakedIsland) private _stakedIslands;

	mapping(address => EnumerableSetUpgradeable.UintSet) internal stakedIslandsOfWallet;

	struct StakedIsland {
		uint256 totalStakeTime;
		uint256 alreadyClaimed;
		uint256 lastStakeStart;
		bool staked;
	}

	////////////////
	/// Events
	////////////////

	/// @notice emitted whenever a wallet claims tokens
	/// @param claimingWallet 		address of the claiming wallet
	/// @param amount 				number of claimed tokens
	event TokensClaimed(address indexed claimingWallet, uint256 indexed amount);

	/// @notice emitted when the amount of the next interval gets lower than the minimumReleaseInInterval
	event VestingEnded();

	/////////////////
	/// Modifiers
	/////////////////

	/// @notice modifier that requires the vesting to be initialized before function can run
	modifier postInitialization() {
		if (initialized == false) revert NotInitialized();
		_;
	}

	/// @notice modifier that checks whether the given island-Id is an ID of a so called Genesis Island
	/// @dev the id range is the same on each chain. It's 1-1000
	modifier onlyGenesisIsland(uint256 islandId) {
		if (islandId == 0 || islandId > 1000) revert NoGenesisIsland(islandId);
		_;
	}

	/// @notice modifier that checks whether the given island-Id belongs to the requesting wallet
	/// @dev the contract for the ERC721 (Islands) will tell us
	modifier onlyIslandOwner(uint256 islandId) {
		/// ensure that the token is a valid contract
		if (!_isOwnerOfIsland(islandId)) revert NotOwnerOfIsland(msg.sender, islandId);
		_;
	}

	/// @notice modifier that checks whether the given island-Id is staked in this contract
	/**
		@dev modifier that checks whether the given island-Id is staked in this contract
		return true if it isn't owned by this contract
	*/
	modifier IslandIsStaked(uint256 islandId) {
		if (
			_stakedIslands[islandId].staked != true
			|| islandContract.ownerOf(islandId) != address(this)
		) revert IslandNotStaked(islandId);
		_;
	}

	/// @notice modifier that checks whether the given island-Id it staked by the wallet of the sender
	modifier IslandIsStakedByWallet(uint256 islandId) {
		if (
			!stakedIslandsOfWallet[msg.sender].contains(islandId)
			|| islandContract.ownerOf(islandId) != address(this)) revert IslandNotStakedByWallet(msg.sender, islandId);
		_;
	}

	/// @notice checks if the vesting has already ended
	modifier vestingNotEnded() {
		if (ended) revert VestingHasEnded();
		_;
	}

	constructor() {
	}

	/// @notice sets all the needed values for starting the vesting of this token
	/// @dev the NFT contract (island contract) needs to be deployed before calling this
	/// @param _initialReleaseForInterval	the token amount that can be minted in the first interval
	/// @param _periodicallyDecreaseRate	the rate (integer percentage) that the amount shall be decreased each interval
	/// @param _intervalLength				duration of each interval in seconds
	/// @param _minimumReleaseInInterval	minimum amount of tokens that can be released in an interval
	/// @param _islandContractAddress		contract address of the NFT-(island) contract
	/// @param _devFundWallet				wallet address of the dev fund that will receive a percentage of minting
	/// @param _devFundShare				integer amount of percentage that will go to the dev fund wallet
	function initialize(
		address _pmlToken,
		uint256 _initialReleaseForInterval,
		uint256 _periodicallyDecreaseRate,
		uint256 _intervalLength,
		uint256 _minimumReleaseInInterval,
		IERC721Upgradeable _islandContractAddress,
		address _devFundWallet,
		uint256 _devFundShare
	) public initializer {
		if (initialized) revert AlreadyInitialized();
		/// amount of releasable tokens can't be higher than the total supply
		/**
		
			TODO
			// if (_initialReleaseForInterval > totalSupply()) revert InvalidInitialReleaseAmount(_initialReleaseForInterval);
		
		**/
		if (_initialReleaseForInterval < _minimumReleaseInInterval) revert InvalidInitialReleaseAmount(_initialReleaseForInterval);
		if (_periodicallyDecreaseRate <= 0) revert InvalidDecreaseRate(_periodicallyDecreaseRate);
		if (_devFundShare <= 0 || _devFundShare >= 100) revert InvalidDevFundShare(_devFundShare);
		intervalLength = _intervalLength;
		/// we need to set all these variables to the amount of tokens that will be released in the first interval
		initialReleaseForInterval = _initialReleaseForInterval;
		periodicallyDecreaseRate = _periodicallyDecreaseRate;
		minimumReleaseInInterval = _minimumReleaseInInterval;
		pmlToken = _pmlToken;
		islandContract = IERC721Upgradeable(_islandContractAddress);
		devFundWallet = _devFundWallet;
		devFundShare = _devFundShare;
		_setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
		_setupRole(PAUSER_ROLE, msg.sender);
		_setupRole(MANAGER_ROLE, msg.sender);
		initialized = true;
	}

	/// @notice public function to pause the contract, callable by owner
	function pause() external onlyRole(PAUSER_ROLE) whenNotPaused {
		_pause();
	}

	/// @notice public function to unpause the contract, callable by owner
	function unpause() external onlyRole(PAUSER_ROLE) whenPaused {
		_unpause();
	}

	/// @notice actually transfer the claimable amount of tokens to the requesting wallet for all islands passed to this function
	/// @param islandIds the list of ids of the islands
	function claimForIslands(uint256[] memory islandIds) external whenNotPaused postInitialization {
		for (uint256 i = 0; i < islandIds.length; i++) {
			claimForIsland(islandIds[i]);
		}
	}

	/// @notice actually transfer the claimable amount of tokens to the requesting wallet if all checks are fine
	/// @dev will emit an event that hold the information of who and how much has been claimed
	/// @param islandId		the id of the island that tokens shall be claimed for
	function claimForIsland(uint256 islandId)
	public
	whenNotPaused
	postInitialization
	IslandIsStakedByWallet(islandId)
	{
		uint256 claimableAmount = _getClaimableAmountForIsland(islandId)[0];
		uint256 devFundShareAmount = _getClaimableAmountForIsland(islandId)[1];
		/// update the value that is already claimed for this island
		_stakedIslands[islandId].alreadyClaimed += claimableAmount;
		/**
			we do this after updating the state of _stakedIslands to stay to the checks-effects-interactions pattern 
			https://fravoll.github.io/solidity-patterns/checks_effects_interactions.html
			https://docs.soliditylang.org/en/v0.8.11/security-considerations.html#use-the-checks-effects-interactions-pattern
		*/
		IERC20Upgradeable(pmlToken).transfer(msg.sender, claimableAmount);
		IERC20Upgradeable(pmlToken).safeTransfer(devFundWallet, devFundShareAmount);

		emit TokensClaimed(msg.sender, claimableAmount);
	}

	function stakeGenesisIslands(uint256[] memory islandIds) external {
		for (uint8 i = 0; i < islandIds.length; i++) {
			uint256 islandId = islandIds[i];

			if (msg.sender != islandContract.ownerOf(islandId)) revert NotOwnerOfIsland(msg.sender, islandId);
			if (islandId > 1000) revert NoGenesisIsland(islandIds[i]);

			_stakedIslands[islandId] = StakedIsland({
			alreadyClaimed : 0,
			totalStakeTime : 0,
			lastStakeStart : block.timestamp,
			staked : true
			});

			stakedIslandsOfWallet[msg.sender].add(islandId);
			/** 
				we do this after updating the state to stay to the checks-effects-interactions pattern
				https://fravoll.github.io/solidity-patterns/checks_effects_interactions.html
				https://docs.soliditylang.org/en/v0.8.11/security-considerations.html#use-the-checks-effects-interactions-pattern
			*/
			islandContract.safeTransferFrom(msg.sender, address(this), islandId);
		}
	}

	function unstakeGenesisIslands(uint256[] memory islandIds) external {
		// claimTokens();

		for (uint8 i = 0; i < islandIds.length; i++) {
			uint256 islandId = islandIds[i];
			
			if(!_stakedIslands[islandId].staked) revert IslandNotStaked(islandId);
			if(!stakedIslandsOfWallet[msg.sender].contains(islandId)) revert IslandNotStakedByWallet(msg.sender, islandId);
			
			_stakedIslands[islandId].totalStakeTime += (block.timestamp - _stakedIslands[islandId].lastStakeStart);
			_stakedIslands[islandId].staked = false;

			stakedIslandsOfWallet[msg.sender].remove(islandId);

			islandContract.safeTransferFrom(address(this), msg.sender, islandId);
		}
	}

	function stakedIslandsOf(address wallet) public view returns (uint256[] memory islandIds) {
		EnumerableSetUpgradeable.UintSet storage stakedIslandIdsOfWallet = stakedIslandsOfWallet[wallet];
		islandIds = new uint256[](stakedIslandIdsOfWallet.length());

		for (uint8 i = 0; i < stakedIslandsOfWallet[wallet].length(); i++) {
			islandIds[i] = stakedIslandsOfWallet[wallet].at(i);
		}
	}

	function numberOfStakedIslandsOf(address wallet) external view returns (uint256) {
		return stakedIslandsOfWallet[wallet].length();
	}

	/// @notice the externally callable function the ask for how much tokens can be claimed in total for a list of islands
	/// @param islandIds the list of ids of the islands
	function getClaimableAmountForIslands(uint256[] memory islandIds) external view postInitialization returns (uint256) {
		uint256 amount = 0;
		for (uint256 i = 0; i < islandIds.length; i++) {
			amount += _getClaimableAmountForIsland(islandIds[i])[0];
		}
		return amount;
	}

	/// @notice the externally callable function the ask for how much tokens can be claimed
	/// @param islandId	the id of the island
	function getClaimableAmountForIsland(uint256 islandId) external view postInitialization returns (uint256) {
		return _getClaimableAmountForIsland(islandId)[0];
	}

	/// @notice function that checks how many tokens have already been claimed for the island and how many are possible to be claimed
	/** @dev
		as the distribution is linear in between one interval, we will use linear interpolation for getting the amount 
		at the time in between the interval
		this is not much more than "the rule of three" ('Dreisatz' in german)
	*/
	/// @param islandId	the id of the island
	function _getClaimableAmountForIsland(uint256 islandId) internal view IslandIsStaked(islandId) returns (uint256[] memory) {
		uint256 currentStakedTime = _stakedIslands[islandId].totalStakeTime + (block.timestamp - _stakedIslands[islandId].lastStakeStart);
		uint256 numberOfIntervals = currentStakedTime / intervalLength;
		uint256 startedIntervalDuration = currentStakedTime % intervalLength;
		
		uint256 claimable;

		uint256 releasableInCurrentInterval = initialReleaseForInterval / 1000;
		for (uint256 i = 0; i < numberOfIntervals; i++) {

			claimable += releasableInCurrentInterval;

			releasableInCurrentInterval = releasableInCurrentInterval * 93 / 100 ;
		}

		uint256 proportionallyRest = 0;
		if (startedIntervalDuration > 0) {
			proportionallyRest = _linearInterpolation(
				0,
				intervalLength,
				startedIntervalDuration,
				0,
				releasableInCurrentInterval
			);
		}
		
		claimable += proportionallyRest;
		
		uint256 devFundShareAmount = claimable * devFundShare / 100;
		uint256 claimableForUser = claimable - _stakedIslands[islandId].alreadyClaimed - devFundShareAmount;
		uint256[] memory claimingValues = new uint256[](2);
		claimingValues[0] = claimableForUser;
		claimingValues[1] = devFundShareAmount;
		return claimingValues;
	}

	/// @notice we need to check if the wallet that claim tokens is the owner of the island
	/// @dev we will call the island nft contract here
	/// @param islandId	the id of the island
	/// @return bool	true if the wallet owns the island or false if not
	function _isOwnerOfIsland(uint256 islandId) internal view returns (bool) {
		return islandContract.ownerOf(islandId) == msg.sender;
	}

	/// @dev can afford this, since unix time stamps are definitely way lower than 256 bit
	uint256 private constant interpolationFactor = 100_000;
	/// @dev highest number which can be interpolated this way
	uint256 private constant maxInterpolatable = type(uint256).max / interpolationFactor;

	/// @dev Gives back the interpolated amount between and including `from` and `to`.
	/// For the calculation `interpolationFactor` is used as a fixed decimal factor,
	/// and `block_start` and `block_end` are used in conjunction with `current_time` to find
	/// the percentage value
	/// @param block_start the time stamp corresponding to `from`
	/// @param block_end the time stamp corresponding to `to`
	/// @param current_time the current time stamp
	/// @param from the lower bound of the interpolation
	/// @param to the upper bound of the interpolation
	/// @return the interpolated value between and including `from` and `to`
	function _linearInterpolation(
		uint256 block_start,
		uint256 block_end,
		uint256 current_time,
		uint256 from,
		uint256 to
	) internal pure returns (uint256) {
		/// sanity check for bounds
		assert(block_start <= block_end);
		assert(block_start <= current_time);
		assert(block_end >= current_time);
		assert(from < maxInterpolatable);
		assert(to < maxInterpolatable);
		assert(current_time < block_end);
		assert(current_time > block_start); // if it fails here: in most cases that means that the staked time is zero
		/// interpolation calculation
		uint256 normalized_end = block_end - block_start;
		uint256 normalized_current = current_time - block_start;
		uint256 percentage = (normalized_current * interpolationFactor) / normalized_end;
		return
		/// (1-p)*from + p*to
		(((interpolationFactor - percentage) * from) + (percentage * to)) / interpolationFactor;
	}
}

