pragma solidity ^0.8.4;












contract TestMarketCore is
    Initializable,
    ERC721Upgradeable,
    OwnableUpgradeable,
    EIP712Upgradeable
{
    string private constant SIGNING_DOMAIN = "testmarket";
    string private constant SIGNATURE_VERSION = "1";

    struct SellOrder {
        address seller; //出品者のアドレス
        address market; // マーケット手数料受け取りアドレス
        address genesis; //アーティストの公式アドレス
        address marketowner; //運営者のアドレス
        uint256 price; // 商品価格
        uint256 fee; // マーケットが受け取る手数料のパーセント
        uint256 royalty; // ロイヤリティのパーセント
        uint256 tokenId; //NFTのトークンID
    }

    struct AuctionSellOrder {
        address seller; //出品者のアドレス
        address market; // マーケット手数料受け取りアドレス
        address genesis; //アーティストの公式アドレス
        address marketowner; //運営者のアドレス
        uint256 fee; // マーケットが受け取る手数料のパーセント
        uint256 royalty; // ロイヤリティのパーセント
        uint256 tokenId; //NFTのトークンID
    }

    struct Order {
        address seller; //出品者のアドレス
        address market; // マーケット手数料受け取りアドレス
        address genesis; //アーティストの公式アドレス
        address buyer; // 購入者のアドレス
        address marketowner; //運営者のアドレス
        uint256 price; // 商品価格
        uint256 fee; // マーケットが受け取る手数料のパーセント
        uint256 royalty; // ロイヤリティのパーセント
        uint256 tokenId; //NFTのトークンID
    }

    function initialize() public initializer {
        __ERC721_init("testmarket", "TMR");
        __Ownable_init();
    }

    function _sellhash(SellOrder calldata sellorder)
        internal
        view
        returns (bytes32)
    {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        keccak256(
                            "SellOrder(address seller,address market,address genesis,address marketowner,uint256 price,uint256 fee,uint256 royalty,uint256 tokenId)"
                        ),
                        sellorder.seller,
                        sellorder.market,
                        sellorder.genesis,
                        sellorder.marketowner,
                        sellorder.price,
                        sellorder.fee,
                        sellorder.royalty,
                        sellorder.tokenId
                    )
                )
            );
    }

    function _auctionhash(AuctionSellOrder calldata auctionsellorder)
        internal
        view
        returns (bytes32)
    {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        keccak256(
                            "AuctionSellOrder(address seller,address market,address genesis,address marketowner,uint256 fee,uint256 royalty,uint256 tokenId)"
                        ),
                        auctionsellorder.seller,
                        auctionsellorder.market,
                        auctionsellorder.genesis,
                        auctionsellorder.marketowner,
                        auctionsellorder.fee,
                        auctionsellorder.royalty,
                        auctionsellorder.tokenId
                    )
                )
            );
    }

    function _hash(Order calldata order) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        keccak256(
                            "Order(address seller,address market,address genesis,address buyer,address marketowner,uint256 price,uint256 fee,uint256 royalty,uint256 tokenId)"
                        ),
                        order.seller,
                        order.market,
                        order.genesis,
                        order.buyer,
                        order.marketowner,
                        order.price,
                        order.fee,
                        order.royalty,
                        order.tokenId
                    )
                )
            );
    }

    function getChainID() external view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }
}

contract testmarketv2 is
    ERC721URIStorageUpgradeable,
    OwnableUpgradeable,
    TestMarketCore,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    ERC2981Upgradeable
{
    mapping(uint256 => address) private tokenIdToOwners;

    event Purchase(
        SellOrder sellorder,
        Order buyorder,
        bytes sellsig,
        bytes buysig,
        bytes ownersellsig,
        bytes ownerbuysig
    );
    event Auction(
        AuctionSellOrder sellorder,
        Order buyorder,
        bytes sellsig,
        bytes buysig,
        bytes ownersellsig,
        bytes ownerbuysig
    );

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721Upgradeable, ERC2981Upgradeable) returns (bool) {
    return super.supportsInterface(interfaceId);
  }

    function _isTokenExist(uint256 tokenId)
        internal
        view
        virtual
        returns (bool)
    {
        return tokenIdToOwners[tokenId] != address(0);
    }

    function _burn(uint256 tokenId)
        internal
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
    {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function lock() public onlyOwner {
        _pause();
    }

    function unlock() public onlyOwner {
        _unpause();
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        initialize();
        // _disableInitializers();
        console.log(address(this));
    }

    address private contractadd;

    function set(address x) internal {
        contractadd = x;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override {
        bool lock_b;
        lock_b = paused();
        if (lock_b == true) {
            console.log("lock_b==true");
            if (address(this) != contractadd) {
                console.log("!= contractadd");
                super._beforeTokenTransfer(from, to, tokenId);
                require(address(this) == contractadd, "this sender is invalid");
            } else {
                console.log("=contractadd");
                super._beforeTokenTransfer(from, to, tokenId);
            }
        } else {
            console.log("lock_b==false");
            super._beforeTokenTransfer(from, to, tokenId);
        }
    }

    function firstTransfer(
        address recipient,
        address genesis,
        uint256 tokenId,
        string memory uri,
        address externaladdress,
        uint96 setroyalty
    ) public {
        set(externaladdress);
        _mint(genesis, tokenId);
        _setTokenURI(tokenId, uri);
        _setTokenRoyalty(tokenId,genesis,setroyalty);
        _transfer(genesis, recipient, tokenId);

        tokenIdToOwners[tokenId] = recipient;
        contractadd = address(0);
    }

    function secondTransfer(
        address owner,
        address to,
        uint256 tokenId,
        address externaladdress
    ) public {
        set(externaladdress);
        _transfer(owner, to, tokenId);

        contractadd = address(0);
    }

    uint256 private feeamount;
    uint256 private royaltyamount;
    uint256 private priceamount;

    function purchase(
        SellOrder calldata sellorder, // 購買情報の個別情報
        Order calldata buyorder, // 購買情報の個別情報
        bytes memory sellsig, // 出品者の署名データ
        bytes memory buysig, // 購入者の署名データ
        bytes memory ownersellsig, // 運営者のSellOrderへの署名データ
        bytes memory ownerbuysig, // 運営者のOrderへの署名データ
        string memory uri,
        address externaladdress,
        uint96 setroyalty
    ) external payable nonReentrant {
        //検証用のorderのハッシュを作成する
        bytes32 sellhash = _sellhash(sellorder);

        //検証用のorderのハッシュを作成する
        bytes32 buyhash = _hash(buyorder);

        // 以下の部分で購入者によって出品情報が改竄されていないことを確認できる（①）
        address sellsigner = ECDSAUpgradeable.recover(sellhash, sellsig);
        require(sellsigner == buyorder.seller, "sellerSignature is invalid");

        // 出品情報と購入情報が同一であることを確認する。(②)
        address buysigner = ECDSAUpgradeable.recover(buyhash, buysig);
        require(
            buysigner == msg.sender,
            "msg.sender is not buyer who sign the order"
        );
        require(
            ECDSAUpgradeable.recover(buyhash, ownerbuysig) ==
                buyorder.marketowner &&
                ECDSAUpgradeable.recover(sellhash, ownersellsig) ==
                sellorder.marketowner,
            "owner is not marketowner"
        ); //オーナーが違う場合はエラー
        require(msg.sender != buyorder.seller, "buyer must not be seller"); //出品者が購入者と同一であった場合はエラー
        // require(sellhash == buyhash, "buyorder contents must be the same"); // オーダーのハッシュ値とsellhashの比較
        require(msg.value == buyorder.price, "payment amount is insufficient"); // 実際に送金される金額と検証された金額が一致しているかを検証

        // 手数料とロイヤリティの価格決定
        feeamount = (msg.value * buyorder.fee) / 1000;
        royaltyamount = (msg.value * buyorder.royalty) / 1000;
        priceamount = msg.value - feeamount - royaltyamount;

        // 決済の実行
        payable(buyorder.market).transfer(feeamount);
        payable(buyorder.genesis).transfer(royaltyamount);
        payable(buyorder.seller).transfer(priceamount);

        // 一次出品か二次流通かの判断
        if (_isTokenExist(buyorder.tokenId)) {
            require(
                ownerOf(buyorder.tokenId) == buyorder.seller,
                "seller must be nft owner"
            );
            secondTransfer(
                buyorder.seller,
                msg.sender,
                buyorder.tokenId,
                externaladdress
            );
            tokenIdToOwners[buyorder.tokenId] = msg.sender;
        } else {
            firstTransfer(
                msg.sender,
                buyorder.genesis,
                buyorder.tokenId,
                uri,
                externaladdress,
                setroyalty
            );
        }

        emit Purchase(
            sellorder,
            buyorder,
            sellsig,
            buysig,
            ownerbuysig,
            ownersellsig
        );
    }

    function auction(
        AuctionSellOrder calldata sellorder, // 購買情報の個別情報
        Order calldata buyorder, // 購買情報の個別情報
        bytes memory sellsig, // 出品者の署名データ
        bytes memory buysig, // 購入者の署名データ
        bytes memory ownersellsig, // 運営者のAuctionSellOrderへの署名データ
        bytes memory ownerbuysig, // 運営者のOrderへの署名データ
        string memory uri,
        address externaladdress,
        uint96 setroyalty
    ) external payable nonReentrant {
        //検証用のorderのハッシュを作成する
        bytes32 sellhash = _auctionhash(sellorder);

        //検証用のorderのハッシュを作成する
        bytes32 buyhash = _hash(buyorder);

        // 以下の部分で購入者によって出品情報が改竄されていないことを確認できる（①）
        address sellsigner = ECDSAUpgradeable.recover(sellhash, sellsig);
        require(sellsigner == buyorder.seller, "sellerSignature is invalid");

        // 出品情報と購入情報が同一であることを確認する。(②)
        address buysigner = ECDSAUpgradeable.recover(buyhash, buysig);
        require(
            buysigner == msg.sender,
            "msg.sender is not buyer who sign the order"
        );
        require(
            ECDSAUpgradeable.recover(buyhash, ownerbuysig) ==
                buyorder.marketowner &&
                ECDSAUpgradeable.recover(sellhash, ownersellsig) ==
                sellorder.marketowner,
            "owner is not marketowner"
        ); //オーナーが違う場合はエラー
        require(msg.sender != buyorder.seller, "buyer must not be seller"); //出品者が購入者と同一であった場合はエラー
        // require(sellhash == buyhash, "order contents must be the same"); // オーダーのハッシュ値とsellhashの比較
        require(msg.value == buyorder.price, "payment amount is insufficient"); // 実際に送金される金額と検証された金額が一致しているかを検証

        // 手数料とロイヤリティの価格決定
        feeamount = (msg.value * buyorder.fee) / 1000;
        royaltyamount = (msg.value * buyorder.royalty) / 1000;
        priceamount = msg.value - feeamount - royaltyamount;

        // 決済の実行
        payable(buyorder.market).transfer(feeamount);
        payable(buyorder.genesis).transfer(royaltyamount);
        payable(buyorder.seller).transfer(priceamount);

        // 一次出品か二次流通かの判断
        if (_isTokenExist(buyorder.tokenId)) {
            require(
                ownerOf(buyorder.tokenId) == buyorder.seller,
                "seller must be nft owner"
            );
            secondTransfer(
                buyorder.seller,
                msg.sender,
                buyorder.tokenId,
                externaladdress
            );
            tokenIdToOwners[buyorder.tokenId] = msg.sender;
        } else {
            firstTransfer(
                msg.sender,
                buyorder.genesis,
                buyorder.tokenId,
                uri,
                externaladdress,
                setroyalty
            );
        }

        emit Auction(
            sellorder,
            buyorder,
            sellsig,
            buysig,
            ownersellsig,
            ownerbuysig
        );
    }
}