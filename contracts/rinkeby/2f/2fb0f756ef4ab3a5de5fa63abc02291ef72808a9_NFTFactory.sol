contract NFTRepresentation is INFTRepresentation {

    function getContractUri(INFT _nft) external view override returns (string memory) {
        return string(
            abi.encodePacked(
                "data:application/json;base64,",
                Base64.encode(
                    bytes(
                        abi.encodePacked(
                            '{"name":"', _nft.name(), '"}'
                        )
                    )
