pragma solidity 0.8.13;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "./external/layerzero/NonblockingLzApp.sol";
import "./NFTDescriptor.sol";

contract Omnispace is Ownable, ERC721, NonblockingLzApp, ReentrancyGuard, Pausable, NFTDescriptor {
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIdCounter;

    uint16 public immutable startIndex;

    uint16 public immutable limit;

    struct Spaceship {
        uint256 nation;
        uint256 specs;
    }

    // Mapping of spaceship by tokenId
    mapping(uint256 => Spaceship) public spaceships;

    constructor(
        uint16 startIndex_,
        uint16 limit_,
        address lzEndpoint_
    ) ERC721("Omnispace", "OMNSP") NonblockingLzApp(lzEndpoint_) NFTDescriptor() {
        startIndex = startIndex_;
        limit = limit_;
    }

    /// @notice Build a new spaceship
    /// @dev the function is payable to allow any donation.
    function build() external payable nonReentrant whenNotPaused {
        // Check if the limit is not exceeded
        if (_tokenIdCounter.current() > limit) revert AllSpaceshipBuilt();

        // Increment the tokenId counter
        _tokenIdCounter.increment();

        // Generate insecure random number to use as specs for the spaceship
        uint256 specs = uint256(keccak256(abi.encode(blockhash(block.number - 1), block.timestamp)));

        // Store the spaceship specs
        spaceships[_tokenIdCounter.current()] = Spaceship(block.chainid, specs);

        // Mint the spaceship to the sender
        _safeMint(msg.sender, startIndex + _tokenIdCounter.current());
    }

    function spacejump(uint16 planetId_, uint256 spaceshipId_) external payable whenNotPaused {
        // Check spaceship ownership
        if (msg.sender != ownerOf(spaceshipId_)) revert NotTokenOwner();

        // Check if planetId (chainId) is present in trusted source
        if (trustedRemoteLookup[planetId_].length == 0) revert PlanetNotFound();

        // Burn the spaceship on current chain
        _burn(spaceshipId_);

        // Create payload with spaceship details
        bytes memory payload = abi.encode(msg.sender, spaceshipId_, spaceships[spaceshipId_]);

        uint16 version = 1;
        uint256 gasForDestinationLzReceive = 350000; // TODO: variable
        bytes memory adapterParams = abi.encodePacked(version, gasForDestinationLzReceive);

        // Estimate gas for sending to destination
        (uint256 messageFee, ) = lzEndpoint.estimateFees(planetId_, address(this), payload, false, adapterParams);

        // Check if enough ether is available to pay for the message
        if (msg.value < messageFee) revert InsufficientGasFees();
        // require(msg.value >= messageFee, "Insufficient gas fees");

        // Send the spaceship to the new chain
        lzEndpoint.send{value: msg.value}(
            planetId_, // destination chainId
            trustedRemoteLookup[planetId_], // destination address
            payload, // payload
            payable(msg.sender), // refund address
            address(0x0), // unused param
            adapterParams // lz adapter params
        );
    }

    function tokenURI(uint256 tokenId_) public view override returns (string memory) {
        if (!_exists(tokenId_)) revert TokenNotFound();

        return NFTDescriptor.constructTokenURI(NFTDescriptor.ConstructTokenURIParams(tokenId_, spaceships[tokenId_].nation, spaceships[tokenId_].specs));
    }

    function _nonblockingLzReceive(
        uint16, /*srcChainId_*/
        bytes memory, /*srcAddress_*/
        uint64, /*nonce_*/
        bytes memory payload_
    ) internal override {
        (address user, uint256 spaceshipId, Spaceship memory spaceship) = abi.decode(payload_, (address, uint256, Spaceship));
        spaceships[spaceshipId] = spaceship;
        _safeMint(user, spaceshipId);
    }

    // TODO: Royalties
    // TODO: Donation
    // TODO: Withdrdraw

    error AllSpaceshipBuilt();
    error NotTokenOwner();
    error TokenNotFound();
    error PlanetNotFound();
    error InsufficientGasFees();
}

