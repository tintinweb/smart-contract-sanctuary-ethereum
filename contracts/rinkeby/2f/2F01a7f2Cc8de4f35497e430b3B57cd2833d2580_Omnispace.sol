pragma solidity 0.8.13;

import "@openzeppelin/contracts/utils/Strings.sol";
import "base64-sol/base64.sol";

abstract contract NFTDescriptor {
    using Strings for uint256;

    struct ConstructTokenURIParams {
        uint256 tokenId;
        uint256 nationId;
        uint256 specs;
    }

    mapping(uint256 => bytes12) nations;

    constructor() {
        nations[1] = "Ethereum";
        nations[56] = "Binance";
        nations[43114] = "Avalanche";
        nations[137] = "Polygon";
        nations[42161] = "Arbitrum";
        nations[10] = "Optimism";
        nations[250] = "Fantom";

        nations[4] = "Rinkeby";
        nations[97] = "BinanceTest";
        nations[43113] = "Fuji";
        nations[80001] = "Mumbai";
        nations[421611] = "ArbitrumRin";
        nations[69] = "OptiKovan";
        nations[4002] = "FantomTest";

        nations[31337] = "Hardhat";
    }

    function constructTokenURI(ConstructTokenURIParams memory params_) internal view returns (string memory) {
        string memory attributes;

        string[7] memory traitType = ["Nation", "Attack", "Defense", "Speed", "Cargo", "Crew", "Booster"];

        uint8[7] memory specs = splitNumber(params_.specs);

        attributes = string(
            abi.encodePacked(attributes, bytes(attributes).length == 0 ? "{" : ", {", '"trait_type": "', traitType[0], '",', '"value": "', bytes12ToString(nations[params_.nationId]), '"', "}")
        );

        for (uint256 i = 1; i < traitType.length - 1; i++) {
            attributes = string(abi.encodePacked(attributes, bytes(attributes).length == 0 ? "{" : ", {", '"trait_type": "', traitType[i], '",', '"value": ', uint256(specs[i] + 1).toString(), "}"));
        }

        attributes = string(
            abi.encodePacked(
                attributes,
                bytes(attributes).length == 0 ? "{" : ", {",
                '"display_type": "boost_number", "trait_type": "',
                traitType[traitType.length - 1],
                '",',
                '"value": ',
                uint256(specs[traitType.length - 1]).toString(),
                "}"
            )
        );

        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                "{",
                                '"name": "Omniship #',
                                params_.tokenId.toString(),
                                '",',
                                '"description": "TODO",',
                                // '"image_data": "',
                                // imageData(_tokenId),
                                // '",',
                                '"external_url": "https://omnispace.xyz/ship/',
                                params_.tokenId.toString(),
                                '",',
                                '"animation_url": "https://omnispace.xyz/viwer/',
                                params_.tokenId.toString(),
                                '",',
                                '"attributes": [',
                                attributes,
                                "]",
                                "}"
                            )
                        )
                    )
                )
            );
    }

    function splitNumber(uint256 number_) internal pure returns (uint8[7] memory) {
        uint8[7] memory numbers;

        for (uint256 i = 0; i < numbers.length; i++) {
            numbers[i] = uint8(number_ % 10);
            number_ /= 10;
        }

        return numbers;
    }

    function bytes12ToString(bytes12 bytes12_) internal pure returns (string memory) {
        uint8 i = 0;
        while (i < 12 && bytes12_[i] != 0) {
            i++;
        }

        bytes memory bytesArray = new bytes(i);
        for (i = 0; i < 12 && bytes12_[i] != 0; i++) {
            bytesArray[i] = bytes12_[i];
        }

        return string(bytesArray);
    }
}

