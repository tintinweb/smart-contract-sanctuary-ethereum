contract TokenClaim{

    using SafeMath for uint;

    IERC721 public nft;
    IERC20 public gvo;

    uint256 public gvoTokenAmount = 2000000000;

    mapping(address => uint) public claimed;

    constructor(IERC721 _nft, IERC20 _gvo){
        nft = _nft;
        gvo = _gvo;
    }

    function claimToken(address userAddress, uint256 tokenAmount) public {
        require(nft.balanceOf(userAddress) > tokenAmount, "No DHNFT available");
        gvo.transfer(userAddress, gvoTokenAmount.mul(tokenAmount));
    }
}