pragma solidity 0.8.11;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/Context.sol";

contract ERC721Minter is ERC721 {
    address private _owner = msg.sender;
    uint256 public TokenId;
    uint256 private tokenid;

    uint64 private _gold;
    uint64 private _silver;
    uint64 private _bronze;

    enum Category{
      GOLD,
      SILVER,
      BRONZE
    }
    // TODO: Set Name & Symbol
    constructor() ERC721("Name", "Symbol") { }

    mapping(uint256 => string) private _tokenURIs;
    mapping(uint256 => address) _ownerof;
    mapping(address => uint256) private _balances;
    mapping(uint256 => Category) public _category;

    modifier onlyOwner(address _owner) {
        require(tx.origin == _owner,"only owner can mint tokens");
        _;
    }
    function viewCategory(uint256 _tokenid) view external returns(string memory)
    {
        if(_category[_tokenid] == Category.GOLD)
        {
            return "GOLD";
        }
        else if(_category[_tokenid] == Category.SILVER)
        {
            return "SILVER";
        }
        else
        {
            return "BRONZE";
        }
    }
    
    function exists(uint256 tokenId) external view virtual returns (bool) {
        return _exists(tokenId);
    } 

    function mintToken(
        Category category
    ) external onlyOwner(_owner) {   
        
        require(_gold < 1000,"cannot mint more than 1000 gold category nfts");
        require(_silver < 2000,"cannot mint more than 1000 silver category nfts");
        require(_bronze < 3000,"cannot mint more than 1000 bronze category nfts");
        tokenid++;    
        _safeMint(msg.sender, tokenid);
        _category[tokenid]=category;
        if(category==Category.GOLD){
            _gold++;
        }
        else if(category==Category.SILVER){
            _silver++;
        }
        else{
            _bronze++;
        }
    }


    function transfer(
        address from,
        address to,
        uint256 tokenId
    ) external {
        _transfer(from, to, tokenId);
    }
  
}

