pragma solidity 0.8.11;

import "./ERC721Minter.sol";
import "./ERC20_.sol";
import "./SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

contract MarketPlace is Ownable {
    ERC721Minter _erc721;
    ERC20_ _erc20;

    address public admin;
    uint256 private _commissionRate;
    uint256 private auc_time;

    //STRUCT
    //For Auction data
    struct ERC721Auction {
        address seller;
        uint256 time;
        uint256 basePrice;
        uint256 bidPrice;
    }
    //For Sale
    struct Sell {
        uint256 erc20Token;
        uint256 amount;
    }

    //Mappings
    mapping(uint256 => ERC721Auction) public auction;
    mapping(uint256 => mapping(address => address)) private _bid;
    mapping(uint256 => Sell) public _sellPriceLead;

    //EVENT
    event ForAuction(
        address indexed operator,
        address indexed owner,
        uint256 indexed id,
        uint256 basePrice
    );
    event Bid(
        address indexed bidder,
        address indexed owner,
        uint256 indexed id,
        uint256 bid
    );
    event RemoveBid(
        address indexed bidder,
        address indexed owner,
        uint256 indexed id
    );
    event ForSell(
        address indexed operator,
        address indexed owner,
        uint256 indexed id,
        uint256 price
    );

    constructor(address _ERC721, address _ERC20) {
        _commissionRate = 10;
        admin = msg.sender;
        _erc721 = ERC721Minter(_ERC721);
        _erc20 = ERC20_(_ERC20);
    }

    //Function
    //Function to set commission
    function setCommission(uint256 _cRate) external {
        require(
            msg.sender == admin,
            "Marketplace :  admin can set commission rate"
        );
        _commissionRate = _cRate;
    }

    //Function to put a token on auction
    function setAuction(
        address from,
        uint256 id,
        uint256 erc20Token,
        uint256 time
    ) public {
        require(
            auction[id].seller == address(0),
            "the token is already on auction"
        );
        require(
            _erc721.ownerOf(id) == msg.sender ||
                _erc721.getApproved(id) == msg.sender ||
                _erc721.isApprovedForAll(_erc721.ownerOf(id), msg.sender),
            "owner or approved only."
        );
        // uint256 erc20Token = erc20Tokens * 10**18;
        auction[id].seller = msg.sender;
        // auction[id].basePrice = erc20Token;
        auction[id].bidPrice = erc20Token;
        auction[id].time = block.timestamp + time;
        auc_time = auction[id].time;

        emit ForAuction(msg.sender, from, id, erc20Token);
    }

    //Function to bid on a token on auction
    function bid(
        address from,
        uint256 id,
        uint256 erc20Token
    ) external {
        // uint256 erc20Token = erc20Tokens * 10**18;
        require(
            msg.sender != from && !_erc721.isApprovedForAll(from, msg.sender),
            "owner or approved addresses can't bid"
        );
        require(auction[id].seller != address(0), "token not for auction");
        require(
            erc20Token > auction[id].bidPrice,
            "you must bid more than the previous bid."
        );

        if (_bid[id][from] != address(0)) {
            _erc20.transfer_(
                address(this),
                _bid[id][from],
                auction[id].bidPrice
            );
        }
        _erc20.transfer_(msg.sender, address(this), erc20Token);
        _bid[id][from] = msg.sender;
        auction[id].bidPrice = erc20Token;
        emit Bid(msg.sender, from, id, erc20Token);
    }

    //Function to get highest bidder
    function getWinner(uint256 id, address from) public view returns (address) {
        require(auction[id].seller != address(0), "Token Not on Auction");
        require(_bid[id][from] != address(0), "No bids in the Auction yet");
        require(
            block.timestamp >= auction[id].time,
            "The Auction is not over yet."
        );
        return _bid[id][from];
    }

    //Function to accept a bid on a token
    function claimNFT(address from, uint256 id) public {
        require(
            msg.sender == getWinner(id, from),
            "Only Auction Winner can claim this"
        );
        uint256 commission = (auction[id].bidPrice * _commissionRate) / 100;
        _erc20.transfer_(address(this), admin, commission);
        _erc20.transfer_(
            address(this),
            from,
            auction[id].bidPrice - commission
        );
        _erc721.transfer(from, msg.sender, id);
        delete _bid[id][from];
        delete auction[id];
    }

    //Function for owner to cancel auction
    function cancelAuction(address from, uint256 id) public onlyOwner {
        if (_bid[id][from] != address(0)) {
            _erc20.transfer_(
                address(this),
                _bid[id][from],
                auction[id].bidPrice
            );
        }
        delete _bid[id][from];
        delete auction[id];
        emit RemoveBid(_bid[id][from], from, id);
    }

    //Function to set price for putting a token for sale
    function setPrice(uint256 id, uint256 erc20Token) external {
        // uint256 erc20Token = erc20Tokens * 10**18;
        require(erc20Token > 0, "Price must be greater than 0");
        require(
            _erc721.ownerOf(id) == msg.sender ||
                _erc721.getApproved(id) == msg.sender ||
                _erc721.isApprovedForAll(_erc721.ownerOf(id), msg.sender),
            "Owner or Approved only."
        );
        _sellPriceLead[id].erc20Token = erc20Token;
    }

    //Function to buy a token put for sale
    function buyToken(uint256 id, uint256 erc20Token) external {
        // uint256 erc20Token = erc20Tokens * 10**18;
        require(block.timestamp > auc_time, "Token is on Auction");
        require(
            msg.sender != _erc721.ownerOf(id) &&
                !_erc721.isApprovedForAll(_erc721.ownerOf(id), msg.sender),
            "Owner or Approved Addresses can't buy"
        );
        uint256 commission = (_sellPriceLead[id].erc20Token * _commissionRate) /
            100;
        _erc20.transfer_(msg.sender, admin, commission);
        _erc20.transfer_(
            msg.sender,
            _erc721.ownerOf(id),
            _sellPriceLead[id].erc20Token - commission
        );
        _erc721.transfer(_erc721.ownerOf(id), msg.sender, id);
        delete _sellPriceLead[id];

        emit ForSell(msg.sender, _erc721.ownerOf(id), id, erc20Token);
    }
}

