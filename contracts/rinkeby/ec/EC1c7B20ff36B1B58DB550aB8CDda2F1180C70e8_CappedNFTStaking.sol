// SPDX-License-Identifier: MIT
pragma solidity ^0.8.7;

import { NFTStaking } from "./NFTStaking.sol";
import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract CappedNFTStaking is NFTStaking {
    uint256 public capacity;
    uint256 public maxStakePerAddress;

    event CapacityChanged(uint256 oldCap, uint256 newCap);
    event MaxStakePerAddressChanged(uint256 oldAmount, uint256 newAmount);
    event Recovered(address indexed token, uint256 amount);

    constructor(
        address stakingNFT_,
        uint256 rewardPerSecond_,
        uint256 startTime_,
        uint256 stopTime_,
        uint256 capacity_,
        uint256 maxStakePerAddress_
    ) NFTStaking(stakingNFT_, rewardPerSecond_, startTime_, stopTime_) {
        capacity = capacity_;
        maxStakePerAddress = maxStakePerAddress_;
    }


    function stake(uint256[] memory tokenIds_) public override checkLimit(tokenIds_) {
        super.stake(tokenIds_);
    }

    function setCapcity(uint256 _newCap) public onlyOwner {
        require(_newCap >= totalStaked, "Staking: new cap less than already staked amount");
        uint256 oldAmount = capacity;
        capacity = _newCap;
        emit CapacityChanged(oldAmount, _newCap);
    }
    
    function setMaxStakePerAddress(uint256 _newMaxStakePerAddress) public onlyOwner {
        require(_newMaxStakePerAddress > 0, "Staking: new maxStakePerAddress less than 1");
        uint256 oldAmount = maxStakePerAddress;
        maxStakePerAddress = _newMaxStakePerAddress;
        emit MaxStakePerAddressChanged(oldAmount, _newMaxStakePerAddress);
    }


    modifier checkLimit(uint256[] memory tokenIds_) {
        require(totalStaked + tokenIds_.length <= capacity, "Staking: over capacity limit");
        Stakeholder memory _holder = _stakeholders[_msgSender()];
        require(_holder.staked + tokenIds_.length <= maxStakePerAddress, "Staking: over maxStakePerAddress limit");
        _;
    }


    function recoverTokens(IERC20 _token) external virtual onlyOwner {
        uint256  balance = _token.balanceOf(address(this));
        _token.transfer(owner(), balance);
        emit Recovered(address(_token), balance);
    }

        
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.7;
import { Address } from "@openzeppelin/contracts/utils/Address.sol";
import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import { IERC1155 } from "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";
import { Pausable } from "@openzeppelin/contracts/security/Pausable.sol";
import { INFTStaking } from "./INFTStaking.sol";
import { ERC1155Holder } from "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import { IERC1155MetadataURI } from "@openzeppelin/contracts/token/ERC1155/extensions/IERC1155MetadataURI.sol";

library ArrUtil {

    function remove(uint256[] storage array,uint256[] memory subArr) internal returns(uint256[] memory) {
        if (subArr.length==0) return array;
        require(subArr.length <=array.length,"ArrUtil: subArr length exceeds");

       for (uint i = 0; i<subArr.length; i++){
            uint256 temp = subArr[i];
           for (uint j = 0; j<array.length; j++){
                if (temp == array[j]){
                   array[j] =  array[array.length-1];
                   array.pop();
                  continue;
                }
             }
        }
        return array;
    }
}


contract NFTStaking is INFTStaking, ERC1155Holder, Ownable, Pausable {
    using Address for address;
    using SafeERC20 for IERC20;

    struct Stakeholder {
        uint256 staked;
        uint256 stakedRatio; // x stakes 1 nft for 10s, so staked ratio = 1 * 10
        uint256[] tokenIds;
    }

    address public override stakingNFT;
    address public override rewardToken;
    uint256 public override rewardPerSecond;
    uint256 public override startTime;
    uint256 public override stopTime;
    uint256 public override duration;
    uint256 public override totalStaked;
    uint256 public override totalStakedRatio;
    bool public rewardLoaded;
    
    mapping(uint256 => uint256) internal _idholders;
    mapping(address => Stakeholder) internal _stakeholders;
    mapping(address => bool) private _withdrawn;

    constructor(
        address stakingNFT_,
        uint256 rewardPerSecond_,
        uint256 startTime_,
        uint256 stopTime_
    ) {
        require(stakingNFT_.isContract(), "Staking: stakingNFT not a contract address");
        require(IERC1155(stakingNFT_).supportsInterface(0xd9b67a26), "Staking: stakingNFT not a ERC-1155 contract address");
        require(rewardPerSecond_ > 0, "Staking: rewardPerSecond must be greater than zero");
        require(startTime_ > block.timestamp && startTime_ < stopTime_, "Staking: incorrect timestamp");
        
        stakingNFT = stakingNFT_;
        rewardPerSecond = rewardPerSecond_;
        startTime = startTime_;
        stopTime = stopTime_;
        duration = stopTime_ - startTime_;
    }


    function totalReward() public view override returns (uint256)  {
        return totalStakedRatio * rewardPerSecond;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function stake(uint256[] memory tokenIds_) public virtual override whenNotPaused {
        uint256 nftNum = tokenIds_.length;
        require(block.timestamp >= startTime, "Staking: staking not started");
        require(block.timestamp <= stopTime, "Staking: staking period over");
        require(nftNum >0, "Staking: size of tokenIds can't be zero");
        for(uint i=0;i<nftNum;i++){
          require(tokenIds_[i] > 0, "Staking: tokenId can't be 0");
          require(_idholders[tokenIds_[i]] == 0, "Staking: tokenId has staked");
          _idholders[tokenIds_[i]] = block.timestamp;
        }

        Stakeholder storage stakeholder = _stakeholders[_msgSender()];
        stakeholder.staked += nftNum;
        uint256 stakedRatio = nftNum * getTimeRemaining();
        stakeholder.stakedRatio += stakedRatio;
        for (uint i = 0;i<nftNum;i++){
          stakeholder.tokenIds.push(tokenIds_[i]);
        }

        totalStaked += nftNum;
        totalStakedRatio += stakedRatio;

        _stake(tokenIds_);
    }


    function unstake(uint256[] memory tokenIds_) public virtual override whenNotPaused {
        uint256 nftNum = tokenIds_.length;
        require(nftNum > 0, "Staking: tokenIds length can't be zero");

        Stakeholder storage stakeholder = _stakeholders[_msgSender()];
        require(stakeholder.staked > 0, "Staking: you have not participated in staking");
        require(stakeholder.staked >= nftNum, "Staking: you don't have enough tokenIds to unstake");

        for(uint i=0;i<nftNum;i++){
          require(tokenIds_[i] > 0, "Staking: tokenId can't be 0");
          require(_idholders[tokenIds_[i]] > 0, "Staking: you had not staked this tokenId");
          stakeholder.stakedRatio -= block.timestamp>=stopTime?0:(stopTime-block.timestamp)*rewardPerSecond;
          delete _idholders[tokenIds_[i]];
        }

        stakeholder.staked -= nftNum;
        uint256[] storage array =  stakeholder.tokenIds;
        stakeholder.tokenIds = ArrUtil.remove(array, tokenIds_);

        _unstake(array);
   }

    function withdraw() public virtual override whenNotPaused {
        require(block.timestamp >= stopTime, "Staking: staking period not over");
        require(rewardLoaded, "Staking: rewards not loaded into the contract yet");
        require(!_withdrawn[_msgSender()], "Staking: you have already _withdrawn");
        _withdrawn[_msgSender()] = true;

        Stakeholder storage stakeholder = _stakeholders[_msgSender()];
        uint256 userReward_ = stakeholder.stakedRatio;
        uint256[] memory _userTokenIds = stakeholder.tokenIds;
        for (uint i=0;i<_userTokenIds.length;i++){
            delete _idholders[_userTokenIds[i]];
        }
        delete _stakeholders[_msgSender()];

        if (_userTokenIds.length>0){
          _unstake(_userTokenIds);
        }
       
       if (userReward_>0){
           IERC20(rewardToken).safeTransferFrom(address(this),_msgSender(),userReward_);
       }
       
       emit  Withdrawal(_msgSender(), rewardToken,userReward_);

    }

    function userEarned(address _stakeholder) public view override returns (uint256) {
        Stakeholder memory stakeholder = _stakeholders[_stakeholder];
        if (_withdrawn[_stakeholder] || stakeholder.stakedRatio == 0) return 0;
        uint256 stakedRatio_ =  stakeholder.stakedRatio - stakeholder.staked * getTimeRemaining();
        return _calcReward(stakedRatio_);
    }

    function userReward(address _stakeholder) public view override returns (uint256) {
        Stakeholder memory stakeholder = _stakeholders[_stakeholder];
        if (_withdrawn[_stakeholder] || stakeholder.stakedRatio == 0) return 0;
        return _calcReward(stakeholder.stakedRatio);
    }

    function userStaked(address _stakeholder) public view override returns (uint256) {
        return _stakeholders[_stakeholder].staked;
    }

    function userTokenIds(address _stakeholder) public view override returns (uint256[] memory) {
        return _stakeholders[_stakeholder].tokenIds;
    }

    function tokenIdEarned(uint256 tokenId_) public view override returns (uint256) {
        uint256 timestamp = _idholders[tokenId_];
        if (timestamp == 0) return 0;
        uint256 stakedRatio_ = (timestamp<stopTime && timestamp<block.timestamp) ? stopTime-block.timestamp:0;
        return _calcReward(stakedRatio_);
    }

    function tokenIdReward(uint256 tokenId_) public view override returns (uint256) {
        uint256 timestamp = _idholders[tokenId_];
        if (timestamp == 0) return 0;
        uint256 stakedRatio_ = timestamp<stopTime?stopTime-timestamp:0;
        return _calcReward(stakedRatio_);
    }

    function getRewardTokenBalance() public view override returns (uint256) {
        return IERC20(rewardToken).balanceOf(address(this));
    }

    function getStakingNFTBalance() public view override returns (uint256) {
        return totalStaked;
    }

    function getTimeRemaining() public view returns (uint256) {
        uint256 timeRemaining = block.timestamp <= stopTime
            ? block.timestamp >= startTime ? stopTime - block.timestamp : duration
            : 0;
        return timeRemaining;
    }

    function getTimeElapsed() public view returns (uint256) {
        uint256 timeElapsed = block.timestamp >= startTime
            ? block.timestamp <= stopTime ? block.timestamp - startTime : duration
            : 0;
        return timeElapsed;
    }

    function isWithdrawn(address staker) external view returns (bool) {
      return _withdrawn[staker];
    }

    function loadReward(IERC20 token_) external onlyOwner {
        require(!rewardLoaded,"Staking: rewardToken is loaded");
        token_.safeTransferFrom(_msgSender(), address(this), totalReward());
        rewardToken = address(token_);
        rewardLoaded = true;
    }


    function _stake(uint256[] memory tokenIds_) internal {
      
        uint256[] memory tokenNums;
        for (uint i=0;i<tokenIds_.length;i++){
            tokenNums[i] = 1;
        }

        IERC1155(stakingNFT).safeBatchTransferFrom(_msgSender(), address(this), tokenIds_, tokenNums,"0x0");

        emit Staked(_msgSender(), tokenIds_);
    }

    function _unstake(uint256[] memory tokenIds_) internal {
      
        uint256[] memory tokenNums;
        for (uint i=0;i<tokenIds_.length;i++){
            tokenNums[i] = 1;
        }
      
       IERC1155(stakingNFT).safeBatchTransferFrom(address(this),_msgSender(), tokenIds_, tokenNums,"0x0");

       emit Unstaked(_msgSender(), tokenIds_);
    }

    function _calcReward(uint256 _stakedRatio) internal view returns (uint256) {
        return (rewardPerSecond * _stakedRatio) ;
    }

}

