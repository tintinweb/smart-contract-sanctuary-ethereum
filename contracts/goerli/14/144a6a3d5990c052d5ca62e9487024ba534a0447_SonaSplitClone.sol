abstract contract SonaAdmin is AccessControl {
	/*//////////////////////////////////////////////////////////////
                                 EVENTS
    //////////////////////////////////////////////////////////////*/
	/// @dev Emitted when an address is made an admin
	event SonaAdminGranted(address account);
	/// @dev Emitted when an address has its admin privileges revoked
	event SonaAdminRevoked(address account);

	/*//////////////////////////////////////////////////////////////
                               CONSTANTS
    //////////////////////////////////////////////////////////////*/
	/// @dev The bytes that represent an admin role
	bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

	/*//////////////////////////////////////////////////////////////
                               FUNCTIONS
    //////////////////////////////////////////////////////////////*/

	/// @dev Modifier that ensures the calling wallet has the admin role
	modifier onlySonaAdmin() {
		_checkRole(ADMIN_ROLE);
		_;
	}

	/// @dev Checks if the specified address is a Sona admin
	/// @param account The address to check
	function isSonaAdmin(address account) public view returns (bool) {
		return hasRole(ADMIN_ROLE, account);
	}

	/// @dev Grants a new address the admin role
	/// @param account The address to make an admin
	function grantSonaAdminRole(address account) public onlyRole(getRoleAdmin(ADMIN_ROLE)) {
		if (!isSonaAdmin(account)) {
			grantRole(ADMIN_ROLE, account);

			emit SonaAdminGranted(account);
		}
	}

	/// @dev Revokes the admin role from an address
	/// @param account The address to remove the admin role from
	function revokeSonaAdminRole(address account) public onlyRole(getRoleAdmin(ADMIN_ROLE)) {
		if (isSonaAdmin(account)) {
			revokeRole(ADMIN_ROLE, account);

			emit SonaAdminRevoked(account);
		}
	}
}

