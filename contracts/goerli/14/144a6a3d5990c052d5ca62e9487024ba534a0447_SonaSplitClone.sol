import "openzeppelin/token/common/ERC2981.sol";
import { OperatorFilterer } from "closedsea/OperatorFilterer.sol";
import { ERC20 } from "openzeppelin/token/ERC20/ERC20.sol";
import { SafeTransferLib } from "solady/utils/SafeTransferLib.sol";
import { Clone } from "solady/utils/Clone.sol";
import { ISplitMain } from "./ISplitMain.sol";
import { ISonaSplitClone } from "../interfaces/ISonaSplitClone.sol";
import { SonaMinter } from "../access/SonaMinter.sol";

contract SonaSplitClone is Clone, ISonaSplitClone, SonaMinter, ERC2981, OperatorFilterer {
	using SafeTransferLib for address;

	address public constant ETH_ADDRESS = address(0);

	ISplitMain public splitMain;
	address internal _liquidSplitFactory;

	address public payoutSplit;
	uint256 public constant DISTRIBUTOR_FEE_OFFSET = 0;

	uint32 public constant ARTIST_SHARE = 50;
	uint32 public constant COLLECTOR_SHARE = 50;

	address private _artistAccount;

	/*//////////////////////////////////////////////////////////////
                               FUNCTIONS
    //////////////////////////////////////////////////////////////*/

	/// @dev Resolves conflict between SonaMinter and ERC2981 interfaces
	function supportsInterface(bytes4 interfaceId) public view virtual override(SonaMinter, ERC2981) returns (bool) {
		return SonaMinter.supportsInterface(interfaceId) || ERC2981.supportsInterface(interfaceId);
	}

	/// @dev equivalent to uint32 public immutable distributorFee;
	function distributorFee() public pure returns (uint32) {
		return _getArgUint32(DISTRIBUTOR_FEE_OFFSET);
	}

	/// @dev Initializes the contract
	function initialize(string memory _name, string memory _symbol, string memory _baseURI, string memory _contractURI) public initializerERC721A {
		// Prevent double initialization.
		if (_nextTokenId() != 0) revert SonaSplit_Unauthorized();

		// Save the factory address so we can ensure only the factory initializes the split.
		_liquidSplitFactory = msg.sender;

		// Initialize ERC721A
		__ERC721A_init(_name, _symbol);

		// Subscribe to OpenSea operator filtering
		_registerForOperatorFiltering();

		emit Initialized(msg.sender, _name, _symbol);
	}

	function initializeSplit(address artistAccount, address collectorAccount, address split) external {
		// Prevent double initialization.
		if (_nextTokenId() != 0) revert SonaSplit_Unauthorized();

		// Save the split address so we can distribute funds
		splitMain = ISplitMain(split);

		// TODO set the royalty fee info
		//_setDefaultRoyalty(receiver, feeNumerator);

		// Only let our factory initialize the split
		if (msg.sender != _liquidSplitFactory) {
			revert SonaSplit_Unauthorized();
		}

		// Set artist information
		_artistAccount = artistAccount;

		// Mint the collector their NFT
		_mint(collectorAccount, 1);
	}

	/// @dev Distributes ETH & ERC20s to split holders
	/// @param token ETH (0x0) or ERC20 token to distribute
	/// @param accounts Ordered, unique list of NFT holders
	/// @param distributorAddress Address to receive distributorFee
	function distributeFunds(address token, address[] calldata accounts, address distributorAddress) external virtual {
		uint256 numRecipients = accounts.length;
		uint32[] memory percentAllocations = new uint32[](numRecipients);
		for (uint256 i; i < numRecipients; ) {
			percentAllocations[i] = scaledPercentBalanceOf(accounts[i]);
			unchecked {
				++i;
			}
		}

		// atomically deposit funds, update recipients to reflect current NFT holders, and distribute
		if (token == ETH_ADDRESS) {
			payoutSplit.safeTransferETH(address(this).balance);
			splitMain.updateAndDistributeETH({
				split: payoutSplit,
				accounts: accounts,
				percentAllocations: percentAllocations,
				distributorFee: distributorFee(),
				distributorAddress: distributorAddress
			});
		} else {
			token.safeTransfer(payoutSplit, ERC20(token).balanceOf(address(this)));
			splitMain.updateAndDistributeERC20({
				split: payoutSplit,
				token: ERC20(token),
				accounts: accounts,
				percentAllocations: percentAllocations,
				distributorFee: distributorFee(),
				distributorAddress: distributorAddress
			});
		}
	}

	/// @dev Determines what percent of a split an account should get
	function scaledPercentBalanceOf(address account) public view virtual returns (uint32) {
		// Artist gets 50% of the split
		if (_artistAccount == account) {
			return ARTIST_SHARE;
		}

		// The owner of the royalty token represents the other 50% of the split
		return uint32(balanceOf(account) * COLLECTOR_SHARE);
	}

	/// @dev Reads an immutable arg with type uint32.
	function _getArgUint32(uint256 argOffset) internal pure returns (uint32 arg) {
		uint256 offset = _getImmutableArgsOffset();
		assembly {
			arg := shr(0xe0, calldataload(add(offset, argOffset)))
		}
	}

	/// @dev Sums array of uint32s
	/// @param numbers Array of uint32s to sum
	/// @return sum Sum of `numbers`
	function _getSum(uint32[] calldata numbers) internal pure returns (uint32 sum) {
		uint256 numbersLength = numbers.length;
		for (uint256 i; i < numbersLength; ) {
			sum += numbers[i];
			unchecked {
				// overflow should be impossible in for-loop index
				++i;
			}
		}
	}
}

