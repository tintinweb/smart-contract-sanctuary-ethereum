contract IndieBrokerV1 is EIP712 {
    using ECDSA for bytes32;

    event ProjectSprintCompleted(uint256 indexed projectId, uint256 indexed sprintId);

    enum ProjectStatus {
        Started,
        Completed,
        Cancelled
    }

    struct Project {
        uint256 projectId;
        address leadAddress;
        address clientAddress;
    }

    mapping(uint256 => Project) public projects;

    // projectId => USDC balance
    mapping(uint256 => uint256) public projectBalances;

    // projectId => sprintId => completed
    mapping(uint256 => mapping(uint256 => bool)) public completedSprints;

    IUSDC public usdc;

    constructor(IUSDC _usdc) EIP712("IndieBroker", "1") {
        usdc = _usdc;
    }

    function startProject(uint256 projectId, address leadAddress, address clientAddress) external {
        projects[projectId] = Project(projectId, leadAddress, clientAddress);
    }

    function sendDeposit(
        uint256 projectId,
        address from,
        uint256 amount,
        uint256 validAfter,
        uint256 validBefore,
        bytes32 nonce,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {
        usdc.receiveWithAuthorization(from, address(this), amount, validAfter, validBefore, nonce, v, r, s);
        projectBalances[projectId] += amount;
    }

    function completeSprint(
        uint256 projectId,
        uint256 sprintId,
        address[] memory payees,
        uint256[] memory amounts,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {
        require(!completedSprints[projectId][sprintId], "IndieBrokerV1: Sprint already completed");

        Project memory project = projects[projectId];

        uint256 totalAmount = 0;
        for (uint256 i = 0; i < amounts.length;) {
            totalAmount += amounts[i];

            unchecked {
                i++;
            }
        }

        bytes32 structHash = keccak256(
            abi.encode(
                keccak256("CompleteSprint(uint256 projectId,uint256 sprintId,uint256 totalAmount)"),
                projectId,
                sprintId,
                totalAmount
            )
        );

        bytes32 digest = _hashTypedDataV4(structHash);

        require(
            _verifySignature(digest, v, r, s, project.clientAddress),
            "The client sprint acceptance signature is invalid"
        );

        uint256 projectBalance = projectBalances[projectId];

        for (uint256 i = 0; i < payees.length; i++) {
            address payee = payees[i];
            uint256 amount = amounts[i];
            projectBalance -= amount;
            usdc.transfer(payee, amount);
        }

        projectBalances[projectId] = projectBalance;
        completedSprints[projectId][sprintId] = true;

        emit ProjectSprintCompleted(projectId, sprintId);
    }

    function _verifySignature(bytes32 digest, uint8 v, bytes32 r, bytes32 s, address signerAddress)
        internal
        pure
        returns (bool)
    {
        (address signedHashAddress, ECDSA.RecoverError error) = ECDSA.tryRecover(digest, v, r, s);

        if (error == ECDSA.RecoverError.NoError) {
            return signedHashAddress == signerAddress;
        } else {
            return false;
        }
    }
}