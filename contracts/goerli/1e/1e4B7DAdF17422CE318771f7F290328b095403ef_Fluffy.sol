contract Erised is
    UUPSUpgradeable,
    ReentrancyGuardUpgradeable,
    OwnableUpgradeable,
    PausableUpgradeable,
    ERC1155Upgradeable,
    ERC1155SupplyUpgradeable
{
    IERC20 public bank;
    mapping(address => bool) guards; // guard contracts for calling functions
    uint256 public taxRate; // tax decimals are 6
    uint256 public taxPool; // tax pool
    mapping(uint256 => uint256) deposits; // company deposite map
    mapping(uint256 => uint256) revenues; // company income map
    mapping(uint256 => address[]) holders; // company stock holders
    mapping(uint256 => bool) forbidPersonalTransfer; // forbid personal transfer
    uint256 private _currentTokenId; // token id counter

    event SetGuard(address indexed guard, bool isGuard);
    event SetTaxRate(uint256 taxRate);
    event WithdrawTaxPool(address indexed account, uint256 money);
    event SetForbidPersonalTransfer(uint256 indexed id, bool forbid);
    event GainRevenue(
        address indexed account,
        uint256 indexed id,
        uint256 money,
        uint256 tax
    );
    event PayTo(uint256 indexed id, address indexed to, uint256 money);
    event InnerPayTo(
        uint256 indexed fromId,
        uint256 indexed toId,
        uint256 money,
        uint256 tax
    );
    event InvestTo(address indexed account, uint256 indexed id, uint256 money);
    event ShareBenefits(uint256 indexed id, address holder, uint256 money);
    event ShareBenefitsTotal(uint256 indexed id, uint256 money);
    event RevenueToDeposit(uint256 indexed id, uint256 money);
    event TransferShares(
        address indexed from,
        address indexed to,
        uint256 indexed id,
        uint256 shares
    );
    event Establish(
        address indexed owner,
        uint256 indexed id,
        uint256 shares,
        uint256 deposite
    );
    event Join(
        address indexed account,
        uint256 indexed id,
        uint256 shares,
        uint256 deposite
    );
    event BurnShares(
        address indexed account,
        uint256 indexed id,
        uint256 shares
    );

    modifier onlyGuard() {
        require(guards[_msgSender()], "Erised: caller is not a guard");
        _;
    }

    function initialize(address _bank) public initializer {
        __UUPSUpgradeable_init();
        __ReentrancyGuard_init();
        __Ownable_init();
        __Pausable_init();
        __ERC1155_init("");
        __ERC1155Supply_init();
        bank = IERC20(_bank);
    }

    // --- settings ---
    function version() public pure returns (string memory) {
        return "1.0.0";
    }
    // get logic contract address
    function implementation() public view returns (address impl) {
        return _getImplementation();
    }

    // stop any transcations
    function setPause(bool _isPaused) external onlyOwner {
        if (_isPaused) {
            _pause();
        } else {
            _unpause();
        }
    }

    // set guard contracts
    function setGuard(address _guard, bool _status) public onlyOwner {
        guards[_guard] = _status;
        emit SetGuard(_guard, _status);
    }

    function getGuard(address _guard) public view returns (bool) {
        return guards[_guard];
    }

    // set income tax
    function setTaxRate(uint256 _taxRate) public onlyOwner {
        taxRate = _taxRate;
        emit SetTaxRate(_taxRate);
    }

    // withdraw tax pool
    function withdrawTaxPool(uint256 money) public onlyOwner {
        require(taxPool >= money, "Erised: not enough tax pool");
        taxPool -= money;
        bank.transfer(owner(), money);
        emit WithdrawTaxPool(owner(), money);
    }

    // calculate tax
    function calTax(uint256 _amount) public view returns (uint256) {
        return (_amount * taxRate) / 1000000;
    }

    // --- company activities ---
    // set forbid personal transfer
    function setForbidPersonalTransfer(
        uint256 id,
        bool _forbiden
    ) public onlyGuard {
        forbidPersonalTransfer[id] = _forbiden;
        emit SetForbidPersonalTransfer(id, _forbiden);
    }

    // get forbid personal transfer status
    function getForbidPersonalTransfer(uint256 id) public view returns (bool) {
        return forbidPersonalTransfer[id];
    }

    // get the deposit by id
    function getDeposit(uint256 id) public view returns (uint256) {
        return deposits[id];
    }

    // get the revenue by id
    function getRevenue(uint256 id) public view returns (uint256) {
        return revenues[id];
    }

    // get the holders by id
    function getHolders(uint256 id) public view returns (address[] memory) {
        address[] memory validateHolders = new address[](holders[id].length);
        uint256 count = 0;
        for (uint256 i = 0; i < holders[id].length; i++) {
            if (balanceOf(holders[id][i], id) > 0) {
                validateHolders[count++] = holders[id][i];
            }
        }
        address[] memory result = new address[](count);
        for (uint256 i = 0; i < count; i++) {
            result[i] = validateHolders[i];
        }
        return result;
    }

    // gain revenue from an address
    function gainRevenue(uint256 id, uint256 money) public whenNotPaused {
        uint256 tax = calTax(money);
        revenues[id] += money - tax;
        taxPool += tax;
        bank.transferFrom(_msgSender(), address(this), money);
        emit GainRevenue(_msgSender(), id, money, tax);
    }

    function payTo(
        uint256 id,
        address to,
        uint256 money
    ) public whenNotPaused onlyGuard {
        require(deposits[id] >= money, "Erised: not enough deposite");
        deposits[id] -= money;
        bank.transfer(to, money);
        emit PayTo(id, to, money);
    }

    // using a company deposite pay to another company as income
    function innerPayTo(
        uint256 fromId,
        uint256 toId,
        uint256 money
    ) public whenNotPaused onlyGuard {
        require(deposits[fromId] >= money, "Erised: not enough deposite");
        require(fromId != toId, "Erised: fromId and toId are same");
        deposits[fromId] -= money;
        uint256 tax = calTax(money);
        revenues[toId] += money - tax;
        taxPool += tax;
        emit InnerPayTo(fromId, toId, money, tax);
    }

    // invest money to a company
    function investTo(uint256 id, uint256 money) public whenNotPaused {
        deposits[id] += money;
