        uint256 actualMoney = 0;
        for (uint256 i = 0; i < holders[id].length; i++) {
            address holder = holders[id][i];
            if (balanceOf(holder, id) == 0) {
                continue;
            }
            uint256 personalIncome = (money * balanceOf(holder, id)) /
                totalSupply(id);
            actualMoney += personalIncome;
            bank.transfer(holder, personalIncome);
            emit ShareBenefits(id, holder, personalIncome);
        }
        revenues[id] -= actualMoney;
        emit ShareBenefitsTotal(id, actualMoney);
    }

