// File: contracts/Fluffy.sol



pragma solidity ^0.8.7;





contract Fluffy is
    UUPSUpgradeable,
    ReentrancyGuardUpgradeable,
    OwnableUpgradeable,
    ERC20Upgradeable
{
    IERC20 public bank;
    Erised public erised;
    address[] public holders;

    event ShareBenefits(address indexed holder, uint256 amount);
    event ShareBenefitsTotal(uint256 amount);

    function initialize(
        address bankAddress,
        address ersiedAddress
    ) public initializer {
        __ERC20_init("Fluffy", "FLF");
        __Ownable_init();
        bank = IERC20(bankAddress);
        erised = Erised(ersiedAddress);
        _mint(msg.sender, 1000000);
    }

    function decimals() public pure override returns (uint8) {
        return 0;
    }

    function version() public pure returns (string memory) {
        return "1.0.0";
    }

    // get logic contract address
    function implementation() public view returns (address impl) {
        return _getImplementation();
    }

    function getHolders() public view returns (address[] memory) {
        address[] memory validateHolders = new address[](holders.length);
        uint256 count = 0;
        for (uint256 i = 0; i < holders.length; i++) {
            if (balanceOf(holders[i]) > 0) {
                validateHolders[count++] = holders[i];
            }
        }
        address[] memory result = new address[](count);
        for (uint256 i = 0; i < count; i++) {
            result[i] = validateHolders[i];
        }
        return result;
    }

    function mint(address account, uint256 amount) public onlyOwner {
        _mint(account, amount);
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) public override onlyOwner returns (bool) {
        require(amount <= balanceOf(from), "Fluffy: Not enough balance");
        _transfer(from, to, amount);
        return true;
    }

    function execute(bytes memory calldatas) public onlyOwner {
        (bool success, bytes memory returnData) = address(erised).call(
            calldatas
        );
        require(success, string(returnData));
    }

    function shareBenefits(uint256 amount) public onlyOwner nonReentrant {
        require(
            amount <= bank.balanceOf(address(this)),
            "Fluffy: Not enough balance"
        );
        uint256 acutalAmount = 0;
        for (uint256 i = 0; i < holders.length; i++) {
            address holder = holders[i];
            if (balanceOf(holder) == 0) {
                continue;
            }
            uint256 personalIncome = (amount * balanceOf(holder)) /
                totalSupply();
            bank.transfer(holder, personalIncome);
            acutalAmount += personalIncome;
            emit ShareBenefits(holder, personalIncome);
        }
        emit ShareBenefitsTotal(acutalAmount);
    }

    /********* forbid functions *********/
    function transfer(address, uint256) public pure override returns (bool) {
        revert("Fluffy: Not allowe to transfer in person");
    }

    function approve(address, uint256) public pure override returns (bool) {
        revert("Fluffy: Not allowe to approve in person");
    }

    function allowance(
        address,
        address
    ) public pure override returns (uint256) {
        revert("Fluffy: Not allowe to check allowance in person");
    }

    /********* private functions *********/
    function _beforeTokenTransfer(
        address,
        address to,
        uint256
    ) internal override {
        if (balanceOf(to) == 0) {
            bool isExist = false;
            for (uint i = 0; i < holders.length; i++) {
                if (holders[i] == to) {
                    isExist = true;
                    break;
                }
            }
            if (!isExist) {
                holders.push(to);
            }
        }
    }

    function _authorizeUpgrade(
        address newImplementation
    ) internal override onlyOwner {}
}