contract OwnableDelegateProxy {}

contract ProxyRegistry {
    mapping(address => OwnableDelegateProxy) public proxies;
}

contract Croats is ERC721, ERC721Enumerable, ERC721Burnable, Ownable, ERC2981 {
    uint256 public constant maxTokenSupply = 2022;
    uint256 public constant maxBAMTokenSupply = 400;
    uint256 public constant maxPerWallet = 12;
    //uint256 public constant price = 1000000000000000000; // This should be 1 Matic
    uint256 public constant price = 10000000000000000; // This should be 0.01 Matic

    string private baseMetadataUri;
    string private BRDMetadataUri;
    string private BAMMetadataUri;
    address private openSeaRegistryAddress;
    mapping(address => uint256) private mintedPerAddress;
    mapping(uint256 => uint256) private tokenIDtoIpfsID;
    mapping(uint256 => uint256) private tokenIDtoIpfsIDTemp;
    mapping(uint256 => bool) private BRDState;
    mapping(uint256 => bool) private BAMState;
    uint256 internal BRDCounter = 0;
    uint256 internal BAMCounter = 0;
    uint256[] IDrepo;
    uint256[] BRDIDrepo;
    uint256[] BAMIDrepo;
    uint256 IDCount;
    uint256 BRDIDCount;
    uint256 BAMIDCount;
    uint256 internal nonce = 0;

    // TODO We should prefill all the numbers after deployment, otherwise it will be unfair distribution and not big enough pool for randomness
    uint256 internal preFillBase = 300;
    uint256 private _currentIndex;
    string public METADATA_PROVENANCE_HASH = ""; // TODO probably obsolere, IPFS hash should be enough

    constructor(
        address openSea,
        string memory base,
        string memory BRDbase,
        string memory BAMbase
    ) ERC721("Lroats", "LRO") {
        openSeaRegistryAddress = openSea;
        baseMetadataUri = base;
        BRDMetadataUri = BRDbase;
        BAMMetadataUri = BAMbase;
        _currentIndex = _startTokenId();
        _setDefaultRoyalty(0xc8A5592031f93dEbeA5D9e67a396944Ee01BB2ca, 500);
        prefillIDs();
    }

    modifier callerIsUser() {
        require(tx.origin == msg.sender, "Cannot be called by a contract");
        _;
    }

    //Declare an Events
    event TokenBurned(address indexed _from, uint _tokenId, string message);

    // interfaces support https://medium.com/@chiqing/ethereum-standard-erc165-explained-63b54ca0d273
    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override(ERC2981, ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    // Minting section
    function mint(uint256 quantity) external payable callerIsUser {
        require(totalSupply() + quantity <= maxTokenSupply, "Cannot exceed total supply");
        require(mintedPerAddress[_msgSender()] + quantity <= maxPerWallet, "Cannot exceed max mint per wallet");

        if (quantity > 4 || mintedPerAddress[_msgSender()] > 4) {
            require(msg.value >= quantity * price, "Must donate some Matic if minting more then 1 item");
        }

        uint256 chosenID;
        uint256 internalCounter = _nextTokenId();

        // loop about to be minted nfts
        for (uint256 i = _nextTokenId(); i < _nextTokenId() + quantity; i++) {
            // up one for total IDs count, this number gives us which number is next to be added
            IDCount++;

            // This is used when we want users to fill numbers to pluck for random
            if (IDCount < maxTokenSupply) {
                IDrepo.push(IDCount);
            }

            // We also need to pluck a number from IDrepo and asign it to tokenIDtoIpfsID
            chosenID = rand() % IDrepo.length;
            tokenIDtoIpfsID[internalCounter] = IDrepo[chosenID];
            removeFromIDrepo(chosenID);
            internalCounter++;
        }

        for (uint256 i = 0; i < quantity; i++) {
            _safeMint(msg.sender, _currentIndex);
            _currentIndex++;
        }
        mintedPerAddress[_msgSender()] += quantity;
    }

    // Randomization section
    function prefillIDs() public onlyOwner {
        //TODO test what happens when this is not run and what happens if pools is empty
        for (uint256 i = 1; i <= preFillBase; i++) {
            IDrepo.push(i);
            BRDIDrepo.push(i);
            BAMIDrepo.push(i);
        }
        IDCount = preFillBase;
        BRDIDCount = preFillBase;
        BAMIDCount = preFillBase;
    }

    function rand() internal virtual returns (uint256) {
        uint256 randomness = uint256(keccak256(abi.encodePacked(msg.sender, block.difficulty, block.timestamp, nonce)));
        nonce++;
        return randomness;
    }

    function removeFromIDrepo(uint256 index) public {
        IDrepo[index] = IDrepo[IDrepo.length - 1];
        IDrepo.pop();
    }

    function removeFromBRDIDrepo(uint256 index) public {
        BRDIDrepo[index] = BRDIDrepo[BRDIDrepo.length - 1];
        BRDIDrepo.pop();
    }

    function removeFromBAMIDrepo(uint256 index) public {
        BAMIDrepo[index] = BAMIDrepo[BAMIDrepo.length - 1];
        BAMIDrepo.pop();
    }

    // URI section
    function setBaseMetadataUri(string memory a) public onlyOwner {
        baseMetadataUri = a;
    }

    function setBRDMetadataUri(string memory a) public onlyOwner {
        BRDMetadataUri = a;
    }

    function setBAMMetadataUri(string memory a) public onlyOwner {
        BAMMetadataUri = a;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseMetadataUri;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {

        if (!_exists(tokenId)) revert URIQueryForNonexistentToken();

        string memory baseURI = baseMetadataUri;
        if (BRDState[tokenId] == false && BAMState[tokenId] == false) {
            baseURI = baseMetadataUri;
        } else if (BRDState[tokenId] == true && BAMState[tokenId] == false) {
            baseURI = BRDMetadataUri;
        } else if (BRDState[tokenId] == false && BAMState[tokenId] == true) {
            baseURI = BAMMetadataUri;
        }
        uint256 ipfsID = tokenIDtoIpfsID[tokenId];

        return bytes(baseURI).length != 0 ? string(abi.encodePacked(baseURI, _toString(ipfsID), ".json")) : "";
    }

    function setProvenanceHash(string memory _hash) public onlyOwner {
        METADATA_PROVENANCE_HASH = _hash;
    }

    // Dynamic NTF functionality
    // Users choose on their own what will collection look in the end

    // Burn Regress Dilute
    function burnRegressDilute(uint256 tokenId) public callerIsUser {
        if (BRDState[tokenId]) revert BRDAlreadyActiviated();
        if (BAMState[tokenId] == true) revert BAMFinalStateActiviated();
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: caller is not token owner nor approved");

        uint256 BRDRandom = rand() % 10;


        if (BRDRandom <= 2) {
            emit TokenBurned(ownerOf(tokenId), tokenId, "Too bad, your token got burned");
            burn(tokenId);

            return;
        } else {
            BRDState[tokenId] = true;
        }

        BRDIDCount++;
        // This is used when we want users to fill numbers to pluck for random
        if (BRDIDCount < maxTokenSupply) {
            BRDIDrepo.push(BRDIDCount);
        }

        // We need to add NEW token ID to tokenIDtoIpfsID that will match other URI location
        uint256 chosenID;
        chosenID = rand() % BRDIDrepo.length;
        tokenIDtoIpfsIDTemp[tokenId] = tokenIDtoIpfsID[tokenId];
        tokenIDtoIpfsID[tokenId] = BRDIDrepo[chosenID];
        removeFromBRDIDrepo(chosenID);
    }

    // Resurect from BRD state
    function burnResurect(uint256 tokenId) public callerIsUser {
        if (BRDState[tokenId] == false) revert BRDNotActiviated();
        if (BAMState[tokenId] == true) revert BAMFinalStateActiviated();
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: caller is not token owner nor approved");

        uint256 BRRandom = rand() % 10;


        if (BRRandom <= 1) {
            emit TokenBurned(ownerOf(tokenId), tokenId, "Too bad, your token got burned");
            burn(tokenId);

            return;
        } else {
            BRDState[tokenId] = false;
            // We just try to bring back the original ID used
            tokenIDtoIpfsID[tokenId] = tokenIDtoIpfsIDTemp[tokenId];
        }
    }

    // Even higher burn chance here, chance to get mutation or advancement
    function burnAdvanceMutate(uint256 tokenId) public callerIsUser {
        if (BAMState[tokenId] == true) revert BAMFinalStateActiviated();
        if (BRDState[tokenId] == true) revert BRDActiviated();
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: caller is not token owner nor approved");

        if (BAMCounter >= maxBAMTokenSupply) revert BAMCounterOverflow();

        uint256 BAMRandom = rand() % 10;


        if (BAMRandom <= 4) {
            emit TokenBurned(ownerOf(tokenId), tokenId, "Too bad, your token got burned");
            burn(tokenId);

            return;
        } else {
            BAMState[tokenId] = true;
        }

        BAMIDCount++;
        // This is used when we want users to fill numbers to pluck for random
        if (BAMIDCount < maxBAMTokenSupply) {
            BAMIDrepo.push(BAMIDCount);
        }

        // We need to add NEW token ID to tokenIDtoIpfsID that will match other URI location
        uint256 chosenID;
        chosenID = rand() % BAMIDrepo.length;
        tokenIDtoIpfsID[tokenId] = BAMIDrepo[chosenID];
        removeFromBAMIDrepo(chosenID);
    }

    // Opensea section
    function setOpenSeaRegistryAddress(address a) public onlyOwner {
        openSeaRegistryAddress = a;
    }

    function isApprovedForAll(address owner, address operator) public view override returns (bool) {
        ProxyRegistry openSeaRegistry = ProxyRegistry(openSeaRegistryAddress);

        if (address(openSeaRegistry.proxies(owner)) == operator) {
            return true;
        }

        return super.isApprovedForAll(owner, operator);
    }

    // Utils
    function _nextTokenId() internal view virtual returns (uint256) {
        // Replaced from ERC721A to match indexer
        return _currentIndex;
    }

    function _startTokenId() internal pure returns (uint256) {
        // What will be the first token ID, usuall 1
        return 1;
    }

    // Withdraw balance if something is added
    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

    /**
     * @dev Converts a uint256 to its ASCII string decimal representation.
     */
    function _toString(uint256 value) internal pure virtual returns (string memory str) {
        assembly {
            // The maximum value of a uint256 contains 78 digits (1 byte per digit), but
            // we allocate 0xa0 bytes to keep the free memory pointer 32-byte word aligned.
            // We will need 1 word for the trailing zeros padding, 1 word for the length,
            // and 3 words for a maximum of 78 digits. Total: 5 * 0x20 = 0xa0.
            let m := add(mload(0x40), 0xa0)
            // Update the free memory pointer to allocate.
            mstore(0x40, m)
            // Assign the `str` to the end.
            str := sub(m, 0x20)
            // Zeroize the slot after the string.
            mstore(str, 0)

            // Cache the end of the memory to calculate the length later.
            let end := str

            // We write the string from rightmost digit to leftmost digit.
            // The following is essentially a do-while loop that also handles the zero case.
            // prettier-ignore
            for { let temp := value } 1 {} {
                str := sub(str, 1)
                // Write the character to the pointer.
                // The ASCII index of the '0' character is 48.
                mstore8(str, add(48, mod(temp, 10)))
                // Keep dividing `temp` until zero.
                temp := div(temp, 10)
                // prettier-ignore
                if iszero(temp) { break }
            }

            let length := sub(end, str)
            // Move the pointer 32 bytes leftwards to make room for the length.
            str := sub(str, 0x20)
            // Store the length.
            mstore(str, length)
        }
    }

    // Add destroy for testing
    function destroy(address apocalypse) public onlyOwner {
        selfdestruct(payable(apocalypse));
    }
}

