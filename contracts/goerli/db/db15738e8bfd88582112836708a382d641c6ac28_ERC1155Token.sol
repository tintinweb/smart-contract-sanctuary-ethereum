        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal virtual {}

    function _doSafeTransferAcceptanceCheck(
        address operator,
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) private {
        if (to.isContract()) {
            try IERC1155Receiver(to).onERC1155Received(operator, from, id, amount, data) returns (bytes4 response) {
                if (response != IERC1155Receiver.onERC1155Received.selector) {
                    revert("ERC1155: ERC1155Receiver rejected tokens");
                }
            } catch Error(string memory reason) {
                revert(reason);
            } catch {
                revert("ERC1155: transfer to non-ERC1155Receiver implementer");
            }
        }
    }

    function _doSafeBatchTransferAcceptanceCheck(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) private {
        if (to.isContract()) {
            try IERC1155Receiver(to).onERC1155BatchReceived(operator, from, ids, amounts, data) returns (
                bytes4 response
            ) {
                if (response != IERC1155Receiver.onERC1155BatchReceived.selector) {
                    revert("ERC1155: ERC1155Receiver rejected tokens");
                }
            } catch Error(string memory reason) {
                revert(reason);
            } catch {
                revert("ERC1155: transfer to non-ERC1155Receiver implementer");
            }
        }
    }

    function _asSingletonArray(uint256 element) private pure returns (uint256[] memory) {
        uint256[] memory array = new uint256[](1);
        array[0] = element;

        return array;
    }
}



contract ERC1155Token is ERC1155{


    string public baseMetadataURI; //the token metadata URI
    struct idDetails{
        string  idName;
        uint256 idNo;
        uint256 totalMinted;
        uint256 maxsupply;
        uint256 price;}

    mapping(uint256   =>  idDetails)    public  idInfo;
    mapping(uint256   =>  bool)         private idAvailable; 




    constructor(
     string         memory _contractName,
     string         memory _contractsymbol,
     string         memory _uri, 
     string     []  memory _names,
     uint256        _ids,
     uint256    []  memory _price,
     uint256    []  memory _maxsupply)ERC1155(_uri){

        createMapping(_ids,_price,_maxsupply,_names);
        _setURI(_uri);
        baseMetadataURI = _uri;
        _name = _contractName;
        _symbol=_contractsymbol;
     }

    function createMapping(uint256 ids,uint256  []memory price_,uint256  []memory maxsupply_,string [] memory names_) private {
        for (uint i = 0; i<ids; i++){
            idInfo[i].maxsupply =   maxsupply_[i];
            idInfo[i].price     =   price_[i];
            idInfo[i].idName    =   names_[i];
            idInfo[i].idNo      =   i;    
        }
    }

    function uri(uint256 _tokenid) override public  view returns  (string memory)  {
        return string(
            abi.encodePacked(
                baseMetadataURI,
                Strings.toString(_tokenid),".json"
            )
        );
    }

    function setURI(string memory newuri) public onlyOwner  {
        _setURI(newuri);
    }

    function mint(address account, uint _id, uint256 amount) 
        public  returns (uint)
    {
        require(idInfo[_id].totalMinted+amount<=idInfo[_id].maxsupply,"supply exceeds");
        _mint(account, _id, amount, "");
        idInfo[_id].totalMinted=idInfo[_id].totalMinted+amount;
        return _id;
    }

    function mintBatch(address to, uint256[] memory _ids, uint256[] memory amounts, bytes memory data)
           public
    {
        uint256 _totalPrice_;
        for (uint256 i=0; i<_ids.length;i++){
        require(idInfo[i].totalMinted+amounts[i]<=idInfo[i].maxsupply,"supply exceeds");
        _totalPrice_=(_totalPrice_+(idInfo[i].price*amounts[i]));} 
        _mintBatch(to, _ids, amounts, data);
        for(uint256 i=0;i<_ids.length;i++){
        idInfo[i].totalMinted=idInfo[i].totalMinted+amounts[i];}
        
    }
}