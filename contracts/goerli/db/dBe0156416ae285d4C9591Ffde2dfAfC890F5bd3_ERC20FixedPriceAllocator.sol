        uint256 claimScheduleStartTimeStamp;
        /// @notice period that tokens are locked before they become claimable
        uint256 claimLockDuration;
        /// @notice period that tokens over which token claims are spread after `claimLockDuration`
        uint256 claimDuration;
        /// @notice Hashed version of the data
        bytes32 inviteCode;
