pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/utils/Strings.sol";


import "./Types.sol";
import "./Venture.sol";
import "./SignatureStore/ERC20FixedPriceAllocatorSignatureStore.sol";
// TODO
import "hardhat/console.sol";

contract ERC20FixedPriceAllocator is Ownable, Initializable {
    /// @notice The name used to identify this Allocator in Dapps
    string public name;

    /// @notice Is the allocator open
    bool public isOpen = true;

    /// @notice Has the escrow amount been claimed
    bool private isEscrowClaimed = false;

    /// @notice Token price
    uint256 public tokenPrice;

    /// @notice Hard cap on allocator
    uint256 public tokensForAllocation;

    /// @notice Hurdle on allocator
    uint256 public hurdle;

    /// @notice The address where Jubi fees will be sent
    address public jubiFundsAddress;

    /// @notice The Jubi fee amount
    uint256 public jubiFee;

    /// @notice The funds a venture will get per investment
    uint256 public ventureFunds;

    /// @notice when the vesting starts
    uint256 public claimScheduleStartTimeStamp;

    /// @notice vesting cliff duration, kicks in after the token
    /// is created and set (ie, after setVentureToken is called)
    uint256 public claimLockDuration;

    /// @notice when the vesting starts
    uint256 public claimDuration;

    /// @notice what token are we accepting in this allocator
    IERC20 public purchaseToken;

    /// @notice what token are we issuing as per vesting conditions
    IERC20 public claimToken;

    /// @notice merkle root that captures all valid invite codes
    bytes32 public inviteCodesMerkleRoot;

    /// @notice maps invitesCodes => wallets
    mapping(bytes32 => address) public claimedInvites;

    /// @notice the Venture that manages this contract
    Venture public venture;

    /// @notice the SignatureStore used by this contract
    ERC20FixedPriceAllocatorSignatureStore public signatureStore;

    /// @notice Allocated tokens to a specific address
    mapping(address => uint256) public allocation;

    /// @notice Total tokens purchased/allocated in this allocator
    uint256 public totalAllocated;

    /// @notice Claimed tokens to a specific address
    mapping(address => uint256) public claimed;

    /// @notice Total Claimed in this allocator
    uint256 public totalClaimed;

    /// @notice This event is emitted an Investment is made
    /// @param account The account that invested
    /// @param investment The amount that was invested
    /// @param allocated The amount that was allocated
    event Purchase(address account, uint256 investment, uint256 allocated);

    /// @notice This event is emitted a Claim is made
    /// @param account The account that claimed
    /// @param amount The amount that was claimed
    event Claimed(address account, uint256 amount);

        /**
    * @notice This event is emitted when an `admin` migrates the venture that manages this allocator
    * from `oldVenture` to `newVenture`
    * @param oldVenture The venture that is been deprecated
    * @param newVenture The venture that is assigned as the new manager
    * @param admin The admin account that perform the migration
    */
    event VentureMigrated(address indexed oldVenture, address indexed newVenture, address admin);

    function initialize(
        address _jubiFundsAddress,
        address _fixedPriceSignatureStoreImpl,
        address _founder,
        Types.Fraction memory _jubiFeePercent,
        Types.ERC20FixedPriceAllocatorConfig memory _config
    ) external initializer {
        _transferOwnership(_config.venture.owner());
        jubiFundsAddress = _jubiFundsAddress;
        name = _config.name;
        Venture newVenture = Venture(_config.venture);
        require(newVenture.tokenSupply() > tokensForAllocation, "FixedPriceAllocator: Venture has insufficient token supply");
        venture = _config.venture;
        tokenPrice = _config.signatureStoreConfig.tokenPrice;
        jubiFee = tokenPrice * _jubiFeePercent.num / _jubiFeePercent.den;
        ventureFunds = tokenPrice - jubiFee;
        tokensForAllocation = _config.tokensForAllocation;
        hurdle = _config.signatureStoreConfig.hurdle;
        claimScheduleStartTimeStamp = _config.signatureStoreConfig.claimScheduleStartTimeStamp;
        claimLockDuration = _config.signatureStoreConfig.claimLockDuration;
        claimDuration = _config.signatureStoreConfig.claimDuration;
        inviteCodesMerkleRoot = _config.inviteCodesMerkleRoot;
        purchaseToken = _config.venture.treasuryToken();
console.log("tokens from contract");
console.log(_config.tokensForAllocation);
console.log(tokensForAllocation);

        isOpen = true;

        signatureStore = ERC20FixedPriceAllocatorSignatureStore(Clones.clone(_fixedPriceSignatureStoreImpl));
        signatureStore.initialize(_config.signatureStoreConfig, name, _founder);
    }

    function purchase(uint256 amount, uint256 minInvestment, uint256 maxInvestment, bytes32 inviteCode, bytes32[] calldata merkleProof, bytes memory signature) external {
        address account = msg.sender;
        require(isOpen, "FixedPriceAllocator: Closed");
        require(MerkleProof.verify(merkleProof, inviteCodesMerkleRoot, keccak256(abi.encode(inviteCode, minInvestment, maxInvestment))), "FixedPriceAllocator: Invalid invite code");
        // We only check minInvestment as, if amount exceeds maxInvestment it will be scale down.
        require(allocationToInvestment(allocation[account]) >= minInvestment || amount >= minInvestment, "FixedPriceAllocator: Can not invest less than minimum amount");
        require(claimedInvites[inviteCode] == account || claimedInvites[inviteCode] == address(0), "FixedPriceAllocator: You can only invest with one wallet per invite code");

        uint256 remainingInvestment = maxInvestment - allocationToInvestment(allocation[account]);
        if (remainingInvestment < amount) {
            amount = remainingInvestment;
        }

        uint256 remainingTokenAllocation = investmentToAllocation(tokensForAllocation) - totalAllocated;
        uint256 purchasedAllocation = investmentToAllocation(amount);

        if (remainingTokenAllocation < purchasedAllocation) {
            purchasedAllocation = remainingTokenAllocation;
            amount = allocationToInvestment(remainingTokenAllocation);
        }

        allocation[account] += purchasedAllocation;
        totalAllocated += purchasedAllocation;
        claimedInvites[inviteCode] = account;

        if (totalAllocated == tokensForAllocation) {
            _close();
        }

        if (totalAllocated >= hurdle) {
            SafeERC20.safeTransferFrom(venture.treasuryToken(), msg.sender, venture.fundsAddress(), ventureFunds * amount);
            SafeERC20.safeTransferFrom(venture.treasuryToken(), msg.sender, jubiFundsAddress, jubiFee * amount);
        } else {
            SafeERC20.safeTransferFrom(purchaseToken, msg.sender, address(this), amount);
        }


        emit Purchase(account, amount, purchasedAllocation);
    }

    function claim() external {
        /* TODO: remove use inline */
        address account = msg.sender;
        require(!isOpen, "FixedPriceAllocator: Can only claim once allocator is closed");

        uint256 claimableAmount = calculateClaimableNow(account);
        require(claimableAmount > 0, "FixedPriceAllocator: Nothing to claim");
        claimed[account] += claimableAmount;
        totalClaimed += claimableAmount;

        SafeERC20.safeTransfer(claimToken, account, claimableAmount);
        emit Claimed(account, claimableAmount);
    }

    function claimEscrow() external onlyOwner {
        require(!isEscrowClaimed, "FixedPriceAllocator: Escrow has already been claimed");
        require(venture.isAdminOrAllocatorManager(msg.sender), "FixedPriceAllocator: only allocator manager can claim escrow funds");
        require(totalAllocated >= hurdle, "FixedPriceAllocator: Sale has not reached hurdle");
        uint256 escrowAmount = purchaseToken.balanceOf(address(this));
        SafeERC20.safeTransferFrom(venture.treasuryToken(), msg.sender, venture.fundsAddress(), ventureFunds * escrowAmount);
        SafeERC20.safeTransferFrom(venture.treasuryToken(), msg.sender, jubiFundsAddress, jubiFee * escrowAmount);
        isEscrowClaimed = true;
    }

    function close() external {
        require(venture.isAdminOrAllocatorManager(msg.sender), "FixedPriceAllocator: only allocator manager can close allocator");
        _close();
    }

    function _close() internal {
        isOpen = false;

        // when the allocator is closed if Hurdle is NOT met set the ventureToken to purchaseToken and remove any vesting so
        // funders can call claimFor to refund their initial investment
        if (totalAllocated <= hurdle) {
            claimToken = purchaseToken;
            // This will make so allocation is claimable right away
            claimScheduleStartTimeStamp = block.timestamp - claimDuration - claimLockDuration;
        } else {
            // If escrow amount is still held in contract, transfer to venture
            if (!isEscrowClaimed) {
                uint256 escrowAmount = purchaseToken.balanceOf(address(this));
                SafeERC20.safeTransferFrom(venture.treasuryToken(), msg.sender, venture.fundsAddress(), ventureFunds * escrowAmount);
                SafeERC20.safeTransferFrom(venture.treasuryToken(), msg.sender, jubiFundsAddress, jubiFee * escrowAmount);
                isEscrowClaimed = true;
            }
        }
    }

    function calculateClaimable(address account, uint256 timestamp) public view returns (uint256 claimableAmount)
    {
        if (claimScheduleStartTimeStamp == 0){
            return 0;
        }
        if(totalAllocated == totalClaimed){
            return 0;
        }
        if(address(claimToken) == address(0)) {
            return 0;
        }

        uint256 vestingStartTimestamp = claimScheduleStartTimeStamp + claimLockDuration;
        if (timestamp < vestingStartTimestamp) {
            return 0;
        }

        uint256 currentclaimDuration = timestamp - vestingStartTimestamp;
        // minimum claimDuration to 1 sec
        uint256 _claimDuration = 1;
        if (claimDuration > 0) {
            _claimDuration = claimDuration;
        }

        if (currentclaimDuration > _claimDuration) {
            currentclaimDuration = _claimDuration;
        }

        claimableAmount = allocation[account] * currentclaimDuration / _claimDuration;
    }

    function setClaimToken(IERC20 _claimToken, bool _setClaimableScheduleStart) external {
        require(venture.isAdminOrAllocatorManager(msg.sender), "FixedPriceAllocator: only allocator manager can set the token");
        require(address(claimToken) == address(0), "FixedPriceAllocator: Claim token already set");
        claimToken = _claimToken;
        if (_setClaimableScheduleStart) {
            claimScheduleStartTimeStamp = block.timestamp;
        }
    }

    /**
    * @notice Migrates current venture to a new venture manager `_newVenture`
    * @param _newVenture The new venture manager for this allocator
    */
    function migrateVenture(address _newVenture) external {
        require(_newVenture != address(0), "FixedPriceAllocator: invalid new venture");
        Venture newVenture = Venture(_newVenture);
        require(newVenture.tokenSupply() > tokensForAllocation, "FixedPriceAllocator: Venture has insufficient token supply");
        require(newVenture != venture, "FixedPriceAllocator: Can not update to same venture");
        require(venture.isAdmin(msg.sender), "FixedPriceAllocator: only venture admin can update allocator");
        require(newVenture.isAdmin(msg.sender), "FixedPriceAllocator: only venture admin can update allocator");
        address oldVenture = address(venture);
        venture = newVenture;
        emit VentureMigrated(oldVenture, address(newVenture), msg.sender);
    }

    function calculateClaimableNow(address account) public view returns (uint256) {
        return calculateClaimable(account, block.timestamp);
    }

    function allocationToInvestment(uint256 _allocation) internal view returns (uint256) {
        return _allocation * tokenPrice;
    }

    function investmentToAllocation(uint256 tokens) internal view returns (uint256) {
        return tokens / tokenPrice;
    }
}

