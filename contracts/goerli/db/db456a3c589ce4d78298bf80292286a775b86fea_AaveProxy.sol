import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./interfaces/ILendingPool.sol";
import "./interfaces/IAToken.sol";
import "./interfaces/ILendingPoolAddressesProvider.sol";
import "./interfaces/IProtocolDataProvider.sol";
import "hardhat/console.sol";

contract AaveProxy {
    ILendingPool public aaveLendingPool; 
    IProtocolDataProvider public provider;

    address public guardian;
    mapping(address => bool) public isWhitelisted;
    bool public isPaused;
    bool public isPermissioned;

    function initialize(address lendingPool, address dataProvider, bool requireWhitelist) external {
        aaveLendingPool = ILendingPool(lendingPool);
        provider = IProtocolDataProvider(dataProvider);
        guardian = msg.sender;
        isPermissioned = requireWhitelist;
    }

    function updateWhitelist(address user, bool whitelistStatus) external {
        require(msg.sender == guardian, "Unauthorized Access");
        isWhitelisted[user] = whitelistStatus;
    }

    function togglePause() external {
        require(msg.sender == guardian, "Unauthorized Access");
        isPaused = !isPaused;
    }

    function aaveDeposit(address token, uint256 amount) external {
        require(!isPaused && (!isPermissioned || isWhitelisted[msg.sender]), 'Not Whitelisted');

        IERC20(token).transferFrom(msg.sender, address(this), amount);
        IERC20(token).approve(address(aaveLendingPool), amount);

        aaveLendingPool.deposit(token, amount, msg.sender, 0);
    }

    function aaveBorrow(address token, uint256 amount) external {
        require(!isPaused && (!isPermissioned || isWhitelisted[msg.sender]), 'Not Whitelisted');
         
        aaveLendingPool.borrow(token, amount, 2, 0, msg.sender);

        uint256 currentAllowance = IERC20(token).allowance(address(this), msg.sender);
        IERC20(token).approve(address(this), currentAllowance + amount);
        IERC20(token).transferFrom(address(this), msg.sender, amount);
    }

    function aaveRepay(address token, uint256 amount) external returns (uint256) {
        require(!isPaused && (!isPermissioned || isWhitelisted[msg.sender]), 'Not Whitelisted');

        IERC20(token).transferFrom(msg.sender, address(this), amount);
        IERC20(token).approve(address(aaveLendingPool), amount);

        aaveLendingPool.repay(token, amount, 2, msg.sender);
    }

    function aaveWithdraw(address token, uint256 amount) external {
        require(!isPaused && (!isPermissioned || isWhitelisted[msg.sender]), 'Not Whitelisted');
        
        (address aToken, , ) = provider.getReserveTokensAddresses(token);
        IAToken(aToken).transferFrom(msg.sender, address(this), amount);

        aaveLendingPool.withdraw(token, amount, msg.sender);
    }

}

