pragma solidity ^0.8.9;

uint256 constant MAX_ESCROW_FEE = 1e18;
uint256 constant MAX_ESCROW_PENDING_PERIOD = 6 * 30 days;
uint256 constant MIN_ESCROW_PENDING_PERIOD = 7 days;
uint256 constant REQUIRED_SIGNATURE_COUNT = 2;

uint256 constant MAX_ESCROW_LOCK_PERIOD = 12 * 30 days;
uint256 constant MIN_ESCROW_LOCK_PERIOD = 6 * 30 days;

enum PaymentType {
    PAYMENT_ETH,
    PAYMENT_USDC
}

enum OrderStatus {
    ORDER_NONE,
    ORDER_CREATED,
    ORDER_PENDING,
    ORDER_TRANSIT,
    ORDER_DELIVERED,
    ORDER_COMPLETED,
    ORDER_CANCELLED,
    ORDER_DISPUTE,
    ORDER_DISPUTE_HANDLED,
    ORDER_ADMIN_WITHDRAWN
}

enum OrderType {
    ESCROW,
    DIRECT
}

struct OrderInfo {
    bytes32 id;
    address buyer;
    address seller;
    OrderStatus status;
    PaymentType paymentType;
    address paymentToken;
    uint256 amount;
    OrderType orderType;
    bytes sellerSignature;
    uint128 createdAt;
    uint128 cancelledAt;
    uint128 completedAt;
    uint128 disputedAt;
}

struct Sig {
    uint8 v;
    bytes32 r;
    bytes32 s;
}

