        keccak256(
            "Release(bytes32 id,address buyer,address seller,uint256 orderStatus,address winner)"
        );

    bytes32 public domainSeperator = 
        keccak256(
            abi.encode(
                DOMAIN_TYPEHASH, 
                keccak256(bytes(CONTRACT_NAME)), 
                getChainId(), 
                address(this)
