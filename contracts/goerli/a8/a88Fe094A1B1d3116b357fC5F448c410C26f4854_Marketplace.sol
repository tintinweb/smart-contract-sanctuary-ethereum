pragma solidity ^0.8.14;

interface events {
    event buy(
        address seller,
        uint256 counter,
        address buyer,
        uint256 amountBought,
        uint256 transferAmount,
        address currency
    );
    event royalty(uint96 royaltyAmount);
    event platFormFee(uint256 feeAmount);
    event newAdmin(address newAdmin);

    event globalOfferMade(
        uint256 offerNumber,
        address offerer,
        uint256 tokenId,
        uint256 offeredPrice
    );

    event singleOfferMade(
        uint256 offerNumber,
        address offerer,
        uint256 tokenId,
        uint256 offeredPrice,
        uint256 quantityAsked,
        address offeree
    );

    event offerUpgrade(
        uint256 _tokenId,
        uint256 _offerNumber,
        uint256 _newOfferedPrice,
        bool _isGlobal
    );

    event offerAccepted(uint256 offerNumber, address offeree, uint256 quantity);
    event refundClaimed(
        uint256 _tokenId,
        uint256 _offerNumber,
        uint256 refundAmount,
        bool _isGlobal
    );
    event redeemedToken(uint256 _tokenId);
}

