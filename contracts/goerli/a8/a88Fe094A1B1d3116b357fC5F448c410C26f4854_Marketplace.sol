pragma solidity ^0.8.14;

import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router01.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "./Interfaces/IERC20.sol";
import "./Interfaces/INFT.sol";
import "./Library/vouchers.sol";
import "./Interfaces/events.sol";

contract Marketplace is EIP712Upgradeable, events, Ownable {
    // Admin address of the contract
    address public admin;
    // Platform fee amount
    uint256 public platformFee;
    // Address for the NFT contract
    address NFTContract;
    // Address for the platform fee wallet
    address platformFeeWallet;
    // Address for recieving the value of NFT
    address valueWallet;
    // Address for USDT contract
    address usdt;
    // Address for the USDC contract
    address usdc;
    // Address for the uniswap router
    address uniswapRouter;
    // Address for the uniswap factory
    address uniswapFactory;
    // Address for weth
    address weth;
    // Mapping for used counter numbers
    mapping(uint256 => bool) public usedCounters;
    // Mapping of the counter to the amount left in voucher
    mapping(uint256 => uint256) public amountLeft;
    // Mapping of white labled currencies
    mapping(address => bool) private allowedCurrencies;
    // Mapping for the operator role
    mapping(address => bool) operators;
    /**
     * @notice  .
     * @dev     .
     */
    modifier onlyOperator() {
        require(operators[msg.sender], "NO"); //Not Operator
        _;
    }

    /**
     * @notice  .
     * @dev Initializes the contract by setting a `admin`, `NFT`, and `platformfee` for the marketplace
     * @param _admin is set as the admin of the marketplace
     * @param _NFT is set as the NFT contract of thr marketplace
     * @param _platformFee is set as the platformFee for the marketplace
     * @param   _platformFeeWallet  .
     * @param   _valueWallet  .
     * @param   _usdc  .
     * @param   _usdt  .
     * @param   _uniswapRouter  .
     * @param   _uniswapFactory  .
     */
    function initialize(
        address _admin,
        address _NFT,
        uint256 _platformFee,
        address _platformFeeWallet,
        address _valueWallet,
        address _usdc,
        address _usdt,
        address _uniswapRouter,
        address _uniswapFactory
    ) external initializer {
        require(_admin != address(0), "ZAA"); //Zero Address for Admin
        require(_NFT != address(0), "ZAN"); //Zero Address for NFT contract
        __EIP712_init("Chroncept_MarketItem", "1");
        admin = _admin;
        NFTContract = _NFT;
        platformFee = _platformFee;
        usdt = _usdt;
        usdc = _usdc;
        uniswapRouter = _uniswapRouter;
        uniswapFactory = _uniswapFactory;
        platformFeeWallet = _platformFeeWallet;
        valueWallet = _valueWallet;
        operators[_admin] = true;
        allowedCurrencies[usdc] = true;
        allowedCurrencies[usdt] = true;
        weth = IUniswapV2Router01(uniswapRouter).WETH();
        INFT(NFTContract).setPlatformfee(_platformFee);
    }

    /**
     * @notice Returns a hash of the given shareSeller, prepared using EIP712 typed data hashing rules.
     * @dev     .
     * @param seller is of type shareSeller provided to hash.
     * @return  bytes32  .
     */
    function hashShareSeller(
        voucher.shareSeller memory seller
    ) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        keccak256(
                            "shareSeller(address seller,address NFTAddress,uint256 tokenId,uint256 shareSellAmount,uint256 sharePrice,uint256 counter,string tokenUri)"
                        ),
                        seller.seller,
                        seller.NFTAddress,
                        seller.tokenId,
                        seller.shareSellAmount,
                        seller.sharePrice,
                        seller.counter,
                        keccak256(bytes(seller.tokenUri))
                    )
                )
            );
    }

    /**
     * @notice Verifies the signature for a given shareSeller, returning the address of the signer.
     * @dev Will revert if the signature is invalid. Does not verify that the signer is owner of the NFT.
     * @param seller is a shareSeller describing the NFT to be sold.
     * @return  address  of the account that signed the voucher.
     */
    function verifyShareSeller(
        voucher.shareSeller memory seller
    ) internal view returns (address) {
        bytes32 digest = hashShareSeller(seller);
        return ECDSAUpgradeable.recover(digest, seller.signature);
    }

    /**
     * @notice Used to buy the shares of the NFT through multiple currencies
     * @dev `seller`, `amountToBuy`, `isPrimary` & `currency` will be used in case of buying in both primary and secondary buy
     * @param seller is a shareSeller describing the NFT to be be sold
     * @param amountToBuy is amount on shares to be baought
     * @param isPrimary is a bool to indicate whether the buy is primary or secondary
     * @param currency is the address of the currency to be used
     */
    function buyShare(
        voucher.shareSeller memory seller,
        uint256 amountToBuy,
        bool isPrimary,
        address currency
    ) external payable {
        address sellerAddress = verifyShareSeller(seller);
        require(seller.seller == sellerAddress, "IS"); //Invalid Seller
        setCounter(seller, amountToBuy);
        uint256 transferAmount;
        if (isPrimary) {
            require(sellerAddress == admin, "NA"); //Not Admin

            if (currency == address(1)) {
                uint256 price = getPrice(
                    (seller.sharePrice * amountToBuy),
                    currency
                );
                transferAmount = price;

                require(
                    msg.value >=
                        getPrice((seller.sharePrice * amountToBuy), currency),
                    "IA"
                ); // Invalid Amount

                uint256 dustETH = msg.value - price;

                uint256 feeAmount = (price * platformFee) / 10000;

                (bool sentAmount, ) = payable(valueWallet).call{
                    value: (price - feeAmount)
                }("");

                require(sentAmount, "ATF"); //Amount Transfer Failed
                (bool sentFee, ) = payable(platformFeeWallet).call{
                    value: feeAmount
                }("");

                require(sentFee, "FTF"); //Fee Transfer Failed
                (bool dust, ) = payable(msg.sender).call{value: dustETH}("");

                require(dust, "DTF"); //Dust ETH transfer failed
            } else if (currency == usdc) {
                require(allowedCurrencies[currency], "IC"); //Invalid Currency

                uint256 amountToPay = seller.sharePrice * amountToBuy;
                transferAmount = amountToPay;

                uint256 finalFeeAmount = (platformFee * amountToPay) / 10000;

                IERC20(currency).transferFrom(
                    msg.sender,
                    platformFeeWallet,
                    finalFeeAmount
                );

                IERC20(currency).transferFrom(
                    msg.sender,
                    valueWallet,
                    (amountToPay - finalFeeAmount)
                );
            } else {
                require(allowedCurrencies[currency], "IC"); //Invalid Currency

                uint256 amountToPay = getPrice(
                    (seller.sharePrice * amountToBuy),
                    currency
                );
                transferAmount = amountToPay;

                uint256 finalFeeAmount = (platformFee * amountToPay) / 10000;

                IERC20(currency).transferFrom(
                    msg.sender,
                    platformFeeWallet,
                    finalFeeAmount
                );

                IERC20(currency).transferFrom(
                    msg.sender,
                    valueWallet,
                    (amountToPay - finalFeeAmount)
                );
            }

            INFT(NFTContract).lazyMintNFT(
                seller.tokenId,
                amountToBuy,
                msg.sender,
                seller.tokenUri
            );
            emit events.buy(
                seller.seller,
                seller.counter,
                msg.sender,
                amountToBuy,
                transferAmount,
                currency
            );
        } else {
            require(
                INFT(NFTContract).balanceOf(seller.seller, seller.tokenId) >=
                    amountToBuy,
                "ISB"
            ); //Insufficient Share Balance

            if (currency == address(1)) {
                uint256 price = getPrice(
                    (seller.sharePrice * amountToBuy),
                    currency
                );
                transferAmount = price;
                require(msg.value >= price, "IA"); // Invalid Amount
                uint256 dustETH = msg.value - price;
                uint256 feeAmount = (price * platformFee) / 10000;
                (address reciever, uint256 royaltyAmount) = INFT(NFTContract)
                    .royaltyInfo(seller.tokenId, msg.value);
                uint256 ETHAmount = price - (feeAmount + royaltyAmount);
                (bool sentAmount, ) = payable(valueWallet).call{
                    value: ETHAmount
                }("");
                require(sentAmount == true, "ATF"); //Amount Transfer Failed
                (bool sentFee, ) = payable(platformFeeWallet).call{
                    value: feeAmount
                }("");
                require(sentFee == true, "FTF"); //Fee Transfer Failed
                (bool sentRoyalty, ) = payable(reciever).call{
                    value: royaltyAmount
                }("");
                require(sentRoyalty == true, "RTF"); //Royalty Transfer Failed
                (bool dust, ) = payable(msg.sender).call{value: dustETH}("");
                require(dust, "DTF"); //Dust ETH transfer failed
            } else if (currency == usdc) {
                require(allowedCurrencies[currency], "IC"); //Invalid Currency
                uint256 amountToPay = seller.sharePrice * amountToBuy;
                transferAmount = amountToPay;
                (address reciever, uint256 royaltyAmount) = INFT(NFTContract)
                    .royaltyInfo(seller.tokenId, amountToPay);
                uint256 finalFeeAmount = (platformFee * amountToPay) / 10000;
                IERC20(currency).transferFrom(
                    msg.sender,
                    platformFeeWallet,
                    finalFeeAmount
                );

                IERC20(currency).transferFrom(
                    msg.sender,
                    reciever,
                    royaltyAmount
                );

                IERC20(currency).transferFrom(
                    msg.sender,
                    seller.seller,
                    (amountToPay - (royaltyAmount + finalFeeAmount))
                );
            } else {
                require(allowedCurrencies[currency], "IC"); //Invalid Currency
                uint256 amountToPay = getPrice(
                    (seller.sharePrice * amountToBuy),
                    currency
                );
                transferAmount = amountToPay;
                (address reciever, uint256 royaltyAmount) = INFT(NFTContract)
                    .royaltyInfo(seller.tokenId, amountToPay);
                uint256 finalFeeAmount = (platformFee * amountToPay) / 10000;
                IERC20(currency).transferFrom(
                    msg.sender,
                    platformFeeWallet,
                    finalFeeAmount
                );

                IERC20(currency).transferFrom(
                    msg.sender,
                    reciever,
                    royaltyAmount
                );

                IERC20(currency).transferFrom(
                    msg.sender,
                    seller.seller,
                    (amountToPay - (royaltyAmount + finalFeeAmount))
                );
            }

            IERC1155Upgradeable(NFTContract).safeTransferFrom(
                seller.seller,
                msg.sender,
                seller.tokenId,
                amountToBuy,
                ""
            );
            emit events.buy(
                seller.seller,
                seller.counter,
                msg.sender,
                amountToBuy,
                transferAmount,
                currency
            );
        }
    }

    /**
     * @notice Function to add new operator for the platform.
     * @dev `newOperator` will be used to set a new operator.
     * @param _newOperator is the new operators address.
     */
    function addOperator(address _newOperator) external onlyOperator {
        require(_newOperator != address(0), "ZA"); //Zero Address
        operators[_newOperator] = true;
    }

    /**
     * @notice Function to add new currency token for the platform.
     * @dev `newToken` will be used to add a new buy currency.
     * @param _newToken is the new currency address.
     */
    function addToken(address _newToken) external onlyOperator {
        require(_newToken != address(0), "ZA"); //Zero Address
        allowedCurrencies[_newToken] = true;
    }

    /**
     * @notice Function to set new platform fee
     * @dev `newFee` will be used to set new platform fee
     * @param _newFee is the new marketplace fee
     */
    function setPlatformfee(uint256 _newFee) external onlyOperator {
        platformFee = _newFee;
        INFT(NFTContract).setPlatformfee(_newFee);
        emit platFormFee(_newFee);
    }

    /**
     * @notice  Function to set new address for the NFT contract
     * @param   _NFT  is the address of new contract
     */
    function setNFTContract(address _NFT) external onlyOperator {
        require(_NFT != address(0), "ZA"); //Zero Address
        require(msg.sender == admin, "NA"); //Not Admin
        NFTContract = _NFT;
    }

    /**
     * @notice Function to set new Admin for the contract
     * @param _newAdmin is the address for the new admin of the contact
     */
    /**
     * @notice  .
     * @dev     .
     * @param   _newAdmin  .
     */
    function setAdmin(address _newAdmin) external onlyOperator {
        require(_newAdmin != address(0), "ZA"); //Zero Address
        require(msg.sender == admin, "NA"); //Not Admin
        admin = _newAdmin;
        operators[admin] = true;
        emit events.newAdmin(_newAdmin);
    }

    /**
     */
    /**
     * @notice  .
     * @dev     .
     * @param   amountOut  .
     * @param   token  .
     * @return  amountIn  .
     */
    function getPrice(
        uint256 amountOut,
        address token
    ) public view returns (uint256 amountIn) {
        address pair = token == address(1)
            ? IUniswapV2Factory(uniswapFactory).getPair(usdc, weth)
            : IUniswapV2Factory(uniswapFactory).getPair(usdc, token);

        address token0 = IUniswapV2Pair(pair).token0();
        (uint256 reserves0, uint256 reserves1, ) = IUniswapV2Pair(pair)
            .getReserves();

        (uint256 reservesA, uint256 reservesB) = token0 == usdc
            ? (reserves1, reserves0)
            : (reserves0, reserves1);

        amountIn = (reservesA * amountOut) / reservesB;
    }

    /**
     * @notice This is the internal function used to set the counter for the seller
     * @param seller is a shareSeller describing the NFT to be sold
     * @param amountToBuy is amount of shares of NFT to be bought
     */
    /**
     * @notice  .
     * @dev     .
     * @param   seller  .
     * @param   amountToBuy  .
     */
    function setCounter(
        voucher.shareSeller memory seller,
        uint256 amountToBuy
    ) internal {
        //Counter used
        require(!usedCounters[seller.counter], "CU");

        uint256 leftCounter = amountLeft[seller.counter];

        if (leftCounter == 0) {
            leftCounter = seller.shareSellAmount - amountToBuy;
        } else {
            leftCounter = leftCounter - amountToBuy;
        }
        require(leftCounter >= 0, "ALZ"); //Amount left less than zero

        amountLeft[seller.counter] = leftCounter;
        if (leftCounter == 0) usedCounters[seller.counter] = true;
    }
}

