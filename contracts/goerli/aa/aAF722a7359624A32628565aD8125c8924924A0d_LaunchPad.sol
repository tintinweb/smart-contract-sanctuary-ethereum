pragma solidity >=0.8.17;

import '@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol';
import '@openzeppelin/contracts/utils/cryptography/ECDSA.sol';
import '@openzeppelin/contracts/utils/math/Math.sol';

import './LaunchPadTicket.sol';
import './VRFV2WrapperConsumerBaseUpgradeable.sol';

contract LaunchPad is
    AccessControlUpgradeable,
    UUPSUpgradeable,
    VRFV2WrapperConsumerBaseUpgradeable
{
    using ECDSA for bytes32;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    event SignerUpdated(address signer);
    event ConfigUpdated(address operator);
    event RevenueShareConfigUpdated(address operator);
    event Deposit(address operator, uint256 shares, uint256 amount);
    event Mint(
        address indexed account,
        WalletType indexed walletType,
        bytes20 indexed accountDigest,
        uint256 shares,
        uint256 paymentAmount
    );
    event DrawLottery(address operator);
    event LotteryDrawn(uint256 winningNumber);
    event Claim(
        address indexed account,
        uint256 tokenAmount,
        uint256 refundAmount
    );
    event DistributeRevenue(
        address operator,
        uint256 coolbitxAmount,
        uint256 projectOwnerAmount,
        uint256 returnedAmount
    );
    event Pause(address manager);
    event Resume(address manager);

    bytes32 public constant MANAGER_ROLE = keccak256('MANAGER_ROLE');
    bytes32 public constant OPERATOR_ROLE = keccak256('OPERATOR_ROLE');
    uint32 public constant VRF_CALLBACK_GAS_LIMIT = 400000;
    uint16 public constant VRF_REQUEST_CONFIRMATIONS = 6;

    enum Phase {
        BeforeSale,
        Sale,
        Lottery,
        Claim
    }

    enum WalletType {
        Hardware,
        Software
    }

    struct Config {
        IERC20Upgradeable token;
        IERC20Upgradeable paymentToken;
        uint256 tokenPerShare;
        uint256 pricePerShare;
        uint256 saleStart;
        uint256 saleEnd;
        uint256 claimStart;
    }

    struct RevenueShareConfig {
        address coolbitxWallet;
        address projectWallet;
        address tokenReturnWallet;
        uint256 coolbitxShareBP; // basis points, coolbitx will get shareBP / 10000
    }

    modifier onlyAdmin() {
        _checkRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _;
    }

    modifier onlyManager() {
        _checkRole(MANAGER_ROLE, _msgSender());
        _;
    }

    modifier onlyOperator() {
        _checkRole(OPERATOR_ROLE, _msgSender());
        _;
    }

    modifier onlyEOA() {
        require(tx.origin == msg.sender, 'not EOA');
        _;
    }

    modifier whenNotPaused() {
        require(!paused, 'paused');
        _;
    }

    LaunchPadTicket public ticket;
    Config public config;
    // wallet type => max share
    mapping(WalletType => uint32) public maxSharePerDigest;
    RevenueShareConfig public revenueShareConfig;
    address public signer;
    bool public paused;
    uint256 public lotteryWinningNumbers;
    uint256 public totalTickets;
    uint256 public totalShares;
    // seed digest => wallet type => owned shares
    mapping(bytes20 => mapping(WalletType => uint256)) public accountShares;
    mapping(bytes => bool) public sigs;
    // ticket id => lottery number
    mapping(uint256 => uint256) public ticketLotteryMap;
    // lottery number => ticket id
    mapping(uint256 => uint256) public lotteryTicketMap;
    // address => accountDigest
    mapping(address => bytes20) public accountDigestMap;
    // VRF
    uint256 public vrfRequestId;
    
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        address launchPadTicket_,
        address link_,
        address vrfV2Wrapper_
    ) external initializer {
        __AccessControl_init();
        __UUPSUpgradeable_init();
        __VRFV2WrapperConsumerBase_init(link_, vrfV2Wrapper_);

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(MANAGER_ROLE, _msgSender());
        _setupRole(OPERATOR_ROLE, _msgSender());

        vrfRequestId = type(uint256).max;
        lotteryWinningNumbers = type(uint256).max;

        ticket = LaunchPadTicket(launchPadTicket_);
    }

    /// ==== VIEW ====

    function phase() public view returns (Phase) {
        if (
            block.timestamp >= config.claimStart &&
            lotteryWinningNumbers != type(uint256).max
        ) {
            return Phase.Claim;
        }
        if (block.timestamp >= config.saleEnd) {
            return Phase.Lottery;
        }
        if (block.timestamp >= config.saleStart) {
            return Phase.Sale;
        }
        return Phase.BeforeSale;
    }

    function digest(
        address account_,
        uint256 share_,
        WalletType walletType_,
        bytes20 accountDigest_,
        string calldata nonce_
    ) public view returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    account_,
                    address(this),
                    share_,
                    walletType_,
                    accountDigest_,
                    nonce_
                )
            );
    }

    /// @notice Get the lottery winner amount of a wallet's shares
    function claimableShares(address wallet_)
        public
        view
        returns (uint256 amount_)
    {
        for (uint256 i = 0; i < ticket.balanceOf(wallet_); i++) {
            if (isWinnerTicket(ticket.tokenOfOwnerByIndex(wallet_, i))) {
                amount_++;
            }
        }
    }

    function isWinnerTicket(uint256 ticketId_) public view returns (bool) {
        if (phase() != Phase.Claim) {
            return false;
        }
        if (lotteryWinningNumbers + totalShares > totalTickets) {
            if (
                ticketLotteryMap[ticketId_] >= lotteryWinningNumbers ||
                ticketLotteryMap[ticketId_] <
                totalShares + lotteryWinningNumbers - totalTickets
            ) {
                return true;
            }
            return false;
        }
        return
            ticketLotteryMap[ticketId_] >= lotteryWinningNumbers &&
            ticketLotteryMap[ticketId_] < lotteryWinningNumbers + totalShares;
    }

    /// ==== Operation Methods ====

    function setConfig(
        Config calldata config_,
        WalletType[] calldata walletTypes_,
        uint32[] calldata maxShares_
    ) external onlyManager {
        require(walletTypes_.length == maxShares_.length, 'invalid input');
        require(
            config.saleStart == 0 || block.timestamp < config.saleStart,
            'sale started'
        );
        require(
            address(config_.token) != address(0),
            'invalid launchpad token'
        );
        require(
            address(config_.paymentToken) != address(0),
            'invalid payment token'
        );
        require(config_.tokenPerShare > 0, 'invalid token per share');
        require(config_.pricePerShare > 0, 'invalid price per share');
        require(config_.saleEnd > config_.saleStart, 'sale end < sale start');
        require(config_.claimStart > config_.saleEnd, 'claim < sale end');
        if (totalShares > 0) {
            require(
                config_.tokenPerShare == config.tokenPerShare,
                'tokenPerShare can not be changed'
            );
        }
        config = config_;
        for (uint256 i = 0; i < walletTypes_.length; i++) {
            maxSharePerDigest[walletTypes_[i]] = maxShares_[i];
        }
        emit ConfigUpdated(_msgSender());
    }

    function setRevenueShareConfig(RevenueShareConfig calldata config_)
        external
        onlyAdmin
    {
        require(config_.coolbitxShareBP <= 10000, 'invalid bp');
        revenueShareConfig = config_;
        emit RevenueShareConfigUpdated(_msgSender());
    }

    function setSigner(address signer_) external onlyAdmin {
        require(signer_ != address(0), 'invalid signer');
        signer = signer_;
        emit SignerUpdated(signer_);
    }

    /// @notice The project owner can use this function to deposit token for sale, the deposited token will be locked in this contract
    /// @param share_ The number of share to deposit for the project token
    function deposit(uint256 share_) external {
        uint256 amount = share_ * config.tokenPerShare;
        require(amount > 0, 'invalid amount');
        require(block.timestamp < config.saleStart, 'sale started');
        config.token.safeTransferFrom(_msgSender(), address(this), amount);
        totalShares += share_;
        emit Deposit(_msgSender(), share_, amount);
    }

    /// @notice Operator draw to draw winner of lottery
    function drawLottery() external onlyOperator {
        require(block.timestamp >= config.saleEnd, 'sale not end');
        require(vrfRequestId == type(uint256).max, 'requested');

        totalTickets = ticket.totalSupply();

        if (totalTickets <= totalShares) {
            lotteryWinningNumbers = 0;
            vrfRequestId = 0;
            emit DrawLottery(_msgSender());
            emit LotteryDrawn(lotteryWinningNumbers);
            return;
        }

        vrfRequestId = requestRandomness(
            VRF_CALLBACK_GAS_LIMIT,
            VRF_REQUEST_CONFIRMATIONS,
            1
        );
        emit DrawLottery(_msgSender());
    }

    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {
        require(_requestId == vrfRequestId, 'invalid requestId');
        lotteryWinningNumbers = _randomWords[0] % totalTickets;
        emit LotteryDrawn(lotteryWinningNumbers);
    }

    /// @notice Distribute payment token to project owner and coolbitx, and return remain token to tokenReturnWallet
    function distributeRevenue() external onlyAdmin {
        require(block.timestamp >= config.saleEnd, 'sale not ended');
        uint256 totalPaymentAmount = Math.min(totalShares, totalTickets) *
            config.pricePerShare;
        uint256 coolbitxAmount = (totalPaymentAmount *
            revenueShareConfig.coolbitxShareBP) / 10000;
        uint256 projectOwnerAmount = totalPaymentAmount - coolbitxAmount;
        config.paymentToken.safeTransfer(
            revenueShareConfig.coolbitxWallet,
            coolbitxAmount
        );
        config.paymentToken.safeTransfer(
            revenueShareConfig.projectWallet,
            projectOwnerAmount
        );
        uint256 remainAmount = 0;
        if (totalShares > totalTickets) {
            remainAmount = (totalShares - totalTickets) * config.tokenPerShare;
            config.token.safeTransfer(
                revenueShareConfig.tokenReturnWallet,
                remainAmount
            );
        }
        emit DistributeRevenue(
            _msgSender(),
            coolbitxAmount,
            projectOwnerAmount,
            remainAmount
        );
    }

    function pause() external onlyManager {
        paused = true;
        emit Pause(_msgSender());
    }

    function resume() external onlyManager {
        paused = false;
        emit Resume(_msgSender());
    }

    /// ==== User Methods ====

    /// @notice User uses this function to mint launchpad lottery ticket
    /// @param share_ The number of shares user want to buy
    /// @param walletType_ The type of wallet
    /// @param accountDigest_ The account digest of the wallet
    /// @param nonce_ Random generate nonce
    /// @param signature_ The signature signed by coolbitx backend
    function mint(
        uint256 share_,
        WalletType walletType_,
        bytes20 accountDigest_,
        string calldata nonce_,
        bytes calldata signature_
    ) external whenNotPaused onlyEOA {
        require(share_ > 0, 'invalid share');
        require(totalShares > 0, 'no shares');
        require(block.timestamp >= config.saleStart, 'sale not start');
        require(block.timestamp < config.saleEnd, 'sale ended');
        require(
            accountShares[accountDigest_][walletType_] + share_ <=
                maxSharePerDigest[walletType_],
            'exceed max share'
        );
        _verify(
            digest(_msgSender(), share_, walletType_, accountDigest_, nonce_),
            signature_
        );
        _validateAccountDigest(accountDigest_);
        accountShares[accountDigest_][walletType_] += share_;
        uint256 paymentAmount = share_ * config.pricePerShare;
        config.paymentToken.safeTransferFrom(
            _msgSender(),
            address(this),
            paymentAmount
        );
        for (uint256 i = 0; i < share_; i++) {
            uint256 ticketId = ticket.mint(_msgSender());

            uint256 seed = uint256(bytes32(bytes(nonce_)));
            uint256 salt = uint256(uint160(msg.sender));
            uint256 lotteryNo = _rand(seed, salt, ticketId + 1);

            // ticket id = ticket.totalSupply() - 1
            if (lotteryNo < ticketId) {
                uint256 preTicketId = lotteryTicketMap[lotteryNo];
                ticketLotteryMap[preTicketId] = ticketId;
                lotteryTicketMap[ticketId] = preTicketId;
            }

            ticketLotteryMap[ticketId] = lotteryNo;
            lotteryTicketMap[lotteryNo] = ticketId;
        }
        emit Mint(
            _msgSender(),
            walletType_,
            accountDigest_,
            share_,
            paymentAmount
        );
    }

    /// @notice User uses this function to claim token after lottery drawn
    function claim() external whenNotPaused onlyEOA {
        require(block.timestamp >= config.claimStart, 'claim not start');
        require(
            lotteryWinningNumbers != type(uint256).max,
            'lottery not drawn'
        );
        uint256 winningShares = 0;
        uint256 refundShares = 0;
        uint256[] memory allTickets = new uint256[](
            ticket.balanceOf(_msgSender())
        );
        for (uint256 i = 0; i < allTickets.length; i++) {
            allTickets[i] = ticket.tokenOfOwnerByIndex(_msgSender(), i);
        }
        for (uint256 i = 0; i < allTickets.length; i++) {
            uint256 ticketId = allTickets[i];
            if (isWinnerTicket(ticketId)) {
                winningShares++;
            } else {
                refundShares++;
            }
            ticket.burn(ticketId);
        }
        if (winningShares > 0) {
            config.token.safeTransfer(
                _msgSender(),
                config.tokenPerShare * winningShares
            );
        }
        if (refundShares > 0) {
            config.paymentToken.safeTransfer(
                _msgSender(),
                config.pricePerShare * refundShares
            );
        }
        emit Claim(
            _msgSender(),
            config.tokenPerShare * winningShares,
            config.pricePerShare * refundShares
        );
    }

    /// ==== Misc ====

    function _rand(
        uint256 seed_,
        uint256 salt_,
        uint256 n_
    ) private view returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        block.difficulty,
                        block.timestamp,
                        seed_,
                        salt_,
                        n_
                    )
                )
            ) % n_;
    }

    function emergencyWithdraw(address token_, uint256 amount_)
        external
        onlyAdmin
    {
        IERC20Upgradeable(token_).safeTransfer(_msgSender(), amount_);
    }

    function _verify(bytes32 hash_, bytes calldata signature_) private {
        require(!sigs[signature_], 'sig used');
        require(
            signer == hash_.toEthSignedMessageHash().recover(signature_),
            'sig mismatch'
        );
        sigs[signature_] = true;
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyAdmin
    {}

    function _validateAccountDigest(bytes20 accountDigest_) private {
        if (accountDigestMap[msg.sender] == bytes20(0)) {
            accountDigestMap[msg.sender] = accountDigest_;
        }
        require(
            accountDigestMap[msg.sender] == accountDigest_,
            'invalid accountDigest'
        );
    }

}

