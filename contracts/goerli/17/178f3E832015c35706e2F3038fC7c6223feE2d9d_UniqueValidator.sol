    INITIAL_ASK_EXCEEDED,
    EMPTY_STATE,
    PUBLIC_VAULT_RECIPIENT,
    COLLATERAL_NOT_LIQUIDATED
  }

  error InvalidState(InvalidStates);
  error InvalidCollateralState(InvalidStates);
}

