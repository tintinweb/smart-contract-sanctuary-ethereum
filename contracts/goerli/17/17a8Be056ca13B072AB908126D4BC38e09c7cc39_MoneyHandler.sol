pragma solidity 0.8.17;

import "../interfaces/IWinrERC20.sol";
import "../interfaces/ILiquidityProvider.sol";
import "../interfaces/IWinrStaking.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "hardhat/console.sol";

contract MoneyHandler is AccessControl {
  /*==================================================== Events =============================================================*/

  event Deposit(address indexed user, uint256 amount, uint256 profitDebt, uint256 period);
  event Withdraw(address indexed user, uint256 amount);

  /*==================================================== State Variables ====================================================*/

  /// @notice Address of token.
  IWinrERC20 public token;
  /// @notice Address of wrapped ether token.
  IERC20 public wETH;
  /// @notice Address of vested winr token.
  IWinrERC20 public vToken;
  /// @notice Address of liquidity provider.
  ILiquidityProvider public lp;
  /// @notice Address of winr staking.
  IWinrStaking public winrStaking;
  /// @notice Winr staking fee rate.
  uint256 public feeRate = 20_000_000_000_000_000;
  /// @notice Min bet amount
  uint256 public minBet;
  /// @notice Max bet amount
  uint256 public maxBet;
  /// @notice The amount will be accumulated per game
  uint256 public accVestedWinrAmount = 5e18;

  /*==================================================== Constant Variables ==================================================*/

  /// @notice used to calculate precise decimals
  uint256 private constant PRECISION = 1e18;
  /// @notice Money handler role to call profit and loss
  bytes32 public constant LIQUIDITY_PROVIDER_ROLE = bytes32(keccak256("LIQUIDITY_PROVIDER"));
  /// @notice Money handler role to call fee
  bytes32 public constant WINR_STAKING_ROLE = bytes32(keccak256("WINR_STAKING"));
  /// @notice Money handler role to call bet
  bytes32 public constant GAME_ROLE = bytes32(keccak256("GAME_ROLE"));

  /*==================================================== FUNCTIONS ===========================================================*/

  constructor(
    IWinrERC20 _token,
    IERC20 _weth,
    IWinrERC20 _vToken,
    ILiquidityProvider _lp,
    IWinrStaking _winrStaking,
    address _admin
  ) {
    require(address(_token) != address(0), "Token address zero");
    require(_admin != address(0), "Admin address zero");

    token = _token;
    wETH = _weth;
    vToken = _vToken;
    lp = _lp;
    winrStaking = _winrStaking;

    _grantRole(WINR_STAKING_ROLE, address(winrStaking));
    _grantRole(LIQUIDITY_PROVIDER_ROLE, address(lp));
    _grantRole(DEFAULT_ADMIN_ROLE, _admin);
  }

  /*================================================== Game Functions =================================================*/

  function setMinBet(uint256 _minBet) external onlyRole(DEFAULT_ADMIN_ROLE) {
    require(_minBet < maxBet, "Amount too high");
    minBet = _minBet;
  }

  function setMaxBet(uint256 _maxBet) external onlyRole(DEFAULT_ADMIN_ROLE) {
    require(_maxBet > minBet, "Amount too low");
    maxBet = _maxBet;
  }

  function setAccVestedWinrAmount(uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE) {
    accVestedWinrAmount = amount;
  }

  function bet(address account, uint256 amount) external onlyRole(GAME_ROLE) returns (uint256 remain) {
    require(amount >= minBet && amount <= maxBet, "Bet is out-range");

    uint256 fee = (amount * feeRate) / PRECISION;

    vToken.mint(account, accVestedWinrAmount);
    winrStaking.share(fee);
    remain = amount - fee;

    require(wETH.transferFrom(account, address(this), amount), "Transfer error");
  }

  function finalize(address account, uint256 amount, bool won) external onlyRole(GAME_ROLE) {
    if (won) {
      wETH.transfer(account, amount);
      lp.shareLoss(amount);
    } else {
      lp.shareProfit(amount);
    }
  }

  /*================================================== Winr Staking Functions =================================================*/

  function deposit(address account, uint256 amount) external onlyRole(WINR_STAKING_ROLE) returns (bool) {
    return token.transferFrom(account, address(this), amount);
  }

  function depositVested(address account, uint256 amount) external onlyRole(WINR_STAKING_ROLE) returns (bool) {
    vToken.burn(account, amount);
    token.mint(address(this), amount);

    return true;
  }

  function withdraw(address account, uint256 amount, uint256 fee) external onlyRole(WINR_STAKING_ROLE) returns (bool) {
    require(token.transfer(account, amount), "ERC20 transfer failed");
    require(wETH.transfer(account, fee), "ERC20 transfer failed");

    return true;
  }

  /*=============================================== Liquidity Provider Functions ==============================================*/

  function depositLiquidity(address account, uint256 amount) external onlyRole(LIQUIDITY_PROVIDER_ROLE) returns (bool) {
    return wETH.transferFrom(account, address(this), amount);
  }

  function withdrawLiquidity(address account, uint256 amount) external onlyRole(LIQUIDITY_PROVIDER_ROLE) returns (bool) {
    return wETH.transfer(account, amount);
  }
}

