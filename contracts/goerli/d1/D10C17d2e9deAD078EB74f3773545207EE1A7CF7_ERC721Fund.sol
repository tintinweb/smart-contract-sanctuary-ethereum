pragma solidity ^0.8.9;

///@dev inhouse implemented smart contracts and interfaces.
import "./interfaces/IManagement.sol";

///@dev ERC721 token standard.
// import "./@chirulabs/ERC721AUpgradeable.sol";
import "./@openzeppelin/upgradeable/token/ERC721Upgradeable.sol";
import "./@openzeppelin/upgradeable/token/ERC2981Upgradeable.sol";

///@dev ERC20 token standard interface.
import "./@openzeppelin/token/IERC20.sol";

///@dev security settings.
import "./@openzeppelin/proxy/utils/Initializable.sol";
import "./@openzeppelin/upgradeable/access/OwnableUpgradeable.sol";
import "./@openzeppelin/upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "./@openzeppelin/upgradeable/security/PausableUpgradeable.sol";

/**
@dev DefaultOperatorFilterer is necessary for enforcing creator's royalty on OpenSea.
To learn more:
- https://www.youtube.com/watch?v=4spapOTVpNA
- https://twitter.com/0xcygaar/status/1589787467443765248?s=46&t=iE4ffqs_8qPzPcuDUftWbg
- https://github.com/ProjectOpenSea/operator-filter-registry
*/
import "./@opensea/DefaultOperatorFiltererUpgradeable.sol";

contract ERC721Art is
    Initializable,
    ERC721Upgradeable,
    OwnableUpgradeable,
    DefaultOperatorFiltererUpgradeable,
    ReentrancyGuardUpgradeable,
    PausableUpgradeable,
    ERC2981Upgradeable
{
    /// -----------------------------------------------------------------------
    /// Storage
    /// -----------------------------------------------------------------------

    ///@dev enum to specify the coin/token of transfer
    enum Coin {
        ETH_COIN,
        USDC_TOKEN,
        DAI_TOKEN,
        CREATORS_TOKEN
    }

    IManagement public management;
    uint256 public maxSupply;
    bool public onlyERC20;
    string public baseURI;
    mapping(Coin => uint256) public price;
    mapping(uint256 => uint256) public lastTransfer;

    ///@dev mapping that specifies price of token for different coins/tokens.
    mapping(uint256 => mapping(Coin => uint256)) public tokenPrice;

    ///@dev mapping for the token contract
    mapping(Coin => IERC20) public tokenContract;

    /// -----------------------------------------------------------------------
    /// Events
    /// -----------------------------------------------------------------------

    /** @dev event for when a new mint price is set.
        @param newPrice: new mint price 
        @param coin: token/coin of transfer */
    event PriceSet(uint256 indexed newPrice, Coin indexed coin);

    /** @dev event for when owner sets new price for his/her token.
        @param tokenId: ID of ERC721 token
        @param price: new token price
        @param coin: token/coin of transfer */
    event TokenPriceSet(
        uint256 indexed tokenId,
        uint256 price,
        Coin indexed coin
    );

    /// -----------------------------------------------------------------------
    /// Initialization
    /// -----------------------------------------------------------------------

    /** @notice initializes the contract. Required function, since a proxy pattern is used.
        @param _name: name of the NFT collection
        @param _symbol: symbol of the NFT collection
        @param _owner: collection owner/creator
        @param _management: address of Managmente smart contract
        @param _maxSupply: maximum NFT supply. If 0 is given, the maximum is 2^255 - 1
        @param _price: mint price of a single NFT
        @param _priceInUSDC: mint price of a single NFT
        @param _priceInCreatorsCoin: mint price of a single NFT
        @param baseURI_: base URI for the collection's metadata 
        @param _royalty: royalty payment to owner 
            (final value = _royalty / 10000 (ERC2981Upgradeable._feeDenominator())) */
    function initialize(
        string memory _name,
        string memory _symbol,
        address _owner,
        address _management,
        uint256 _maxSupply,
        uint256 _price,
        uint256 _priceInUSDC,
        uint256 _priceInCreatorsCoin,
        string memory baseURI_,
        uint256 _royalty
    ) public virtual initializer {
        __ERC721_init(_name, _symbol);
        __Ownable_init();
        transferOwnership(_owner);
        __DefaultOperatorFilterer_init();
        __ReentrancyGuard_init();
        __ERC2981_init();
        _setDefaultRoyalty(_owner, uint96(_royalty));

        management = IManagement(_management);
        maxSupply = _maxSupply;
        price[Coin.ETH_COIN] = _price;
        price[Coin.USDC_TOKEN] = _priceInUSDC;
        price[Coin.DAI_TOKEN] = _priceInUSDC;
        price[Coin.CREATORS_TOKEN] = _priceInCreatorsCoin;
        baseURI = baseURI_;

        /**@dev Mumbai address = 0xe6b8a5CF854791412c1f6EFC7CAf629f5Df1c747
            Polygon mainnet address = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174
            Goerli address = 0xA2025B15a1757311bfD68cb14eaeFCc237AF5b43
            Ethereum mainnet address = 0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48 */
        tokenContract[Coin.USDC_TOKEN] = IERC20(
            0xA2025B15a1757311bfD68cb14eaeFCc237AF5b43
        );

        /**@dev DAI stablecoin 
            Mumbai address = 0xd393b1E02dA9831Ff419e22eA105aAe4c47E1253
            Polygon mainnet address = 0x8f3Cf7ad23Cd3CaDbD9735AFf958023239c6A063            
            Goerli address = 0xDF1742fE5b0bFc12331D8EAec6b478DfDbD31464
            Ethereum mainnet address = 0x6B175474E89094C44Da98b954EedeAC495271d0F */
        tokenContract[Coin.DAI_TOKEN] = IERC20(
            0xDF1742fE5b0bFc12331D8EAec6b478DfDbD31464
        );

        tokenContract[Coin.CREATORS_TOKEN] = IERC20(management.creatorsCoin());
    }

    /// -----------------------------------------------------------------------
    /// Modifiers
    /// -----------------------------------------------------------------------

    ///@dev checks if it has reached the max supply
    modifier checkSupply(uint256 _tokenId) {
        if (maxSupply > 0) {
            require(_tokenId < maxSupply, "ERC721Art: max supply reached");
        }
        _;
    }

    ///@dev checks if caller is a smart contract/bot
    modifier checkIfBot() {
        require(msg.sender == tx.origin, "ERC721Art: BOT");
        _;
    }

    ///@dev checks the price for mint
    modifier checkPrice(
        Coin _coin,
        uint256 _value,
        address _from
    ) {
        require(
            _checkPrice(_coin, _value, _from),
            "ERC721Art: not enough value sent/allowance"
        );
        _;
    }

    /// -----------------------------------------------------------------------
    /// Implemented functions
    /// -----------------------------------------------------------------------

    // --- ERC721 functions ---

    /** @notice mints given the NFT of given tokenId, using ETH/MATIC.
        @param _tokenId: tokenId to be minted */
    function mintWithETH(uint256 _tokenId)
        public
        payable
        virtual
        whenNotPaused
        nonReentrant
        checkIfBot
        checkSupply(_tokenId)
        checkPrice(Coin.ETH_COIN, msg.value, address(0))
    {
        require(!onlyERC20, "ERC721Art: only ERC20 tokens allowed");

        _doAllRoyaltyTransfers(Coin.ETH_COIN, price[Coin.ETH_COIN]);

        _safeMint(msg.sender, _tokenId);
    }

    /** @notice mints given the NFT of given tokenId, using USDC.
        @param _tokenId: tokenId to be minted */
    function mintWithUSDC(uint256 _tokenId)
        public
        virtual
        whenNotPaused
        nonReentrant
        checkIfBot
        checkSupply(_tokenId)
        checkPrice(Coin.USDC_TOKEN, 0, msg.sender)
    {
        _doAllRoyaltyTransfers(Coin.USDC_TOKEN, price[Coin.USDC_TOKEN]);

        _safeMint(msg.sender, _tokenId);
    }

    /** @notice mints given the NFT of given tokenId, using CreatorsCoin.
        @param _tokenId: tokenId to be minted */
    function mintWithCreatorsCoin(uint256 _tokenId)
        public
        virtual
        whenNotPaused
        nonReentrant
        checkIfBot
        checkSupply(_tokenId)
        checkPrice(Coin.CREATORS_TOKEN, 0, msg.sender)
    {
        _doAllRoyaltyTransfers(Coin.CREATORS_TOKEN, price[Coin.CREATORS_TOKEN]);

        _safeMint(msg.sender, _tokenId);
    }

    /** @notice mints an NFT of the given tokenId to the given address
        @param _to: address to which the NFT is going to be minted
        @param _tokenId: tokenId of the NFT to be minted */
    function mintToAddress(address _to, uint256 _tokenId)
        external
        whenNotPaused
        nonReentrant
        checkSupply(_tokenId)
    {
        require(
            management.managers(msg.sender) || msg.sender == owner(),
            "ERC721Art: caller not manager or owner"
        );

        _safeMint(_to, _tokenId);
    }

    /** @notice burns NFT of the given tokenId.
        @param _tokenId: token ID to be burned */
    function burn(uint256 _tokenId) external onlyOwner {
        _burn(_tokenId);
    }

    /** @notice safeTransferFrom function especifically for CreatorPRO. It enforces (onchain) the transfer of the 
    correct token price. 
        @param coin: which coin to use (0 => ETH, 1 => USD, 2 => CreatorsCoin)
        The other parameters are the same from safeTransferFrom function. */
    function creatorProSafeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        Coin coin
    ) public payable nonReentrant {
        ///@custom:question precisa colocar onlyAllowedOperators nessa?

        uint256 _tokenPrice = tokenPrice[tokenId][coin];
        if (_tokenPrice == 0) {
            _tokenPrice = price[coin];
        }

        require(
            _checkPrice(coin, msg.value, from),
            "ERC721Art: not enough ERC20 allowance/ETH sent"
        );

        ERC721Upgradeable.safeTransferFrom(from, to, tokenId);

        _doAllRoyaltyTransfers(coin, _tokenPrice);

        // NFT owner payment
        uint256 CreatorsProRoyalty = (_tokenPrice * management.fee()) /
            _feeDenominator();
        (, uint256 ownerRoyalty) = royaltyInfo(tokenId, _tokenPrice);
        uint256 ownerValue = _tokenPrice - CreatorsProRoyalty - ownerRoyalty;
        tokenContract[coin].transferFrom(to, from, ownerValue);
    }

    /** @notice sets NFT mint price.
        @param _price: new NFT mint price */
    function setPrice(uint256 _price, Coin _coin) external onlyOwner {
        price[_coin] = _price;

        emit PriceSet(_price, _coin);
    }

    /** @notice sets the price of the ginve token ID.
        @param _tokenId: ID of token
        @param _price: new price to be set */
    function setTokenPrice(
        uint256 _tokenId,
        uint256 _price,
        Coin _coin
    ) external {
        require(
            msg.sender == ERC721Upgradeable.ownerOf(_tokenId),
            "ERC721Art: not token owner"
        );

        tokenPrice[_tokenId][_coin] = _price;

        emit TokenPriceSet(_tokenId, _price, _coin);
    }

    /** @notice sets new base URI for the collection.
        @param _uri: new base URI to be set */
    function setBaseURI(string memory _uri) external onlyOwner {
        baseURI = _uri;
    }

    // --- Pause and Unpause functions ---

    ///@notice pauses the contract so that functions cannot be executed
    function pause() external onlyOwner {
        _pause();
    }

    ///@notice unpauses the contract so that functions can be executed
    function unpause() external onlyOwner {
        _unpause();
    }

    // --- Internal functions ---

    /** @dev transfers the given amount of the given coin to the given payee (royalty).
        @param _value: value to be transfered
        @param _coin: coin of which the transfer is made
        @param _payee: receiver of payment */
    function _royaltyTransfer(
        uint256 _value,
        Coin _coin,
        address _payee
    ) internal {
        if (_coin == Coin.ETH_COIN) {
            payable(_payee).transfer(_value);
        } else {
            tokenContract[_coin].transferFrom(msg.sender, _payee, _value);
        }
    }

    /** @dev checks if the value sent or allowance is enough to mint NFT
        @param _coin: coin if transfer        
        @param _value: value sent or allowed to compare with
        @param _from: from address to check allowance*/
    function _checkPrice(
        Coin _coin,
        uint256 _value,
        address _from
    ) internal view returns (bool) {
        if (_coin == Coin.ETH_COIN) {
            return !(_value < price[_coin]);
        } else {
            return
                !(tokenContract[_coin].allowance(_from, address(this)) <
                    price[_coin]);
        }
    }

    /** @dev executes all the royalty transfers
        @param _coin: which coin/token to use for transfer
        @param _price: price of NFT */
    function _doAllRoyaltyTransfers(Coin _coin, uint256 _price) internal {
        // CreatorsPRO royalty
        uint256 CreatorsProRoyalty = (_price * management.fee()) /
            _feeDenominator();
        _royaltyTransfer(CreatorsProRoyalty, _coin, management.multiSig());

        // collection creator transfer
        _royaltyTransfer(_price - CreatorsProRoyalty, _coin, owner());
    }

    /// -----------------------------------------------------------------------
    /// Overridden functions
    /// -----------------------------------------------------------------------

    // --- ERC721AUpgradeable ---

    /// @inheritdoc ERC721Upgradeable
    function setApprovalForAll(address operator, bool approved)
        public
        virtual
        override(ERC721Upgradeable)
        onlyAllowedOperatorApproval(operator)
    {
        ERC721Upgradeable.setApprovalForAll(operator, approved);
    }

    /// @inheritdoc ERC721Upgradeable
    function approve(address operator, uint256 tokenId)
        public
        virtual
        override(ERC721Upgradeable)
        onlyAllowedOperatorApproval(operator)
    {
        ERC721Upgradeable.approve(operator, tokenId);
    }

    /// @inheritdoc ERC721Upgradeable
    function tokenURI(uint256 _tokenId)
        public
        view
        virtual
        override(ERC721Upgradeable)
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(ERC721Upgradeable.tokenURI(_tokenId), ".json")
            );
    }

    /// @inheritdoc ERC721Upgradeable
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721Upgradeable, ERC2981Upgradeable)
        returns (bool)
    {
        return
            ERC721Upgradeable.supportsInterface(interfaceId) ||
            ERC2981Upgradeable.supportsInterface(interfaceId);
    }

    /// @inheritdoc ERC721Upgradeable
    function _transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721Upgradeable) {
        require(
            !(block.timestamp - lastTransfer[tokenId] < 30 days),
            "ERC721Art: transfer deadline still going"
        );

        // update last transfer timestamp
        lastTransfer[tokenId] = block.timestamp;

        ERC721Upgradeable._transfer(from, to, tokenId);
    }

    /// @inheritdoc ERC721Upgradeable
    function _baseURI()
        internal
        view
        override(ERC721Upgradeable)
        returns (string memory)
    {
        return baseURI;
    }
}

