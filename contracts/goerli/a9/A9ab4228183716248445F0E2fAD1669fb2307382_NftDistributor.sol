pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/MerkleProofUpgradeable.sol";
import "../interfaces/IMerkleDistributor.sol";

contract MerkleClaim is IMerkleDistributor {
  // using MerkleProofUpgradeable for bytes32[];

  mapping (address => bytes32) public override merkleRoot;

  // This is a packed array of booleans.
  mapping (address => mapping(uint256 => uint256)) private claimedBitMap;

  function isClaimed(address contractAddress, uint256 index) public view override returns (bool) {
    uint256 claimedWordIndex = index / 256;
    uint256 claimedBitIndex = index % 256;
    uint256 claimedWord = claimedBitMap[contractAddress][claimedWordIndex];
    uint256 mask = (1 << claimedBitIndex);
    return claimedWord & mask == mask;
  }

  function _hasValidClaim(uint256 index, uint256 claimCode, address contractAddress, uint256 tokenId, bytes32[] calldata merkleProof) internal virtual returns(bool) {
    if (isClaimed(contractAddress, index)) { return false; }

    // Verify the merkle proof.
    bytes32 node = keccak256(abi.encodePacked(index, claimCode, contractAddress, tokenId));
    return MerkleProofUpgradeable.verifyCalldata(merkleProof, merkleRoot[contractAddress], node);
    // return merkleProof.verifyCalldata(merkleRoot[contractAddress], node);
  }

  function _setClaimed(address contractAddress, uint256 index) internal virtual {
    uint256 claimedWordIndex = index / 256;
    uint256 claimedBitIndex = index % 256;
    claimedBitMap[contractAddress][claimedWordIndex] = claimedBitMap[contractAddress][claimedWordIndex] | (1 << claimedBitIndex);
  }

  function _setMerkleRoot(address contractAddress, bytes32 merkleRoot_) internal virtual {
    merkleRoot[contractAddress] = merkleRoot_;
  }
}

