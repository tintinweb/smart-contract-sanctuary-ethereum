// File contracts/blin/BlinkTrader.sol



pragma solidity ^0.8.4;
/**
 * @notice Blink trader bot wrapper contract
 *
 * @dev Jan 6th, 2023
 */
contract BlinkTrader is Ownable {
    using Address for address payable;
    using SafeERC20 for IERC20;

    uint16 private constant MAX_FEE = 2000; // Max trade fee is 20% constantly

    address private _dexRouter;
    uint16 private _tradeFee;

    error InvalidValue(uint16 limit, uint16 value);

    constructor(address dexRouter_, uint16 tradeFee_) {
        _dexRouter = dexRouter_;
        _tradeFee = tradeFee_;
    }

    /// @notice Buy token with ETH
    function buy(
        uint256 amountOutMin_,
        address[] calldata path_,
        address to_,
        uint256 deadline_
    ) external payable {
        address outToken = path_[path_.length - 1];
        IUniswapV2Router02 tradeRouter = IUniswapV2Router02(_dexRouter);
        uint256 balanceBefore = IERC20(outToken).balanceOf(address(this));

        uint256 amountIn = msg.value;
        // Deduct fee amount
        amountIn -= (amountIn * _tradeFee) / 10000;
        // amountOutMin_ should be deducted as well because users pay fee in ETH
        if (amountOutMin_ > 0)
            amountOutMin_ -= (amountOutMin_ * _tradeFee) / 10000;

        // If target token is WETH, we do not use swap function
        if (outToken == tradeRouter.WETH())
            IWETH(outToken).deposit{value: amountIn}();
        else
            tradeRouter.swapExactETHForTokensSupportingFeeOnTransferTokens{
                value: amountIn
            }(amountOutMin_, path_, address(this), deadline_);

        uint256 amountOut = IERC20(outToken).balanceOf(address(this)) -
            balanceBefore;
        IERC20(outToken).safeTransfer(to_, amountOut);
    }

    /// @notice Sell token to get ETH
    function sell(
        uint256 amountIn_,
        uint256 amountOutMin_,
        address[] calldata path_,
        address to_,
        uint256 deadline_
    ) external {
        address inToken = path_[0];
        IUniswapV2Router02 tradeRouter = IUniswapV2Router02(_dexRouter);
        uint256 balanceBefore = IERC20(inToken).balanceOf(address(this));
        IERC20(inToken).safeTransferFrom(
            _msgSender(),
            address(this),
            amountIn_
        );
        amountIn_ = IERC20(inToken).balanceOf(address(this)) - balanceBefore;

        balanceBefore = payable(address(this)).balance;
        if (inToken == tradeRouter.WETH()) IWETH(inToken).withdraw(amountIn_);
        else {
            IERC20(inToken).safeApprove(address(tradeRouter), amountIn_);
            tradeRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
                amountIn_,
                amountOutMin_,
                path_,
                address(this),
                deadline_
            );
        }
        uint256 amountOut = payable(address(this)).balance - balanceBefore;
        // Deduct fee from the swapped ETH
        amountOut -= (amountOut * _tradeFee) / 10000;

        payable(_msgSender()).sendValue(amountOut);
    }

    /// @notice Update dex router address
    /// @dev Only owner is allowed to call this function
    /// @param dexRouter_ New dex router address
    function updateDexRouter(address dexRouter_) external onlyOwner {
        // Validation code to check if new dex router is UniswapV2 standard
        IUniswapV2Router02(dexRouter_).WETH();
        _dexRouter = dexRouter_;
    }

    /// @notice View current dex router address
    function dexRouter() external view returns (address) {
        return _dexRouter;
    }

    /// @notice Update trade fee
    /// @dev Only owner is allowed to call this function
    /// @param tradeFee_ trade fee in 100 multiplied percent value, should be less than 20%
    function updateTradeFee(uint16 tradeFee_) external onlyOwner {
        if (tradeFee_ > MAX_FEE) revert InvalidValue(MAX_FEE, tradeFee_);
        _tradeFee = tradeFee_;
    }

    /// @notice View current trade fee value
    function tradeFee() external view returns (uint16) {
        return _tradeFee;
    }

    /**
     * @notice It allows the admin to recover tokens sent to the contract
     * @param token_: the address of the token to withdraw
     * @param amount_: the number of tokens to withdraw
     * @dev This function is only callable by admin.
     */
    function recoverTokens(address token_, uint256 amount_) external onlyOwner {
        // Zero address means recovering ETH
        if (token_ == address(0)) payable(_msgSender()).sendValue(amount_);
        else IERC20(token_).safeTransfer(_msgSender(), amount_);
    }

    /// @notice To recieve ETH
    receive() external payable {}
}