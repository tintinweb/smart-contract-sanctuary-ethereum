import "../bridge-factory/BridgeRouter.sol";

contract CrossChainBridge_R1 is
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    OwnableUpgradeable,
    ICrossChainBridge
{
    mapping(uint256 => address) private _bridgeAddressByChainId;
    address private _consensusAddress;
    mapping(bytes32 => bool) private _usedProofs;
    uint256 internal _globalNonce;
    address private _tokenImplementation;
    mapping(address => address) private _peggedTokenOrigin;
    Metadata _nativeTokenMetadata;
    BridgeRouter private _bridgeRouter;
    address payable private _treasuryAddress;
    mapping(uint256 => uint16) private _depositCommission;
    uint16 private _defDepositCommission;
    uint16 constant MAX_COMMISSION = 10000; // 100.00%
    mapping(bytes32 => address) private _warpDestinations; // KECCAK256(fromToken,toChain,toBridge) => destinationToken

    function initialize(
        address consensusAddress,
        SimpleTokenFactory tokenFactory,
        string memory nativeTokenSymbol,
        string memory nativeTokenName,
        BridgeRouter router
    ) public initializer {
        __Pausable_init();
        __ReentrancyGuard_init();
        __Ownable_init();
        __CrossChainBridge_init(
            consensusAddress,
            tokenFactory,
            nativeTokenSymbol,
            nativeTokenName,
            router
        );
    }

    modifier onlyConsensus() {
        require(
            msg.sender == _consensusAddress,
            "CrossChainBridge: caller is not the consensus"
        );
        _;
    }

    function getDepositCommission(uint256 toChain)
        public
        view
        override
        returns (uint16 commission)
    {
        commission = _depositCommission[toChain];
        if (commission == 0) {
            commission = _defDepositCommission;
        }
    }

    function getTokenImplementation() public view override returns (address) {
        return _tokenImplementation;
    }

    function setTokenFactory(SimpleTokenFactory factory) public onlyOwner {
        _tokenImplementation = factory.getImplementation();
        require(_tokenImplementation != address(0x0));
        emit TokenImplementationChanged(_tokenImplementation);
    }

    function getNativeAddress() public view returns (address) {
        return _nativeTokenMetadata.originAddress;
    }

    function getOrigin(address token) external view returns (uint256, address) {
        if (token == _nativeTokenMetadata.originAddress) {
            return (0, address(0x0));
        }
        try IERC20Pegged(token).getOrigin() returns (
            uint256 chain,
            address origin
        ) {
            return (chain, origin);
        } catch {}
        return (0, address(0x0));
    }

    function __CrossChainBridge_init(
        address consensusAddress,
        SimpleTokenFactory tokenFactory,
        string memory nativeTokenSymbol,
        string memory nativeTokenName,
        BridgeRouter router
    ) internal {
        _consensusAddress = consensusAddress;
        _tokenImplementation = tokenFactory.getImplementation();
        _nativeTokenMetadata = Metadata(
            Utils.stringToBytes32(nativeTokenSymbol),
            Utils.stringToBytes32(nativeTokenName),
            block.chainid,
            // generate unique address that will not collide with any contract address
            address(
                bytes20(
                    keccak256(
                        abi.encodePacked("CrossChainBridge:", nativeTokenSymbol)
                    )
                )
            )
        );
        _bridgeRouter = router;
    }

    // HELPER FUNCTIONS

    function isPeggedToken(address toToken)
        public
        view
        override
        returns (bool)
    {
        return _peggedTokenOrigin[toToken] != address(0x00);
    }

    // DEPOSIT FUNCTIONS

    function depositNative(uint256 toChain, address toAddress)
        public
        payable
        override
        nonReentrant
        whenNotPaused
    {
        _depositNative(toChain, toAddress, msg.value);
    }

    function depositToken(
        address fromToken,
        uint256 toChain,
        address toAddress,
        uint256 amount
    ) public override nonReentrant whenNotPaused {
        (uint256 chain, address origin) = this.getOrigin(fromToken);
        if (warpDestination(fromToken, toChain) != address(0)) {
            // BURN
            /* if we have warp destination then its warped token */
            _depositWarped(fromToken, toChain, toAddress, amount);
        } else if (chain != 0) {
            // BURN
            /* if we have pegged contract then its pegged token */
            _depositPegged(
                fromToken,
                toChain,
                toAddress,
                amount,
                chain,
                origin
            );
        } else {
            // LOCK
            /* otherwise its erc20 token, since we can't detect is it erc20 token it can only return insufficient balance in case of any errors */
            _depositErc20(fromToken, toChain, toAddress, amount);
        }
    }

    function _depositNative(
        uint256 toChain,
        address toAddress,
        uint256 totalAmount
    ) internal {
        /* sender is our from address because he is locking funds */
        address fromAddress = address(msg.sender);
        /* lets determine target bridge contract */
        address toBridge = _bridgeAddressByChainId[toChain];
        require(toBridge != address(0x00), "CrossChainBridge: bad chain");

        address toToken;
        toToken = _peggedTokenOrigin[_nativeTokenMetadata.originAddress];
        if (toToken == address(0x00)) {
            // simple token mapping
            toToken = _bridgeRouter.peggedTokenAddress(
                address(toBridge),
                _nativeTokenMetadata.originAddress
            );
        }
        _globalNonce++;

        uint256 fee = Utils.multiplyAndDivideCeil(
            totalAmount,
            getDepositCommission(toChain),
            MAX_COMMISSION
        );
        (bool success, ) = _treasuryAddress.call{value: fee}("");
        require(success, "CrossChainBridge: cannot charge fee");

        emit DepositLocked(
            toChain,
            fromAddress,
            toAddress,
            _nativeTokenMetadata.originAddress,
            toToken,
            totalAmount - fee,
            _globalNonce,
            _nativeTokenMetadata
        );
    }

    /**
     * @dev Tokens on source and destination chains are linked with independent supplies.
     * Burns tokens on source chain (to later mint on destination chain).
     * @param fromToken one of many warp-able token on source chain.
     * @param toChain one of many destination chain ID.
     * @param toAddress claimer of 'totalAmount' on destination chain. Usually consensus address.
     * @param totalAmount amout of tokens to be warped.
     */
    function _depositWarped(address fromToken, uint256 toChain, address toAddress, uint256 totalAmount) internal {
        address fromAddress = address(msg.sender);
        {            
            require(_bridgeAddressByChainId[toChain] != address(0x00), "bad chain");
            uint256 balanceBefore = IERC20(fromToken).balanceOf(address(this));
            IERC20(fromToken).transferFrom(fromAddress, address(this), totalAmount);
            uint256 balanceAfter = IERC20(fromToken).balanceOf(address(this));
            require(balanceAfter >= balanceBefore + totalAmount, "incorrect behaviour");
        }

        totalAmount = _amountErc20Token(fromToken, totalAmount);
        address toToken = warpDestination(fromToken, toChain);

        IERC20Mintable(fromToken).burn(address(this), totalAmount); 
        
        /* fromToken and toToken are independent, originChain and originAddress are invalid */
        /* this is how we differenciate between burn event in _depositPegged and _depositWarped */
        Metadata memory metaData = Metadata(
            Utils.stringToBytes32(IERC20Extra(fromToken).symbol()),
            Utils.stringToBytes32(IERC20Extra(fromToken).name()),
            0,
            address(0)
        );

       _globalNonce++;

        emit DepositBurned(
            toChain,
            fromAddress,
            toAddress,
            fromToken,
            toToken,
            totalAmount,
            _globalNonce,
            metaData
        );
    }
    function warpDestination(address fromToken, uint256 toChain) public view returns(address) {
        return _warpDestinations[keccak256(abi.encodePacked(fromToken, block.chainid, toChain))];
    }
    function addWarpDestination(address fromToken, uint256 toChain, address toToken) external onlyOwner {
        bytes32 direction = keccak256(abi.encodePacked(fromToken, block.chainid, toChain));
        require(_warpDestinations[direction] == address(0), "known destination");
        _warpDestinations[direction] = toToken;
    }

    function _depositPegged(
        address fromToken,
        uint256 toChain,
        address toAddress,
        uint256 totalAmount,
        uint256 chain,
        address origin
    ) internal {
        uint256 fee = Utils.multiplyAndDivideCeil(
            totalAmount,
            getDepositCommission(toChain),
            MAX_COMMISSION
        );
        uint256 amountWithoutFee = totalAmount - fee;
        /* sender is our from address because he is locking funds */
        address fromAddress = address(msg.sender);
        /* check allowance and transfer tokens */
        require(
            IERC20Upgradeable(fromToken).balanceOf(fromAddress) >= totalAmount,
            "CrossChainBridge: insufficient balance"
        );
        address toToken = _peggedDestinationErc20Token(
            fromToken,
            origin,
            toChain,
            chain
        );
        require(
            IERC20Mintable(fromToken).chargeFrom(
                fromAddress,
                _treasuryAddress,
                fee
            ),
            "CrossChainBridge: cannot charge fee"
        );
        IERC20Mintable(fromToken).burn(fromAddress, amountWithoutFee);
        Metadata memory metaData = Metadata(
            Utils.stringToBytes32(IERC20Extra(fromToken).symbol()),
            Utils.stringToBytes32(IERC20Extra(fromToken).name()),
            chain,
            origin
        );

        _globalNonce++;

        emit DepositBurned(
            toChain,
            fromAddress,
            toAddress,
            fromToken,
            toToken,
            amountWithoutFee,
            _globalNonce,
            metaData
        );
    }

    function _depositErc20(
        address fromToken,
        uint256 toChain,
        address toAddress,
        uint256 totalAmount
    ) internal {
        uint256 fee = Utils.multiplyAndDivideCeil(
            totalAmount,
            getDepositCommission(toChain),
            MAX_COMMISSION
        );
        uint256 amountWithoutFee = totalAmount - fee;
        /* sender is our from address because he is locking funds */
        address fromAddress = address(msg.sender);
        /* check allowance and transfer tokens */
        {
            uint256 balanceBefore = IERC20(fromToken).balanceOf(address(this));
            uint256 allowance = IERC20(fromToken).allowance(
                fromAddress,
                address(this)
            );
            require(
                totalAmount <= allowance,
                "CrossChainBridge: insufficient allowance"
            );
            require(
                IERC20(fromToken).transferFrom(
                    fromAddress,
                    address(this),
                    amountWithoutFee
                ),
                "CrossChainBridge: can't transfer"
            );
            require(
                IERC20(fromToken).transferFrom(
                    fromAddress,
                    _treasuryAddress,
                    fee
                ),
                "CrossChainBridge: cannot charge fee"
            );
            uint256 balanceAfter = IERC20(fromToken).balanceOf(address(this));
            require(
                balanceAfter >= balanceBefore + amountWithoutFee,
                "CrossChainBridge: incorrect behaviour"
            );
        }
        /* lets determine target bridge contract */
        address toBridge = _bridgeAddressByChainId[toChain];
        require(toBridge != address(0x00), "CrossChainBridge: bad chain");
        /* lets pack ERC20 token meta data and scale amount to 18 decimals */
        uint256 amt = _amountErc20Token(fromToken, amountWithoutFee);
        // try to find token in specific map
        address toToken;
        toToken = _peggedTokenOrigin[fromToken];
        if (toToken == address(0x00)) {
            // simple token mapping
            toToken = _bridgeRouter.peggedTokenAddress(
                address(toBridge),
                fromToken
            );
        }

        Metadata memory metaData = Metadata(
            Utils.stringToBytes32(IERC20Extra(fromToken).symbol()),
            Utils.stringToBytes32(IERC20Extra(fromToken).name()),
            block.chainid,
            fromToken
        );
        _globalNonce++;

        emit DepositLocked(
            toChain,
            fromAddress,
            toAddress,
            fromToken,
            toToken,
            amt,
            _globalNonce,
            metaData
        );
    }

    function _peggedDestinationErc20Token(
        address fromToken,
        address origin,
        uint256 toChain,
        uint256 originChain
    ) internal view returns (address) {
        /* lets determine target bridge contract */
        address toBridge = _bridgeAddressByChainId[toChain];
        require(toBridge != address(0x00), "CrossChainBridge: bad chain");
        require(
            _peggedTokenOrigin[fromToken] == origin,
            "CrossChainBridge: non-pegged contract not supported"
        );
        if (toChain == originChain) {
            return _peggedTokenOrigin[fromToken];
        } else {
            return _bridgeRouter.peggedTokenAddress(address(toBridge), origin);
        }
    }

    function _amountErc20Token(address fromToken, uint256 totalAmount)
        internal
        returns (uint256)
    {
        /* lets pack ERC20 token meta data and scale amount to 18 decimals */
        require(
            IERC20Extra(fromToken).decimals() <= 18,
            "CrossChainBridge: decimals overflow"
        );
        totalAmount *= (10**(18 - IERC20Extra(fromToken).decimals()));
        return totalAmount;
    }

    // WITHDRAWAL FUNCTIONS

    function withdraw(
        bytes calldata, /* encodedProof */
        bytes calldata rawReceipt,
        bytes memory proofSignature,
        bytes32 /* payload */
    ) external nonReentrant whenNotPaused {
        uint256 proofOffset;
        uint256 receiptOffset;
        assembly {
            proofOffset := add(0x4, calldataload(4))
            receiptOffset := add(0x4, calldataload(36))
        }
        /* we must parse and verify that tx and receipt matches */
        (
            EthereumVerifier.State memory state,
            EthereumVerifier.PegInType pegInType
        ) = EthereumVerifier.parseTransactionReceipt(receiptOffset);

        require(
            state.chainId == block.chainid,
            "CrossChainBridge: receipt points to another chain"
        );

        ProofParser.Proof memory proof = ProofParser.parseProof(proofOffset);
        require(
            _bridgeAddressByChainId[proof.chainId] == state.contractAddress,
            "CrossChainBridge: crosschain event from not allowed contract"
        );

        state.receiptHash = keccak256(rawReceipt);
        proof.status = 0x01;
        proof.receiptHash = state.receiptHash;

        bytes32 proofHash;
        assembly {
            proofHash := keccak256(proof, 0x100)
        }

        // we can trust receipt only if proof is signed by consensus
        require(
            ECDSAUpgradeable.recover(proofHash, proofSignature) ==
                _consensusAddress,
            "CrossChainBridge: bad signature"
        );

        // withdraw funds to recipient
        _withdraw(state, pegInType, proofHash);
    }

    function _withdraw(
        EthereumVerifier.State memory state,
        EthereumVerifier.PegInType pegInType,
        bytes32 payload
    ) internal {
        /* make sure these proofs wasn't used before */
        require(!_usedProofs[payload], "CrossChainBridge: proof already used");
        _usedProofs[payload] = true;
        if (state.originChain == 0 && state.originToken == address(0)) {
            _withdrawWarped(state);
        } else if (state.toToken == _nativeTokenMetadata.originAddress) {
            _withdrawNative(state);
        } else if (pegInType == EthereumVerifier.PegInType.Lock) {
            _withdrawPegged(state);
        } else if (state.toToken != state.originToken) {
            // origin token is not deployed by our bridge so collision is not possible
            _withdrawPegged(state);
        } else {
            _withdrawErc20(state);
        }
    }

    function _withdrawWarped(EthereumVerifier.State memory state) internal {
        IERC20Mintable(state.toToken).mint(state.toAddress, state.totalAmount);
        emit WithdrawMinted(
            state.receiptHash,
            state.fromAddress,
            state.toAddress,
            state.fromToken,
            state.toToken,
            state.totalAmount
        );
    }

    function _withdrawNative(EthereumVerifier.State memory state) internal {
        payable(state.toAddress).transfer(state.totalAmount);
        emit WithdrawUnlocked(
            state.receiptHash,
            state.fromAddress,
            state.toAddress,
            state.fromToken,
            state.toToken,
            state.totalAmount
        );
    }

    function _withdrawPegged(EthereumVerifier.State memory state) internal {
        /* create pegged token if it doesn't exist */
        Metadata memory metadata = EthereumVerifier.getMetadata(state);

        IERC20Mintable mintableToken = _factoryPeggedToken(
            state.toToken,
            metadata
        );

        try IERC20Pegged(state.toToken).getOrigin() returns (
            uint256 chain,
            address origin
        ) {
            mintableToken.mint(state.toAddress, state.totalAmount);
            /* emit peg-out event (its just informative event) */
            emit WithdrawMinted(
                state.receiptHash,
                state.fromAddress,
                state.toAddress,
                state.fromToken,
                state.toToken,
                state.totalAmount
            );
        } catch {
            _withdrawErc20(state);
        }
    }

    function _withdrawErc20(EthereumVerifier.State memory state) internal {
        /* we need to rescale this amount */
        uint8 decimals = IERC20Extra(state.toToken).decimals();
        require(decimals <= 18, "CrossChainBridge: decimals overflow");
        uint256 scaledAmount = state.totalAmount / (10**(18 - decimals));
        /* transfer tokens and make sure behaviour is correct (just in case) */
        uint256 balanceBefore = IERC20(state.toToken).balanceOf(
            state.toAddress
        );
        require(
            IERC20Upgradeable(state.toToken).transfer(
                state.toAddress,
                scaledAmount
            ),
            "CrossChainBridge: can't transfer"
        );
        uint256 balanceAfter = IERC20(state.toToken).balanceOf(state.toAddress);
        require(
            balanceBefore <= balanceAfter,
            "CrossChainBridge: incorrect behaviour"
        );
        /* emit peg-out event (its just informative event) */
        emit WithdrawUnlocked(
            state.receiptHash,
            state.fromAddress,
            state.toAddress,
            state.fromToken,
            state.toToken,
            state.totalAmount
        );
    }

    // OWNER MAINTENANCE FUNCTIONS (owner functions will be reduced in future releases)

    function factorySpecificPeggedToken(
        address fromToken,
        uint256 toChain,
        address toToken
    ) external onlyOwner {
        // make sure this chain is supported
        require(
            _bridgeAddressByChainId[toChain] != address(0x00),
            "CrossChainBridge: bad chain id"
        );
        require(
            _peggedTokenOrigin[fromToken] == address(0x00),
            "CrossChainBridge: already exists"
        );
        /* now we can mark this token as pegged */
        _peggedTokenOrigin[fromToken] = toToken;
    }

    function factoryPeggedToken(uint256 fromChain, Metadata calldata metaData)
        external
        override
        onlyOwner
    {
        // make sure this chain is supported
        require(
            _bridgeAddressByChainId[fromChain] != address(0x00),
            "CrossChainBridge: bad chain id"
        );
        // calc target token
        address toToken = _bridgeRouter.peggedTokenAddress(
            address(this),
            metaData.originAddress
        );
        require(
            _peggedTokenOrigin[toToken] == address(0x00),
            "CrossChainBridge: already exists"
        );
        // deploy new token (its just a warmup operation)
        _factoryPeggedToken(toToken, metaData);
    }

    function _factoryPeggedToken(address toToken, Metadata memory metaData)
        internal
        returns (IERC20Mintable)
    {
        address fromToken = metaData.originAddress;

        /* if pegged token exist we can just return its address */
        if (_peggedTokenOrigin[toToken] != address(0x00)) {
            return IERC20Mintable(toToken);
        }
        /* we must use delegate call because we need to deploy new contract from bridge contract to have valid address */
        (bool success, bytes memory returnValue) = address(_bridgeRouter)
            .delegatecall(
                abi.encodeWithSignature(
                    "factoryPeggedToken(address,address,(bytes32,bytes32,uint256,address),address)",
                    fromToken,
                    toToken,
                    metaData,
                    address(this)
                )
            );
        if (!success) {
            // preserving error message
            uint256 returnLength = returnValue.length;
            assembly {
                revert(add(returnValue, 0x20), returnLength)
            }
        }
        /* now we can mark this token as pegged */
        _peggedTokenOrigin[toToken] = fromToken;
        /* to token is our new pegged token */
        return IERC20Mintable(toToken);
    }

    function addAllowedContract(address allowedContract, uint256 toChain)
        public
        onlyOwner
    {
        require(
            _bridgeAddressByChainId[toChain] == address(0x00),
            "CrossChainBridge: already allowed"
        );
        require(toChain > 0, "CrossChainBridge: chain id must be positive");
        _bridgeAddressByChainId[toChain] = allowedContract;
        emit ContractAllowed(allowedContract, toChain);
    }

    function removeAllowedContract(uint256 toChain) public onlyOwner {
        require(
            _bridgeAddressByChainId[toChain] != address(0x00),
            "already disallowed"
        );
        require(toChain > 0, "CrossChainBridge: chain id must be positive");
        address wasContract = _bridgeAddressByChainId[toChain];
        delete _bridgeAddressByChainId[toChain];
        emit ContractDisallowed(wasContract, toChain);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function changeConsensus(address consensus) public onlyOwner {
        require(
            consensus != address(0x0),
            "CrossChainBridge: zero address disallowed"
        );
        _consensusAddress = consensus;
        emit ConsensusChanged(_consensusAddress);
    }

    function changeRouter(address router) public onlyOwner {
        require(
            router != address(0x0),
            "CrossChainBridge: zero address disallowed"
        );
        _bridgeRouter = BridgeRouter(router);
        // We don't have special event for router change since it's very special technical contract
        // In future changing router will be disallowed
    }

    function changeMetadata(
        address token,
        bytes32 name,
        bytes32 symbol
    ) external onlyOwner {
        IERC20MetadataChangeable(token).changeName(name);
        IERC20MetadataChangeable(token).changeSymbol(symbol);
    }

    function changeDefaultDepositCommission(uint16 newValue)
        external
        onlyOwner
    {
        require(
            newValue <= MAX_COMMISSION,
            "CrossChainBridge: commission is too high"
        );
        emit DefaultDepositCommissionChanged(_defDepositCommission, newValue);
        _defDepositCommission = newValue;
    }

    function changeDepositCommission(uint16 newValue, uint256 chain)
        external
        onlyOwner
    {
        require(
            newValue <= MAX_COMMISSION,
            "CrossChainBridge: commission is too high"
        );
        _depositCommission[chain] = newValue;
        emit DepositCommissionChanged(newValue, chain);
    }

    function changeTreasuryAddress(address newValue) external onlyOwner {
        emit TreasuryAddressChanged(_treasuryAddress, newValue);
        _treasuryAddress = payable(newValue);
    }

    receive() external payable {}
}

