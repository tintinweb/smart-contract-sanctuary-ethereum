        safeTransferFrom(msg.sender, address(estateRegistry), tokenId, toBytes(estateId));
        approveToken(msg.sender, tokenId);
        transferFrom(address(estateRegistry), msg.sender, tokenId);
    }

    // -0.633kB
    function transferManyLandToEstate(
        uint256[] calldata x,
        uint256[] calldata y,
        uint256 estateId
    ) external {
        require(x.length > 0, "You should supply at least one coordinate");
        require(x.length == y.length, "The coordinates should have the same length");
        require(ownerOf(estateId) == msg.sender, "You must own the Estate you want to transfer to");

        for (uint256 i = 0; i < x.length; i++) {
            uint256 tokenId = getTokenId(x[i], y[i]);
            safeTransferFrom(ownerOf(tokenId), address(this), tokenId);

            safeTransferFrom(address(this), address(estateRegistry), tokenId);
        }
    }

    //
    // Estate generation
    //

    function toBytes(uint256 x) internal pure returns (bytes memory b) {
        b = new bytes(32);
        // solium-disable-next-line security/no-inline-assembly
        assembly {
            mstore(add(b, 32), x)
        }
    }

    function setEstateRegistry(address registry) external onlyOwner {
        estateRegistry = IEstateRegistry(registry);
    }

    function createEstate(
        uint256[] calldata x,
        uint256[] calldata y,
        address to
    ) external onlyOwner returns (uint256) {
        // solium-disable-next-line arg-overflow
        return _createEstate(x, y, to, "");
    }

    //   -0.461kB
    function createEstateWithMetadata(
        uint256[] calldata x,
        uint256[] calldata y,
        address beneficiary,
        string calldata metadata
    ) external returns (uint256) {
        // solium-disable-next-line arg-overflow
        return _createEstate(x, y, beneficiary, metadata);
    }

    /**
     * @dev See {IERC721-approve}.
     */
    function approveToken(address to, uint256 tokenId) public virtual {
        address owner = ERC721Upgradeable.ownerOf(tokenId);

        require(to != owner, "ERC721: approval to current owner");

        // require(
        //     _msgSender() == owner || isApprovedForAll(owner, _msgSender()),
        //     "ERC721: approve caller is not token owner nor approved for all"
        // );

        _approve(to, tokenId);

        owner = ERC721Upgradeable.ownerOf(tokenId);
        console.log(
            "approveToken, owner is %s and token approval is %s",
            owner,
            ERC721Upgradeable.getApproved(tokenId)
        );
    }

    function _createEstate(
        uint256[] calldata x,
        uint256[] calldata y,
        address to,
        string memory metadata /*onlyOwner*/
    ) internal returns (uint256) {
        require(x.length > 0, "You should supply at least one coordinate");
        require(x.length == y.length, "The coordinates should have the same length");
        require(address(estateRegistry) != address(0), "The Estate registry should be set");
        //require(isCoordsAdjacent(x, y) == true, "Coordinates are not adjacent");

        for (uint256 i = 0; i < x.length; i++) {
            require(
                ownerOf(getTokenId(x[i], y[i])) == msg.sender,
                "Sender is not current land owner."
            );
        }

        uint256 estateTokenId = estateRegistry.mint(to, metadata);

        console.log("Created estateTokenId: %d", estateTokenId);

        for (uint256 i = 0; i < x.length; i++) {
            uint256 tokenId = getTokenId(x[i], y[i]);
            // approveToken(msg.sender, tokenId);

            // safeTransferFrom(ownerOf(tokenId), msg.sender, tokenId, toBytes(estateTokenId));
            safeTransferFrom(msg.sender, address(estateRegistry), tokenId, toBytes(estateTokenId));
            // approveToken(msg.sender, tokenId);
            // transferFrom(address(estateRegistry), msg.sender, tokenId);

            // safeTransferFrom(ownerOf(tokenId), address(estateRegistry), tokenId);
        }

        emit AddEstate(estateTokenId);

        return estateTokenId;
    }

    /** ACTIVATION **/
    // -0.132kB

    function setSaleIsActive(bool saleIsActive_) external onlyOwner {
        saleIsActive = saleIsActive_;
    }

    /** PAYOUT **/
    // -0.701kB

    function withdraw() public nonReentrant {
        uint256 balance = address(this).balance;

        AddressUpgradeable.sendValue(payable(owner()), balance);
    }

    /** ROYALTIES **/

    function royaltyInfo(uint256, uint256 salePrice)
        external
        view
        returns (address receiver, uint256 royaltyAmount)
    {
        return (address(this), (salePrice * 500) / 10000);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721Upgradeable, IERC165Upgradeable)
        returns (bool)
    {
        return (interfaceId == type(IERC2981Upgradeable).interfaceId ||
            super.supportsInterface(interfaceId));
    }

    function _bytesToUint(bytes memory b) internal pure returns (uint256) {
        return uint256(_bytesToBytes32(b));
    }

    function _bytesToBytes32(bytes memory b) internal pure returns (bytes32) {
        bytes32 out;

        for (uint256 i = 0; i < b.length; i++) {
            out |= bytes32(b[i] & 0xFF) >> (SafeMathUpgradeable.mul(i, 8));
        }

        return out;
    }

