import {AppStorage} from "../libraries/AppStorage.sol";
import {LibAccessControl} from "../libraries/LibAccessControl.sol";
import "hardhat/console.sol";

/// @notice System status
/// @dev status == 0 contract is Migrate, disable deposit-related features, keeping all withdrawal-related features enabled during the migration time. Prepare for migrating to a new contract version.
/// @dev status == 1 contract is Active, allow all features.
/// @dev status == 2 The contract is working normally.contract is Maintenance, disable all deposit & withdrawal-related features. Anyone can turn off Maintenance mode after it reachs the expiration time.
/// @dev status == 3 contract is MassExit, enable exit features, only available when no operations are performed before the expiration time.

contract StatusControlFacet {
    /* ------ TYPE & VARIABLES ------ */
    /// @notice declare AppStorage
    AppStorage internal s;

    /// @notice
    uint256 constant MAINTENANCE_EXPIRATION_TIME = 14 days;

    /// @notice
    uint256 constant MIGRATION_EXPIRATION_TIME = 14 days;

    /* ------ EVENTS------ */
    /// @notice
    event MassExit();

    /// @notice
    event Migrate();

    /// @notice
    event Maintenance();

    /// @notice
    event Active();

    /* ------ EXTERNAL FUNTIONS ------ */

    /// @notice
    function enableMigrate() external {
        LibAccessControl._requireGov();
        require(s.status == 1, "SCF: Contract is not active");
        s.status = 0;
        s.migrationExpirationTime = block.timestamp + MIGRATION_EXPIRATION_TIME;
        emit Migrate();
    }

    function enableMassExit(uint256 _requestId) external {
        require(s.status == 1, "SCF: Contract is not active");
        require(
            s.pendingDepositRecord[_requestId].expirationTimestamp != 0 &&
                s.pendingDepositRecord[_requestId].expirationTimestamp < block.timestamp,
            "SCF: Pending deposit request must be expired"
        );
        s.status = 3;
        emit MassExit();
    }

    // @notice
    function switchMaintenance() external {
        if (s.status == 2) {
            require(
                s.maintenanceExpirationTime < block.timestamp ||
                    LibAccessControl._hasRole(LibAccessControl.GOV_ROLE, LibAccessControl._msgSender()),
                "SCF: Maintenance is not expired"
            );
            s.status = 1;
            emit Active();
        } else {
            if (s.status == 0) {
                require(
                    s.migrationExpirationTime < block.timestamp ||
                        LibAccessControl._hasRole(LibAccessControl.GOV_ROLE, LibAccessControl._msgSender()),
                    "SCF:invalid timestamp or unauthorized"
                );
            } else {
                require(
                    LibAccessControl._hasRole(LibAccessControl.GOV_ROLE, LibAccessControl._msgSender()),
                    "SCF:unauthorized"
                );
            }
            s.status = 2;
            s.maintenanceExpirationTime = block.timestamp + MAINTENANCE_EXPIRATION_TIME;
            emit Maintenance();
        }
    }
    
}

