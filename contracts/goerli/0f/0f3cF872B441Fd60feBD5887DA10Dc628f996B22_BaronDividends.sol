    function owner(bytes32 node) external view returns (address);
}

// Partial interface to ReverseRegistrar.sol
// See https://github.com/ensdomains/ens/blob/master/contracts/ReverseRegistrar.sol#L32-L38
interface IReverseRegistrar {
    function claim(address owner) external returns (bytes32);
}

// This allows an owner to control the reverse ENS address for the contract.
// NOTE: implementations must invoke _claimReverseENS
abstract contract ENSReversible {
    function _claimReverseENS(address owner) internal virtual {
        _claimReverseENS(ENS_ADDRESS, owner);
    }

    function _claimReverseENS(address ensRegistryAddress, address owner)
        internal
        virtual
    {
        IENS ens = IENS(ensRegistryAddress);
        IReverseRegistrar rr = IReverseRegistrar(ens.owner(ADDR_REVERSE_NODE));
        rr.claim(owner);
    }

    function _ensAddress() internal virtual returns (address) {
        return ENS_ADDRESS;
    }
}

