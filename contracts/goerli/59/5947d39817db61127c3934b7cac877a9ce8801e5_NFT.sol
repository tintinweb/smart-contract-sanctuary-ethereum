contract NFT is IERC4907, INFT, ERC721PresetMinterPauserAutoIdUpgradeable {
    event MintNFT(uint256 nftTokenId, uint8 class, uint256 metaId);

    using StringsUpgradeable for uint256;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    CountersUpgradeable.Counter private _idTracker;

    string public _baseTokenURI;
    address public playground;
    uint8 public classNum;

    // ERC4907 users
    struct UserInfo {
        address user; // address of user role
        uint64 expires; // unix timestamp, user expires
    }

    // ERC4907 users
    mapping(uint256 => UserInfo) internal _users;

    // tokenId -> imageId
    mapping(uint256 => uint256) public nftMetaId;

    mapping(uint8 => uint256) public maxLimit;

    mapping(address => uint256) public memberNNumber;
    mapping(uint256 => mapping(uint8 => uint256)) public totalNft;
    mapping(uint256 => mapping(uint8 => uint256)) public mintCount;
    mapping(uint256 => uint8[2]) public classRange;

    function initialize(
        string memory _name,
        string memory _symbol,
        string memory baseTokenURI,
        address _playground
    ) public initializer {
        super.initialize(_name, _symbol, "");
        _baseTokenURI = baseTokenURI;
        playground = _playground;
        classNum = 6;
    }

    function _baseURI() internal view override returns (string memory) {
        return _baseTokenURI;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        uint256 class = uint256(uint8(tokenId));
        return
            string.concat(
                _baseTokenURI,
                uint256(class).toString(),
                "/",
                nftMetaId[tokenId].toString(),
                ".json"
            );
    }

    function changeBaseURI(string memory baseURI) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "role error");
        _baseTokenURI = baseURI;
    }

    /// @notice set the user and expires of a NFT
    /// @dev The zero address indicates there is no user
    /// Throws if `tokenId` is not valid NFT
    /// @param user  The new user of the NFT
    /// @param expires  UNIX timestamp, The new user could use the NFT before expires
    function setUser(
        uint256 tokenId,
        address user,
        uint64 expires
    ) public {
        require(
            _isApprovedOrOwner(msg.sender, tokenId),
            "ERC721: transfer caller is not owner nor approved"
        );
        UserInfo storage info = _users[tokenId];
        info.user = user;
        info.expires = expires;
        emit UpdateUser(tokenId, user, expires);
    }

    /// @notice Get the user address of an NFT
    /// @dev The zero address indicates that there is no user or the user is expired
    /// @param tokenId The NFT to get the user address for
    /// @return The user address for this NFT
    function userOf(uint256 tokenId) public view returns (address) {
        if (uint256(_users[tokenId].expires) >= block.timestamp) {
            return _users[tokenId].user;
        } else {
            return address(0);
        }
    }

    /// @notice Get the user expires of an NFT
    /// @dev The zero value indicates that there is no user
    /// @param tokenId The NFT to get the user expires for
    /// @return The user expires for this NFT
    function userExpires(uint256 tokenId) public view returns (uint256) {
        return _users[tokenId].expires;
    }

    /// @dev See {IERC165-supportsInterface}.
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override
        returns (bool)
    {
        return
            interfaceId == type(IERC4907).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override {
        super._beforeTokenTransfer(from, to, tokenId);

        if (from != to && _users[tokenId].user != address(0)) {
            delete _users[tokenId];
            emit UpdateUser(tokenId, address(0), 0);
        }
    }

    function __mint(address user, uint8 class) internal returns (uint256) {
        uint256 tokenId = _idTracker.current();
        tokenId = (tokenId << 8) | class;
        _safeMint(user, tokenId);
        _idTracker.increment();
        return tokenId;
    }

    function mintNFT(
        uint256 _productId,
        address user,
        uint256 num
    ) public {
        require(msg.sender == playground, "send err");
        for (uint256 i = 0; i < num; i++) {
            uint8 class = getClassByHash(_productId);
            uint256 batchNum = getBatchNum(_productId, class);
            if (mintCount[_productId][class] >= batchNum) {
                // console.log("class %d", class);
                // console.log("have minted %d", mintCount[_productId][class]);
                // console.log("batchNum %d", batchNum);
                class = 0;
                uint8[2] memory _classRange = classRange[_productId];
                // console.log("start %d", _classRange[0]);
                // console.log("end %d", _classRange[1]);

                for (uint8 j = _classRange[0]; j <= _classRange[1]; j++) {
                    // console.log("for class %d", j);

                    // console.log("for have minted %d", mintCount[_productId][j]);
                    // console.log(
                    //     "for batchNum %d",
                    //     getBatchNum(_productId, j)
                    // );
                    if (
                        mintCount[_productId][j] <
                        getBatchNum(_productId, j)
                    ) {
                        class = j;
                        break;
                    }
                }
                require(class > 0, "no enough");
            }
            require(class > 0, "class err");
            uint256 _tokenId = __mint(user, class);
            uint256 startMetaId = getStartMetaId(_productId, class);
            uint256 metadataId = startMetaId +
                FPEMap.fpeMappingFeistelAuto(
                    mintCount[_productId][class],
                    _tokenId,
                    batchNum
                );
            mintCount[_productId][class]++;
            nftMetaId[_tokenId] = metadataId;
            emit MintNFT(_tokenId, class, metadataId);
        }
    }

    function getStartMetaId(uint256 _productId, uint8 class)
        public
        view
        returns (uint256)
    {
        return totalNft[_productId - 1][class];
    }

    function getBatchNum(uint256 _productId, uint8 class)
        public
        view
        returns (uint256)
    {
        return
            _productId == 1
                ? totalNft[_productId][class]
                : (totalNft[_productId][class] -
                    totalNft[_productId - 1][class]);
    }

    function issueNFT(
        uint256 _productId,
        uint256[] memory numPerClass,
        uint8[2] memory _classRange
    ) public {
        require(msg.sender == playground, "send err");
        require(numPerClass.length == classNum, "cla num err");
        require(_classRange[0] <= _classRange[1], "cla ran err");
        classRange[_productId] = _classRange;
        for (uint8 class = 1; class <= classNum; class++) {
            if (_productId == 1) {
                totalNft[_productId][class] = numPerClass[class - 1];
            } else {
                totalNft[_productId][class] =
                    totalNft[_productId - 1][class] +
                    numPerClass[class - 1];
            }
        }
    }

    function getClassByHash(uint256 _productId) internal view returns (uint8) {
        uint8[2] memory _classRange = classRange[_productId];
        uint256 randomNonce = _idTracker.current();
        if (_classRange[1] == _classRange[0]) {
            return _classRange[1];
        }
        uint256 classCount = _classRange[1] - _classRange[0];
        uint256 random = uint256(
            keccak256(
                abi.encodePacked(
                    randomNonce,
                    msg.sender,
                    block.difficulty,
                    block.timestamp
                )
            )
        ) % classCount;
        return uint8(random) + _classRange[0];
    }

    function setPlayground(address _playground) public {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()),
            "Box: must have admin role"
        );
        playground = _playground;
    }
}