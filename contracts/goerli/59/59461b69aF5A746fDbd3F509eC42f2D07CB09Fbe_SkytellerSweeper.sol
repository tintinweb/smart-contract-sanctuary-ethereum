pragma solidity 0.8.17;

import {IERC20Metadata} from "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import {Math} from "@openzeppelin/contracts/utils/math/Math.sol";
import {AggregatorV3Interface} from
    "chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

import {Skyteller_NegativePrice} from "../SkytellerErrors.sol";
import {Skyteller_ZeroPrice} from "../SkytellerErrors.sol";

library TokenPricing {
    /**
     * @notice Derives the price of tokenIn in terms of tokenOut
     * @dev
     * @param feedIn The token to derive the price of
     * @param feedOut The token to derive the price in terms of
     * @param decimals The number of decimals in the price
     * @return price The price of tokenIn in terms of tokenOut
     */
    function _derivePriceFromFeeds(address feedIn, address feedOut, uint256 decimals)
        internal
        view
        returns (uint256 price)
    {
        uint256 priceIn;
        uint8 decimalsIn;
        uint256 priceOut;
        uint8 decimalsOut;

        (priceIn, decimalsIn) = _tokenPrice(feedIn);
        (priceOut, decimalsOut) = _tokenPrice(feedOut);
        if (priceIn == 0) {
            revert Skyteller_ZeroPrice(feedIn);
        }
        if (priceOut == 0) {
            revert Skyteller_ZeroPrice(feedOut);
        }
        if (decimalsIn > decimalsOut) {
            priceOut *= 10 ** (decimalsIn - decimalsOut);
        } else if (decimalsOut > decimalsIn) {
            priceIn *= 10 ** (decimalsOut - decimalsIn);
        }
        return Math.mulDiv(priceIn, 10 ** decimals, priceOut);
    }

    /**
     * @notice Calculate the amount of tokenOut to receive
     *         based on current price feed data
     * @dev The  amount is calculated as follows:
     *      amountOut = derivedPrice * amountIn.
     *
     * @param amountIn The token to swap from
     * @param amountInDecimals The token to swap to
     * @param amountOutDecimals The amount of tokenIn to swap
     * @param feedIn The token to derive the price of
     * @param feedOut The token to derive the price in terms of
     * @return The expected amount of tokenOut
     */
    function _amountOut(
        uint256 amountIn,
        uint256 amountInDecimals,
        uint256 amountOutDecimals,
        address feedIn,
        address feedOut
    ) internal view returns (uint256) {
        return Math.mulDiv(
            _derivePriceFromFeeds(feedIn, feedOut, amountOutDecimals),
            amountIn,
            10 ** amountInDecimals
        );
    }

    /**
     * Get the latest price from the price feed
     * @param feed The token to fetch the price of
     * @return price The price of the token
     * @return decimals The number of decimals in the price
     */
    function _tokenPrice(address feed) internal view returns (uint256 price, uint8 decimals) {
        (, int256 answer,,,) = AggregatorV3Interface(feed).latestRoundData();
        if (answer < 0) {
            revert Skyteller_NegativePrice(feed);
        }
        return (uint256(answer), AggregatorV3Interface(feed).decimals());
    }
}

