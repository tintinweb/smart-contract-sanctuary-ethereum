pragma solidity ^0.8.17;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/finance/PaymentSplitter.sol";
import "hardhat/console.sol"; // delete me

contract FloorTradeV1 is Ownable, Pausable, PaymentSplitter, ReentrancyGuard {
    using Counters for Counters.Counter;

    /// Enum used for handle the status of a deal
    enum swapStatus {
        Opened,
        Closed,
        Cancelled
    }

    /// Enum used for define which kind of standards are used during the deal
    enum typeStd {
        ERC20,
        ERC721,
        ERC1155
    }
    /* swapStruct is the swap order structure of a counterparty
           the swapStruct contains all the data needed for setup a trade with
           ERC20/ERC721/ERC11555
        */
    struct swapStruct {
        address theContract; // dapp address
        typeStd typeStd; // identify the standard related to the DAPP
        uint256 tokenId; // tokenID if is an ERC721/ERC1155
        uint256 balance; // balance if is an ERC20/ERC1155
    }

    /* swapIntent is the master structure of a swap
           the swapIntent contains the address of the Maker and the Taker
           and the native token involved. 
           The Taker address could be the address(0) if the swap is open.
           It contains also the swap status (if opened or closed)
         */
    struct swapIntent {
        address payable addressMaker; // maker address of the swap
        address payable addressTaker; // taker address of the swap
        uint256 swapStart; // creation date
        uint256 swapEnd; // closing / expiring date
        swapStatus status; // status of the deal. Could be Opened, Closed or Cancelled
    }

    /*  closeStruct is used in _close function
            This structure is used for a better handling of
            the details of a deal
        */
    struct closeStruct {
        address from; // From side
        address to; // To side
        uint256 dealValue; // Original deal value. Used for ERC20
        uint256 fee; // Fee sent to the counterparty
    }

    Counters.Counter private _swapIds; // Counter of the swaps

    /// swapStruct mapping for the details of makers and takers. The uint256 identify the swapId
    mapping(uint256 => swapStruct[]) assetsMaker;
    mapping(uint256 => swapStruct[]) assetsTaker;

    /// Mapping key/value for get the swap infos
    mapping(uint256 => swapIntent) public swapMapping; //TODO public needed?

    /// @dev This event used for query the blockchain for check the situation of the swap
    /// @param _creator is the address that identify the creator of the deal
    /// @param _time is used to identify the date in which the swap happened
    /// @param _status is used to identify the status of the deal (open/closed)
    /// @param _swapId is the id of the swap
    /// @param _counterpart is the address that identify the counterpart of the deal
    event swapEvent(
        address indexed _creator,
        uint256 indexed _time,
        swapStatus indexed _status,
        uint256 _swapId,
        address _counterpart
    );

    /// @dev This event is used for query the blockchain for recover the new counterpart of the specified swap
    /// @param _swapId is the id of the swap
    /// @param _counterpart is the address that identify the counterpart of the deal
    event counterpartEvent(
        uint256 indexed _swapId,
        address indexed _counterpart
    );

    uint256 public swapFee = 100000000000000; // 0.0001 ETH default

    //payment splitter
    address[] private addressList = [
        0x036Ad3eF0F55b42C2d08a8274E92bd84b3F37B07, //m
        0x0000e86Cc1220414743321056FA5D8290dCbD402 //a
    ];
    uint256[] private shareList = [90, 10];

    /*///////////////////////////////////////////////////////////////
                         START FloorTradev1
    //////////////////////////////////////////////////////////////*/
    constructor() PaymentSplitter(addressList, shareList) {}

    /// @notice This function is used for create a swap
    /// @dev This function is used for create a swap. The order is stored on the blockchain and will emit a swapEvent once executed
    /// @param taker address of the taker (eg: address(0))
    /// @param swapEnds when the swap will end
    /// @param _assetsMaker swap structure that contains the detail content of swap creator
    /// @param _assetsTaker swap structure that contains the detail content of swap counterparty
    function createSwapIntent(
        address taker,
        uint256 swapEnds,
        swapStruct[] memory _assetsMaker,
        swapStruct[] memory _assetsTaker
    ) external payable whenNotPaused nonReentrant {
        require(
            _assetsMaker.length > 0 && _assetsTaker.length > 0,
            "no assets"
        );

        // Check if the amount needed is fine for accomplish the deal creation
        require(msg.value == swapFee, "exact wei needed");

        if (swapEnds == 0) {
            swapEnds = block.timestamp + 7 days;
        } else {
            require(swapEnds > block.timestamp + 3600, "minimum 1 hour");
        }

        swapIntent memory _swapIntent = swapIntent({
            addressMaker: payable(_msgSender()),
            addressTaker: payable(taker),
            swapStart: block.timestamp,
            swapEnd: swapEnds,
            status: swapStatus.Opened
        });

        swapMapping[_swapIds.current()] = _swapIntent;

        _validateAssets(_swapIntent, _swapIds.current(), _assetsMaker, true);
        _validateAssets(_swapIntent, _swapIds.current(), _assetsTaker, false);

        emit swapEvent(
            _msgSender(),
            block.timestamp,
            _swapIntent.status,
            _swapIds.current(),
            _swapIntent.addressTaker
        );
        _swapIds.increment();
    }

    /// @notice This function is used interally for create a deal
    /// @dev This function is used interally for create a deal. It will check if everything is setup properly
    /// @param _assets swap structure is the content of a swap
    /// @param _maker identify is is the maker or the taker
    function _validateAssets(
        swapIntent memory swapInt,
        uint256 _swapId,
        swapStruct[] memory _assets,
        bool _maker
    ) private {
        uint256 i;

        //     struct swapStruct {
        //     address theContract; // dapp address
        //     typeStd typeStd; // identify the standard related to the DAPP
        //     uint256 tokenId; // tokenID if is an ERC721/ERC1155
        //     uint256 balance; // balance if is an ERC20/ERC1155

        for (i = 0; i < _assets.length; i++) {
            if (_assets[i].typeStd == typeStd.ERC721) {
                if (_maker) {
                    require(
                        IERC721(_assets[i].theContract).ownerOf(
                            _assets[i].tokenId
                        ) == swapInt.addressMaker,
                        "maker: not enough erc721 balance"
                    );
                } //else taker is not required to have the assets
            }

            if (_assets[i].typeStd == typeStd.ERC20) {
                require(_assets[i].balance > 0, "ERC20 - Check values");

                if (_maker) {
                    require(
                        IERC20(_assets[i].theContract).balanceOf(
                            swapInt.addressMaker
                        ) > _assets[i].balance,
                        "maker: not enough erc20 balance"
                    );
                } //else taker is not required to have the erc20
            }

            if (_assets[i].typeStd == typeStd.ERC1155) {
                require(_assets[i].balance > 0, "ERC1155 - Missing tokenId");

                if (_maker) {
                    require(
                        IERC1155(_assets[i].theContract).balanceOf(
                            swapInt.addressMaker, _assets[i].balance
                        ) >= _assets[i].balance,
                        "maker: not enough erc1155 balance"
                    );
                }//else taker is not required to have the balance

            }

            if (_maker) assetsMaker[_swapId].push(_assets[i]);
            else assetsTaker[_swapId].push(_assets[i]);
        }
    }

    /// @notice This is used for close a swap.
    /// @dev This function close an order that is stored on the blockchain and it will emit a swapEvent once executed
    /// @param _swapId identifier of the order
    function closeSwapIntent(uint256 _swapId)
        public
        whenNotPaused
        nonReentrant
    {
        swapIntent memory swap = swapMapping[_swapId];

        // Status
        require(swap.status == swapStatus.Opened, "not open");
        // Counterpart
        require(
            swap.addressTaker == msg.sender || swap.addressTaker == address(0),
            "not the interested counterpart"
        );

        require(swap.swapEnd >= block.timestamp, "expired");

        // SwapId must be lower or equal to the current one
        require(_swapId < _swapIds.current(), "wrong swap id");

        swapMapping[_swapId].addressTaker = payable(msg.sender);
        swapMapping[_swapId].status = swapStatus.Closed;
        swapMapping[_swapId].swapEnd = block.timestamp;

        _close(_swapId);

        emit swapEvent(
            swapMapping[_swapId].addressTaker,
            block.timestamp,
            swapStatus.Closed,
            _swapId,
            _msgSender()
        );
    }

    /// @notice This function is used interally for close a deal
    /// @dev This function is used interally for close a deal.
    /// @param _swapId swap identifier
    function _close(uint256 _swapId) private {
        //struct swapIntent {
        //     address payable addressMaker; // maker address of the swap
        //     address payable addressTaker; // taker address of the swap
        //     uint256 swapStart; // creation date
        //     uint256 swapEnd; // closing / expiring date
        //     swapStatus status; // status of the deal. Could be Opened, Closed or Cancelled
        // }
        swapIntent memory swap = swapMapping[_swapId];

        swapStruct[] memory aMaker;
        swapStruct[] memory aTaker;

        aMaker = assetsMaker[_swapId];
        aTaker = assetsTaker[_swapId];

        for (uint256 i = 0; i < aMaker.length; i++) {
            if (aMaker[i].typeStd == typeStd.ERC20) {
                IERC20(aMaker[i].theContract).transferFrom(
                    swap.addressMaker,
                    swap.addressTaker,
                    aMaker[i].balance
                );
                console.log(
                    "erc20 transfer from",
                    swap.addressMaker,
                    "to",
                    swap.addressTaker
                );
            } else {
                if (aMaker[i].typeStd == typeStd.ERC721) {
                    IERC721(aMaker[i].theContract).safeTransferFrom(
                        swap.addressMaker,
                        swap.addressTaker,
                        aMaker[i].tokenId
                    );
                    console.log(
                        "erc721 transfer from",
                        swap.addressMaker,
                        "to",
                        swap.addressTaker
                    );
                } else if (aMaker[i].typeStd == typeStd.ERC1155) {
                    IERC1155(aMaker[i].theContract).safeTransferFrom(
                        swap.addressMaker,
                        swap.addressTaker,
                        aMaker[i].tokenId,
                        aMaker[i].balance,
                        ""
                    );
                    console.log(
                        "erc1155 transfer from",
                        swap.addressMaker,
                        "to",
                        swap.addressTaker
                    );
                }
            }
        }

        for (uint256 i = 0; i < aTaker.length; i++) {
            if (aTaker[i].typeStd == typeStd.ERC20) {
                IERC20(aTaker[i].theContract).transferFrom(
                    swap.addressTaker,
                    swap.addressMaker,
                    aTaker[i].balance
                );
                console.log(
                    "erc20 transfer from",
                    swap.addressTaker,
                    "to",
                    swap.addressMaker
                );
            } else {
                if (aTaker[i].typeStd == typeStd.ERC721) {
                    IERC721(aTaker[i].theContract).safeTransferFrom(
                        swap.addressTaker,
                        swap.addressMaker,
                        aTaker[i].tokenId
                    );
                    console.log(
                        "erc721 transfer from",
                        swap.addressTaker,
                        "to",
                        swap.addressMaker
                    );
                } else if (aTaker[i].typeStd == typeStd.ERC1155) {
                    IERC1155(aTaker[i].theContract).safeTransferFrom(
                        swap.addressTaker,
                        swap.addressMaker,
                        aTaker[i].tokenId,
                        aTaker[i].balance,
                        ""
                    );
                    console.log(
                        "erc1155 transfer from",
                        swap.addressTaker,
                        "to",
                        swap.addressMaker
                    );
                }
            }
        }
    }

    /// @notice This is used for cancel a swap.
    /// @dev This function cancel an order that is stored on the blockchain and will emit a swapEvent once executed
    /// @param _swapId identifier of the order
    function cancelSwapIntent(uint256 _swapId) public nonReentrant {
        swapIntent memory swap = swapMapping[_swapId];
        require(swap.addressMaker == _msgSender(), "not owner"); // Check if is the owner
        require(swap.status == swapStatus.Opened, "not open"); // Check swap status

        swapMapping[_swapId].swapEnd = block.timestamp;
        swapMapping[_swapId].status = swapStatus.Cancelled;

        emit swapEvent(
            _msgSender(),
            block.timestamp,
            swapStatus.Cancelled,
            _swapId,
            address(0)
        );

        // Refund the fee to the maker
        (bool success, ) = payable(_msgSender()).call{value: swapFee}("");
        require(success, "failed to send monies");
    }

    /// @notice This function is used to get the master infos about a swap
    /// @dev This function is used to get infos about a swap.
    /// @param _swapId identifier of the order
    /// @return swapIntent that is master order of the interested swap
    function getSwapIntentById(uint256 _swapId)
        public
        view
        returns (swapIntent memory)
    {
        return swapMapping[_swapId];
    }

    /// @notice This function is used to get the details of the assets involved in a swap
    /// @dev This function is used to get the details of the assets involved in a swap, from tokenID for ERC721/ERC1155 to the balances for ERC1155/ERC20
    /// @param _swapId identifier of the order
    /// @param _index uint256 inded used to get the detail of an asset
    /// @return the detail of the interested struct
    function getSwapStructMaker(uint256 _swapId, uint256 _index)
        public
        view
        returns (swapStruct memory)
    {
        return assetsMaker[_swapId][_index];
    }

    /// @notice This function is used to get the details of the assets involved in a swap
    /// @dev This function is used to get the details of the assets involved in a swap, from tokenID for ERC721/ERC1155 to the balances for ERC1155/ERC20
    /// @param _swapId identifier of the order
    /// @param _index uint256 inded used to get the detail of an asset
    /// @return the detail of the interested struct
    function getSwapStructTaker(uint256 _swapId, uint256 _index)
        public
        view
        returns (swapStruct memory)
    {
        return assetsTaker[_swapId][_index];
    }

    /*///////////////////////////////////////////////////////////////
                    CONTRACT MANAGEMENT OPERATIONS
    //////////////////////////////////////////////////////////////*/

    /// @notice sets the swap fee
    function setSwapFee(uint256 _newPrice) external onlyOwner {
        swapFee = _newPrice;
    }

    /// @notice This function is used for pause/unpause the smart contract
    /// @param _paused true or false for enable/disable the pause
    function pauseContract(bool _paused) public onlyOwner {
        _paused ? _pause() : _unpause();
    }

    /// @notice This function is used internally for send the native tokens to other accounts
    /// @dev This function is used internally for send the the native tokens to other accounts
    /// @param _to address is the address that will receive the _amount
    /// @param _amount uint256 value involved in the deal
    /// @return true or false if the transfer worked out
    function transferFees(address _to, uint256 _amount) private returns (bool) {
        bool success = true;
        if (_amount > 0) (success, ) = payable(_to).call{value: _amount}("");
        return success;
    }
}

