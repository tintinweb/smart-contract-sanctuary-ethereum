pragma solidity >=0.8.0 <0.9.0;

import '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol';
import './interfaces/IMagpieLPToken.sol';
import './interfaces/IMagpieTokenDisplayInfo.sol';
import './interfaces/IMagpieListManager.sol';
import './interfaces/IMagpieProviders.sol';
import './security/PausableUpgradeable.sol';

contract MagpieLPToken is
  ReentrancyGuardUpgradeable,
  ERC721EnumerableUpgradeable,
  ERC721URIStorageUpgradeable,
  OwnableUpgradeable,
  PausableUpgradeable
{
  address private magpieProvidersAddress;
  IMagpieListManager private magpieListManager;
  mapping(uint256 => IMagpieLPToken.MagpieLPTokenInfo) private tokenMetadata;
  mapping(address => IMagpieTokenDisplayInfo) private magpieTokenDisplayInfos;

  event MagpieProvidersUpdated(address indexed lpm);
  event MagpieListManagerUpdated(address indexed manager);
  event MagpieTokenDisplayInfoUpdated(address indexed tokenAddress, IMagpieTokenDisplayInfo indexed magpieTokenDisplayInfo);

  function initialize(
    string calldata _name,
    string calldata _symbol,
    address _pauser
  ) external initializer {
    __Ownable_init();
    __ERC721_init(_name, _symbol);
    __ERC721Enumerable_init();
    __Pausable_init(_pauser);
    __ERC721URIStorage_init();
    __ReentrancyGuard_init();
  }

  modifier onlyMagpiePools() {
    require(msg.sender == magpieProvidersAddress, 'MagpieLPToken: unauthorized');
    _;
  }

  function setMagpieTokenDisplayInfo(address _tokenAddress, IMagpieTokenDisplayInfo _magpieTokenDisplayInfo) external onlyOwner {
    require(_magpieTokenDisplayInfo != IMagpieTokenDisplayInfo(address(0)), 'MagpieLPToken: invalid magpieTokenDisplayInfo');
    require(_tokenAddress != address(0), 'MagpieLPToken: invalidTokenAddress');
    magpieTokenDisplayInfos[_tokenAddress] = _magpieTokenDisplayInfo;
    emit MagpieTokenDisplayInfoUpdated(_tokenAddress, _magpieTokenDisplayInfo);
  }

  function setMagpieProviders(address _magpieProvider) external onlyOwner {
    require(_magpieProvider != address(0), 'MagpieLPToken: invalid magpieProvider');
    magpieProvidersAddress = _magpieProvider;
    emit MagpieProvidersUpdated(_magpieProvider);
  }

  function setMagpieListManager(address _magpieListManager) external onlyOwner {
    require(_magpieListManager != address(0), 'MagpieLPToken: invalid magpieListManager');
    magpieListManager = IMagpieListManager(_magpieListManager);
    emit MagpieListManagerUpdated(_magpieListManager);
  }

  function getAllNftIdsByUser(address _owner) public view returns (uint256[] memory) {
    uint256[] memory nftIds = new uint256[](balanceOf(_owner));
    uint256 length = nftIds.length;
    for (uint256 i; i < length; ) {
      nftIds[i] = tokenOfOwnerByIndex(_owner, i);
      unchecked {
        ++i;
      }
    }
    return nftIds;
  }

  function mint(address _to) external onlyMagpiePools whenNotPaused nonReentrant returns (uint256) {
    uint256 tokenId = totalSupply() + 1;
    _safeMint(_to, tokenId);
    return tokenId;
  }

  function updateTokenInfo(uint256 _tokenId, IMagpieLPToken.MagpieLPTokenInfo calldata _lpTokenMetadata)
    external
    onlyMagpiePools
    whenNotPaused
  {
    require(_exists(_tokenId), 'MagpieLPToken: tokenId invalid');
    tokenMetadata[_tokenId] = _lpTokenMetadata;
  }

  function exists(uint256 _tokenId) external view returns (bool) {
    return _exists(_tokenId);
  }

  function supportsInterface(bytes4 interfaceId)
    public
    view
    virtual
    override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }

  function tokenURI(uint256 tokenId)
    public
    view
    virtual
    override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
    returns (string memory)
  {
    address tokenAddress = tokenMetadata[tokenId].token;
    require(
      magpieTokenDisplayInfos[tokenAddress] != IMagpieTokenDisplayInfo(address(0)),
      'MagpieLPToken: magpieTokenDisplayInfo invalid'
    );

    IMagpieTokenDisplayInfo magpieTokenDisplayInfo = IMagpieTokenDisplayInfo(magpieTokenDisplayInfos[tokenAddress]);

    string memory svgData = magpieTokenDisplayInfo.getTokenDetails(
      tokenId,
      tokenMetadata[tokenId].suppliedLiquidity,
      IMagpieProviders(magpieProvidersAddress).totalReserve(tokenAddress)
    );

    string memory description = magpieTokenDisplayInfo.getDescription(
      tokenMetadata[tokenId].suppliedLiquidity,
      IMagpieProviders(magpieProvidersAddress).totalReserve(tokenAddress)
    );

    string memory attributes = magpieTokenDisplayInfo.getAttributes(
      tokenMetadata[tokenId].suppliedLiquidity,
      IMagpieProviders(magpieProvidersAddress).totalReserve(tokenAddress)
    );

    string memory json = Base64Upgradeable.encode(
      bytes(
        string(
          abi.encodePacked(
            '{"name": "',
            name(),
            '", "description": "',
            description,
            '", "image": "data:image/svg+xml;base64,',
            Base64Upgradeable.encode(bytes(svgData)),
            '", "attributes": ',
            attributes,
            '}'
          )
        )
      )
    );
    return string(abi.encodePacked('data:application/json;base64,', json));
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 tokenId
  ) internal virtual override(ERC721EnumerableUpgradeable, ERC721Upgradeable) whenNotPaused {
    super._beforeTokenTransfer(from, to, tokenId);

    // Only call list manager for NFT Transfers, not mint and burns
    if (from != address(0) && to != address(0)) {
      magpieListManager.beforeTransferringLiquidity(
        from,
        to,
        tokenMetadata[tokenId].token,
        tokenMetadata[tokenId].suppliedLiquidity
      );
    }
  }

  function _burn(uint256 tokenId)
    internal
    virtual
    override(ERC721URIStorageUpgradeable, ERC721Upgradeable)
  {
    ERC721URIStorageUpgradeable._burn(tokenId);
  }
}

