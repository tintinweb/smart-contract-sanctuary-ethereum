import {ECDSA} from '@openzeppelin/contracts/utils/cryptography/ECDSA.sol';

// contracts
import {Initializable} from '@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol';
import {AccessControlUpgradeable} from '@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol';

import 'hardhat/console.sol';

contract OfficerUpgradeable is
    Initializable,
    AccessControlUpgradeable,
    IOfficerUpgradeable
{
    using Bytes32 for string;
    using SafeERC20 for IERC20;

    bytes32 public constant ADMIN_ROLE = keccak256('ADMIN');
    bytes32 public constant SUPERADMIN_ROLE = DEFAULT_ADMIN_ROLE;

    address public superAdmin;
    address public permissionOracle;

    mapping(address => uint256) public withdrawCooldown;
    mapping(uint256 => address) public getDexAddress;

    // read

    function hasRoles(
        bytes32[] memory roles,
        address account
    ) external view returns (bool) {
        require(roles.length != 0, 'empty roles');

        for (uint256 i = 0; i < roles.length; ) {
            if (hasRole(roles[i], account)) return true;
            unchecked {
                ++i;
            }
        }
        return false;
    }

    function hasPermission(
        address user,
        address contract_,
        uint256 expiresAt,
        bytes calldata data,
        bytes calldata signature
    ) external view returns (bool) {
        if (block.timestamp > expiresAt) return false;
        if (data.length == 0) return false;
        if (signature.length == 0) return false;

        // console.log('hasPermission');
        // console.log('user', user);
        // console.log('contract_', contract_);
        // console.log('expiresAt', expiresAt);
        // console.log('data length', data.length);
        // console.log('data start');
        // console.logBytes32(bytes32(data[0:32]));
        // console.log('data end');
        // console.logBytes32(bytes32(data[data.length - 32:data.length]));
        // console.log('signature length', signature.length);
        // console.log('signature start');
        // console.logBytes32(bytes32(signature[0:32]));
        // console.log('signature end');
        // console.logBytes32(bytes32(signature[signature.length - 32:signature.length]));

        bytes memory message = abi.encodePacked(user, contract_, expiresAt, data);
        // console.log('message length', message.length);
        // console.log('message start');
        // console.logBytes32(bytes32(message));

        bytes32 hash1 = keccak256(message);
        // console.log('hash1');
        // console.logBytes32(hash1);

        bytes32 hash2 = ECDSA.toEthSignedMessageHash(hash1);
        // console.log('hash2');
        // console.logBytes32(hash2);

        (address signer, ECDSA.RecoverError error) = ECDSA.tryRecover(hash2, signature);
        return error == ECDSA.RecoverError.NoError && signer == permissionOracle;
    }

    // write

    function _setSuperAdmin(address superAdmin_) internal {
        if (superAdmin != address(0)) _revokeRole(SUPERADMIN_ROLE, superAdmin);
        superAdmin = superAdmin_;
        _grantRole(SUPERADMIN_ROLE, superAdmin_);
    }

    function setSuperAdmin(address superAdmin_) external onlyRole(SUPERADMIN_ROLE) {
        require(superAdmin_ != address(0), 'zero address');
        _setSuperAdmin(superAdmin_);
    }

    function grantRole(
        bytes32 role,
        address account
    )
        public
        virtual
        override(AccessControlUpgradeable, IAccessControlUpgradeable)
        onlyRole(getRoleAdmin(role))
    {
        require(role != SUPERADMIN_ROLE, 'SUPERADMIN_ROLE');
        _grantRole(role, account);
    }

    function batchGrantRoles(
        bytes32[] calldata roles,
        address[] calldata accounts
    ) external {
        require(roles.length == accounts.length, 'bad length');

        for (uint256 i = 0; i < roles.length; ) {
            grantRole(roles[i], accounts[i]);
            unchecked {
                ++i;
            }
        }
    }

    function batchRevokeRoles(
        bytes32[] calldata roles,
        address[] calldata accounts
    ) external {
        require(roles.length == accounts.length, 'bad length');

        for (uint256 i = 0; i < roles.length; ) {
            revokeRole(roles[i], accounts[i]);
            unchecked {
                ++i;
            }
        }
    }

    function setPermissionOracle(
        address permissionOracle_
    ) external onlyRole(SUPERADMIN_ROLE) {
        permissionOracle = permissionOracle_;
    }

    function adminWithdraw(
        address vault,
        address token,
        address to,
        uint256 amount
    ) external onlyRole(ADMIN_ROLE) {
        require(block.timestamp > withdrawCooldown[msg.sender], 'cooldown in progress');
        withdrawCooldown[msg.sender] = block.timestamp + 6 hours;
        IVaultMainUpgradeable(vault).withdraw(token, to, amount);
    }

    constructor() {
        _disableInitializers();
    }

    function init(
        address superAdmin_,
        address permissionOracle_,
        address[] memory dexAddresses
    ) external initializer {
        _setSuperAdmin(superAdmin_);
        permissionOracle = permissionOracle_;
        for (uint256 i = 0; i < dexAddresses.length; ) {
            getDexAddress[i] = dexAddresses[i];
            unchecked {
                ++i;
            }
        }
    }

    function batchSetDexAddress(
        uint256[] calldata dexIds,
        address[] calldata dexAddresses
    ) external {
        require(
            hasRole(SUPERADMIN_ROLE, msg.sender) || hasRole(ADMIN_ROLE, msg.sender),
            'no access'
        );
        require(dexIds.length != 0, 'empty dexIds');
        require(dexIds.length == dexAddresses.length, 'bad length');

        for (uint256 i = 0; i < dexIds.length; ) {
            address oldDex = getDexAddress[dexIds[i]];
            require(
                oldDex == address(0) ||
                    IUniswapV2Router02(oldDex).factory() ==
                    IUniswapV2Router02(dexAddresses[i]).factory(),
                'different factory'
            );

            getDexAddress[dexIds[i]] = dexAddresses[i];
            unchecked {
                ++i;
            }
        }
    }
}

