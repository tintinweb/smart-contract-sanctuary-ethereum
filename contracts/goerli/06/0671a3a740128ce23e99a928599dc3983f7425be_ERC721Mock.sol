contract ERC721Mock is ERC721 {
    bool is_open = false;
    address owner;
    uint256 totalSupply = 0;

    function set_open() external {
        require(msg.sender == owner);

        is_open = !is_open;
    }

    modifier onlyOpen() {
        require(is_open, "mint not available");
        _;
    }

    constructor(string memory name, string memory symbol) ERC721(name, symbol) {
        owner = msg.sender;
    }

    function baseURI() public view returns (string memory) {
        return _baseURI();
    }

    function exists(uint256 tokenId) public view returns (bool) {
        return _exists(tokenId);
    }

    function mint(address to) public onlyOpen {
        _mint(to, totalSupply++);
    }

    function safeMint(address to) public onlyOpen {
        _safeMint(to, totalSupply++);
    }

    function safeMint(address to, bytes memory _data) public onlyOpen {
        _safeMint(to, totalSupply++, _data);
    }

    function burn(uint256 tokenId) public {
        _burn(tokenId);
    }
}

