contract StriptoMarketplace is Ownable, ReentrancyGuard {
    using Counters for Counters.Counter;
    Counters.Counter private _itemIds; // Id for each individual item
    // Counters.Counter private _itemsSold; // Number of items sold
    Counters.Counter private _tokenIds;

    address payable ownerAddr; // The owner of the StriptoMarket contract (transfer and send function availabe to payable addresses)
    uint256 listingFee = 0.0001 ether;
    uint256 saleFee = 100;
    uint256 buyFee = 100;
    uint256 stripingSaleFee = 100 ;
    uint256 stripingBuyFee = 0;
    uint256 mintingFee = 0.0001 ether;

    struct whiteListItem {
        uint8 status;
        uint256 endTime;
    }

    struct signObj {
        address to;
        address nftContract;
        uint256 itemId;
        uint256 amount;
        uint256 actualPrice;
        uint256 tokenValueinWei;
        string message;
        string nonce;
        bytes signature;
    }

    // now < endTime;
    mapping(address => uint8) private _allowList;
    mapping(address => whiteListItem) private _whiteList;

    address nftContractAddress;
    StriptoNft striptoNftContract;

    constructor(address[] memory addresses) {
        ownerAddr = payable(msg.sender);
        striptoNftContract = new StriptoNft();
        nftContractAddress = address(striptoNftContract); // here

        for (uint256 i = 0; i < addresses.length; i++) {
            _allowList[addresses[i]] = 1;
        }
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function setAllowList(address _address, uint8 _status) public {
        require(
            _allowList[msg.sender] > 0,
            "You are not authorised to perform this action"
        );

        _allowList[_address] = _status;
    }

    // function removeAllowList(address _address) public {
    //     require(
    //         _allowList[msg.sender] > 0,
    //         "You are not authorised to perform this action"
    //     );
    //     _allowList[_address] = 0;
    // }

    function setWhiteList(
        address[] calldata addresses,
        uint8 status,
        uint256 endTime
    ) public {
        require(
            _allowList[msg.sender] > 0,
            "You are not authorised to perform this action"
        );
        for (uint256 i = 0; i < addresses.length; i++) {
            _whiteList[addresses[i]] = whiteListItem(status, endTime);
        }
    }

    event Sold(address sender, uint256 soldPrice);

    event Minted(uint256 tokenId);

    struct MarketItem {
        uint256 itemId;
        address nftContract;
        uint256 tokenId;
        address payable seller;
        address payable creator;
        uint256 soldOn;
        uint256 price;
        uint256 strippedPrice;
        uint256 royality;
        uint256 listingPrice;
        bool isSold;
    }

    mapping(uint256 => MarketItem) private idToMarketItem;

    function getListingFee() public view returns (uint256) {
        return listingFee;
    }

    function getNftAddress() public view returns (address) {
        return nftContractAddress;
    }

    function setListingFee(uint256 fee) public onlyOwner {
        listingFee = fee;
    }

    function setMintingFee(uint256 fee) public onlyOwner {
        mintingFee = fee;
    }

    function getMintingFee() public view returns (uint256) {
        return mintingFee;
    }

    function getSaleFee() public view returns (uint256) {
        return saleFee;
    }

    function getStrippingSaleFee() public view returns (uint256) {
        return stripingSaleFee;
    }

    function setStrippingSaleFee(uint256 fee) public onlyOwner {
        stripingSaleFee = fee;
    }

    function getStrippingBuyFee() public view returns (uint256) {
        return stripingBuyFee;
    }

    function setStrippingBuyFee(uint256 fee) public onlyOwner {
        stripingBuyFee = fee;
    }

    function setSaleFee(uint256 fee) public onlyOwner {
        saleFee = fee;
    }

    function getBuyFee() public view returns (uint256) {
        return buyFee;
    }

    function setBuyFee(uint256 fee) public onlyOwner {
        buyFee = fee;
    }

    function setFees(
        uint256 minting_fee,
        uint256 listing_fee,
        uint256 sale_fee,
        uint256 buy_fee,
        uint256 stripping_sale_fee,
        uint256 stripping_buy_fee
    ) public {
        require(
            _allowList[msg.sender] > 0,
            "You are not authorised to perform this action"
        );
        saleFee = sale_fee;
        buyFee = buy_fee;
        listingFee = listing_fee;
        mintingFee = minting_fee;
        stripingBuyFee = stripping_buy_fee;
        stripingSaleFee = stripping_sale_fee;
    }

    // Event is an inhertable contract that can be used to emit events
    event MarketItemCreated(
        uint256 indexed itemId,
        uint256 indexed tokenId,
        address seller,
        address creator,
        uint256 price,
        uint256 strippedPrice,
        uint256 soldOn,
        uint256 royality,
        uint256 listingPrice,
        bool isSold
    );

    function mintNFT(string memory tokenURI) public payable nonReentrant {
        if (
            ((_whiteList[msg.sender].status == 0) ||
                (block.timestamp > _whiteList[msg.sender].endTime))
        ) {
            require(msg.value >= mintingFee, "Minting Fee need to be pay");
        }
        uint256 tokenId = striptoNftContract.mintNFT(
            address(0),
            msg.sender,
            tokenURI
        );

        emit Minted(tokenId);
    }

    function listOnMarket(
        address nftContract,
        uint256 tokenId,
        uint256 price,
        uint256 royality
    ) public payable nonReentrant {
        require(price > 0, "No item for free here");
        if (
            ((_whiteList[msg.sender].status == 0) ||
                (block.timestamp > _whiteList[msg.sender].endTime))
        ) {
            require(msg.value >= listingFee, "Listing Fee need to be pay");
        }

        uint256 soldOn = price;
        uint256 listingPrice = price;
        _itemIds.increment();

        uint256 itemId = _itemIds.current();

        idToMarketItem[itemId] = MarketItem(
            itemId,
            nftContract,
            tokenId,
            payable(msg.sender),
            payable(msg.sender),
            price,
            price,
            soldOn,
            royality,
            listingPrice,
            false
        );
        //IERC721(nftContract).transferFrom(msg.sender, address(this), tokenId);

        emit MarketItemCreated(
            itemId,
            tokenId,
            msg.sender,
            msg.sender,
            price,
            price,
            soldOn,
            royality,
            listingPrice,
            false
        );
    }

    function resell(
        uint256 itemId,
        uint256 price,
        uint256 royality
    ) public payable {
        if (
            ((_whiteList[msg.sender].status == 0) ||
                (block.timestamp > _whiteList[msg.sender].endTime))
        ) {
            require(msg.value >= listingFee, "Listing Fee need to be pay");
        }

        require(
            idToMarketItem[itemId].seller == msg.sender,
            "Only item owner can perform this operation"
        );
        idToMarketItem[itemId].isSold = false;
        idToMarketItem[itemId].price = price;
        if (idToMarketItem[itemId].creator == idToMarketItem[itemId].seller)
            idToMarketItem[itemId].royality = royality;

        idToMarketItem[itemId].strippedPrice = price;
        idToMarketItem[itemId].seller = payable(msg.sender);
    }

    /* function privateNft(address nftContract, uint256 itemId) public {
        uint256 tokenId = idToMarketItem[itemId].tokenId;
        if (
            IERC721(nftContract).ownerOf(idToMarketItem[itemId].tokenId) ==
            address(this)
        ) {
            IERC721(nftContract).transferFrom(
                address(this),
                msg.sender,
                tokenId
            );
        }
    } */

    function getMessageHash(signObj memory _signObj)
        public
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encodePacked(
                    _signObj.to,
                    _signObj.nftContract,
                    _signObj.itemId,
                    _signObj.amount,
                    _signObj.tokenValueinWei,
                    _signObj.actualPrice,
                    _signObj.message,
                    _signObj.nonce
                )
            );
    }

    function getEthSignedMessageHash(bytes32 _messageHash)
        public
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encodePacked(
                    "\x19Ethereum Signed Message:\n32",
                    _messageHash
                )
            );
    }

    function recoverSigner(
        bytes32 _ethSignedMessageHash,
        bytes memory _signature
    ) public pure returns (address) {
        (bytes32 r, bytes32 s, uint8 v) = splitSignature(_signature);

        return ecrecover(_ethSignedMessageHash, v, r, s);
    }

    function splitSignature(bytes memory sig)
        public
        pure
        returns (
            bytes32 r,
            bytes32 s,
            uint8 v
        )
    {
        require(sig.length == 65, "invalid signature length");

        assembly {
            // first 32 bytes, after the length prefix
            r := mload(add(sig, 32))
            // second 32 bytes
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 bytes)
            v := byte(0, mload(add(sig, 96)))
        }

        // implicitly return (r, s, v)
    }

    function buyStrippedNFT(signObj memory _signObj)
        public
        payable
        nonReentrant
    {
        uint256 itemId = _signObj.itemId;
        uint256 tokenId = idToMarketItem[itemId].tokenId;
        uint256 nftPrice =_signObj.actualPrice;

        require(
            idToMarketItem[itemId].isSold == false,
            "Sorry NFT is already sold"
        );

        _signObj.to = msg.sender;
        _signObj.amount = msg.value;

        bytes32 messageHash = getMessageHash(_signObj);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);

        require(
            recoverSigner(ethSignedMessageHash, _signObj.signature) ==
                ownerAddr,
            "Unauthorised request."
        );


        uint256 soldPrice = SafeMath.add(nftPrice, _signObj.tokenValueinWei);
        uint256 salePercentage = 0;
        if (stripingSaleFee > 0) {
            salePercentage =SafeMath.div(SafeMath.mul(nftPrice, stripingSaleFee),10000);

        }

        if (
            idToMarketItem[itemId].royality > 0 &&
            (idToMarketItem[itemId].creator != idToMarketItem[itemId].seller)
        ) {
    
            uint256 royality=SafeMath.div(SafeMath.mul(nftPrice, idToMarketItem[itemId].royality),10000);
            // transfer royality  to creator
            idToMarketItem[itemId].creator.transfer(royality);

            nftPrice = SafeMath.sub(nftPrice, royality);
        }

        if (salePercentage > 0) {
            nftPrice = SafeMath.sub(nftPrice, salePercentage);
        }

        // transfer left amount to seller

        idToMarketItem[itemId].seller.transfer(nftPrice);

        // transfer nft to buyer
        IERC721(_signObj.nftContract).transferFrom(
            address(idToMarketItem[itemId].seller),
            msg.sender,
            tokenId
        );

        idToMarketItem[itemId].soldOn = soldPrice;
        idToMarketItem[itemId].isSold = true;
        idToMarketItem[itemId].seller = payable(msg.sender);
        // _itemsSold.increment();

        emit Sold(msg.sender, msg.value);
    }

    function buyNFT(signObj memory _signObj) public payable nonReentrant {
        uint256 itemId = _signObj.itemId;
        uint256 tokenId = idToMarketItem[itemId].tokenId;
        uint256 nftPrice =_signObj.actualPrice;

        require(
            idToMarketItem[itemId].isSold == false,
            "Sorry NFT is already sold"
        );
        _signObj.to = msg.sender;
        _signObj.amount = msg.value;

        bytes32 messageHash = getMessageHash(_signObj);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);

        require(
            recoverSigner(ethSignedMessageHash, _signObj.signature) ==
                ownerAddr,
            "Unauthorised request."
        );

        uint256 soldPrice = SafeMath.add(nftPrice, _signObj.tokenValueinWei);
        uint256 salePercentage = 0;
        if (saleFee > 0) {
            salePercentage =SafeMath.div(SafeMath.mul(nftPrice, saleFee),10000);
        }

        if (
            idToMarketItem[itemId].royality > 0 &&
            (idToMarketItem[itemId].creator != idToMarketItem[itemId].seller)
        ) {
            uint256 royality = SafeMath.div(SafeMath.mul(nftPrice, idToMarketItem[itemId].royality),10000);
            
            // transfer royality  to creator
            if(royality>0)
               idToMarketItem[itemId].creator.transfer(royality);

            nftPrice = SafeMath.sub(nftPrice, royality);
        }

        if (salePercentage > 0) {
            nftPrice = SafeMath.sub(nftPrice, salePercentage);
        }

        // transfer left amount to seller

        idToMarketItem[itemId].seller.transfer(nftPrice);

        // transfer nft to buyer
        IERC721(_signObj.nftContract).transferFrom(
            address(idToMarketItem[itemId].seller),
            msg.sender,
            tokenId
        );

        idToMarketItem[itemId].soldOn = soldPrice;
        idToMarketItem[itemId].isSold = true;
        idToMarketItem[itemId].seller = payable(msg.sender);
        // _itemsSold.increment();

        emit Sold(msg.sender, msg.value);
    }

    function getItemDetails(uint256 itemId)
        public
        view
        returns (MarketItem memory)
    {
        MarketItem storage currentItem = idToMarketItem[itemId];
        return currentItem;
    }

    function getTokenBalance(address token) public view returns (uint256) {
        return IERC20(token).balanceOf(address(this));
    }

    function transferTokens(
        address token,
        uint256 _tokenamount,
        address seller
    ) public {
        require(
            _allowList[msg.sender] > 0,
            "You are not authorised to perform this action"
        );
        require(
            IERC20(token).balanceOf(address(this)) >= _tokenamount,
            "Insufficient funds"
        );

        IERC20(token).approve(address(this), _tokenamount);

        IERC20(token).transferFrom(
            address(this),
            payable(seller),
            _tokenamount
        );
    }
}

