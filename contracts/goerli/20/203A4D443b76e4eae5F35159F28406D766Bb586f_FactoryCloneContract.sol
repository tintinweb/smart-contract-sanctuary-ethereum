import "@openzeppelin/contracts/access/Ownable.sol";
import "./proxy.sol";
import "./emitter.sol";
import "./implementation.sol";
import "./nft.sol";

/// @title StationXFactory Cloning Contract
/// @dev Contract create proxies of DAO Token and Governor contract
contract FactoryCloneContract is Ownable {
    address public ImplementationAddress;
    address public USDCAddress;
    address public emitterAddress;

    error AmountInvalid(string _param, uint256 _amount);
    error AddressInvalid(string _param, address _address);
    error DepositAmountInvalid(
        uint256 _maxDepositPerUser,
        uint256 _minDepositPerUser
    );
    error RaiseAmountInvalid(
        uint256 _totalRaiseAmount,
        uint256 _maxDepositPerUser
    );

    /// @dev Initial Deployment of Token and Governor contract and storing their addresses for proxies creation
    constructor(address _USDC) {
        if (_USDC == address(0))
            revert AddressInvalid("_USDC", _USDC);

        //Deploying implementation contract for the reference
        ImplementationAddress = address(new ERC20NonTransferable());

        //USDC address
        USDCAddress = _USDC;

        //Deploying Emitter contract
        address emitterImplementation = address(new Emitter());

        //data
        bytes memory data = abi.encodeWithSignature(
            "Initialize(address,address,address)",
            ImplementationAddress,
            _USDC,
            address(this)
        );

        //owner
        address owner = msg.sender;

        //proxy
        emitterAddress = address(
            new ProxyContract(emitterImplementation, owner, data)
        );
    }

    /// @dev Function to change base Governor contract address
    /// @param _newImplementationAddress address of new Governor contract
    function changeDAOImplementation(address _newImplementationAddress)
        external
        payable
        onlyOwner
    {
        if (_newImplementationAddress == address(0))
            revert AddressInvalid(
                "_newImplementationAddress",
                _newImplementationAddress
            );
        ImplementationAddress = _newImplementationAddress;
        Emitter(emitterAddress).changeImplementationAddress(
            _newImplementationAddress
        );
    }

    /// @dev Function to create proxies and initialization of Token and Governor contract
    function createDAO(
        string memory _DaoName,
        string memory _DaoSymbol,
        uint256 _totalRaiseAmount,
        uint256 _minDepositPerUser,
        uint256 _maxDepositPerUser,
        uint256 _ownerFeePerDeposit,
        uint256 _depositFunctioningDays,
        bool _feeUSDC,
        uint256 _quorum,
        uint256 _threshold,
        address _gnosisAddress,
        address[] memory _daoAdmins,
        bool _isGovernanceActive
    ) external returns (address) {
        if (_quorum == 0) revert AmountInvalid("_quorum", _quorum);
        if (_quorum > 100) revert AmountInvalid("_quorum", _quorum);

        if (_threshold == 0) revert AmountInvalid("_threshold", _threshold);
        if (_threshold > 100) revert AmountInvalid("_threshold", _threshold);

        if (_depositFunctioningDays == 0)
            revert AmountInvalid(
                "_depositFunctioningDays",
                _depositFunctioningDays
            );

        if (!(_ownerFeePerDeposit < 100))
            revert AmountInvalid("_ownerFeePerDeposit", _ownerFeePerDeposit);

        if (_gnosisAddress == address(0))
            revert AddressInvalid("_gnosisAddress", _gnosisAddress);

        if (_minDepositPerUser == 0)
            revert AmountInvalid("_minDepositPerUser", _minDepositPerUser);

        if (_maxDepositPerUser == 0)
            revert AmountInvalid("_maxDepositPerUser", _maxDepositPerUser);

        if (_maxDepositPerUser < _minDepositPerUser)
            revert DepositAmountInvalid(_maxDepositPerUser, _minDepositPerUser);

        if (_totalRaiseAmount < _maxDepositPerUser)
            revert RaiseAmountInvalid(_totalRaiseAmount, _maxDepositPerUser);

        bytes memory data = abi.encodeWithSignature(
            "initializeERC20(string,string,uint256,uint256,uint256,uint256,uint256,bool,uint256,uint256,address,address,address,address[],bool)",
            _DaoName,
            _DaoSymbol,
            _totalRaiseAmount,
            _minDepositPerUser,
            _maxDepositPerUser,
            _ownerFeePerDeposit,
            _depositFunctioningDays,
            _feeUSDC,
            _quorum,
            _threshold,
            _gnosisAddress,
            USDCAddress,
            emitterAddress,
            _daoAdmins,
            _isGovernanceActive
        );

        address owner = owner();
        address proxy = address(
            new ProxyContract(ImplementationAddress, owner, data)
        );

        Emitter(emitterAddress).createDao(
            _gnosisAddress,
            proxy,
            _DaoName,
            _DaoSymbol,
            _totalRaiseAmount,
            _minDepositPerUser,
            _maxDepositPerUser,
            _ownerFeePerDeposit,
            _depositFunctioningDays,
            _feeUSDC,
            _quorum,
            _threshold,
            emitterAddress,
            USDCAddress,
            _daoAdmins,
            _isGovernanceActive
        );

        return proxy;
    }

    function createDaoERC721(
        string memory _DaoName,
        string memory _DaoSymbol,
        uint256 _ownerFeePerDeposit,
        uint256 _depositFunctioningDays,
        uint256 _quorum,
        uint256 _threshold,
        address _gnosisAddress,
        address[] memory _daoAdmins,
        uint256 _maxTokensPerUser,
        uint256 _totalSupplyOfToken,
        uint256 _priceOfNft,
        bool _isNftTransferable,
        bool _isNftTotalSupplyUnlimited,
        bool _isGovernanceActive
    ) external returns (address) {
        if (_quorum == 0) revert AmountInvalid("_quorum", _quorum);
        if (_quorum > 100) revert AmountInvalid("_quorum", _quorum);

        if (_threshold == 0) revert AmountInvalid("_threshold", _threshold);
        if (_threshold > 100) revert AmountInvalid("_threshold", _threshold);

        if (_depositFunctioningDays == 0)
            revert AmountInvalid(
                "_depositFunctioningDays",
                _depositFunctioningDays
            );

        if (!(_ownerFeePerDeposit < 100))
            revert AmountInvalid("_ownerFeePerDeposit", _ownerFeePerDeposit);

        if (_gnosisAddress == address(0))
            revert AddressInvalid("_gnosisAddress", _gnosisAddress);

        if (_totalSupplyOfToken == 0)
            revert AmountInvalid("_totalSupplyOfToken", _totalSupplyOfToken);

        if (_maxTokensPerUser == 0)
            revert AmountInvalid("_maxTokensPerUser", _maxTokensPerUser);

        bytes memory data = abi.encodeWithSignature(
            "initializeERC721(uint256,uint256,uint256,uint256,address,address,address,address[],uint256,bool)",
            _ownerFeePerDeposit,
            _depositFunctioningDays,
            _quorum,
            _threshold,
            _gnosisAddress,
            USDCAddress,
            emitterAddress,
            _daoAdmins,
            _priceOfNft,
            _isGovernanceActive
        );

        address owner = owner();
        address proxy = address(
            new ProxyContract(ImplementationAddress, owner, data)
        );

        address nftContractAddress = address(
            new Nft(
                _DaoName,
                _DaoSymbol,
                _maxTokensPerUser,
                _totalSupplyOfToken,
                _isNftTransferable,
                _isNftTotalSupplyUnlimited,
                proxy,
                emitterAddress
            )
        );

        ERC20NonTransferable(proxy).setNftAddressFunction(nftContractAddress);

        Emitter(emitterAddress).createDaoNftTemplate(
            nftContractAddress,
            _gnosisAddress,
            proxy,
            _DaoName,
            _DaoSymbol,
            _totalSupplyOfToken,
            _maxTokensPerUser,
            _ownerFeePerDeposit,
            _depositFunctioningDays,
            _quorum,
            _threshold,
            emitterAddress,
            USDCAddress,
            _daoAdmins
        );

        return proxy;
    }

    /// @dev Function to assign USDC address
    /// @param _newUSDCAddress address of USDC token
    function changeUSDCAddress(address _newUSDCAddress)
        external
        payable
        onlyOwner
    {
        if (_newUSDCAddress == address(0))
            revert AddressInvalid("_newUSDCAddress", _newUSDCAddress);

        USDCAddress = _newUSDCAddress;
        Emitter(emitterAddress).changedUSDCAddress(_newUSDCAddress);
    }
}

