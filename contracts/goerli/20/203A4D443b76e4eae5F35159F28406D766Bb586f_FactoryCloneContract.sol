pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "./implementation.sol";
import "./emitter.sol";

contract Nft is ERC721Enumerable, Ownable {
    uint256 public _tokenIdTracker;

    bool public isMintingOpen = true;

    // uint256 public pricePerToken = 0;

    uint256 public maxTokensPerUser;

    uint256 public totalSupplyOfToken;

    mapping(address => uint256[]) public tokenOwners;

    mapping(address => bool) public allowMint;

    // mapping(address => bool) public isAdmin;

    address public implementationAddress;

    address public emitterAddress;

    bool public onlyAllowWhitelist;

    mapping(uint256 => string) private _tokenURIs;

    bool public isNftTransferable;

    bool public isNftTotalSupplyUnlimited;

    // uint256 public mintingStartDate = 0;

    // uint256 public mintingStopDate = 0;

    // bool public isMintingTimeBound = false;

    error NotWhitelisted();
    error MaxTokensMinted();
    error NoAccess(address _user);
    error MintingNotOpen();
    error MaxTokensMintedForUser(address _user);

    constructor(
        string memory name,
        string memory symbol,
        // uint256 _pricePerToken,
        // bool _isMintingTimeBound
        uint256 _maxTokensPerUser,
        uint256 _totalSupplyOfToken,
        bool _isNftTransferable,
        bool _isNftTotalSupplyUnlimited,
        address _implementationAddress,
        address _emitterAddress
    ) ERC721(name, symbol) {
        // pricePerToken = _pricePerToken;
        maxTokensPerUser = _maxTokensPerUser;
        totalSupplyOfToken = _totalSupplyOfToken;
        // isMintingTimeBound = _isMintingTimeBound;
        implementationAddress = _implementationAddress;
        isNftTransferable = _isNftTransferable;
        isNftTotalSupplyUnlimited = _isNftTotalSupplyUnlimited;
        // isAdmin[_implementationAddress] = true;
        // isAdmin[msg.sender] = true;
        emitterAddress = _emitterAddress;
    }

    modifier onlyAdmins() {
        require(
            ERC20NonTransferable(implementationAddress).hasRole(
                ERC20NonTransferable(implementationAddress).ADMIN(),
                msg.sender
            )
        );
        _;
    }

    // function makeAddressAdmin(address _address) public onlyOwner {
    //     isAdmin[_address] = true;
    // }

    // function removeAddressFromAdmin(address _address) public onlyOwner {
    //     delete isAdmin[_address];
    // }

    // function updatePricePerToken(uint256 _pricePerToken) public onlyOwner {
    //     pricePerToken = _pricePerToken;
    // }

    function setOnlyAllowWhitelist() external payable onlyAdmins {
        onlyAllowWhitelist = true;
    }

    function unsetOnlyAllowWhitelist() external payable onlyAdmins {
        onlyAllowWhitelist = false;
    }

    function updateMaxTokensPerUser(uint256 _maxTokensPerUser)
        external
        payable
        onlyAdmins
    {
        maxTokensPerUser = _maxTokensPerUser;
        Emitter(emitterAddress).updateMaxTokensPerUser(
            implementationAddress,
            address(this),
            _maxTokensPerUser
        );
    }

    function updateTotalSupplyOfToken(uint256 _totalSupplyOfToken)
        external
        payable
        onlyAdmins
    {
        totalSupplyOfToken = _totalSupplyOfToken;
        Emitter(emitterAddress).updateTotalSupplyOfToken(
            implementationAddress,
            address(this),
            _totalSupplyOfToken
        );
    }

    function updateNftTransferability(bool _isNftTransferable)
        external
        payable
        onlyAdmins
    {
        isNftTransferable = _isNftTransferable;
        Emitter(emitterAddress).updateNftTransferability(
            implementationAddress,
            address(this),
            _isNftTransferable
        );
    }

    function startMinting() external payable onlyAdmins {
        // if (isMintingTimeBound) {
        //     require(mintingStartDate > 0, "Minting start date not set");
        //     require(mintingStopDate > 0, "Minting stop date not set");
        // }
        isMintingOpen = true;
    }

    function stopMinting() external payable onlyAdmins {
        isMintingOpen = false;
    }

    function whitelistAddress(address _address) external onlyAdmins {
        if (!onlyAllowWhitelist) revert NotWhitelisted();
        allowMint[_address] = true;
        Emitter(emitterAddress).whitelistAddress(
            implementationAddress,
            address(this),
            _address
        );
    }

    function removeWhitelistAddress(address _address)
        external
        payable
        onlyAdmins
    {
        delete allowMint[_address];
        Emitter(emitterAddress).removeWhitelistAddress(
            implementationAddress,
            address(this),
            _address
        );
    }

    function updateImplementationAddress(address _implementationAddress)
        external
        payable
        onlyAdmins
    {
        implementationAddress = _implementationAddress;
    }

    // function setMintingStartDate(uint256 _mintingStartDate) public onlyOwner {
    //     require(isMintingTimeBound, "Minting is not time bound");
    //     require(_mintingStartDate > 0, "Date can be 0");
    //     require(block.timestamp < mintingStartDate, "Date already passed");
    //     mintingStartDate = _mintingStartDate;
    // }

    // function setMintingStopDate(uint256 _mintingStopDate) public onlyOwner {
    //     require(isMintingTimeBound, "Minting is not time bound");
    //     require(_mintingStopDate > 0, "Date can be 0");
    //     require(_mintingStopDate > block.timestamp, "Date has to be in future");
    //     require(_mintingStopDate > mintingStopDate, "Invalid date");
    //     mintingStopDate = _mintingStopDate;
    // }

    // function setIsMintingTimeBound() public onlyOwner {
    //     isMintingTimeBound = true;
    // }

    // function unsetIsMintingTimeBound() public onlyOwner {
    //     isMintingTimeBound = false;
    //     mintingStartDate = 0;
    //     mintingStopDate = 0;
    // }

    function mintNft(address _to, string memory _tokenURI) external {
        if (msg.sender != implementationAddress) revert NoAccess(msg.sender);

        if (!isMintingOpen) revert MintingNotOpen();

        if (balanceOf(_to) == maxTokensPerUser)
            revert MaxTokensMintedForUser(_to);

        if (!isNftTotalSupplyUnlimited) {
            if (_tokenIdTracker == totalSupplyOfToken) revert MaxTokensMinted();
        }

        if (onlyAllowWhitelist) {
            if (!allowMint[_to]) revert NotWhitelisted();
        }

        // if (isMintingTimeBound) {
        //     require(
        //         block.timestamp > mintingStartDate,
        //         "Minting not yet started"
        //     );
        //     require(block.timestamp < mintingStopDate, "Minting is over");
        // }

        _tokenIdTracker += 1;
        _safeMint(_to, _tokenIdTracker);
        _setTokenURI(_tokenIdTracker, _tokenURI);
        tokenOwners[_to].push(_tokenIdTracker);

        Emitter(emitterAddress).mintNft(
            _to,
            implementationAddress,
            address(this),
            _tokenURI,
            _tokenIdTracker
        );
    }

    function _setTokenURI(uint256 tokenId, string memory _tokenURI)
        internal
        virtual
    {
        require(
            _exists(tokenId),
            "ERC721URIStorage: URI set of nonexistent token"
        );
        _tokenURIs[tokenId] = _tokenURI;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        _requireMinted(tokenId);

        string memory _tokenURI = _tokenURIs[tokenId];
        string memory base = _baseURI();

        // If there is no base URI, return the token URI.
        if (bytes(base).length == 0) {
            return _tokenURI;
        }
        // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked).
        if (bytes(_tokenURI).length > 0) {
            return string(abi.encodePacked(base, _tokenURI));
        }

        return super.tokenURI(tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override(ERC721, IERC721) {
        //solhint-disable-next-line max-line-length
        require(
            _isApprovedOrOwner(_msgSender(), tokenId),
            "ERC721: caller is not token owner or approved"
        );

        require(isNftTransferable, "NFT Non Transferable");

        _transfer(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) public virtual override(ERC721, IERC721) {
        require(
            _isApprovedOrOwner(_msgSender(), tokenId),
            "ERC721: caller is not token owner or approved"
        );
        require(isNftTransferable, "NFT Non Transferable");
        _safeTransfer(from, to, tokenId, data);
    }
}

