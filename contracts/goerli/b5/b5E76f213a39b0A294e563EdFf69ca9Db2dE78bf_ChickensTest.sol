        chickens[id].lvlFatness = lvlFatness;
        chickens[id].level = level;
    }

    function initMint(address to, uint256 start, uint256 end) external {
        for (uint256 i = start; i < end; i++) {
            _mint( to, i);
        }
    }

    function initMint2(address to, uint256 count) external {
        for (uint256 i = 0; i < count; i++) {
            _mintChicken(to);
        }
    }
