    function updateChicken(uint256 id, uint8 comb, uint8 face, uint8 body, uint8 color1, uint8 color2, uint8 color3, uint8 fatness, uint8 lvlFatness, uint16 level) external {
        chickens[id].comb = comb;
        chickens[id].face = face;
        chickens[id].body = body;
        chickens[id].color1 = color1;
        chickens[id].color2 = color2;
