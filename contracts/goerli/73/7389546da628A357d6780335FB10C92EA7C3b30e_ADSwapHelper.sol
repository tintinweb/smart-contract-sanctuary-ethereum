contract ADSwapHelper is Operator {
    using SafeMath for uint256;
    using Address for address;
    using SafeBEP20 for IBEP20;

    IBEP20 public ADAOToken;
    IBEP20 public DAOTToken;
    IBEP20 public BNB2Token;

    IPancakeRouter02 public PancakeRouter;

    address public ADAOETHPairAddress;
    IBEP20 ADAOETHPair;

    constructor(address pancakeRouter) public {
        PancakeRouter = IPancakeRouter02(pancakeRouter);
    }

    function _changeADAOETHPair(address addr) internal virtual {
        ADAOETHPairAddress = addr;
        ADAOETHPair = IBEP20(ADAOETHPairAddress);

        ADAOETHPair.safeApprove(address(this), 100000000000000000000 ether);
        ADAOToken.safeApprove(addr, 100000000000000000000 ether);
    }

    function changeADAOETHPair(address addr) external onlyOwner {
        _changeADAOETHPair(addr);
    }

    function changeADAOToken(address adao_) external onlyOwner {
        ADAOToken = IBEP20(adao_);
        ADAOToken.safeApprove(address(PancakeRouter), 1000000000000000 ether);
    }

    function changeDAOTToken(address daot) external onlyOwner {
        DAOTToken = IBEP20(daot);
        DAOTToken.safeApprove(address(PancakeRouter), 1000000000000000 ether);
    }

    function changeBNB2Token(address bnb2) external onlyOwner {
        BNB2Token = IBEP20(bnb2);
        BNB2Token.safeApprove(address(PancakeRouter), 1000000000000000 ether);
    }

    function changePancakeRouter(address pancakeRouter) external onlyOwner {
        PancakeRouter = IPancakeRouter02(pancakeRouter);

        ADAOToken.safeApprove(address(PancakeRouter), 1000000000000000 ether);
    }

    function initADAO(address adao) external onlyOwner {
        ADAOToken = IBEP20(adao);
        ADAOToken.safeApprove(address(PancakeRouter), 1000000000000000 ether);

        _createADAOETHPair();
    }

    function _createADAOETHPair() internal virtual {
        address _uniswapV2Pair = IPancakeFactory(PancakeRouter.factory())
            .createPair(PancakeRouter.WETH(), address(ADAOToken));
        _changeADAOETHPair(_uniswapV2Pair);
    }

    function _getTokenPriceFromPancake(address token)
        internal
        view
        virtual
        returns (uint256)
    {
        uint112 reserve0;
        uint112 reserve1;
        uint32 blockTimestampLast;

        IPancakeFactory factory = IPancakeFactory(PancakeRouter.factory());
        IPancakePair pair = IPancakePair(
            factory.getPair(PancakeRouter.WETH(), token)
        );
        (reserve0, reserve1, blockTimestampLast) = pair.getReserves();
        uint256 price = reserve1 / reserve0;
        return price;
    }

    function getBNB2PriceInBNB() public view returns (uint256 price) {
        return _getTokenPriceFromPancake(address(BNB2Token));
    }

    function getDAOTPriceInBNB() public view returns (uint256 price) {
        return _getTokenPriceFromPancake(address(DAOTToken));
    }

    function getADAOPriceInBNB() public view returns (uint256 price) {
        return _getTokenPriceFromPancake(address(ADAOToken));
    }

    function getTokenPriceInBNB(address token)
        public
        view
        returns (uint256 price)
    {
        return _getTokenPriceFromPancake(token);
    }

    function swapADAOToBNB(uint256 tokenAmount, address receiver) external {
        ADAOToken.safeTransferFrom(msg.sender, address(this), tokenAmount);
        _swapTokensForEth(address(ADAOToken), tokenAmount, receiver);
    }

    function addADAOLiquidity(uint256 tokenAmount) external payable {
        ADAOToken.safeTransferFrom(msg.sender, address(this), tokenAmount);
        _addLiquidity(address(ADAOToken), tokenAmount, msg.value);
    }

    function _swapTokensForEth(
        address tokenAddress,
        uint256 tokenAmount,
        address receiver
    ) internal virtual {
        // generate the uniswap pair path of token -> weth
        ADAOToken.approve(address(PancakeRouter), 10000000000000 ether);

        address[] memory path = new address[](2);
        path[0] = tokenAddress;
        path[1] = PancakeRouter.WETH();

        // make the swap
        //swapExactTokensForTokensSupportingFeeOnTransferTokens
        PancakeRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            receiver,
            block.timestamp
        );
    }

    function _addLiquidity(
        address tokenAddress,
        uint256 tokenAmount,
        uint256 ethAmount
    ) internal virtual {
        ADAOToken.approve(address(PancakeRouter), 10000000000000 ether);
        // add the liquidity
        PancakeRouter.addLiquidityETH{value: ethAmount}(
            tokenAddress,
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(this),
            block.timestamp
        );
    }
}

