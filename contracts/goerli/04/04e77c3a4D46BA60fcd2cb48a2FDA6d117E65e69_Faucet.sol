pragma solidity ^0.8.0;

import "hardhat/console.sol";

import { ISuperToken } from "@superfluid-finance/ethereum-contracts/contracts/interfaces/superfluid/ISuperToken.sol";

contract Faucet {

    mapping (address => uint) lastTap;
    ISuperToken public token;
    address public owner;

    constructor(ISuperToken _token) {
        token = _token;
        owner = msg.sender;
    }

    function tapFaucet() public {
        require(lastTap[msg.sender] <= block.timestamp - 12 hours, "need to wait at least 12 hours between calls");
        lastTap[msg.sender] = block.timestamp;
        //send 100k daix
        token.transfer(msg.sender, 100000 ether);
        //send 0.01 eth
        (bool sent, ) = msg.sender.call{value: 10000000000000000}("");
        require(sent, "Failed to send native asset");
    }

    function drainFaucetTokens(uint amountToDrain) external {
        require(msg.sender == owner, "only owner can call this function");
        token.transfer(msg.sender, amountToDrain);
    } 

    function drainFaucetNativeAsset(uint amountToDrain) external {
        require(msg.sender == owner, "only owner can call this function");
        (bool sent, ) = msg.sender.call{value: amountToDrain}("");
        require(sent, "failed to send native asset");
    } 

    function changeOwner(address newOwner) external {
        require(msg.sender == owner, "only owner can call this function");
        owner = newOwner;
    }

    event Received(address, uint);

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }
}

