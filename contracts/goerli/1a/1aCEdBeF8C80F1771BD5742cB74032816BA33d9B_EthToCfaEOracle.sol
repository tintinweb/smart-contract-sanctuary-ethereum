pragma solidity ^0.8;

import "../../interfaces/chainlink/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import {INormalisedAssetAToAssetBOracle} from "../../interfaces/oracles/INormalisedAssetAToAssetBOracle.sol";
// import {EurToCfaEOracle} from "./EurToCfaEOracle.sol";

error EthToCfaEOracle__ethUsdPriceFeedDecimalsTooLarge();
error EthToCfaEOracle__supportOracleAndTargetDifferentDecimals();

/// @title UsdToCfaEOracle
/// @author Maxence Soumare
/// @notice The oracles in the oracles folder are inspired by certain mechanisms from the AAVEOracle
/// - This occurrence benefits from the peg of the real CFA (France CFA) and the Euro akin to the peg EUR Franc Francais
/// - Those oracles contracts lay out the fondation for the proxy contract clost to the AAVEORACLE implementation whilst taking into consideration
///   the availability of the the pairs from the chainlink oracles on Goerli and Mainnet
///   The general idea is to have a getAssetPrice that could be called by the proxy. The proxy Matches each asset with the proper address
/// @dev To optimize for gas we deploy only one EurToCFAE contract so this contract should be deployed after the EurToCFAE one
contract EthToCfaEOracle is Ownable {
    AggregatorV3Interface internal ethUsdPriceFeed;
    INormalisedAssetAToAssetBOracle internal iNormalisedAssetAToAssetBOracle;

    // For Goerli Testnet on 11 Feb 2023 from below url
    // https://docs.chain.link/data-feeds/price-feeds/addresses/#Goerli%20Testnet
    // ETH_USD 0xD4a33860578De61DBAbDc8BFdb98FD742fA7028e
    // EUR_USD 0x44390589104C9164407A0E0562a9DBe6C24A0E05
    constructor(address ethUsdPriceFeedAddress, address _usdToCfaEOracleAddress)
    {
        ethUsdPriceFeed = AggregatorV3Interface(ethUsdPriceFeedAddress);
        iNormalisedAssetAToAssetBOracle = INormalisedAssetAToAssetBOracle(
            _usdToCfaEOracleAddress
        );
        if (getDecimals() != iNormalisedAssetAToAssetBOracle.getDecimals()) {
            revert EthToCfaEOracle__supportOracleAndTargetDifferentDecimals();
        }
    }

    /// @notice Returns the Number of decimals for the price of the asset in CFAE value
    /// @dev this is made ti help future handling of the numbers
    /// @return decimals the number of decimals for the number representation
    function getDecimals() public pure returns (uint256) {
        return 18;
    }

    /// @notice Returns the Price of the foreign Oracle (not the contract already deployed by the proxy)
    ///         with a number of decimals equals to target
    /// @dev This contract is sensitive to Oracle and cannot function if the number of decimals is > 18
    /// or if the price is <= 0
    ///      we could introduce a fallback oracle like the one in AAVE to prevent such dependency.
    /// @param targetDecimals the number of decimals for the output price representation
    /// @return NormalizedPrice The price congruent with a representation with targetDecimals
    function getForeingOracleNormalizedPrice(uint8 targetDecimals)
        internal
        view
        returns (uint256)
    {
        uint8 ethUsdPriceFeedDecimals = ethUsdPriceFeed.decimals();
        if (ethUsdPriceFeedDecimals > 18) {
            revert EthToCfaEOracle__ethUsdPriceFeedDecimalsTooLarge();
        }

        // Since by transition the target will always be 18 unless we change the source Orcale
        // We do not need to test wether this difference is >= 0
        uint8 differenceDecimals = (targetDecimals - ethUsdPriceFeedDecimals);

        (
            ,
            // uint80 roundId
            int256 oneEthInUsdPrice, // uint256 startedAt, // uint256 updatedAt, // uint80 answeredInRound
            ,
            ,

        ) = ethUsdPriceFeed.latestRoundData();

        return
            10**uint256(differenceDecimals) *
            SafeCast.toUint256(oneEthInUsdPrice);
    }

    /// @notice Calculate the price of one ETH in CFAE
    /// @return assetPrice
    function getAssetPrice() public view returns (uint256) {
        uint256 usdToCfaEPrice = iNormalisedAssetAToAssetBOracle
            .getAssetPrice();

        // We make the number of decimals from the distant Oracle (chainlink in the program case)
        // the same as the one deployed by the program itself
        uint256 ethToUsdNormalizedDecimals = getForeingOracleNormalizedPrice(
            iNormalisedAssetAToAssetBOracle.getDecimals()
        );

        // uint256 conversionRateethToUsdTimesGetDecimals = 10 **
        //     (getDecimals() * 2) /
        //     ethToUsdNormalizedDecimals;

        return
            (ethToUsdNormalizedDecimals * usdToCfaEPrice) / 10**(getDecimals());
    }
}

