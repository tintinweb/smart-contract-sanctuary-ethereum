pragma solidity >=0.8.15;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "./TeroAposentadoria.sol";
import "./TeroDestokenizacao.sol";


interface Transfer {
    function mintTransfer(uint256 _amount, address _to) external payable returns(uint256);
}



contract TeroCarbon is Initializable, ERC1155Upgradeable, 
OwnableUpgradeable,ReentrancyGuardUpgradeable{ 


    event destoken(address indexed account);

    //Transfer batch function not available on this contract
    error notAvailable();


    //Retirement policy for CO2 compensation
    TeroAposentadoria public aposentadoria;
    TeroDestokenizacao public destokenizacao;
    Transfer public transferCertif;



    //ERC1155 variables
    uint256 public minted;
    uint256 public burned;
    uint256 public frozen;

    // CO2 certificate program compliance
    string public certificado;
    uint256 public certificado_id;
    bool public quarantine;

    mapping(address => bool) public blacklist;
    mapping(address => bool) public allow;

    uint256 public totalCO2;



    /// @custom:oz-upgrades-unsafe-allow constructor
    // constructor() {
    //     _disableInitializers();
    // }
    
    /// -----------------------------------------------------------------------
    /// Initialization
    /// -----------------------------------------------------------------------
    /// @notice initialize is going to be used instead of constructor for implementation ERC1155
    /// @param _certificado Name of the CO2 compensation project
    /// @param allowed_controller Owner of the contract
    /// @param _supply Supply of ERC1155 tokens
    /// @param _totalCO2 Amount of CO2 to be compensated on this contract
    /// @param _distributions People involved on the project
    /// @param _shares Shares of the compensated CO2 to be distributed among _distributions
    /// @param _aposentadoria ERC721 to retire tokens from this specific contract
    /// @param _id Certificate id
    function initialize(string memory _certificado,address allowed_controller, 
        uint256 _supply,uint256 _totalCO2, address[] calldata _distributions, 
        uint256[] calldata _shares, address _aposentadoria, uint256 _id) public initializer  {

        require(_distributions.length == _shares.length, "Wrong amount of initial holders and amounts");
        
        uint256 total;
        for(uint i=0; i<_shares.length; i++){
            total+=_shares[i];
        }
        
        require(total == 100, "Not summing right");
        
        allow[msg.sender] = true;
        allow[allowed_controller] = true;
        

        totalCO2 = _totalCO2;
        certificado = _certificado;
        certificado_id = _id;
        aposentadoria = TeroAposentadoria(_aposentadoria);

        __ReentrancyGuard_init();
        __ERC1155_init(_certificado);
        distribute(_distributions,_shares,_supply,_id);
        __Ownable_init();
        transferOwnership(allowed_controller);
    }


    ///@dev Private function to distribute amounts on the Initializer 
    function distribute(address[] calldata _distributions, uint256[] calldata _shares,
    uint256 _supply,uint256 _cert) private {

        uint256 amount;
        uint256 checkAmount;
        uint256 mint;

        for(uint i=0;i<_shares.length;i++){
            amount = (_shares[i] * _supply)/100;
            checkAmount+=amount;
            mint += amount;
            _mint(_distributions[i], _cert, amount, "");
        }

        if(checkAmount < _supply){
            _mint(owner(), _cert, (_supply-checkAmount), "");
        }

        minted = mint;
    }


    /// -----------------------------------------------------------------------
    /// ERC1155 functions
    /// -----------------------------------------------------------------------

    function retrivecert() public view returns(string memory){
        return certificado;
    }

     function setURI(string memory newuri) public allowed {
        _setURI(newuri);
    }

     function retriveuri() public view returns (string memory) {
        string memory _certif = certificado;
        return (keccak256 (bytes(uri(certificado_id))) == keccak256(bytes(_certif)) ? "" : uri(certificado_id));
    }

    /// -----------------------------------------------------------------------
    /// User and Project controllers
    /// -----------------------------------------------------------------------

    function addBlakclist(address _addr) public allowed {
        blacklist[_addr] = true;
    }
    
    function removeBlakclist(address _addr) public allowed {
        blacklist[_addr] = false;
    }

    function startQuarantine() public allowed quarantined {
        quarantine = true;
    }
    function stopQuarantine() public allowed {
        require(quarantine,"Not quarantined!");
        quarantine = false;
    }
    function adminMint(uint256 _id, uint256 _amount) external payable allowed quarantined{
       _mint(msg.sender, _id, _amount, "");
       minted += _amount;
    }

    function setTransferCertif(address _transferCertif) public allowed {
        transferCertif = Transfer(_transferCertif);
    }

    function addAllow(address _addr) public allowed {
        require(!allow[_addr],"Already allowed!");
        allow[_addr] = true;
    }

    function removeAllow(address _addr) public allowed {
        require(allow[_addr],"Not allowed yet!");
        allow[_addr] = false;
    }

    /// -----------------------------------------------------------------------
    /// Retirement
    /// -----------------------------------------------------------------------

    function retire(address from, uint256 amount) public quarantined allowed nonReentrant returns(uint256){
        _burn(from, certificado_id, amount);
        burned += amount;
        uint256 _token = aposentadoria.mintRetire(from, 1);
        return _token;

    }


    /// -----------------------------------------------------------------------
    /// Remove token from Tero Platform
    /// -----------------------------------------------------------------------

    function setDestokenizacao(address _destokenizacao) public allowed {
        destokenizacao = TeroDestokenizacao(_destokenizacao);
    }

    /**
     * @dev See {IERC1155-setApprovalForAll}.
     */
    function setApprovalForAll(address _address, bool approved) public allowed override {
        _setApprovalForAll(_address, address(this), approved);
    }

    function destokenization(address from) public quarantined allowed nonReentrant{
        setApprovalForAll(from, true);
        frozen += balanceOf(from, 1);
        destokenizacao.mintDestokenizacao(1, from);

        emit destoken(from);
    }

    function retoken(address _newAddr, address _oldAddr, uint256 _721tokenId) public quarantined allowed{
        destokenizacao.burnRetoken(_721tokenId, _oldAddr);
        safeTransferFrom(_oldAddr, _newAddr, 1, balanceOf(_oldAddr, 1), "");
    }


    /// -----------------------------------------------------------------------
    /// Transfers
    /// -----------------------------------------------------------------------

    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public override blacklisted(from) quarantined allowed{
        require(address(transferCertif) != address(0),"Transfer Certificate contract not set yet");
        super._safeTransferFrom(from, to, 1, amount, data);
        transferCertif.mintTransfer(1,from);
    }

    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public override{
        revert notAvailable();
    }

    /// -----------------------------------------------------------------------
    /// unlock funds
    /// -----------------------------------------------------------------------

    function withdraw(address _addr) allowed public{
        require(_addr != address(0), "Transfer to zero address");
        payable(_addr).transfer(address(this).balance);
    }




    /// -----------------------------------------------------------------------
    /// Modifiers
    /// -----------------------------------------------------------------------


    modifier blacklisted(address _addr) {
        require(!blacklist[_addr], "Quarantined address");
        _;
    }

    modifier quarantined() {
        require(!quarantine,"Project quarantined");
        _;
    }

    modifier allowed() {
        require(allow[msg.sender] || msg.sender == owner(),"Not allowed");
        _;
    }

}

