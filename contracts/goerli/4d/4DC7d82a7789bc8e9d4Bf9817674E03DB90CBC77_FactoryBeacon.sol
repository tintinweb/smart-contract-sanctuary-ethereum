pragma solidity >=0.8.15;


import "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";
import "@openzeppelin/contracts/proxy/beacon/IBeacon.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

///@param TeroCarbon is the ERC1155Upgradeable regarding the certificate
import "./TeroCarbon.sol";


///@dev This is the contract that will be cloned and will 
//allow implementations of new certificates
contract FactoryBeacon is Ownable{

    struct _deployed {
        address proxy_CO2;
        address proxy_Aposentadoria;
    }

    struct builder {
        string _certificado;
        uint256 _supply;
        uint256 totalCO2; 
        address[] distributions;
        uint256[] _shares;
        string _aposentadoria; 
        string _aposentadoriaSymbol; 
        uint256 _id;
        address allowed_controller;
    }

    address public transferCertif;
    address public destokenizacao;

    address immutable beacon1155;
    address immutable beacon721;

    uint256[] deploys;

    mapping(uint256 => _deployed)private teroCertific;
    mapping(address => bool) private allow;

    ///@dev the implementation contract will own the TeroBeacon
    constructor(address _beacon1155, address _beacon721,address _transferCertif, address destoken, address _owner){
        require(_beacon1155 != address(0), "Beacon1155 on zero address");
        require(_beacon721 != address(0), "Beacon721 on zero address");
        require(_transferCertif != address(0), "Transfer Certificate on zero address");
        require(destoken != address(0), "Destokenization on zero address");
        require(_owner != address(0), "Owner on zero address");
        
        
        beacon1155 = _beacon1155;
        beacon721 = _beacon721;
        transferCertif = _transferCertif;
        destokenizacao = destoken;

        transferOwnership(_owner);
        allow[msg.sender] = true;
    }

    function buildTero(builder calldata _build) external payable allowed returns(_deployed memory){
        ///@param TeroCarbon contract initialize parameters are encoded and selected, 
        //passing in the BeaconProxy constructor imported from Openzeppelin


        BeaconProxy teroAposentadoriaProxy = new BeaconProxy(beacon721,
        abi.encodeWithSelector(TeroAposentadoria(address(0)).initialize.selector, 
        _build.allowed_controller, _build._aposentadoria,
         _build._aposentadoriaSymbol, _build._id));


        BeaconProxy terocarbonProxy = new BeaconProxy(beacon1155,
        abi.encodeWithSelector(TeroCarbon(address(0)).initialize.selector, 
        _build._certificado, _build.allowed_controller, _build._supply, _build.totalCO2,
        _build.distributions, _build._shares, address(teroAposentadoriaProxy), _build._id));

        TeroAposentadoria(address(teroAposentadoriaProxy)).addAllow(address(terocarbonProxy));
        TeroAposentadoria(address(teroAposentadoriaProxy)).setMainCertificate(address(terocarbonProxy));
        TeroCarbon(address(terocarbonProxy)).setTransferCertif(transferCertif);

        (bool success, ) = transferCertif.call(abi.encodeWithSelector(0x53537cf9, address(terocarbonProxy)));
        require(success);
        (success, ) = destokenizacao.call(abi.encodeWithSelector(0x53537cf9, address(terocarbonProxy)));
        require(success);

        deploys.push(_build._id);

        teroCertific[_build._id] = _deployed({proxy_CO2:address(terocarbonProxy), 
        proxy_Aposentadoria:address(teroAposentadoriaProxy)});

        return teroCertific[_build._id];

    }

    function getTeroAddresses(uint256 _Idcertificate) public view returns (_deployed memory) {
        return teroCertific[_Idcertificate];
    }

    function getBeacon1155() public view returns (address){
        return beacon1155;
    }

    function getBeacon721()public view returns (address) {
        return beacon721;
    }

    function getImplementation1155() public view returns (address){
        return IBeacon(beacon1155).implementation();
    }
    function getImplementation721() public view returns (address){
        return IBeacon(beacon721).implementation();
    }

    function changeTransferContract(address _newAddr) public allowed returns(address){
        return transferCertif = _newAddr;
    }
    function changeDestokenContract(address _newAddr) public allowed returns(address){
        return destokenizacao = _newAddr;
    }


    function addAllow(address _addr) public allowed {
        require(!allow[_addr],"Already allowed!");
        allow[_addr] = true;
    }

    function removeAllow(address _addr) public allowed {
        require(allow[_addr],"Not allowed yet!");
        allow[_addr] = false;
    }

    /// -----------------------------------------------------------------------
    /// unlock funds
    /// -----------------------------------------------------------------------

    function withdraw(address _addr) allowed public{
        require(_addr != address(0), "Transfer to zero address");
        payable(_addr).transfer(address(this).balance);
    }




    modifier allowed(){
        require(allow[msg.sender] || msg.sender == owner(),"Not alowed");
        _;
    }


}

