contract RobotDropAccessControl is AccessExtension {
    //ROLES
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    bytes32 public constant BURNER_ROLE = keccak256("BURNER_ROLE");

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    bytes32 public constant URI_SETTER_ROLE = keccak256("URI_SETTER_ROLE");

    bytes32 public constant FACTORY_ROLE = keccak256("FACTORY_ROLE");

    //MODIFIERS

    modifier onlyMinter() {
        require(
            hasRole(MINTER_ROLE, _msgSender()),
            "User does not have minter role"
        );
        _;
    }
    modifier onlyBurner() {
        require(
            hasRole(BURNER_ROLE, _msgSender()),
            "User does not have burner role"
        );
        _;
    }
    modifier onlyPauser() {
        require(hasRole(PAUSER_ROLE, _msgSender()), "User is not Pauser");
        _;
    }
    modifier onlyURISetter() {
        require(
            hasRole(URI_SETTER_ROLE, _msgSender()),
            "User is not URI setter"
        );
        _;
    }
    modifier onlyFactory() {
        require(
            hasRole(FACTORY_ROLE, _msgSender()),
            "User is not a Factory contract"
        );
        _;
    }

    modifier onlyAdmin() {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()),
            "User is not an Admin"
        );
        _;
    }

    // solhint-disable-next-line func-name-mixedcase
    function __RobotDropAccessControl_init() internal onlyInitializing {
        AccessExtension.__AccessControlExtension_init();

        _setRoleAdmin(BURNER_ROLE, DEFAULT_ADMIN_ROLE);

        _setRoleAdmin(PAUSER_ROLE, DEFAULT_ADMIN_ROLE);

        _setRoleAdmin(URI_SETTER_ROLE, DEFAULT_ADMIN_ROLE);

        _setRoleAdmin(MINTER_ROLE, DEFAULT_ADMIN_ROLE);

        _setRoleAdmin(FACTORY_ROLE, DEFAULT_ADMIN_ROLE);
    }

    //FUNCTIONS

    function grantRoleMinter(address _to) public {
        grantRole(MINTER_ROLE, _to);
    }

    function grantRoleFactory(address _to) public {
        grantRole(FACTORY_ROLE, _to);
    }

    function revokeRoleFactory(address _to) public {
        revokeRole(FACTORY_ROLE, _to);
    }

    function revokeRoleMinter(address _to) public {
        revokeRole(MINTER_ROLE, _to);
    }

    function grantRoleBurner(address _to) public {
        grantRole(BURNER_ROLE, _to);
    }

    function revokeRoleBurner(address _to) public {
        revokeRole(BURNER_ROLE, _to);
    }

    function grantRolePauser(address _to) public {
        grantRole(PAUSER_ROLE, _to);
    }

    function revokeRolePauser(address _to) public {
        revokeRole(PAUSER_ROLE, _to);
    }

    function grantRoleURISetter(address _to) public {
        grantRole(URI_SETTER_ROLE, _to);
    }

    function revokeRoleURISetter(address _to) public {
        revokeRole(URI_SETTER_ROLE, _to);
    }

    function transferAdminRole(address _to) public {
        grantRole(DEFAULT_ADMIN_ROLE, _to);
        revokeRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }
}

