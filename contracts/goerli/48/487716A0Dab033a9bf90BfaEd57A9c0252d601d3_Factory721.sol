pragma solidity ^0.8.2;

library RobotdropFee {
    uint256 private constant BASIS = 10**6;

    function getFeeAmount(uint256 _points, uint256 _totalAmount)
        public
        pure
        returns (uint256)
    {
        return computePercentage(_totalAmount, _points);
    }

    function computePercentage(uint256 _wholeAmount, uint256 _point)
        internal
        pure
        returns (uint256)
    {
        require(
            (_wholeAmount * _point) >= BASIS,
            "Amount*BPS is less than basis"
        );
        return (_wholeAmount * _point) / BASIS;
    }
}

