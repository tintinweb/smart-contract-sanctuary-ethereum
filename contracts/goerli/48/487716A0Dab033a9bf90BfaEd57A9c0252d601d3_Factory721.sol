pragma solidity ^0.8.2;
import "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./Collectible721.sol";
import "./ICollectible721.sol";
import "../../helper/Beacon.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

contract Factory721 is ReentrancyGuardUpgradeable, OwnableUpgradeable {
    address public tokenBeacon;

    mapping(bytes32 => address) public cloneAddresses;
    event CloneCreated(
        bytes32 indexed cloneId,
        address indexed cloneAddress,
        address indexed caller
    );

    // solhint-disable-next-line func-name-mixedcase
    function __Factory721_init() public initializer {
        __ReentrancyGuard_init();
        __Ownable_init();
        Beacon _tokenBeacon = new Beacon(address(new Collectible721()));
        tokenBeacon = address(_tokenBeacon);
        _tokenBeacon.transferOwnership(_msgSender());
    }

    function createNFT721(bytes32 _cloneId, uint256 _royalityBasisPoints)
        public
        nonReentrant
        returns (address cloneAddress)
    {
        require(
            cloneAddresses[_cloneId] == address(0),
            "Clone Id already exists"
        );
        BeaconProxy proxy = new BeaconProxy(
            tokenBeacon,
            abi.encodeWithSelector(
                Collectible721.__Collectible721_init.selector,
                _msgSender(),
                _royalityBasisPoints
            )
        );
        cloneAddresses[_cloneId] = address(proxy);
        emit CloneCreated(_cloneId, address(proxy), _msgSender());
        return address(proxy);
    }

    function setCollectibleRoyalty(bytes32 _id, uint256 _royalty)
        public
        onlyOwner
    {
        require(cloneAddresses[_id] != address(0), "No such clone id exists");
        ICollectible721 collectible = ICollectible721(cloneAddresses[_id]);
        collectible.setRoyalty(_royalty);
    }

    function enableRoyalty(bytes32 _id) public onlyOwner {
        require(cloneAddresses[_id] != address(0), "No such clone id exists");
        ICollectible721 collectible = ICollectible721(cloneAddresses[_id]);
        collectible.disableRoyalty();
    }

    function disableRoyalty(bytes32 _id) public onlyOwner {
        require(cloneAddresses[_id] != address(0), "No such clone id exists");
        ICollectible721 collectible = ICollectible721(cloneAddresses[_id]);
        collectible.enableRoyalty();
    }

    function getCloneAddress(bytes32 _id)
        public
        view
        returns (address cloneAddress)
    {
        return cloneAddresses[_id];
    }

    function implementation()
        public
        view
        returns (address implementationContract)
    {
        Beacon _tokenBeacon = Beacon(tokenBeacon);
        return _tokenBeacon.implementation();
    }

    function beaconOwner() public view returns (address beaconAddress) {
        Beacon _tokenBeacon = Beacon(tokenBeacon);
        return _tokenBeacon.owner();
    }
}

