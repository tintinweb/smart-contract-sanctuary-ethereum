pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "../../access-control/RobotDropAccessControl.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "./ICollectible721.sol";
import "../../helper/RobotdropFee.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165StorageUpgradeable.sol";
import "hardhat/console.sol";

contract Collectible721 is
    ICollectible721,
    ERC721URIStorageUpgradeable,
    PausableUpgradeable,
    RobotDropAccessControl,
    EIP712Upgradeable,
    ERC165StorageUpgradeable
{
    using StringsUpgradeable for uint256;

    struct NftVoucher {
        uint256 tokenId;
        string uri;
        bytes signature;
    }

    mapping(string => bool) private existingTokenURIs;
    mapping(address => uint256[]) public myTokens;
    mapping(uint256 => string) private tokenAssets;
    address public signer;
    uint256 private royaltyPoints;
    bool private royaltyAllowed;

    string private constant SIGNING_DOMAIN = "RobotDrop";
    string private constant SIGNATURE_VERSION = "1";

    // solhint-disable-next-line func-name-mixedcase
    function __Collectible721_init(address _admin, uint256 _royaltyPoints)
        public
        initializer
    {
        require(_admin != address(0), "Invalid address");
        PausableUpgradeable.__Pausable_init();
        RobotDropAccessControl.__RobotDropAccessControl_init();
        ERC165StorageUpgradeable.__ERC165Storage_init();
        EIP712Upgradeable.__EIP712_init(SIGNING_DOMAIN, SIGNATURE_VERSION);
        ERC721Upgradeable.__ERC721_init("Collectible721", "COL");
        _registerInterface(type(ICollectible721).interfaceId);
        signer = _admin;
        royaltyPoints = _royaltyPoints;
        royaltyAllowed = true;
        grantRoleFactory(_msgSender());
        transferAdminRole(_admin);
    }

    function safeMint(
        address _to,
        string memory _tokenURI,
        uint256 _tokenId
    ) public returns (uint256 tokenId) {
        require(!existingTokenURIs[_tokenURI], "TokenURI already exists");
        require(!_exists(_tokenId), "Token Id already exists");

        tokenAssets[_tokenId] = _tokenURI;

        existingTokenURIs[_tokenURI] = true;

        _safeMint(_to, _tokenId);
        _setTokenURI(_tokenId, _tokenURI);

        myTokens[_to].push(_tokenId);
        return _tokenId;
    }

    function lazyMint(
        uint256 _tokenId,
        string memory _uri,
        bytes memory _signature
    ) public override {
        NftVoucher memory nftVoucher = NftVoucher(_tokenId, _uri, _signature);
        require(signer == recover(nftVoucher), "Invalid signature");
        safeMint(signer, nftVoucher.uri, nftVoucher.tokenId);
    }

    function exists(uint256 tokenId) public view override returns (bool) {
        return _exists(tokenId);
    }

    function setRoyalty(uint256 _royalty) public override onlyFactory {
        require(royaltyAllowed, "royalty feature disabled");
        royaltyPoints = _royalty;
    }

    function getMyTokenIds() public view returns (uint256[] memory) {
        return myTokens[_msgSender()];
    }

    function getRoyalty()
        public
        view
        override
        returns (uint256 royalty, bool enabled)
    {
        royalty = royaltyPoints;
        enabled = royaltyAllowed;
    }

    function enableRoyalty() public override {
        require(!royaltyAllowed, "royalty feature enabled");
        royaltyAllowed = true;
    }

    function disableRoyalty() public override {
        require(royaltyAllowed, "royalty feature disabled");
        royaltyAllowed = false;
    }

    function getCreator() public view override returns (address creator) {
        creator = signer;
    }

    function isURIAvailable(string memory _tokenURI)
        public
        view
        returns (bool isAvailable)
    {
        return existingTokenURIs[_tokenURI];
    }

    function setTokenURI(uint256 _tokenId, string memory _tokenURI)
        public
        onlyURISetter
    {
        _setTokenURI(_tokenId, _tokenURI);
    }

    function recover(NftVoucher memory nftVoucher)
        private
        view
        returns (address voucherSigner)
    {
        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    keccak256("NftVoucher(uint256 tokenId,string uri)"),
                    nftVoucher.tokenId,
                    keccak256(bytes(nftVoucher.uri))
                )
            )
        );
        voucherSigner = ECDSAUpgradeable.recover(digest, nftVoucher.signature);
    }

    function supportsInterface(bytes4 _interfaceId)
        public
        view
        virtual
        override(ERC721Upgradeable, AccessExtension, ERC165StorageUpgradeable)
        returns (bool interfaceSupported)
    {
        return super.supportsInterface(_interfaceId);
    }

    function pause() public onlyPauser {
        _pause();
    }

    function unpause() public onlyPauser {
        _unpause();
    }

    // function calculateSelector() public pure returns (bytes4) {
    //     ICollectible721 icollectible721;
    //     return
    //         icollectible721.lazyMint.selector ^
    //         icollectible721.exists.selector ^
    //         icollectible721.setRoyalty.selector ^
    //         icollectible721.getRoyalty.selector ^
    //         icollectible721.getCreator.selector;
    // }
}

