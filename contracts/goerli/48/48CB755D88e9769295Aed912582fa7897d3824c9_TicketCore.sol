        value = success ? value : 0;
        ticketsSaleQty.set(key, value+1);

        // 3
        key = keccak256(abi.encodePacked(tickets[ticketId].a, tickets[ticketId].b, tickets[ticketId].c, tickets[ticketId].d, tickets[ticketId].f, tickets[ticketId].drawId));
        (success, value) = ticketsSaleQty.tryGet(key);
        value = success ? value : 0;
        ticketsSaleQty.set(key, value+1);

        key = keccak256(abi.encodePacked(tickets[ticketId].a, tickets[ticketId].b, tickets[ticketId].c, tickets[ticketId].e, tickets[ticketId].f, tickets[ticketId].drawId));
        (success, value) = ticketsSaleQty.tryGet(key);
        value = success ? value : 0;
        ticketsSaleQty.set(key, value+1);

        key = keccak256(abi.encodePacked(tickets[ticketId].b, tickets[ticketId].c, tickets[ticketId].d, tickets[ticketId].e, tickets[ticketId].f, tickets[ticketId].drawId));
        (success, value) = ticketsSaleQty.tryGet(key);
        value = success ? value : 0;
        ticketsSaleQty.set(key, value+1);

        // 4
        key = keccak256(abi.encodePacked(tickets[ticketId].a, tickets[ticketId].b, tickets[ticketId].c, tickets[ticketId].d, tickets[ticketId].drawId));
        (success, value) = ticketsSaleQty.tryGet(key);
        value = success ? value : 0;
        ticketsSaleQty.set(key, value+1);

        key = keccak256(abi.encodePacked(tickets[ticketId].a, tickets[ticketId].b, tickets[ticketId].c, tickets[ticketId].e, tickets[ticketId].drawId));
        (success, value) = ticketsSaleQty.tryGet(key);
        value = success ? value : 0;
        ticketsSaleQty.set(key, value+1);

        key = keccak256(abi.encodePacked(tickets[ticketId].b, tickets[ticketId].c, tickets[ticketId].d, tickets[ticketId].e, tickets[ticketId].drawId));
        (success, value) = ticketsSaleQty.tryGet(key);
        value = success ? value : 0;
        ticketsSaleQty.set(key, value+1);
    }

    function getTicketSaleByHash(bytes32 _key) external view returns(uint256) {
      
        (bool success, uint256 value) = ticketsSaleQty.tryGet(_key);
        return success ? value : 0;
    }

    function toString(uint256 value) internal pure returns (string memory) {
    // Inspired by OraclizeAPI's implementation - MIT license
    // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol
        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
      }

    function getMyPrizedTickets(address _user) external view returns(uint256[] memory) {
        uint256[] memory ticketIds = getTokenIdsByOwner(_user);
        uint256 pending;
        uint256 count;
        uint256 amount;
        uint256 i;
        for (i; i<ticketIds.length; ++i) {
            pending = IRouter(router).pending(ticketIds[i]);
            if (pending > 0) {
                amount = amount.add(pending);
                count++;
            }
        }
        uint256[] memory ret = new uint256[](count+1);
        ret[0] = amount;
        count = 0;
        for (i = 0; i<ticketIds.length; ++i) {
            pending = IRouter(router).pending(ticketIds[i]);
            if (pending > 0) {
                ret[count+1] = ticketIds[i]; 
                count++;
            }
        }
        return ret;
    }

    // mapping(uint256 => address => EnumerableSet.UintSet) userHasTickets
    // function getMyTicketsByDrawId(uint256 _drawId, address _user) external view returns(uint256[] memory) {

    // }


    function insertionSort(uint256[] memory a) public pure returns(uint256[] memory) {
        // note that uint can not take negative value
        for (uint i = 1;i < a.length;i++){
            uint temp = a[i];
            uint j=i;
            while( (j >= 1) && (temp < a[j-1])){
                a[j] = a[j-1];
                j--;
            }
            a[j] = temp;
        }
        return(a);
    }
}

