contract MontrealAuction is Ownable, ReentrancyGuard{
    // ============ Events ============
    event LogBid(address bidder, uint256 qty, uint256 unit_bid, uint256 blocknumber);
    event LogWithdrawal(address withdrawalAccount, uint256 amount);
    event LogRefund(address refundAccount, uint256 amount);

    // ============ State ============
    struct BidderInformation { 
      uint256 totalFunds;
      uint256 unitBid;
      uint256 qty;
    }

    bool public canceled;

    address[] internal bidders;
    mapping(address => uint256) internal index;
    mapping(address => BidderInformation) internal infoByBidder;

    uint256 public minBid;
    uint256 public startBlock;
    uint256 public endBlock;
    uint256 public maxQty;

    address public totmContractAddress;
    address public multiverseLabAddress;

    uint256 public libPrice;

    uint amoutAvailableToWithdraw;

    constructor(
            uint256 _minBid,
            uint256 _maxQty,
            uint256 _startBlock,
            uint256 _endBlock
        ) {
            minBid = _minBid;
            maxQty = _maxQty;
            startBlock = _startBlock;
            endBlock = _endBlock;
            libPrice = 0;
        }

    // ============ Auction Main Functions ============
    //Register Bid, function that will allow user to register a bid or increase their current bid.
    function registerBid(uint _qty) external payable nonReentrant onlyAuctionOngoing {
        address _bidder = msg.sender;
        uint256 _totalAmount = msg.value;
        BidderInformation memory info = infoByBidder[_bidder]; 

        if(info.totalFunds == 0){//First Bid
            require(_qty != 0, "A Quantity is required");
            require(_qty <= maxQty, "Maximum quantity reached");
            require(_totalAmount >= minBid, "Must send at least minimum bid");
            require(_totalAmount/_qty >= minBid, "Must send at least minimum bid");

            info.totalFunds = _totalAmount;
            info.qty = _qty;
            info.unitBid = _totalAmount/_qty;
            infoByBidder[_bidder] = info;

            bidders.push(_bidder); 
            index[_bidder] = bidders.length - 1;
        }else{//Increasing current bid
            require(info.qty + _qty <= maxQty, "Maximum quantity reached");
            require((info.totalFunds + _totalAmount)/(info.qty + _qty) >= minBid, "Must send at least minimum bid");

            info.totalFunds = info.totalFunds + _totalAmount;
            info.qty = info.qty +  _qty;
            info.unitBid = info.totalFunds/info.qty;
            infoByBidder[_bidder] = info;
        }
   
        emit LogBid(_bidder,info.qty,info.unitBid,block.number);
    }

    //Withdraw Bid, function that will allow user to withdraw their bid. 
    //If the user is one of the Montreal Auction winners, they won't be able to withdraw since they will get refunded the extra payment via the mint function.
    function bidderWithdraw() public nonReentrant {
        BidderInformation memory info = infoByBidder[msg.sender]; 
        require(info.totalFunds != 0, "There are no bid associated with this address");
        if(isAuctionExpired() == true && !isAuctionCancelled()){
            require(isBidderWinner(msg.sender),"Please mint your NFT");
        }

        address payable _withdrawalAccount = payable(msg.sender);
        uint256 _withdrawalAmount = info.totalFunds;
        if(sendFunds(_withdrawalAccount,_withdrawalAmount)){
            delete infoByBidder[msg.sender];

            address lastbidder = bidders[bidders.length - 1];
            bidders[index[msg.sender]] = lastbidder;
            delete bidders[bidders.length - 1];
            index[lastbidder] = index[msg.sender];
            delete index[msg.sender];

            emit LogWithdrawal(msg.sender,_withdrawalAmount);
        }
    }

    //sendAmountToRefund, function that will refund the extra paid by the bidder during the minting process.
    //Only TravellersOfTheMultiverse contract can interact with the function.
    //Lucky winners are bidders that were not able to make the cut into the Montreal Auction. A total of XXXX were selected randomly to thank them
    //for participating. 
    //Only libPrice amount can be withdraw, this is to make sure the owners can not withdraw more than the actual lib amount and therefore 
    //user can get their full refund.
    function sendAmountToRefund(address _minter) public onlyTOTMContract  {
        BidderInformation memory info = infoByBidder[_minter]; 
        require(info.totalFunds != 0, "There are no bid associated with this address");
        uint256 totalToPay = 0;
        if(info.unitBid < libPrice){//lucky winners can only receive one.
            totalToPay = info.unitBid;
        }else{
            totalToPay = info.qty * libPrice;
        }

        uint256 totalToRefund = info.totalFunds - totalToPay;
        if(totalToRefund > 0){
            address payable _recipient = payable(_minter);
            sendFunds(_recipient,totalToRefund);
            emit LogRefund(_recipient,totalToRefund);
        }
        amoutAvailableToWithdraw += totalToPay;
    }

    function ownerWithdraw() public nonReentrant onlyOwner onlyAuctionExpired {
        uint256 teamWithdraw = amoutAvailableToWithdraw / 20; //5% to team
        uint256 multiVerseLabWithdraw = amoutAvailableToWithdraw / 5; //20% to multiverse lab
        amoutAvailableToWithdraw = 0;
        sendFunds(payable(owner()),teamWithdraw);
        emit LogWithdrawal(owner(),teamWithdraw);
        sendFunds(payable(multiverseLabAddress),multiVerseLabWithdraw);
        emit LogWithdrawal(multiverseLabAddress,multiVerseLabWithdraw);
    }
    
    function sendFunds(address payable _to, uint256 _amount) internal returns (bool) {
        (bool sent, bytes memory data) = _to.call{value: _amount}("");
        return sent;
    }

    // =========== Retrieve Bid Information ==================
    function getBidders() external view returns (address[] memory){
        return bidders;
    }
    function getBidderInfo(address _bidder) external view returns (uint256,uint256,uint256){
        BidderInformation memory info = infoByBidder[_bidder]; 
        return (info.totalFunds,info.unitBid,info.qty);
    }
    function getBidderTotalFunds(address _bidder) external view returns (uint256){
        BidderInformation memory info = infoByBidder[_bidder]; 
        return (info.totalFunds);
    }
    function getBidderQty(address _bidder) external view returns (uint256){
        BidderInformation memory info = infoByBidder[_bidder]; 
        return (info.qty);
    }
    function getBidderUnitBid(address _bidder) external view returns (uint256){
        BidderInformation memory info = infoByBidder[_bidder]; 
        return (info.unitBid);
    }

    // ============ Variable update ============
    function setMinBid(uint256 _newMinBid) external onlyOwner {
        minBid = _newMinBid;
    }

    function setMaxQty(uint256 _newMaxQty) external onlyOwner {
        maxQty = _newMaxQty;
    }

    function setEndBlock(uint256 _endBlock) external onlyOwner {
        endBlock = _endBlock;
    }

    function setTOTMAddress(address _totmAddress) external onlyOwner {
       totmContractAddress = _totmAddress;
    }

    function setMultiverseLabAddress(address _multiverseLabAddress) external onlyOwner {
       multiverseLabAddress = _multiverseLabAddress;
    }

    //Winners are sorted in real time via our back end servers and will be use to set the final list at the end of the auction
    //The sorting is not on chain since it would be too expensive to do.
    function setlibPrice(uint256 _libPrice) external onlyOwner onlyAuctionExpired  {
        libPrice = _libPrice;
    }

    function cancelAuction() external onlyOwner onlyAuctionOngoing {
        canceled = true;
    }

    // ============ Auction Check Functions ============
    function isAuctionOngoing() public view returns (bool){
        return (block.number < endBlock && !canceled);
    }

    function isAuctionExpired() public view returns (bool){
        return (block.number > endBlock && !canceled);
    }

    function isAuctionCancelled() public view returns (bool){
        return (canceled);
    }

    function isOnlyTOTMContract() public view returns (bool){
        return (totmContractAddress == msg.sender);
    }

    function isBidderWinner(address _bidder) public view returns (bool){
        BidderInformation memory info = infoByBidder[_bidder]; 
        return (info.unitBid >= libPrice && isAuctionExpired());
    }

    // ============ Modifiers ============
    modifier onlyAuctionOngoing {
        require(isAuctionOngoing(), "Auction must be ongoing");
        _;
    }

    modifier onlyAuctionExpired {
        require(isAuctionExpired(), "Auction has not expired yet");
        _;
    }

    modifier onlyAuctionCanceled {
        require(isAuctionCancelled(), "Auction must be canceled");
        _;
    }

    modifier onlyTOTMContract {
        require(isOnlyTOTMContract(), "Only TOTM contract can use this function");
        _;
    }
}

