contract FeeManagement is Ownable, ReentrancyGuard{
    using Counters for Counters.Counter;
    Counters.Counter private _feeIds;
    
    struct FeeItem {
        uint256 feeId;
        address nftContract;
        uint256 tokenId;
        address setter;
        address receiver;
        uint256 feeRatio;
        uint    feeType;
        uint    status;
        uint256 timeStamp;
    }

    event FeeItemUpdated (
        uint256 indexed feeId,
        address indexed nftContract,
        uint256 indexed tokenId,
        address setter,
        address receiver,
        uint256 feeRatio,
        uint    feeType,
        uint    status,
        uint256 timeStamp
    );

    mapping(uint256 => FeeItem) private idToFeeItem;

    function createFeeItemIfAdmin(
        address nftContract,
        uint256 tokenId,
        address setter,
        address receiver,
        uint256 feeRatio,
        uint    feeType
    ) public {

        require(this.owner()==msg.sender,"You can not the admin of the platform.");

        createFeeItem(nftContract,tokenId,setter,receiver,feeRatio,feeType);
    
    }

    
    function createFeeItemIfOwner(
        address nftContract,
        uint256 tokenId,
        address setter,
        address receiver,
        uint256 feeRatio,
        uint    feeType
    ) public {

        require(msg.sender == IOwnable(nftContract).owner(),"You can not the contract owner of the collection.");

        createFeeItem(nftContract,tokenId,setter,receiver,feeRatio,feeType);
    
    }

    function createFeeItem(
        address nftContract,
        uint256 tokenId,
        address setter,
        address receiver,
        uint256 feeRatio,
        uint    feeType
    ) private nonReentrant{

        require(nftContract != 0x0000000000000000000000000000000000000000,"Contract address shouldn't be 0.");
        require(setter != 0x0000000000000000000000000000000000000000,"Setter shouldn't be 0.");
        require(receiver != 0x0000000000000000000000000000000000000000,"Receiver shouldn't be 0.");
        
        uint256 tempIndex = getFeeItem(nftContract,tokenId,feeType).feeId;

        require(tempIndex==0, "There's existing Fee Item, please Edit/ Delete Fee item.");

        //Create New Fee Item
        if(tempIndex==0)
        {

            _feeIds.increment();
            uint256 feeId = _feeIds.current();
            
            idToFeeItem[feeId] =  FeeItem(
                feeId,
                nftContract,
                tokenId,
                setter,
                receiver,
                feeRatio,
                feeType,
                1,
                block.timestamp
            );

            emit FeeItemUpdated(
                feeId,
                nftContract,
                tokenId,
                setter,
                receiver,
                feeRatio,
                feeType,
                1,
                block.timestamp
            );

        }
        
    }

    function editFeeItemIfSetter(
        uint256 feeId,
        address setter,
        address receiver,
        uint256 feeRatio
    ) public {

        require(idToFeeItem[feeId].setter==msg.sender, "User don't have righ to edit Fee Item.");
        
        editFeeItem(feeId,setter,receiver,feeRatio);
    }

    function editFeeItemIfAdmin(
        uint256 feeId,
        address setter,
        address receiver,
        uint256 feeRatio
    ) public {

        require(this.owner()==msg.sender, "User don't have righ to edit Fee Item.");
        
        editFeeItem(feeId,setter,receiver,feeRatio);
    }

    
    function editFeeItemIfOwner(
        uint256 feeId,
        address setter,
        address receiver,
        uint256 feeRatio
    ) public {

        require(IOwnable(idToFeeItem[feeId].nftContract).owner()==msg.sender, "User don't have righ to edit Fee Item.");
        
        editFeeItem(feeId,setter,receiver,feeRatio);
    }

    function editFeeItem(
        uint256 feeId,
        address setter,
        address receiver,
        uint256 feeRatio
    ) private nonReentrant{

        require(idToFeeItem[feeId].nftContract != 0x0000000000000000000000000000000000000000,"No such Fee Item");
        
        idToFeeItem[feeId].setter   = setter;
        idToFeeItem[feeId].receiver = receiver;
        idToFeeItem[feeId].feeRatio = feeRatio;

        emit FeeItemUpdated(
                idToFeeItem[feeId].feeId,
                idToFeeItem[feeId].nftContract,
                idToFeeItem[feeId].tokenId,
                idToFeeItem[feeId].setter,
                idToFeeItem[feeId].receiver,
                idToFeeItem[feeId].feeRatio,
                idToFeeItem[feeId].feeType,
                idToFeeItem[feeId].status,
                block.timestamp
        );

    }

    function deleteFeeItemIfSetter(
        uint256 feeId
    ) public {

        require(idToFeeItem[feeId].setter==msg.sender, "User don't have righ to delete Fee Item.");
        
        deleteFeeItem(feeId);
    }

    function deleteFeeItemIfAdmin(
        uint256 feeId
    ) public {

        require(this.owner()==msg.sender, "User don't have righ to delete Fee Item.");
        
        deleteFeeItem(feeId);
    }

    
    function deleteFeeItemIfOwner(
        uint256 feeId
    ) public {

        require(IOwnable(idToFeeItem[feeId].nftContract).owner()==msg.sender, "User don't have righ to delete Fee Item.");
        
        deleteFeeItem(feeId);
    }


    function deleteFeeItem(
        uint256 feeId
    ) private nonReentrant{

        require(idToFeeItem[feeId].nftContract != 0x0000000000000000000000000000000000000000,"No such Fee Item");
        
        idToFeeItem[feeId].status   = 0;

        emit FeeItemUpdated(
                idToFeeItem[feeId].feeId,
                idToFeeItem[feeId].nftContract,
                idToFeeItem[feeId].tokenId,
                idToFeeItem[feeId].setter,
                idToFeeItem[feeId].receiver,
                idToFeeItem[feeId].feeRatio,
                idToFeeItem[feeId].feeType,
                idToFeeItem[feeId].status,
                block.timestamp
        );

        delete idToFeeItem[feeId];
    }

    function getFeeItem(
        address nftContract,
        uint256 tokenId,
        uint    feeType
    ) public view returns (FeeItem memory){

        uint counter = _feeIds.current();
        FeeItem memory currentItem;

        //check duplication FeeItem
        for(uint i=0;i<counter; i++)
        {
            uint currentId = i + 1;
            if((idToFeeItem[currentId].nftContract==nftContract)
                &&(idToFeeItem[currentId].tokenId==tokenId)
                    &&(idToFeeItem[currentId].feeType==feeType))
            {
                currentItem = idToFeeItem[currentId];
                return currentItem;
            }
        }
        return currentItem;
    }

    function getFeeItems(
        ) public view returns (FeeItem[] memory){
        uint feeCount = _feeIds.current();
        uint currentIndex = 0;

        FeeItem[] memory items = new FeeItem[](feeCount);
        for (uint i = 0; i < feeCount; i++) {
            uint currentId = i + 1;
            FeeItem storage currentItem = idToFeeItem[currentId];
            items[currentIndex] = currentItem;
            currentIndex += 1;
        }

        return items;
    }
    
}

