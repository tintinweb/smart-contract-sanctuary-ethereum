/**
 * @title BetData
 * @author Michael Larsen
 * @notice Central definition for what a bet is with utilities to make/check etc.
 */
library BetData {
    using SafeMath for uint256;

    uint256 public constant ODDS_PRECISION = 1e10;

    struct Bet {
        bytes32 marketHash;
        bytes32 signature;
        address betToken;
        uint256 amount;
        uint256 percentageOdds;
        uint256 expiry;
        address owner;
        MarketData.MarketOutcome outcome;
    }

    struct IncomingBet {
        bytes32 betHash;
        bytes32 marketHash;
        bytes32 signature;
        address betToken;
        uint256 amount;
        uint256 percentageOdds;
        uint256 expiry;
        address owner;
        uint8 outcome; // 1-3 is the valid options to bet on
    }

    struct BetList {
        Bet[] bets;
    }

    /**
     * @notice Checks if a bet exists (otherwise 0).
     * @param bet The bet to check.
     */
    function assertBetExists(Bet memory bet) internal pure {
        require(
            bet.amount > 0,
            "BET_NOT_FOUND"
        );
    }

    /**
     * @notice Checks the parameters of a bet to see if they are valid.
     * @param bet The bet to check.
     * @return string A status string in UPPER_SNAKE_CASE.
     *         It will return "OK" if everything checks out.
     */
    function getParamValidity(Bet memory bet)
        internal
        view
        returns (string memory)
    {
        if (bet.amount == 0) {return "BET_AMOUNT_ZERO";}
        if (bet.percentageOdds == 0 || bet.percentageOdds >= ODDS_PRECISION) {
            return "INVALID_PERCENTAGE_ODDS";
        }
        if (bet.expiry < block.timestamp) {return "ORDER_EXPIRED";}
        if (bet.betToken == address(0)) {return "BAD_TOKEN";}
        return "OK";
    }

    /**
     * @notice Checks the signature of a bet to see if it was signed by
     *         a given signer.
     * @param bet The bet to check.
     * @param signature The signature to compare to the signer.
     * @param signer The signer to compare to the signature.
     * @return bool True if the signature matches, false otherwise.
     */
    function checkSignature(
        Bet calldata bet,
        bytes calldata signature,
        address signer
    )
        internal
        pure
        returns (bool)
    {
        return checkSignature(getBetHash(bet), signature, signer);
    }

    /**
     * @notice Checks the signature of a data hash to see if it was signed by
     *         a given signer.
     * @param dataHash The data to check.
     * @param signature The signature to compare to the signer.
     * @param signer The signer to compare to the signature.
     * @return bool True if the signature matches, false otherwise.
     */
    function checkSignature(
        bytes32 dataHash,
        bytes calldata signature,
        address signer
    )
        internal
        pure
        returns (bool)
    {
        bytes32 signedMsgHash = ECDSA.toEthSignedMessageHash(dataHash);
        return ECDSA.recover(signedMsgHash, signature) == signer;
    }

    /**
     * @notice Checks if a bets parameters conforms to the specifications.
     * @param bet The bet to check.
     */
    function assertValidParams(Bet memory bet) internal view {
        require(
            bet.amount > 0,
            "TOTAL_BET_SIZE_ZERO"
        );
        require(
            bet.percentageOdds > 0 && bet.percentageOdds < ODDS_PRECISION,
            "INVALID_PERCENTAGE_ODDS"
        );
        require(bet.betToken != address(0), "INVALID_BASE_TOKEN");
        require(bet.expiry > block.timestamp, "ORDER_EXPIRED");
    }

    /**
     * @notice Checks if an bet has valid parameters including the signature.
     * @param bet The Bet to check.
     * @param signature The signature to check.
     * @param signer The signer to compare to the signature.
     */
    function assertValid(
        Bet calldata bet,
        bytes calldata signature,
        address signer
    )
        internal
        view
    {
        assertValidParams(bet);
        require(
            checkSignature(bet, signature, signer),
            "SIGNATURE_MISMATCH"
        );
    }

    /**
     * @notice Checks if the bet has valid parameters
     *         and checks if the sender is the owner.
     * @param bet The bet to check.
     * @param sender The address to compare the owner to.
     */
    function assertValidAsSender(Bet memory bet, address sender)
        internal
        view
    {
        assertValidParams(bet);
        require(bet.owner == sender, "CALLER_NOT_OWNER");
    }

    /**
     * @notice Computes the hash of a bet. Packs the arguments in order
     *         of the Bet struct.
     * @param bet The Bet to compute the hash of.
     * @return bytes32 The calculated hash of of the bet.
     */
    function getBetHash(Bet memory bet) internal pure returns (bytes32) {
        return keccak256(
            abi.encodePacked(
                bet.marketHash,
                bet.betToken,
                bet.amount,
                bet.percentageOdds,
                bet.expiry,
                bet.owner,
                bet.outcome
            )
        );
    }

    /**
     * @notice Logs the content of a bet to the Hardhat console log.
     * @param bet The Bet to log the content of.
     */
    function logBet(Bet memory bet) internal view {
        console.log("Bet details:");
        console.log("hash:", Strings.toHexString(uint256(bet.marketHash), 32));
        console.log("token", bet.betToken);
        console.log("amount:", bet.amount / 1 ether);
        console.log("odds:", bet.percentageOdds * 100 / ODDS_PRECISION);
        console.log("expiry:", bet.expiry);
        console.log("owner:", bet.owner);
        console.log("outcome:", uint8(bet.outcome));
    }

    /**
     * @notice Getter for odds precision.
     * @return uint256 The const ODDS_PRECISION.
     */
    function getOddsPrecision() internal pure returns (uint256) {
        return ODDS_PRECISION;
    }
}

