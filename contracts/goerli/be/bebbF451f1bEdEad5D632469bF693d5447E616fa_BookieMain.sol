pragma solidity ^0.8.14;

import "./interfaces/IMarketHistory.sol";
import "./utils/AccessHandler.sol";
import "./libs/MarketData.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

/**
 * @title Market History
 * @author Michael Larsen
 * @notice Simple contract for historical market data.
 * @notice This is a sub contract for the Bookie app.
 * @notice Accesshandler is Initializable.
 */
contract MarketHistory is IMarketHistory, AccessHandler {

    using SafeMath for uint256;

    uint256 private marketCounter;

    // market hash => market details
    mapping(bytes32 => MarketData.Market) public markets;
    // market hash => market state
    mapping(bytes32 => MarketData.MarketState) public marketStates;
    // market hash => market outcome
    mapping(bytes32 => MarketData.MarketOutcome) public marketOutcomes;

    /**
     * @notice Event that fires when a market is added.
     * @param market is the market detail struct.
     * @param marketHash is the hash used to identify the market.
     */
    event MarketAdded(
        MarketData.Market market,
        MarketData.MarketState state,
        bytes32 marketHash
    );

    /**
     * @notice Event that fires when a market is activated.
     * @param marketHash is the hash used to identify the market.
     */
    event MarketActivated(
        bytes32 marketHash
    );

    /**
     * @notice Event that fires when a market is settled.
     * @param marketHash is the hash used to identify the market.
     * @param outcome is the outcome of the market.
     */
    event MarketSettled(
        bytes32 marketHash,
        MarketData.MarketOutcome outcome
    );

    /**
     * @notice Error when trying to access a market that does not exists.
     * @param marketHash is the hash of the requested market.
     */
    error UnknownMarket(bytes32 marketHash);

    /**
     * @notice Error when trying to add a market that already exists.
     * @param marketHash is the hash of the existing market.
     */
    error DuplicateMarket(bytes32 marketHash);

    /**
     * @notice Error when trying to access a market that has the wrong state.
     * @param expected is the expected state of the requested market.
     * @param current is the actual state of the requested market.
     */
    error InvalidMarketState(
        MarketData.MarketState expected,
        MarketData.MarketState current
    );

    /**
     * @notice Simple constructor that just initizializes.
     */
    constructor() {
        AccessHandler.initialize();
    }

    /**
     * @notice Makes a market for testing.
     * @notice Error is emitted if the market already exists, Event if not.
     */
    function addMarket()
        public
        override
        isInitialized()
        onlyRole(AccessHandler.REPORTER_ROLE)
    {
        marketCounter++;
        MarketData.Market memory market = MarketData.Market({
            sport: "",
            league: "",
            name: "",
            desc: "",
            startTime: marketCounter,
            createTime: block.timestamp
        });
        bytes32 hash = MarketData.getMarketHash(market);
        // Checks that it does not exist already
        MarketData.Market storage existingMarket = markets[hash];
        if (existingMarket.startTime > 0)
            revert DuplicateMarket({marketHash: hash});

        markets[hash] = market;
        marketStates[hash] = MarketData.MarketState.Pending;
        marketOutcomes[hash] = MarketData.MarketOutcome.Undefined;

        emit MarketAdded (market, MarketData.MarketState.Pending, hash);
    }

    /**
     * @notice Updates a market to Active state.
     * @notice Revert Error is emitted if not exist.
     * @param marketHash is the hash of the market to update.
     */
    function activateMarket(bytes32 marketHash)
        public
        override
        onlyRole(AccessHandler.REPORTER_ROLE)
    {
        MarketData.Market storage market = markets[marketHash];

        // Checks that it exists already
        if (market.startTime == 0)
            revert UnknownMarket({marketHash: marketHash});
        // Checks that it is pending
        if (marketStates[marketHash] != MarketData.MarketState.Pending) {
            revert InvalidMarketState({
                expected: MarketData.MarketState.Pending,
                current: marketStates[marketHash]
            });
        }
        marketStates[marketHash] = MarketData.MarketState.Active;
        emit MarketActivated(marketHash);
    }

    /**
     * @notice Updates a market to Settled state.
     * @notice Revert Error is emitted if not exist or invalid.
     * @param marketHash is the hash of the market to update.
     * @param outcome is the outcome of market, eg Win, loss etc.
     */
    function settleMarket(bytes32 marketHash, MarketData.MarketOutcome outcome)
        public
        override
        onlyRole(AccessHandler.REPORTER_ROLE)
    {
        MarketData.Market storage market = markets[marketHash];

        // Checks that it exists already
        if (market.startTime == 0)
            revert UnknownMarket({marketHash: marketHash});
        // Checks that it is Active
        if (marketStates[marketHash] != MarketData.MarketState.Active) {
            revert InvalidMarketState({
                expected: MarketData.MarketState.Active,
                current: marketStates[marketHash]
            });
        }
        // Below is disabled for now, as its not a possible error
        // Checks that it is has no outcome yet
//        if (marketOutcomes[marketHash] != MarketData.MarketOutcome.Undefined) {
//            revert InvalidMarketOutcome({
//                expected: MarketData.MarketOutcome.Undefined,
//                current: marketOutcomes[marketHash]
//            });
//        }
        marketStates[marketHash] = MarketData.MarketState.Completed;
        marketOutcomes[marketHash] = outcome;
        emit MarketSettled(marketHash, outcome);
    }

    /**
     * @notice Check if market exists and is active.
     * @notice Errors are emitted if check fails.
     * @param marketHash is the hash of the market to check.
     */
    function assertMarketIsActive(bytes32 marketHash) public view override {
        // Check that its a valid market
        MarketData.Market memory market = markets[marketHash];
        if (market.startTime == 0)
            revert UnknownMarket({marketHash: marketHash});
        MarketData.MarketState state = MarketHistory.marketStates[marketHash];
        if (state != MarketData.MarketState.Active) {
            revert InvalidMarketState({
                expected: MarketData.MarketState.Active,
                current: state
            });
        }
    }

    /**
     * @notice Check if market exists and is completed.
     * @notice Errors are emitted if check fails.
     * @param marketHash is the hash of the market to check.
     */
    function assertMarketIsCompleted(bytes32 marketHash) public view override {
        // Check that its a valid market
        MarketData.Market storage market = markets[marketHash];
        if (market.startTime == 0)
            revert UnknownMarket({marketHash: marketHash});
        MarketData.MarketState state = MarketHistory.marketStates[marketHash];
        if (state != MarketData.MarketState.Completed) {
            revert InvalidMarketState({
                expected: MarketData.MarketState.Completed,
                current: state
            });
        }
    }

    /**
     * @notice Check if a market has a specific outcome.
     * @param marketHash is the hash of the market to check.
     * @param testOutcome is the outcome to compare with.
     * @return bool is TRUE if they match, FALSE if not.
     */
    function isMarketOutcome(
        bytes32 marketHash,
        MarketData.MarketOutcome testOutcome
    )
        public
        view
        override
        returns (bool)
    {
        //MarketData.Market storage market = markets[marketHash];
        MarketData.MarketOutcome outcome = marketOutcomes[marketHash];
        if (outcome == testOutcome) return true;
        return false;
    }

    /**
     * @notice Get a market state.
     * @param marketHash is the hash of the market to find.
     * @return MarketData.MarketState is the state of the supplied market.
     */
    function getMarketState(bytes32 marketHash)
        public
        view
        override
        returns (MarketData.MarketState)
    {
        return marketStates[marketHash];
    }

}

