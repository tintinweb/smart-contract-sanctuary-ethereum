    /**
     * @notice Makes a bet and locks the betters and the LPs tokens.
     * @param token The token type to bet.
     * @param amount The amount to bet.
     * @param odds The odds of the bet in percentage notation.
     * @param outcome Is the out that is bet upon.
     * @param marketHash Is the market hash to identy the market.
     */
    function testBet(
        address token,
        uint256 amount,
        uint256 odds,
        MarketData.MarketOutcome outcome,
        bytes32 marketHash
    )
        public
        override
        isInitialized()
    {
        address owner = msg.sender;
        BetData.Bet memory bet = BetData.Bet({
            marketHash: marketHash,
            signature: "DummySignature",
            betToken: token,
            amount: amount,
            percentageOdds: odds,
            expiry: block.timestamp + 1000,
            owner: owner,
            outcome: outcome
        });

        // TODO verify Bet validity
        if (amount == 0) {
            emit BetZero(owner, token);
            return;
        }
        string memory validity = BetData.getParamValidity(bet);
        if (!validity.equals("OK"))
            revert(validity);

        // Check that its a valid market
        marketHistory.assertMarketIsActive(marketHash);
        (
            TokenAvailability res,
            uint256 available
        ) = TokenAmountValidator.checkAllowanceAndBalance(
            amount,
            token,
            owner,
            address(tokenTransferProxy)
        );
        if (res == TokenAvailability.InsufficientBalance) {
            revert InsufficientBalance({
                available: available,
                required: amount
            });
        } else if (res == TokenAvailability.InsufficientAllowance) {
            revert InsufficientAllowance({
                available: available,
                required: amount
            });
        }

        // Transfer the betters tokens first
        bool success = transferViaProxy(
            token,
            owner,
            address(betLockBox),
            amount
        );
        if (!success) {
            revert TokenPaymentFailed({
                better: owner,
                token: token,
                amount: amount
            });
        }

        //Now match, persist, and token-lock the bet
        betHistory.createBet(bet);
    }

    /**
     * @notice Settle a bet and pay the pot to the winner or to our LP
     * @param betHash The key/hash of the bet settle.
     */
    function settleBet(bytes32 betHash) public override {
        (
            address owner,
            address token,
            uint256 paidToBetter,
            uint256 paidToLP
        ) = betHistory.settleBet(betHash);

        if (paidToBetter == 0 && paidToLP == 0) {
            emit BetNothingToSettle(owner, token);
            return;
        }
        console.log("Ready To PAY:", paidToBetter / 1 ether, "and to return:", paidToLP / 1 ether);

        // Win or cancel of market
        if (paidToBetter > 0) {
            bool success = transferViaProxy(
                token,
                address(betLockBox),
                owner,
                paidToBetter
            );
            if (!success) {
                revert PayoutFailed({
                    receiver: owner,
                    token: token,
                    amount: paidToBetter
                });
            }
        }
        // Loss or cancel of market
        if (paidToLP > 0) {
            bool success = transferViaProxy(
                token,
                address(betLockBox),
                address(lpLockBox),
                paidToLP
            );
            if (!success) {
                revert PayoutFailed({
                    receiver: address(lpLockBox),
                    token: token,
                    amount: paidToLP
                });
            }
        }
        emit BetSettled(owner, token, paidToBetter, betHash);
    }

   /**
     * @notice Let the admin of this contract cancel an active bet
     * @param betHash The key/hash of the bet cancel.
     */
    function cancelBetAsAdmin(bytes32 betHash)
        public
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        (
            address owner,
            address token,
            uint256 paidToBetter,
            uint256 paidToLP
        ) = betHistory.cancelBet(betHash);

        if (paidToBetter == 0 && paidToLP == 0) {
            emit BetNothingToCancel(owner, token);
            return;
        }

        // Return to better
        if (paidToBetter > 0) {
            bool success = transferViaProxy(
                token,
                address(betLockBox),
                owner,
                paidToBetter
            );
            if (!success) {
                revert PayoutFailed({
                    receiver: owner,
                    token: token,
                    amount: paidToBetter
                });
            }
        }
        // return to LP
        if (paidToLP > 0) {
            bool success = transferViaProxy(
                token,
                address(betLockBox),
                address(lpLockBox),
                paidToLP
            );
            if (!success) {
                revert PayoutFailed({
                    receiver: address(lpLockBox),
                    token: token,
                    amount: paidToLP
                });
            }
        }
        emit BetCanceled(owner, token, paidToBetter, betHash);
    }

    /**
      * @notice Transfers a token using TokenTransferProxy.transferFrom().
      * @param token Address of token to transferFrom.
      * @param from Address transfering token.
      * @param to Address receiving token.
      * @param value Amount of token to transfer.
      * @return Success of token transfer.
      */
    function transferViaProxy(
        address token,
        address from,
        address to,
        uint256 value
    )
        private
        returns (bool)
    {
        return tokenTransferProxy.transferFrom(token, from, to, value);
    }
}

