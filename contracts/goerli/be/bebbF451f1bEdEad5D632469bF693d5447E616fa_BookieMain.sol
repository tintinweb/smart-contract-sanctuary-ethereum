/**
 * @title MarketData
 * @author Michael Larsen
 * @notice Central definition for what a market is along with utilities
 *         to make/check etc.
 */
library MarketData {
    // State is all possible, only Completed has a defined outcome.
    enum MarketState { Undefined, Pending, Active, Completed } // Enum
    // Outcomes are defined as seen for the home team.
    enum MarketOutcome { Undefined, Win, Draw, Loss, Cancel } // Enum

    struct Market {
        string sport;
        string league;
