import "./interfaces/ILockBox.sol";
import "./interfaces/ILiquidityPool.sol";
import "./interfaces/IMarketHistory.sol";
import "./interfaces/utils/ITokenTransferProxy.sol";
import "./interfaces/IBetHistory.sol";
import "./utils/AccessHandler.sol";
import "./libs/BetData.sol";
import "./libs/MarketData.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
// For debugging only
import "hardhat/console.sol";

/**
 * @title Bet History
 * @author Michael Larsen
 * @notice Simple contract for historical bet data.
 * @notice This is a sub contract for the Bookie app.
 * @notice Accesshandler is Initializable.
 */
contract BetHistory is IBetHistory, AccessHandler {
    using SafeMath for uint256;

    ILockBox private lpLockBox;
    ILockBox private betLockBox;
    IMarketHistory private marketHistory;
    ITokenTransferProxy private tokenTransferProxy;

    // market hash => outcome => better => token => odds => Bet (struct)
//  replaced below  mapping(bytes32 => mapping(MarketData.MarketOutcome => mapping(address => mapping(address => mapping(uint256 => BetData.Bet))))) public allBets;
    // market hash => outcome => better => token => odds => pot
//  replaced below  mapping(bytes32 => mapping(MarketData.MarketOutcome => mapping(address => mapping(address => mapping(uint256 => uint256))))) public unsettledPots;
    // TODO: Figure out if the above storage is sufficient. Bet with identical market + outcome + better + odds will be additive and have a combined pot.
    //       This is probably fine for the contract alone, but not is the backend needs to address the bets individually. We could add a bethash to the chain
    //       and expand the wagered event to contain that hash too:
    //            * @param betHash is the hash used to identify the bet.
    //       Then the bet settled event could hold the hash too, instead of the full bet details.

    // bet hash => Bet (struct)
    mapping(bytes32 => BetData.Bet) public allBets;
    // bet hash => pot size
    mapping(bytes32 => uint256) public unsettledPots;


    /**
     * @notice Error for Insufficient liquidity to match a bet.
     * Needed `required` but only `available` available.
     * @param available balance available.
     * @param required requested amount to transfer.
     */
    error InsufficientLiquidityBalance(uint256 available, uint256 required);

    /**
     * @notice Error for Invaid bet.
     * @param bet is the bet details.
     */
    error InvalidBet(BetData.Bet bet);

    /**
     * @notice Default Constructor.
     */
    constructor()
    {
/* So far done by init to be able to use proxy-upgrade pattern
        lpLockBox = _lpLockBox;
        betLockBox = _betLockBox;
        marketHistory = _marketHistory;
        tokenTransferProxy = _tokenTransferProxy; // Just used as address for approve
        AccessHandler.initialize();
 */
    }

    /*
     * @notice Initializes this contract with reference to other contracts.
     * @param _lpLockBox The Liquidity Pools LockBox contract, to match bets.
     * @param _betLockBox The Bet LockBox contract address.
     * @param _marketHistory The market history contract for storing markets.
     * @param _tokenTransferProxy The TokenTransferProxy contract address.
     */
    function init(
        ILockBox _lpLockBox,
        ILockBox _betLockBox,
        IMarketHistory _marketHistory,
        ITokenTransferProxy _tokenTransferProxy
    )
        external
        notInitialized
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        lpLockBox = _lpLockBox;
        betLockBox = _betLockBox;
        marketHistory = _marketHistory;
        tokenTransferProxy = _tokenTransferProxy;
        AccessHandler.initialize();
    }

    /**
     * @notice Create bet, by storing the bet details.
     * @param bet is the details data.
     * @return betHash is the calculated bet hash.
     */
    function createBet(BetData.Bet calldata bet)
        public
        override
        isInitialized()
        onlyRole(BETTER_ROLE)
        returns (bytes32 betHash, uint256 pot)
    {
        betHash = BetData.getBetHash(bet);

        // Lock the betters and the LPs tokens
        betLockBox.lockAmount(bet.owner, bet.betToken, bet.amount);
        //Transfer LP tokens to the lockbox too.
        pot = bet.amount.mul(BetData.ODDS_PRECISION).div(bet.percentageOdds);
        uint256 matchedAmount = pot.sub(bet.amount);
        if (matchedAmount == 0)
            revert InvalidBet({bet: bet});

        matchBet(bet.betToken, matchedAmount);

        // bet hash => Bet (struct)
        allBets[betHash] = bet;
        // bet hash => pot
        unsettledPots[betHash] = pot;

        // Just debugging
        console.log("betHash:", Strings.toHexString(uint256(betHash), 32));
    }

    /**
     * @notice Settle bet, by resetting the unsettled amount,
               unlocking tokens and paying the pot to LP or better.
     * @param betHash is the key used to look up the bet data.
     * @return owner is the owner of the bet.
     * @return token is the token type to settle.
     * @return paidToBetter is the amount won from the bet
     * @return paidToLP is the amount returned to the LP.
     */
    function settleBet(bytes32 betHash)
        public
        override
        onlyRole(BETTER_ROLE)
        returns
        (
            address owner,
            address token,
            uint256 paidToBetter,
            uint256 paidToLP
        )
    {
        BetData.Bet storage bet = allBets[betHash];
        BetData.logBet(bet);
        console.log("Proxy:", address(tokenTransferProxy));
        
        // TODO verify Bet validity
        BetData.assertBetExists(bet);

        // Check that market is ready for settling
        marketHistory.assertMarketIsCompleted(bet.marketHash);

        // Get the pot and reset
        uint256 potSize = unsettledPots[betHash];
        uint256 matchedAmount = potSize.sub(bet.amount);
        unsettledPots[betHash] = 0;
        owner = bet.owner;
        token = bet.betToken;
        paidToBetter = 0;
        paidToLP = 0;

        console.log("potSize:", potSize / 1 ether, "amount:", bet.amount/ 1 ether);
        uint256 lockedAmount = betLockBox.getLockedAmount(owner, token);
        console.log("locked:", lockedAmount / 1 ether);
        if (lockedAmount == 0 || potSize == 0) {
            return (owner, token, 0, 0);
        }

        console.log("Unlock for better ....");
        // Unlock the betters and the LPs tokens
        betLockBox.unlockAmountTo(
            owner,
            address(tokenTransferProxy),
            token,
            bet.amount);
        console.log("Unlock for LP Box ....");
        betLockBox.unlockAmountTo(
            address(lpLockBox),
            address(tokenTransferProxy),
            token,
            matchedAmount);

        bool canceled = marketHistory.isMarketOutcome(
            bet.marketHash,
            MarketData.MarketOutcome.Cancel
        );
        // Market was cancelled, just return both parties investment
        if (canceled) {
            paidToBetter = bet.amount;
            paidToLP = matchedAmount;
        } else {
            bool win = marketHistory.isMarketOutcome(
                bet.marketHash,
                bet.outcome
            );
            if (win) {
                //Better won give them the full pot - win fee
                paidToBetter = potSize; // TODO handle fee
            } else {
                //Better lost send the full pot to the LP
                paidToLP = potSize;
            }
        }
        console.log("pay to better:", paidToBetter / 1 ether);
        console.log("pay to LP:", paidToLP / 1 ether);
    }

    /**
     * @notice Cancel bet, by resetting the unsettled amount,
               unlocking and paying back tokens to LP and better.
     * @param betHash is the key used to look up the bet data.
     * @return owner is the owner of the bet.
     * @return token is the token type to settle.
     * @return paidToBetter is the amount won from the bet
     * @return paidToLP is the amount returned to the LP.
     */
    function cancelBet(bytes32 betHash)
        public
        override
        onlyRole(BETTER_ROLE)
        returns
        (
            address owner,
            address token,
            uint256 paidToBetter,
            uint256 paidToLP
        )
    {
        BetData.Bet storage bet = allBets[betHash];

        // TODO verify Bet validity
        BetData.assertBetExists(bet);

        // Check that market is not settled yet (active)
        marketHistory.assertMarketIsActive(bet.marketHash);

        // Get the pot and reset
        uint256 potSize = unsettledPots[betHash];
        uint256 matchedAmount = potSize.sub(bet.amount);
        unsettledPots[betHash] = 0;
        owner = bet.owner;
        token = bet.betToken;

        uint256 lockedAmount = betLockBox.getLockedAmount(owner, token);
        if (lockedAmount == 0 || potSize == 0) {
            return (owner, token, 0, 0);
        }

        // Unlock the betters and the LPs tokens
        betLockBox.unlockAmountTo(
            owner,
            address(tokenTransferProxy),
            token,
            bet.amount);
        betLockBox.unlockAmountTo(
            address(lpLockBox),
            address(tokenTransferProxy),
            token,
            matchedAmount);

        paidToBetter = bet.amount;
        paidToLP = matchedAmount;
    }

        /**
     * @notice Match bet, transfer tokens from the LP box to the bet box.
     * @param token The token type.
     * @param matchedAmount The amount to match the bet.
     */
    function matchBet(address token, uint256 matchedAmount) private {
        uint256 liquidityBalance = IERC20(token).balanceOf(address(lpLockBox));
        if (liquidityBalance < matchedAmount)
        {
            revert InsufficientLiquidityBalance({
                available: liquidityBalance,
                required: matchedAmount
            });
        }

        lpLockBox.transferAmountTo(address(betLockBox), token, matchedAmount);
        betLockBox.lockAmount(address(lpLockBox), token, matchedAmount);
    }
}

