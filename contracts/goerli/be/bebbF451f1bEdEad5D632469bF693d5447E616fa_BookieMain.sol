/**
 * @title Bookie Main contract
 * @author Michael Larsen
 * @notice This is the main contract for the app:
 *         Bet Matchup With Liquidity Pool (BetMatchupWithLP).
 * @notice Multi contract app taking bets and locking tokens until bets settle.
 * @notice Token bets are matched from a liquidity pool (LP) of same token type.
 * @notice The LP Box holds tokens of a certain type, and issues LP tokens as
 *         proof of deposit.
 * @notice It has a simple next.js react app for easy interfacing.
 * @notice Accesshandler is Initializable.
 */
contract BookieMain is IBookieMain, AccessHandler {
    using SafeMath for uint256;
    using LibString for string;

    ILiquidityPool private lp; // Just used as an address
    ILockBox private lpLockBox; // Just used as an address
    ILockBox private betLockBox; // Just used as an address
    IBetHistory private betHistory;
    IMarketHistory private marketHistory;
    ITokenTransferProxy private tokenTransferProxy;

    /**
     * @notice Event fires when bets for 0 tokens is attempted
     * @param better is the better address
     * @param token is the token contract address
     */
    event BetZero(
        address indexed better,
        address indexed token
    );

    /**
     * @notice Event that fires when a bet is accepted.
     * @param better is the address that made the bet.
     * @param token is the token contract address
     * @param betPot is the potsize of the bet (better + house).
     * @param bet is the static data describing the bet details.
     * @param betHash is calculated hash of the bet data.
     */
    event BetWagered(
        address indexed better,
        address indexed token, // TODO: Is this necessary if we only accept 1 token type?
        uint256 betPot,
        BetData.Bet bet,
        bytes32 betHash
    );

    /**
     * @notice Event that fires when a bet is decided/settled.
     * @param better is the address that won the bet.
     * @param token is the token contract address
     * @param paid is the prize paid out (0 if its a loss).
     * @param betHash is calculated hash of the bet data.
     */
    event BetSettled(
        address indexed better,
        address indexed token, // TODO: Is this necessary if we only accept 1 token type?
        uint256 paid,
        bytes32 betHash
    );

    /**
     * @notice Event that fires when a bet is canceled.
     *         Can be due to a canceled market or an admin action.
     * @param better is the address made the bet.
     * @param token is the token contract address
     * @param paid is the amount paid back.
     * @param betHash is calculated hash of the bet data.
     */
    event BetCanceled(
        address indexed better,
        address indexed token, // Is this necessary if we only accept 1 token type?
        uint256 paid,
        bytes32 betHash
    );

    /**
     * @notice Event fires there is no bet to settle
     * @param owner is the owner of the bet
     * @param token is the token contract address
     */
    event BetNothingToSettle(
        address indexed owner,
        address indexed token
    );

    /**
     * @notice Event fires there is no bet to cancel
     * @param owner is the owner of the bet
     * @param token is the token contract address
     */
    event BetNothingToCancel(
        address indexed owner,
        address indexed token
    );

    /**
     * @notice Error for Insufficient balance for betting.
     *         Needed `required` but only `available` available.
     * @param available balance available.
     * @param required requested amount to bet.
     */
    error InsufficientBalance(uint256 available, uint256 required);

    /**
     * @notice Error for Insufficient allowance for betting.
     *         Needed `required` but only `available` available.
     * @param available allowance available.
     * @param required requested amount to bet.
     */
    error InsufficientAllowance(uint256 available, uint256 required);


    /**
     * @notice Error for token transfer when betting,
     *         although balance should be available.
     * @param better is the address to receive the tokens.
     * @param token is the token contract address
     * @param amount is the requested amount to transfer.
     */
    error TokenPaymentFailed(address better, address token, uint256 amount);

    /**
     * Error for token transfer failure, prob due to lack of tokens.
     * @param receiver is the address of the payout receiver.
     * @param token is the token contract address
     * @param amount is the desired amount to pay.
     */
    error PayoutFailed(address receiver, address token, uint256 amount);

    constructor() AccessHandler() {}

    /**
     * @notice Initializes this contract with reference to other contracts.
     * @param _lp The Liquidity Pool contract address, for matching bets.
     * @param _lpLockBox The LP LockBox contract address.
     * @param _betLockBox The Bet LockBox contract address.
     * @param _betHistory The bet history contract address for storing bets.
     * @param _marketHistory The market history contract for storing markets.
     * @param _tokenTransferProxy The TokenTransferProxy contract  address.
     */
    function init(
        ILiquidityPool _lp,
        ILockBox _lpLockBox,
        ILockBox _betLockBox,
        IBetHistory _betHistory,
        IMarketHistory _marketHistory,
        ITokenTransferProxy _tokenTransferProxy
    )
        external
        notInitialized
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        lp = _lp;
        lpLockBox = _lpLockBox;
        betLockBox = _betLockBox;
        betHistory = _betHistory;
        marketHistory = _marketHistory;
        tokenTransferProxy = _tokenTransferProxy;
        AccessHandler.initialize();
    }

    /**
     * @notice Makes a bet and locks the betters and the LPs tokens.
     * @param token The token type to bet.
     * @param amount The amount to bet.
     * @param odds The odds of the bet in percentage notation.
     * @param outcome Is the out that is bet upon.
     * @param marketHash Is the market hash to identy the market.
     */
    function makeBet(
        address token,
        uint256 amount,
        uint256 odds,
        MarketData.MarketOutcome outcome,
        bytes32 marketHash
    )
        public
        override
        isInitialized()
    {
        address owner = msg.sender;
        BetData.Bet memory bet = BetData.Bet({
            marketHash: marketHash,
            signature: "DummySignature",
            betToken: token,
            amount: amount,
            percentageOdds: odds,
            expiry: block.timestamp + 1000,
            owner: owner,
            outcome: outcome
        });

        // TODO verify Bet validity
        if (amount == 0) {
            emit BetZero(owner, token);
            return;
        }
        string memory validity = BetData.getParamValidity(bet);
        if (!validity.equals("OK"))
            revert(validity);

        // Check that its a valid market
        marketHistory.assertMarketIsActive(marketHash);
        (
            TokenAvailability res,
            uint256 available
        ) = TokenAmountValidator.checkAllowanceAndBalance(
            amount,
            token,
            owner,
            address(tokenTransferProxy)
        );
        if (res == TokenAvailability.InsufficientBalance) {
            revert InsufficientBalance({
                available: available,
                required: amount
            });
        } else if (res == TokenAvailability.InsufficientAllowance) {
            revert InsufficientAllowance({
                available: available,
                required: amount
            });
        }

        // Transfer the betters tokens first
        bool success = transferViaProxy(
            token,
            owner,
            address(betLockBox),
            amount
        );
        if (!success) {
            revert TokenPaymentFailed({
                better: owner,
                token: token,
                amount: amount
            });
        }

        //Now match, persist, and token-lock the bet
        (bytes32 betHash, uint256 pot) = betHistory.createBet(bet);
        emit BetWagered(bet.owner, bet.betToken, pot, bet, betHash);
    }

