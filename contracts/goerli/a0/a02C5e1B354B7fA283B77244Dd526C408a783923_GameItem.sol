  string public LICENSE_URI;

  // Flag indicating if item is remixable
  bool public REMIXABLE;

  // URI pointing to a resource item
  string public RESOURCE_URI;

  // URI pointing to the document containing the constraints on how/where this item can be applied
  string public USAGE_CONSTRAINTS_URI;

  // Roaylty percentage. Default is 3%. Can be updated by admin or owner later.
  uint256 public royaltyPercentage = 0.03 ether; // 3%

  // Max Supply of token
  uint256 public MAX_SUPPLY;

  // token id => token status
  mapping(uint256 => Status) public tokenStatus;

  // token id => token storage
  mapping(uint256 => GameItemStorage) public gameItemStorage;

  IRoyaltyOverseer public royaltyOverseer;

  // checks if a token exits, revert if not.
  modifier tokenExists(uint256 _tokenId) {
    if (!_exists(_tokenId)) {
      revert TokenDoesNotExist();
    }
    _;
  }

  // check if the caller is an admin or owner.
  modifier onlyOwnerOrAdmin() {
    if (
      !(hasRole(DEFAULT_ADMIN_ROLE, msg.sender) ||
        hasRole(ADMINISTRATOR_ROLE, msg.sender) ||
        msg.sender == contractOwner)
    ) {
      revert Unauthorized(msg.sender);
    }
    _;
  }

  constructor(
    GameItemConstructorParams memory _params,
    IRoyaltyOverseer _royaltyOverseer
  ) ERC721(_params._name, _params._symbol) {
    if (
      _params._ownerAddress == address(0) || _params._adminAddress == address(0)
    ) {
      revert InvalidAddress();
    }

    _setupRole(DEFAULT_ADMIN_ROLE, _params._ownerAddress);
    _setupRole(ADMINISTRATOR_ROLE, _params._adminAddress);
    _setRoleAdmin(ADMINISTRATOR_ROLE, DEFAULT_ADMIN_ROLE);
    _setRoleAdmin(MINTER_ROLE, ADMINISTRATOR_ROLE);
    contractOwner = _params._ownerAddress;

    // set contract level storage
    TYPE = _params._type;
    LICENSE = _params._license;
    LICENSE_URI = _params._licenseURI;
    RESOURCE_URI = _params._resourceURI;
    USAGE_CONSTRAINTS_URI = _params._usageConstraintsURI;
    MAX_SUPPLY = _params._maxSupply;
    REMIXABLE = _params._remixable;
    tokenURIBase = _params._tokenURI;

    // set royalty overseer if specified
    if (address(_royaltyOverseer) != address(0)) {
      royaltyOverseer = _royaltyOverseer;

      // Give overseer the minter role
      _setupRole(MINTER_ROLE, address(royaltyOverseer));
    }
  }

  function mint(
    address _to,
    address _origin,
    string calldata _id
  ) public override returns (uint256 _tokenId) {
    if (
      !(hasRole(MINTER_ROLE, msg.sender) ||
        hasRole(ADMINISTRATOR_ROLE, msg.sender) ||
        hasRole(DEFAULT_ADMIN_ROLE, msg.sender))
    ) {
      revert Unauthorized(msg.sender);
    }

    uint256 currentTokenId = _tokenIds.current() + 1;

    // Skip max supply check if max supply is 0, which is used to indicate infinite supply
    if (currentTokenId > MAX_SUPPLY && MAX_SUPPLY != 0) {
      revert SupplyExhausted();
    }

    // increment properly after supply check
    _tokenIds.increment();

    gameItemStorage[currentTokenId] = GameItemStorage({
      creator: _to,
      origin: _origin,
      id: _id,
      mintDate: block.timestamp
    });

    _safeMint(_to, currentTokenId);
    _tokenId = currentTokenId;
  }

  // ------------ CONTRACT MANAGEMENT ------------
  function updateTokenURI(string calldata _tokenURI)
    public
    override
    onlyOwnerOrAdmin
  {
    tokenURIBase = _tokenURI;
  }

  function setType(string calldata _type) external override onlyOwnerOrAdmin {
    TYPE = _type;
  }

  /// @notice For a given tokenId, updates that tokens status
  function updateTokenStatus(uint256 _tokenId, Status _status)
    public
    override
    tokenExists(_tokenId)
    onlyOwnerOrAdmin
  {
    emit TokenStatusUpdated(_tokenId, _status);
    if (_status == Status.ACTIVE) {
      delete tokenStatus[_tokenId];
    } else {
      tokenStatus[_tokenId] = _status;
    }
  }

  // Update name of the license. Access limited to owner.
  function updateLicense(string calldata _license)
    external
    override
    onlyOwnerOrAdmin
  {
    LICENSE = _license;
  }

  // Update the license uri. Access limited to owner.
  function updateLicenseURI(string calldata _licenseURI)
    external
    override
    onlyOwnerOrAdmin
  {
    LICENSE_URI = _licenseURI;
  }

  // Updated remixable flag. Must be public for use by other protocols.
  function updateRemixable(bool _flag) public override onlyOwnerOrAdmin {
    REMIXABLE = _flag;
  }

  // Update resource URI
  function updateResourceURI(string calldata _resourceUri)
    external
    override
    onlyOwnerOrAdmin
  {
    RESOURCE_URI = _resourceUri;
  }

  // Update usage constraints uri
  function updateUsageConstraintsURI(string calldata _usageConstraintsURI)
    external
    override
    onlyOwnerOrAdmin
  {
    USAGE_CONSTRAINTS_URI = _usageConstraintsURI;
  }

  // Update royalty overseer
  function updateRoyaltyOverseer(address _royaltyOverseer)
    external
    override
    onlyOwnerOrAdmin
  {
    if (_royaltyOverseer == address(0)) {
      revert InvalidAddress();
    }
    // Remove minter role from old overseer
    address oldRoyaltyOverseer = address(royaltyOverseer);
    if (oldRoyaltyOverseer != address(0)) {
      _revokeRole(MINTER_ROLE, oldRoyaltyOverseer);
    }

    // Update royalty overseer
    royaltyOverseer = IRoyaltyOverseer(_royaltyOverseer);

    // Give new overseer the minter role
    _setupRole(MINTER_ROLE, _royaltyOverseer);
  }

  // Viweable function that generates the token uri
  function tokenURI(uint256 _tokenId)
    public
    view
    virtual
    override(ERC721, IGameItem)
    tokenExists(_tokenId)
    returns (string memory)
  {
    return string(abi.encodePacked(tokenURIBase, Strings.toString(_tokenId)));
  }

  /// @notice a mock implementation of ownership as most exchanges use this as proof of ownership
  // This function is more like `updateOwner` of the contract rather than transferring ownership.
  function transferOwnership(address _newOwner)
    public
    virtual
    override
    onlyRole(DEFAULT_ADMIN_ROLE)
  {
    if (_newOwner == address(0)) {
      revert InvalidAddress();
    }

    address oldOwner = contractOwner;
    contractOwner = _newOwner;

    emit OwnershipTransferred(oldOwner, _newOwner);
  }

  function setRoyaltyPercentage(uint256 _percentage) public onlyOwnerOrAdmin {
    royaltyPercentage = _percentage;
  }

  // -----------------Getters--------------------
  function getTokenStatus(uint256 _tokenId)
    public
    view
    override
    tokenExists(_tokenId)
    returns (Status _status)
  {
    _status = tokenStatus[_tokenId];
  }

  /// @notice a mock implementation of ownership as most exchanges use this as proof of ownership
  function owner() public view returns (address) {
    return contractOwner;
  }

  // ------------ERC2981 Implementation------------------
  function royaltyInfo(uint256 _tokenId, uint256 _salePrice)
    external
    view
    returns (address receiver, uint256 royaltyAmount)
  {
    royaltyAmount = _salePrice.mul(royaltyPercentage).div(1e18);

    if (address(royaltyOverseer) == address(0)) {
      receiver = contractOwner;
    } else {
      receiver = royaltyOverseer.getRoyaltyAddress(_tokenId, address(this));
    }
  }

  function supportsInterface(bytes4 _interfaceId)
    public
    view
    virtual
    override(AccessControl, ERC721)
    returns (bool)
  {
    return super.supportsInterface(_interfaceId);
  }
}

