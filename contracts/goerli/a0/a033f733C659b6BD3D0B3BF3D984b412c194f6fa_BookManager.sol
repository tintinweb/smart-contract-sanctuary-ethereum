// File contracts/BookManager.sol


pragma solidity ^0.8.0;
contract BookManager is Ownable, ReentrancyGuard, AccessControl {
    using SafeERC20 for IBLP;
    using SafeERC20 for IERC20;

    enum BetStatus {
        Pedding,
        Confirmed,
        Closed
    }

    struct Event {
        bytes32 id;
        bool allowBetting;
        uint256 fee;
    }

    struct EventAlloc {
        bytes32 id;
        uint256 alloc;
    }

    struct Bet {
        bytes32 betId;
        address addr;
        uint256 amount;
        uint256 odds;
        bytes32 eventId;
        bytes32 option;
        uint256 fee;
        uint256 placeTime;
        BetStatus status;
    }
    /*============================= VARIABLES ============================ */
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant POOL_MGR_ROLE = keccak256("POOL_MGR_ROLE");

    IBLP public blp;
    IERC20 public usdc;
    IEventFeeds public oracle;

    uint256 public constant PRICE_PRECISION = 1e18;
    uint256 public constant PRECISION = 1e6;
    uint256 public constant MISSING_DECIMALS = 12;

    mapping(bytes32 => Bet) private bets;
    mapping(bytes32 => Bet[]) private eventBets;
    mapping(address => bool) private whitelist;
    mapping(bytes32 => Event) private events;
    mapping(bytes32 => bool) private betOptions;
    mapping(bytes32 => uint256) public groupEvents;
    mapping(bytes32 => mapping(bytes32 => uint256)) public groupEventAlloc;
    bytes32 public constant BET_1 = keccak256("BET_1");
    bytes32 public constant BET_X = keccak256("BET_X");
    bytes32 public constant BET_2 = keccak256("BET_2");
    uint256 public defaultBettingFee = 5000;
    address public devFund;

    /*============================= CONTRUCTOR ============================ */
    constructor(
        address _blp,
        address _usdc,
        address _devFund
    ) {
        require(
            _blp != address(0),
            "BookManager::constructor: Invalid blp address"
        );
        require(
            _usdc != address(0),
            "BookManager::constructor: Invalid _usdc address"
        );
        require(
            _usdc != address(0),
            "BookManager::constructor: Invalid _devFund address"
        );
        devFund = _devFund;
        blp = IBLP(_blp);
        usdc = IBLP(_usdc);
        _setupRole(ADMIN_ROLE, _msgSender());
        _setupRole(POOL_MGR_ROLE, _msgSender());
        betOptions[keccak256("BET_1")] = true;
        betOptions[keccak256("BET_X")] = true;
        betOptions[keccak256("BET_2")] = true;
    }

    /*============================= VIEWS ============================ */

    function calculateBlpPrice() public view returns (uint256 _price) {
        uint256 usdcBalance = usdc.balanceOf(address(this));
        uint256 blpTotalSuply = blp.totalSupply();
        if (blpTotalSuply == 0) {
            _price = 1e6;
        } else {
            _price = (usdcBalance * PRICE_PRECISION) / blpTotalSuply;
        }
    }

    function calculateMint(uint256 _usdcIn)
        public
        view
        returns (uint256 _blpOut)
    {
        uint256 _blpPrice = calculateBlpPrice();
        require(
            _blpPrice > 0,
            "BookManager::calculateMint: usdc price invalid"
        );
        _blpOut = (_usdcIn * 10**MISSING_DECIMALS * _blpPrice) / PRECISION;
    }

    function calculateRedeem(uint256 _blpIn)
        public
        view
        returns (uint256 _usdcOut)
    {
        uint256 blpPrice = calculateBlpPrice();
        _usdcOut = (_blpIn * blpPrice) / PRICE_PRECISION;
    }

    /*============================= MULTATIVE ============================ */
    function setDefaultBettingFee(uint256 _fee) external onlyOwner {
        defaultBettingFee = _fee;
        emit BettingFeeUpdated(_fee);
    }

    function addAdmin(address _addr) external onlyOwner {
        _setupRole(ADMIN_ROLE, _addr);
    }

    function removeAdmin(address _addr) external onlyOwner {
        _revokeRole(ADMIN_ROLE, _addr);
    }

    function addPoolMgr(address _addr) external onlyOwner {
        _setupRole(POOL_MGR_ROLE, _addr);
    }

    function removePoolMgr(address _addr) external onlyOwner {
        _revokeRole(POOL_MGR_ROLE, _addr);
    }

    function setEventOracle(IEventFeeds _oracle) external onlyOwner {
        oracle = _oracle;
        emit EventOracleUpdated(address(_oracle));
    }

    function mint(uint256 _usdcIn, uint256 _minOut) external nonReentrant {
        require(_usdcIn > 0, "BookManager::mint: _usdc > 0");
        usdc.safeTransferFrom(msg.sender, address(this), _usdcIn);
        uint256 _blpOut = calculateMint(_usdcIn);
        require(_blpOut >= _minOut, "Slippage!!");
        blp.mint(msg.sender, _blpOut);
        emit Minted(msg.sender, _usdcIn, _blpOut);
    }

    function redeem(uint256 _blpIn, uint256 _minOut) external nonReentrant {
        require(_blpIn > 0, "BookManager::redeem: _blpIn > 0");
        blp.burnFrom(msg.sender, _blpIn);
        uint256 _usdcOut = calculateRedeem(_blpIn);
        require(_usdcOut >= _minOut, "Slippage!!");
        usdc.transfer(msg.sender, _usdcOut);
        emit Redeemed(msg.sender, _blpIn, _usdcOut);
    }

    function addEvent(bytes32 _id, uint256 _fee)
        external
        onlyRole(ADMIN_ROLE)
        nonReentrant
    {
        console.log("_id");
        IEventFeeds.Event memory _oracleEvent = oracle.getEvent(_id);
        console.log("event");
        require(_oracleEvent.id.length > 0, "Event not found");
        Event memory _event = Event({id: _id, allowBetting: true, fee: _fee});
        events[_id] = _event;
        emit EventAdded(_id);
    }

    function addGroupEvent(
        bytes32 _id,
        uint256 _maxAlloc,
        EventAlloc[] calldata _events
    ) external {
        for (uint i = 0; i < _events.length; i++) {
            groupEventAlloc[_id][_events[i].id] = _events[i].alloc;
        }
        groupEvents[_id] = _maxAlloc;
    }

    function updateAllocToGroupEvent(bytes32 _id, uint256 _maxAlloc) external {
        groupEvents[_id] = _maxAlloc;
    }

    function allowBettingEvent(bytes32 _id)
        external
        onlyRole(ADMIN_ROLE)
        nonReentrant
    {
        Event storage _event = events[_id];
        require(_event.id.length > 0, "Event not found!");
        _event.allowBetting = true;
        emit EventBettingAllowed(_id);
    }

    function stopBettingEvent(bytes32 _id)
        external
        onlyRole(ADMIN_ROLE)
        nonReentrant
    {
        Event storage _event = events[_id];
        require(_event.id.length > 0, "Event not found!");
        _event.allowBetting = false;
        emit EventBettingStoped(_id);
    }

    function placeBet(
        bytes32 _randomSecret,
        bytes32 _eventId,
        bytes32 _option,
        uint256 _usdcIn,
        uint256 _minOdds
    ) external nonReentrant {
        Event storage _event = events[_eventId];
        require(_event.allowBetting == true, "Event betting stoped");
        uint256 _fee = 0;
        if (_event.fee > 0) {
            _fee = (_usdcIn * _event.fee) / PRECISION;
        } else if (defaultBettingFee > 0) {
            _fee = (_usdcIn * defaultBettingFee) / PRECISION;
        }

        uint256 _amount = _usdcIn - _fee;
        if (_fee > 0) {
            usdc.safeTransferFrom(msg.sender, devFund, _fee);
        }
        usdc.safeTransferFrom(msg.sender, address(this), _amount);
        bytes32 _betId = keyOf(
            msg.sender,
            _eventId,
            _option,
            _usdcIn,
            block.timestamp,
            _randomSecret
        );
        uint256 _oracleOdds = oracle.getOdds(_eventId, _option);
        require(_oracleOdds >= _minOdds, "Slippage!");
        Bet memory _bet = Bet({
            betId: _betId,
            addr: msg.sender,
            amount: _amount,
            odds: _oracleOdds,
            eventId: _eventId,
            option: _option,
            status: BetStatus.Pedding,
            placeTime: block.timestamp,
            fee: _fee
        });
        bets[_betId] = _bet;
        emit PlacedBet(
            _betId,
            _eventId,
            _option,
            _amount,
            msg.sender,
            block.timestamp,
            _oracleOdds,
            _fee
        );
    }

    /*============================= INTERNAL ============================ */

    function keyOf(
        address _owner,
        bytes32 _eventId,
        bytes32 _option,
        uint256 _amount,
        uint256 _placeTime,
        bytes32 _randomSecret
    ) public pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    _owner,
                    _eventId,
                    _option,
                    _amount,
                    _placeTime,
                    _randomSecret
                )
            );
    }

    /*============================= EVENTS ============================ */
    event Minted(address reviecer, uint256 usdcIn, uint256 blpOut);
    event Redeemed(address reviecer, uint256 blpIn, uint256 usdcOut);
    event AddWhitelist(address addr);
    event RemoveWhitelist(address addr);
    event EventAdded(bytes32 id);
    event PlacedBet(
        bytes32 betId,
        bytes32 eventId,
        bytes32 optionId,
        uint256 amount,
        address owner,
        uint256 placeTime,
        uint256 odds,
        uint256 fee
    );
    event EventOracleUpdated(address oracle);
    event EventBettingAllowed(bytes32 id);
    event EventBettingStoped(bytes32 id);
    event BettingFeeUpdated(uint256 _fee);
}