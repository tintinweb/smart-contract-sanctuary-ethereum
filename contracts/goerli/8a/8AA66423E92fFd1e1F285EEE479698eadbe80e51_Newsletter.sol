pragma solidity >= 0.8.17;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@manifoldxyz/royalty-registry-solidity/contracts/specs/IEIP2981.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "operator-filter-registry/src/DefaultOperatorFilterer.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./Newsletter.sol";

contract IMPAKT is ERC1155, DefaultOperatorFilterer {
    
    address payable private royalties_recipient;
    address public newsletterAddress;
    address public ashAddress;
    address public constant iseult = 0x337fc30e77431580e065c35e61B73395436d46dc;
    address public constant smartcontrart = 0x709E6c5abc1157986a3a9A236e1c94BcA9e678c2;

    string [] uriComponents;
    string public contractName = "IMPAKT";

    uint256 public pixelChangePrice = 1*10**18;
    uint256 private nonce;
    uint256 private ashBalance;
    uint256 private royaltyAmount; //in % 

    string public firstEditionColors;

    struct Edition{
        string title;
        uint256 quantity;
        bytes32 editionHash;
        uint256 charCountSqr;
        string date;
    }

    mapping(uint256 => Edition) public editions;
    mapping(address => bool) public isAdmin;

    error InvalidAdmin();
    error InvalidOwner();
    
    constructor () ERC1155("") {
        uriComponents = [
                'data:application/json;utf8,{"name":"',
                '", "description":"',
                '", "created_by":"Iseult.eth", "image":"data:image/svg+xml;utf8,',
                '", "attributes":[',
                ']}'];
        isAdmin[msg.sender] = true;
        royalties_recipient = payable(msg.sender);
        royaltyAmount = 10;
    } 

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC1155)
        returns (bool)
    {
        return
        ERC1155.supportsInterface(interfaceId) ||
        interfaceId == type(IEIP2981).interfaceId ||
        super.supportsInterface(interfaceId);
    }

    modifier adminRequired(){
        if(!isAdmin[msg.sender]) revert InvalidAdmin();
        _;
    }

    function name() public view returns (string memory) {
        return contractName;
    }

    function mint( 
        address _to,
        uint256 _id,
        uint256 _amount
    ) external adminRequired{
        _mint(_to, _id, _amount, "0x0");
    }

    function mintBatch(
        address _to,
        uint256[] memory _ids,
        uint256[] memory _amounts
    )external adminRequired{
        _mintBatch(_to, _ids, _amounts, "0x0");
    }

    function toggleAdmin(address _admin)external adminRequired{
        isAdmin[_admin] = !isAdmin[_admin];
    }

   function burn(uint256 _tokenId, uint256 _quantity) public {
        _burn(msg.sender, _tokenId, _quantity);
    }

    function burnBatch(
        uint256[] memory _ids,
        uint256[] memory _amounts
    )external{
        _burnBatch(msg.sender, _ids, _amounts);
    }

    function setEdition(
        uint256 _tokenId,
        string calldata _title,
        string calldata _text,
        uint256 _editionQuantity,
        string calldata _date
    )external adminRequired{
        Edition memory _edition;
        _edition.title = _title;
        _edition.charCountSqr = Math.max(200, Math.sqrt(bytes(_text).length)*3);
        _edition.editionHash = keccak256(abi.encodePacked(_text));
        _edition.quantity = _editionQuantity;
        _edition.date = _date;
        editions[_tokenId] = _edition;
    }

    function returnColors() external view returns(string memory){
        return firstEditionColors;
    }

    function setNewsletterAddress(address _newsletterAddress) external adminRequired {
        newsletterAddress = _newsletterAddress;
    }

    function setAshAddress(address _ashAddress) external adminRequired {
        ashAddress = _ashAddress;
    }

    function setFirstEditionColors(string calldata _firstEditionColors) external adminRequired{
        firstEditionColors = _firstEditionColors;
    }

    function changeColorAtIndex(uint256 index) internal {
        bytes memory bStr = bytes(firstEditionColors);
        bytes memory hexChars = "123456789abcdef";
        uint256 rand = uint256(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce)));
        for (uint256 i = index; i < index + 6; i++) {
            uint256 randIndex = rand % hexChars.length;
            bStr[i] = hexChars[randIndex];
            rand /= hexChars.length;
        }
        nonce ++;
        firstEditionColors = string(bStr);
    }

    function changeColors(uint256 numOfColors) external {
        uint256 price = pixelChangePrice * numOfColors;
        IERC20(ashAddress).transferFrom(msg.sender, address(this), price);
        ashBalance += price;
        for(uint256 i = 0; i < numOfColors; i++){
            changeColorAtIndex(i*6);
        }
    }

    function setPixelChangePrice(uint256 _price) external{
        if(IERC1155(address(this)).balanceOf(msg.sender, 0) < 1) revert InvalidOwner();
        pixelChangePrice = _price;
    }

    function uri(uint256 _tokenId) public view virtual override returns (string memory) {
        string memory _title;
        string memory _description;
        string memory _image;
        string memory _attributes;

        if(_tokenId == 0){
            _image = string(Newsletter(newsletterAddress).generateSpecialEdition(
                firstEditionColors
            ));
        }else{
            _image = string(Newsletter(newsletterAddress).generateEdition(
                _tokenId,
                editions[_tokenId].charCountSqr,
                editions[_tokenId].editionHash
            ));
        }
        Edition memory _edition = editions[_tokenId];
        _title = _edition.title;
        _description = '';
        _attributes = string(abi.encodePacked(
            '{"trait_type": "Edition of", "value": "',
            Strings.toString(_edition.quantity),
            '"},{"trait_type": "Edition date", "value": "',
            _edition.date,
            '"}'));
        bytes memory _byteString = abi.encodePacked(
            abi.encodePacked(uriComponents[0], _title),
            abi.encodePacked(uriComponents[1], _description),
            abi.encodePacked(uriComponents[2], _image),
            abi.encodePacked(uriComponents[3], _attributes),
            abi.encodePacked(uriComponents[4])
        );
        return string(_byteString);
    }

    // OS Operator Filterer Updates

    function setApprovalForAll(address operator, bool approved) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, uint256 amount, bytes memory data)
        public
        override
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId, amount, data);
    }

    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public virtual override onlyAllowedOperator(from) {
        super.safeBatchTransferFrom(from, to, ids, amounts, data);
    }

    // Royalites mgmt

    function setRoyalties(address payable _recipient, uint256 _royaltyPerCent) external adminRequired {
        royalties_recipient = _recipient;
        royaltyAmount = _royaltyPerCent;
    }

    function royaltyInfo(uint256 _salePrice) external view returns (address, uint256) {
        if(royalties_recipient != address(0)){
            return (royalties_recipient, (_salePrice * royaltyAmount) / 100 );
        }
        return (address(0), 0);
    }

    function withdraw(address _recipient) external adminRequired {
        payable(_recipient).transfer(address(this).balance);
    }

    function withdrawAsh(address _token0Owner) external adminRequired {
        if(IERC1155(address(this)).balanceOf(_token0Owner, 0) < 1) revert InvalidOwner();
        IERC20(ashAddress).transferFrom(address(this), iseult, ashBalance/3);
        IERC20(ashAddress).transferFrom(address(this), smartcontrart, ashBalance/3);
        IERC20(ashAddress).transferFrom(address(this), _token0Owner, ashBalance/3);
        ashBalance = 0;
    }

}

