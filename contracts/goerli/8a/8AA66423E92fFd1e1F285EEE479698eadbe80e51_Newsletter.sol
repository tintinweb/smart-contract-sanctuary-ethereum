pragma solidity >= 0.8.17;

import "@openzeppelin/contracts/utils/Strings.sol";

contract Newsletter{

    function bytes32ToLiteralString(bytes32 data) 
        public
        pure
        returns (string memory result) 
    {
        bytes memory temp = new bytes(65);
        uint256 count;

        for (uint256 i = 0; i < 32; i++) {
            bytes1 currentByte = bytes1(data << (i * 8));
            
            uint8 c1 = uint8(
                bytes1((currentByte << 4) >> 4)
            );
            
            uint8 c2 = uint8(
                bytes1((currentByte >> 4))
            );
        
            if (c2 >= 0 && c2 <= 9) temp[++count] = bytes1(c2 + 48);
            else temp[++count] = bytes1(c2 + 87);
            
            if (c1 >= 0 && c1 <= 9) temp[++count] = bytes1(c1 + 48);
            else temp[++count] = bytes1(c1 + 87);
        }
        
        result = string(temp);
    }

    function getColors(string memory colors, uint256 i) public pure returns (bytes memory) {
        bytes memory colorsAsBytes = bytes(colors);
        bytes memory result = new bytes(6);
        for (uint j = 0; j < 6; j++) {
            result[j] = colorsAsBytes[i + j];
        }
        return result;
    }

    function generateSVGHeader(uint256 height)internal pure returns (bytes memory){
        return abi.encodePacked(
          "<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 256 ",
          Strings.toString(height + 10),
          "' width='512' height='512'><defs><linearGradient id='gradient'><stop offset='0%' stop-color='#FFC300'><animate attributeName='stop-color' values='#9daa7e; #FF5733; #9daa7e; #daa7ec' dur='2s' repeatCount='indefinite' /></stop><stop offset='100%' stop-color='#581845'><animate attributeName='stop-color' values='#9daa7e; #9daa7e; #19daa7; #daa7ec' dur='2s' repeatCount='indefinite' /></stop></linearGradient></defs>"
        );
    }

    function generateSVGBase(uint256 tokenId, uint256 height) internal pure returns (bytes memory){
        return abi.encodePacked(
            "<filter id='shadow'><feDropShadow dx='5' dy='5' stdDeviation='5' flood-color='#333'/></filter><rect x='10' y='10' width='180' height='",
            Strings.toString(height -10),
            "' style='stroke: rgb(0, 0, 0); fill: #f5f5f5; filter: url(#shadow);'/><text style='font-family: Arial, sans-serif; font-size: 12px; font-weight: 700; text-anchor: middle; white-space: pre;' x='100' y='30'> IMPAKT VOL ",
            Strings.toString(tokenId),
            "</text><text style='font-family: Arial, sans-serif; font-size: 8px; font-weight: 700; text-anchor: middle; white-space: pre;' x='150' y='",
            Strings.toString(height -20),
            "'>Iseult.eth</text>"
        );
    }

    function generateSVGLines(uint256 height, uint8 i, bytes memory color1, bytes memory color2) public pure returns(bytes memory){
        return abi.encodePacked(
                "<line x1='30' y1='",
                Strings.toString((height-70)*i/7 + 60),
                "' x2='170' y2='",
                Strings.toString((height-70)*i/7 + 60),
                "' style='stroke-width:3'><animate attributeName='stroke' values='",
                abi.encodePacked(
                    '#',
                    color1,
                    ';#',
                    color2,
                    ';#',
                    color1
                ),
                "' dur='1s' repeatCount='indefinite' /></line>"
            );
    }

    function generateEdition(
        uint256 tokenId,
        uint256 height,
        bytes32 newsletterHash
    ) public pure returns(bytes memory){
        bytes memory svg = generateSVGHeader(height);
        string memory newsletterHashAsString = bytes32ToLiteralString(newsletterHash);
        svg = abi.encodePacked(svg, generateSVGBase(tokenId, height));
        for(uint8 i = 0; i <= 6; i++){
            bytes memory color1 = getColors(newsletterHashAsString, i);
            bytes memory color2;
            if(i == 6){
                color2 = getColors(newsletterHashAsString, 0);
            }else{
                color2 = getColors(newsletterHashAsString, i + 1);
            }
            svg = abi.encodePacked(svg, generateSVGLines(height, i, color1, color2));
        }
        return abi.encodePacked(svg, "<path stroke-width='0.3' stroke='black' fill='url(#gradient)' d='M30.25 27.6q3.7-4.8 10.85-9.25l1.25 2.1-.8-2.5q2-1.65 4.95-2.6l1.9 3.5-1-3.85q1.45-.45 3.15-.75Q58.3 13 61.4 15q-4.85 5.55-15.8 8.95l-1.45-1.9.85 2.1q-4.45 2.4-9.1 3.45l-.85-2.05.3 2.4q-2 .45-4.6.2-3.85 2.45-5.85 4.7l-3.45 1.85q5.35-4.9 8.8-7.1 8.95-5.4 18.15-8.75 6.45-2.5 13-3.85-16.25 5.3-26.35 10.55-2.4 1.35-4.3 2.6' id='a'> <path stroke-width='0.5' id='g' fill='url(#gradient)' stroke='black' d='M 19.45 274.75Q 22.6 274.9 19.65 270.25 16.65 274.65 19.45 274.75 Z'/><animate id='trace' attributeName='stroke-dasharray' from='0,500' to='500,0' dur='5s' repeatCount='indefinite' /></path><use href='#g' stroke='black'><animate attributeName='y' stroke='#FA9E72' fill='ur' values='-240;200' begin='5s' dur='5s' repeatCount='indefinite'/></use></svg>");
    }

    function renderPixel(uint256 x, uint256 y, uint256 n, string calldata colors) internal pure returns(bytes memory){
        return (
            abi.encodePacked(
                "<rect x='",
                Strings.toString((2 * x) - 1),
                "' y='",
                Strings.toString((2 * y) - 1),
                "' width='1' height='1' style='fill: #",
                getColors(colors, (6*(y-1)+6*(n-1)*(x-1))),
                "'/>"
            )
        );
    }

    function generateSpecialEdition(string calldata colors) public pure returns(bytes memory){
        bytes memory svg = "<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 21 21' width='1080' height='1080'>";
        uint8 size = 11;
        for (uint8 i=1; i < size; i++ ){
            for(uint8 j=1; j < size; j++){
                svg = abi.encodePacked(
                    svg,
                    renderPixel(i, j, size, colors)
                );
            }
        }
        return abi.encodePacked(svg, "</svg>");
    }

}

