contract Sigverify is EIP712 {
    using ECDSA for bytes32;

    // eip-712 related
    struct TransferRequest {
        address recipient;
        uint256 nonce;
        uint256 deadline;
    }

    mapping(address => uint256) private _nonces;
    string private constant SIGNING_DOMAIN = "Nuxify-Sigverify";
    string private constant SIGNATURE_VERSION = "1";
    // typed data
    bytes32 private immutable _TRANSFER_REQUEST_TYPEHASH =
        keccak256(
            "TransferRequest(address recipient,uint256 nonce,uint256 deadline)"
        );

    constructor() EIP712(SIGNING_DOMAIN, SIGNATURE_VERSION) {}

    error DeadlineExpired();
    error InvalidMessage();
    error InvalidSignature();
    error UnauthorizedAccess();

    // WRITE FUNCTIONS

    /**
     * Verify EIP712 signature
     * @param _recipient recipient address
     * @param _deadline expiration in seconds
     * @param _signature signed message
     */
    function verifyEIP712Signature(
        address _signer, // this should be from msg.sender or controller account
        address _recipient,
        uint256 _deadline,
        bytes calldata _signature
    ) external {
        if (block.timestamp >= _deadline) revert DeadlineExpired();

        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    _TRANSFER_REQUEST_TYPEHASH,
                    _recipient,
                    _nonces[_signer],
                    _deadline
                )
            )
        );

        console.log(_recipient, _nonces[_signer], _deadline);

        address recoveredSigner = ECDSA.recover(digest, _signature);
        console.log(recoveredSigner, _signer);
        if (recoveredSigner != _signer) revert UnauthorizedAccess();

        // increment nonce so it can't be reused anymore even if deadline is not reached
        _nonces[_signer]++;
    }

    // READ FUNCTIONS

    /**
     * Verify EIP191 signedm message
     * @param _message original message
     * @param _signature signature of the message
     */
    function verifyEIP191Signature(
        address _signerAddress, // this should be msg.sender or controller account
        string calldata _message,
        bytes calldata _signature
    ) external pure returns (bool) {
        if (bytes(_message).length == 0) revert InvalidMessage();
        if (_signature.length == 0) revert InvalidSignature();

        bytes32 prefixedMessage = ECDSA.toEthSignedMessageHash(bytes(_message));
        return
            isEIP191SignatureValid(_signerAddress, prefixedMessage, _signature);
    }

    /**
     * Get nonce by signer address
     * @param _signer signer address
     */
    function nonces(address _signer) public view returns (uint256) {
        return _nonces[_signer];
    }

    // HELPER FUNCTIONS

    function isEIP191SignatureValid(
        address _signerAddress,
        bytes32 _hash,
        bytes calldata _signature
    ) internal pure returns (bool) {
        return ECDSA.recover(_hash, _signature) == _signerAddress;
    }
}

