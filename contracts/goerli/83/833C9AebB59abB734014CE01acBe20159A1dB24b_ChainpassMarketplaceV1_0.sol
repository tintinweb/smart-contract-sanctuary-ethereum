pragma solidity ^0.8.0;

import "./tokens/ERC721.sol";
import "./interfaces/ChainpassInterfaces.sol";
import "./interfaces/IERC2981Royalties.sol";
import "./types/ChainpassTypes.sol";

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "hardhat/console.sol";

contract ChainpassMarketplaceV1_0 is Ownable, ReentrancyGuard {
    using Strings for uint256;

    event AsksSet(address ticketContractAddress, uint256[] tokenIds, uint256 price);
    event TicketsBought(address ticketContractAddress, uint256[] tokenIds);

    address public chainpassGlobalRegistry;
    mapping(address => mapping(uint256 => Ask)) ticketAsks;

    constructor(address  _chainpassGlobalRegistry) {
        chainpassGlobalRegistry = _chainpassGlobalRegistry;
    }

    function getAsk(address ticketContractAddress, uint256 tokenId) external view returns (Ask memory) {
        return ticketAsks[ticketContractAddress][tokenId];
    }

    function setAsk(address ticketContractAddress, uint256 tokenId, uint256 price) external {
        IChainpassEventTickets ticketContract = IChainpassEventTickets(ticketContractAddress);

        require(
            address(ticketContract) == msg.sender || ticketContract.ownerOf(tokenId) == msg.sender,
            "Only token owner or contract can set asks."
        );

        ticketAsks[ticketContractAddress][tokenId] = Ask(true, price);
    }

    function setAsks(address ticketContractAddress, uint256[] memory tokenIds, uint256 price) external {
        IChainpassEventTickets ticketContract = IChainpassEventTickets(ticketContractAddress);

        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            require(
                address(ticketContract) == msg.sender || ticketContract.ownerOf(tokenId) == msg.sender,
                "Only token owner or contract can set asks."
            );

            ticketAsks[ticketContractAddress][tokenId] = Ask(true, price);
        }

        emit AsksSet(ticketContractAddress, tokenIds, price);
    }

    function buyTickets(address ticketContractAddress, uint256[] memory tokenIds) external payable nonReentrant {
        require(tokenIds.length > 0, "There needs to be at least one tokenId");

        IChainpassEventTickets ticketContract = IChainpassEventTickets(ticketContractAddress);

        // sum up the total price and transfer each token
        uint256 totalPrice;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            Ask memory ask = ticketAsks[ticketContractAddress][tokenId];

            require(ask.exists, "Ticket is not for sale.");
            totalPrice += ask.price;

            ticketContract.transferFrom(ticketContract.ownerOf(tokenId), msg.sender, tokenId);
            ticketAsks[ticketContractAddress][tokenId] = Ask(false, 0);
        }

        // check that the buyer has enough usdc
        IChainpassGlobalRegistry cpGlobalRegistry = globalRegistry();
        IERC20 usdcContract = IERC20(cpGlobalRegistry.getUSDCContractAddress());
        require(usdcContract.balanceOf(msg.sender) >= totalPrice, "Insufficient USDC.");

        IChainpassEventRegistry cpEventRegistry = eventRegistry();

        uint256 royaltyAmount;
        (address royaltyReceiver, uint256 royaltyBPS) = ticketContract.getRoyaltyReceiverAndBPS();
        if (royaltyReceiver != address(0)) {
            // transfer royalties
            royaltyAmount = (totalPrice * royaltyBPS) / 10_000;
            usdcContract.transferFrom(msg.sender, royaltyReceiver, royaltyAmount);
        }

        // transfer CP cut
        uint256 chainpassCut = (totalPrice * cpEventRegistry.getChainpassPrimaryBPS(ticketContractAddress)) / 10_000;
        usdcContract.transferFrom(msg.sender, cpGlobalRegistry.getChainpassWallet(), chainpassCut);
    
        // transfer the rest to the event host
        uint256 eventHostCut = totalPrice - royaltyAmount - chainpassCut;
        usdcContract.transferFrom(msg.sender, ticketContract.owner(), eventHostCut);

        emit TicketsBought(ticketContractAddress, tokenIds);
    }

    function globalRegistry() private view returns (IChainpassGlobalRegistry) {
        return IChainpassGlobalRegistry(chainpassGlobalRegistry);
    }

    function eventRegistry() private view returns (IChainpassEventRegistry) {
        return IChainpassEventRegistry(globalRegistry().getChainpassEventRegistryContractAddress());
    }
}

