            // IF ERC721Enumerable supported
            if (supported[4]) {
                collectionInfos.totalSupply = IERC721Enumerable(collection).totalSupply();
            }

            if (account != address(0)) {
                collectionInfos.balanceOf = IERC721(collection).balanceOf(account);
            }
        }
    }
