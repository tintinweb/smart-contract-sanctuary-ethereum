/// @author Cameron Askew
contract SpaceRouter {

    // events
    event LiquidityAdded(address indexed provider, uint256 eth, uint256 spc);
    event LiquidityRemoved(address indexed provider, uint256 lp);
    event Swap(address indexed trader, bool ethToSpc, uint256 input, uint256 output);

    // errors
    error InsufficientETH(address account, uint256 amount);
    error InsufficientSPC(address account, uint256 amount);
    error InsufficientLP(address account, uint256 amount);
    error InvalidArgument();
    error InsufficientFundsReturned();

    // immutable
    SpaceLP private immutable spaceLP;
    SpaceCoin private immutable spaceCoin;

    constructor(SpaceLP _spaceLP, SpaceCoin _spaceCoin) {
        spaceLP = _spaceLP;
        spaceCoin = _spaceCoin;
     }

    /// @notice Provides ETH-SPC liquidity to LP contract
    /// @param spc The amount of SPC to be deposited
    function addLiquidity(uint256 spc) external payable {
        if (spc == 0) revert InvalidArgument();
        if (msg.value == 0) revert InvalidArgument();

        uint256 ethReserves = spaceLP.ethReserves();
        uint256 spcReserves = spaceLP.spcReserves();

        // note 1: the sender will need to first allow this amount of their SPC to be transferred from this contract
        // note 2: we don't know how much this coin taxes internally, so we just check the balance before and after
        // TODO: some sort of safe transfer? (like uniswap)
        uint256 prevBalance = spaceCoin.balanceOf(address(spaceLP));
        spaceCoin.transferFrom(msg.sender, address(spaceLP), spc);
        uint256 newBalance = spaceCoin.balanceOf(address(spaceLP));
        uint256 spcAfterTax = newBalance - prevBalance;

        if (ethReserves > 0 && spcReserves > 0) {
            // We already have some reserves, so let's try to maintain (roughly)
            // the same reserve ratio. There will very often be very minor excess
            // of one of the coins which will be effectively "donated" to the pool.
            uint256 newSpcReserves = spcReserves + spcAfterTax;
            uint256 minEth = ethReserves * newSpcReserves / spcReserves - ethReserves;
            if (msg.value < minEth) revert InsufficientETH(msg.sender, msg.value);

            uint256 newEthReserves = ethReserves + msg.value;
            uint256 minSpc = spcReserves * newEthReserves / ethReserves - spcReserves;
            if (spcAfterTax < minSpc) revert InsufficientSPC(msg.sender, spcAfterTax);
        }

        spaceLP.deposit{ value: msg.value }(msg.sender);
        emit LiquidityAdded(msg.sender, msg.value, spcAfterTax);
    }

    /// @notice Removes ETH-SPC liquidity from LP contract
    /// @param lpToken The amount of LP tokens being returned
    function removeLiquidity(uint256 lpToken) external {
        if (lpToken == 0) revert InvalidArgument();
        uint256 lpBalance = spaceLP.balanceOf(msg.sender);
        if (lpBalance < lpToken) revert InsufficientLP(msg.sender, lpBalance);

        // first we move LP tokens from the sender to the SpaceLP contract with the intention of burning them
        spaceLP.transferFrom(msg.sender, address(spaceLP), lpToken);

        // now we call withdraw on SpaceLP which can infer what to do based on how many LP tokens it has
        spaceLP.withdraw(msg.sender);
        emit LiquidityRemoved(msg.sender, lpToken);
    }

    /// @notice Swaps ETH for SPC in LP contract
    /// @param spcOutMin The minimum acceptable amout of SPC to be received
    function swapETHForSPC(uint256 spcOutMin) external payable {
        if (msg.value == 0) revert InvalidArgument();

        uint256 spcOut = spaceLP.swap{ value: msg.value }(msg.sender, true);

        // note that `spcOut` is the *before tax* amount meaning the user could get less than `spcOutMin` after tax
        if (spcOut < spcOutMin) revert InsufficientFundsReturned();
        emit Swap(msg.sender, true, msg.value, spcOut);
    }

    /// @notice Swaps SPC for ETH in LP contract
    /// @param spcIn The amount of inbound SPC to be swapped
    /// @param ethOutMin The minimum acceptable amount of ETH to be received
    function swapSPCForETH(uint256 spcIn, uint256 ethOutMin) external { 
        if (spcIn == 0) revert InvalidArgument();

        // first send SPC to the pool so it can internally calculate how much ETH to send
        // note that this strategy naturally handles SPC tax since SpaceLP will only see the after-tax amount
        spaceCoin.transferFrom(msg.sender, address(spaceLP), spcIn);

        uint ethOut = spaceLP.swap(msg.sender, false);
        if (ethOut < ethOutMin) revert InsufficientFundsReturned();
        emit Swap(msg.sender, false, spcIn, ethOut);
    }

    /// @notice returns estimated amount out based on the amount in. Does not account for taxes.
    function estimatedOut(bool ethToSpc, uint256 amountIn) public view returns (uint256) {
        if (ethToSpc) {
            return spaceLP.amountOut(amountIn * 99 / 100, spaceLP.ethReserves(), spaceLP.spcReserves());
        } else {
            return spaceLP.amountOut(amountIn * 99 / 100, spaceLP.spcReserves(), spaceLP.ethReserves());
        }
    }

}

