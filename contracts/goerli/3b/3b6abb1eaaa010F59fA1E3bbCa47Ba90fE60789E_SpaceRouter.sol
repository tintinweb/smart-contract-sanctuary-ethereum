import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./SpaceCoin.sol";

/// @author Cameron Askew
contract SpaceLP is ERC20 {

    // events
    event Deposit(address indexed provider, uint256 eth, uint256 spc, uint256 lp);
    event Withdrawal(address indexed provider, uint256 eth, uint256 spc, uint256 lp);
    event Swap(address indexed trader, bool ethToSpc, uint256 input, uint256 output);

    // errors
    error NoDeposit();
    error InvalidRatio(uint256 expectedRatio);
    error NoNewTokens();
    error FailedToSendEther(address account);
    error NoFunds();
    error Reentrancy();

    // immutable
    SpaceCoin private immutable spaceCoin;

    // state
    uint256 public ethReserves;
    uint256 public spcReserves;
    bool private locked;

    modifier noReentrancy() {
        if (locked) revert Reentrancy();
        locked = true;
        _;
        locked = false;
    }

    constructor(SpaceCoin _spaceCoin) ERC20("LP Coin", "LP") {
        spaceCoin = _spaceCoin;
    }

    /// @notice Adds ETH-SPC liquidity to LP contract
    /// @param to The address that will receive the LP tokens
    function deposit(address to) external payable {
        // note: this is payable because it's meant to accept ETH, but we don't actually read msg.value but instead
        //       just base calculations on our current ETH balance.
        (uint256 ethBalance, uint256 spcBalance) = _getBalances();
        uint256 ethDeposit = ethBalance - ethReserves;
        uint256 spcDeposit = spcBalance - spcReserves;

        if (ethDeposit == 0 || spcDeposit == 0) revert NoDeposit();
        
        uint256 tokensToMint;
        uint256 supply = totalSupply();
        if (supply == 0) {
            // When we don't already have any LP tokens, just mint the arithmetic mean of ETH + SPC deposits
            tokensToMint = (ethDeposit + spcDeposit) / 2;
        } else {
            // Calculate how many tokens to issue to this address, based off the delta of our ETH & SPC reserves
            // since the last txn.
            // There's no guarantee we're maintaining the current ratio of spc-eth because:
            // (1) Rounding issues: it's very prohibitive to require providers to deposit the exact ratio,
            //                      so there's very likely a small excess in one of the coins.
            // (2) External deposits: somebody could increase our balances without calling this function,
            //                        e.g. force-feeding us ETH or just transferring us SPC.
            // So we just mint LP tokens to this provider based on the lesser _value_ of the 2 deposits. Any minor
            // excess deposit for the other coin is effectively a donation to the pool.
            uint256 ethCalculation = ethDeposit * supply / ethReserves;
            uint256 spcCalculation = spcDeposit * supply / spcReserves;
            tokensToMint = _min(ethCalculation, spcCalculation);
        }

        if (tokensToMint == 0) revert NoNewTokens();

        _mint(to, tokensToMint);
        _updateReserves();
        emit Deposit(to, ethDeposit, spcDeposit, tokensToMint);
    }

    /// @notice Returns ETH-SPC liquidity to liquidity provider
    /// @param to The address that will receive the outbound token pair
    function withdraw(address to) external noReentrancy {
        uint256 tokensToBurn = balanceOf(address(this));
        (uint256 ethBalance, uint256 spcBalance) = _getBalances();
        uint256 supply = totalSupply();
        uint256 ethReturned = ethBalance * tokensToBurn / supply;
        uint256 spcReturned = spcBalance * tokensToBurn / supply;

        if (ethReturned == 0 && spcReturned == 0) revert NoFunds();

        _burn(address(this), tokensToBurn);
        spaceCoin.transfer(to, spcReturned);
        (bool success, ) = to.call{ value: ethReturned }("");
        if (!success) revert FailedToSendEther(to);

        _updateReserves();
        emit Withdrawal(to, ethReturned, spcReturned, tokensToBurn);
    }

    /// @notice Swaps ETH for SPC, or SPC for ETH
    /// @param to The address that will receive the outbound SPC or ETH
    /// @param isETHtoSPC Boolean indicating the direction of the trade
    function swap(address to, bool isETHtoSPC) external payable noReentrancy returns (uint256) {
        if (isETHtoSPC) return _swapETHForSPC(to);
        else return _swapSPCForETH(to);
    }

    function _swapETHForSPC(address to) private returns (uint256) {
        (uint256 ethBalance, ) = _getBalances();

        uint256 ethSent = ethBalance - ethReserves;
        uint256 valueAfterTax = ethSent * 99 / 100;
        if (valueAfterTax == 0) revert NoDeposit();
        uint256 spc = amountOut(valueAfterTax, ethReserves, spcReserves);
        spaceCoin.transfer(to, spc);

        _updateReserves();
        emit Swap(to, true, ethSent, spc);
        return spc;
    }

    function _swapSPCForETH(address to) private returns (uint256) {
        (, uint256 spcBalance) = _getBalances();

        uint256 spcSent = spcBalance - spcReserves;
        uint256 valueAfterTax = spcSent * 99 / 100;
        if (valueAfterTax == 0) revert NoDeposit();
        uint256 eth = amountOut(valueAfterTax, spcReserves, ethReserves);

        (bool success, ) = to.call{ value: eth }("");
        if (!success) revert FailedToSendEther(to);

        _updateReserves();
        emit Swap(to, false, spcSent, eth);
        return eth;
    }

    function _updateReserves() private {
        (uint256 ethBalance, uint256 spcBalance) = _getBalances();
        ethReserves = ethBalance;
        spcReserves = spcBalance;
    }

    function _getBalances() private view returns (uint256 _ethBalance, uint256 _spcBalance) {
        _ethBalance = address(this).balance;
        _spcBalance = spaceCoin.balanceOf(address(this));
    }

    function _min(uint256 a, uint256 b) private pure returns (uint256) {
        return a <= b ? a : b;
    }

    /// @notice calculates the amount to send out of a liquidity pool based on the amount being sent in and the
    ///         current reserves in order to maintain a constant product K (reserve1 * reserve2)
    function amountOut(uint256 amountIn, uint256 inReserves, uint256 outReserves) public pure returns (uint256) {
        return outReserves - (outReserves * inReserves / (inReserves + amountIn));
    }

}

