pragma solidity ^0.8.0;
pragma abicoder v2;

import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "./lib/LibTransfer.sol";
import "./lib/LibAsset.sol";
import "../token/erc721/interfaces/INFT721.sol";

abstract contract TransferExecutor {
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using LibTransfer for address;

    function _transferToken(
        LibAsset.Asset memory asset,
        address from,
        address to
    ) internal {
        if (asset.assetType.assetClass == LibAsset.ETH_ASSET_CLASS) {
            to._transferEth(asset.value);
        } else if (asset.assetType.assetClass == LibAsset.ERC20_ASSET_CLASS) {
            address token = abi.decode(asset.assetType.data, (address));
            SafeERC20Upgradeable.safeTransferFrom(
                IERC20Upgradeable(token),
                from,
                to,
                asset.value
            );
        } else {
            revert("asetClass is invalid");
        }
    }

    function _transferNFT(
        LibAsset.Asset memory asset,
        uint64 typeMint,
        address from,
        address to
    ) internal {
        if (asset.assetType.assetClass == LibAsset.ERC721_ASSET_CLASS) {
            require(asset.value == 1, "erc721 value error");
            IERC721Upgradeable(asset.token).safeTransferFrom(
                from,
                to,
                asset.tokenId,
                ""
            );
        } else if (asset.assetType.assetClass == LibAsset.ERC721_LAZY_ASSET_CLASS) {
            require(asset.value == 1, "erc721 value error");
            INFT721(asset.token).mint(from, to, typeMint);
        } else {
            revert("asetClass is invalid");
        }
    }
}

