pragma solidity ^0.8.0;

import "./LibMath.sol";
import "./LibAsset.sol";
import "./LibOrderData.sol";

library LibOrder {
    using SafeMathUpgradeable for uint256;

    // keccak256("Order(address maker,Asset makeAsset,address taker,Asset takeAsset,uint256 salt,uint256 start,uint256 end,bytes4 dataType,bytes data,uint64 typeMint)Asset(AssetType assetType,uint256 value,address token,uint256 tokenId)AssetType(bytes4 assetClass,bytes data)")
    bytes32 constant ORDER_TYPEHASH =
        0x8a52423eebf0127faa87c8b03d44e451375af417e24b076e08405a3ae3d0d766;

    // keccak256("Input(address maker,Asset makeAsset,address taker,Asset[] takeAssets,uint256 salt,uint256 start,uint256 end,bytes4 dataType,bytes data)Asset(AssetType assetType,uint256 value,address token,uint256 tokenId)AssetType(bytes4 assetClass,bytes data)")
    bytes32 constant INPUT_TYPEHASH =
        0x2e79377bb4a3ac7b8b74a0c944d66df335d1448fa257bb55031ad281a791650c;

    struct Order {
        address maker;
        LibAsset.Asset makeAsset;
        address taker;
        LibAsset.Asset takeAsset;
        uint256 salt;
        uint256 start;
        uint256 end;
        bytes4 dataType;
        bytes data;
        uint64 typeMint;
    }

    struct Input {
        address maker;
        LibAsset.Asset makeAsset;
        address taker;
        LibAsset.Asset[] takeAssets;
        uint256 salt;
        uint256 start;
        uint256 end;
        bytes4 dataType;
        bytes data;
    }

    function _calculateRemaining(
        Order memory order,
        uint256 fill,
        bool isMakeFill
    ) internal pure returns (uint256 makeValue, uint256 takeValue) {
        if (isMakeFill) {
            makeValue = order.makeAsset.value.sub(fill);
            takeValue = LibMath._safeGetPartialAmountFloor(
                order.takeAsset.value,
                order.makeAsset.value,
                makeValue
            );
        } else {
            takeValue = order.takeAsset.value.sub(fill);
            makeValue = LibMath._safeGetPartialAmountFloor(
                order.makeAsset.value,
                order.takeAsset.value,
                takeValue
            );
        }
    }

    function _hashKey(Order memory order) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    order.maker,
                    LibAsset._hash(order.makeAsset.assetType),
                    LibAsset._hash(order.takeAsset.assetType),
                    order.salt,
                    order.data
                )
            );
    }

    function _hashKey(Input memory input) internal pure returns (bytes32) {
        bytes32[] memory assetsBytes = new bytes32[](input.takeAssets.length);
        for (uint256 i = 0; i < input.takeAssets.length; i++) {
            assetsBytes[i] = LibAsset._hash(input.takeAssets[i]);
        }
        return
            keccak256(
                abi.encode(
                    input.maker,
                    LibAsset._hash(input.makeAsset),
                    input.taker,
                    keccak256(abi.encodePacked(assetsBytes)),
                    input.salt,
                    input.data
                )
            );
    }

    function _hash(Order memory order) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    ORDER_TYPEHASH,
                    order.maker,
                    LibAsset._hash(order.makeAsset),
                    order.taker,
                    LibAsset._hash(order.takeAsset),
                    order.salt,
                    order.start,
                    order.end,
                    order.dataType,
                    keccak256(order.data),
                    order.typeMint
                )
            );
    }

    function _hash(Input memory input) internal pure returns (bytes32) {
        bytes32[] memory assetsBytes = new bytes32[](input.takeAssets.length);
        for (uint256 i = 0; i < input.takeAssets.length; i++) {
            assetsBytes[i] = LibAsset._hash(input.takeAssets[i]);
        }
        return
            keccak256(
                abi.encode(
                    INPUT_TYPEHASH,
                    input.maker,
                    LibAsset._hash(input.makeAsset),
                    input.taker,
                    keccak256(abi.encodePacked(assetsBytes)),
                    input.salt,
                    input.start,
                    input.end,
                    input.dataType,
                    keccak256(input.data)
                )
            );
    }

    function _verifyTime(LibOrder.Order memory order) internal view {
        require(
            order.start == 0 || order.start < block.timestamp,
            "Order start validation failed"
        );
        require(
            order.end == 0 || order.end > block.timestamp,
            "Order end validation failed"
        );
    }

    function _verifyTimeExtention(LibOrder.Input memory input) internal view {
        require(
            input.start == 0 || input.start < block.timestamp,
            "Order start validation failed"
        );
        require(
            input.end == 0 || input.end > block.timestamp,
            "Order end validation failed"
        );
    }
}

