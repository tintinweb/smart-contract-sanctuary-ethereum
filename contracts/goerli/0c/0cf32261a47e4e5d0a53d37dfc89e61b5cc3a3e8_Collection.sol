        override
        returns (uint256)
    {
        return _erc1155CollectionId[_id];
    }

    /**
     * @dev Returns the number of tokens purchased by an account in the primary market
     * @param account the address of the user to return the number of tokens purchased
     * @param _collectionId the id of the collection
     * @return uint256 the number of tokens purchased by a user in the primary market
     */
    function getPrimarySaleUserCount(address account, uint256 _collectionId)
        public
        view
        override
        returns (uint256)
    {
        return _primarySaleUserCount[account][_collectionId];
    }

    /**
     * @dev creates a new collection
     * @param _collection the data for the collection to be created
     * @return id the id of the collection created
     * Requirements:
     * caller must have CREATOR_ROLE role
     * {_collection.title} cannot be empty
     * {_collection.description} cannot be empty
     * {_collection.thumbnail} cannot be empty
     * {_collection.bgImage} cannot be empty
     * {_collection.bgColor} cannot be empty
     * {_collection.owner} cannot be zero address
     * {_collection.userPurchaseLimit} must be greater than zero or equal to -1
        Note: using -1 would mean there is no user purchase limit for the collection 
     * {_collection.allowedPurchaseLimit} must be greater than zero or equal to -1
        Note: using -1 would mean there is no allowed purchase limit for the entire collection 
     */
    function createCollection(Collection calldata _collection)
        external
        onlyRole(CREATOR_ROLE)
        returns (uint256 id)
    {
        require(
            bytes(_collection.title).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            bytes(_collection.description).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            bytes(_collection.thumbnail).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            bytes(_collection.bgImage).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            bytes(_collection.bgColor).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(_collection.owner != address(0), throwError(ZERO_ADDRESS));
        require(
            _collection.userPurchaseLimit > 0 ||
                _collection.userPurchaseLimit == -1,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            _collection.allowedPurchaseLimit > 0 ||
                _collection.allowedPurchaseLimit == -1,
            throwError(INVALID_COLLECTION_DETAIL)
        );

        id = _collectionIdTracker.current();

        _collections[id].title = _collection.title;
        _collections[id].description = _collection.description;
        _collections[id].thumbnail = _collection.thumbnail;
        _collections[id].bgImage = _collection.bgImage;
        _collections[id].bgColor = _collection.bgColor;
        _collections[id].tokenIds = _collection.tokenIds;
        _collections[id].owner = _collection.owner;
        _collections[id].createdAt = uint64(block.timestamp);
        _collections[id].userPurchaseLimit = _collection.userPurchaseLimit;
        _collections[id].allowedPurchaseLimit = _collection
            .allowedPurchaseLimit;
        _collections[id].tokenType = _collection.tokenType;
        _collections[id].exists = true;

        uint256 len = _collection.whitelistParams.length;
        for (uint256 i = 0; i < len; i++) {
            _collections[id].whitelistParams.push(
                _collection.whitelistParams[i]
            );



        }
        // Sid: we're not adding tokens to erc721Collection/erc1155collection mapping here, only in add tokens
        // this is fine if we only ever add tokens with the addTokensToCollection method - fixed
        uint256 tokenIdLen = _collection.tokenIds.length;
        for (uint256 i = 0; i < tokenIdLen; i++) {
            if (_collection.tokenType == TokenType.ERC721) {
                _erc721CollectionId[_collection.tokenIds[i]] = id;
            } else {
                _erc1155CollectionId[_collection.tokenIds[i]] = id;
            }
        }

        _collectionIdTracker.increment();

        emit CollectionCreated(id);

        return id;
    }

    /**
     * @dev adds tokens to an existing collection
     * @param _collectionId the id of the collection to add tokens to
     * @param _tokenIds the array of token ids
     * Requirements:
     * caller must have CREATOR_ROLE role
     * {_collectionId} must be greater than or equal to zero
     * {_collectionId} must exist / already created
     * caller must be the owner of the collection
     * {_tokenIds} length must be greater than zero
    
    Note: using -1 would mean there is no allowed purchase limit for the entire collection 
     */
    function addTokensToCollection(
        uint256 _collectionId,
        uint256[] calldata _tokenIds
    ) external {
        require(
            _collections[_collectionId].exists == true,
            throwError(COLLECTION_DOES_NOT_EXIST)
        );
        require(
            hasRole(MAINTAINER_ROLE, msg.sender) || msg.sender == _collections[_collectionId].owner,
            throwError(UNAUTHORISED_ACCESS)
        );

        uint256 tokenIdLen = _tokenIds.length;
        require(tokenIdLen > 0, throwError(TOKEN_IDS_REQUIRED));

        for (uint256 i = 0; i < tokenIdLen; i++) {
            if (_collections[_collectionId].tokenType == TokenType.ERC721) {
                _erc721CollectionId[_tokenIds[i]] = _collectionId;
            } else {
                _erc1155CollectionId[_tokenIds[i]] = _collectionId;
            }
        }
        for(uint256 i = 0; i < tokenIdLen; i++) {
            _collections[_collectionId].tokenIds.push(_tokenIds[i]);
         }
       // _collections[_collectionId].tokenIds = _tokenIds; // Sid: we're overwriting tokenIds here, not adding to existing - fixed
        _collections[_collectionId].updatedAt = uint64(block.timestamp);
    }

    /**
     * @dev updates the primary sale count in the collection
     * @param account the address to increase the primary sale count for
     * @param _collectionId the id of the collection to add tokens to
     * @return bool returns if the update is successful or not
     * Requirements:
     * {_collectionId} must exist / already created
     * caller must be the marketplace contract
     */
    function updateCollectionAfterPrimarySale(
        address account,
        uint256 _collectionId
    ) external override returns (bool) {
        require(
            _collections[_collectionId].exists == true,
            throwError(COLLECTION_DOES_NOT_EXIST)
        );

        require(
            msg.sender == market,
            throwError(SENDER_SHOULD_BE_MARKETPLACE_CONTRACT)
        );

        _collections[_collectionId].primarySoldCount++; // to track allowedPurchaseLimit - collection-level
        _primarySaleUserCount[account][_collectionId]++; // to track userPurchaseLimit - user-level

        return true;
    }

    /**
     * @dev updates an existing collection
     * @param _id id of the collection
     * @param _collection the data for the collection to be created
     * @return bool returns whether the collection is updated 
     * Requirements:
     * caller must have CREATOR_ROLE role
     * {_id} must be greater than or equal to zero
     * {_id} must be associated with an existing collection
     * caller must be the owner of the collection
     * {_collection.title} cannot be empty
     * {_collection.description} cannot be empty
     * {_collection.thumbnail} cannot be empty
     * {_collection.bgImage} cannot be empty
     * {_collection.bgColor} cannot be empty
     * {_collection.userPurchaseLimit} must be greater than zero or equal to -1
        Note: using -1 would mean there is no user purchase limit for the collection 
     * {_collection.allowedPurchaseLimit} must be greater than zero or equal to -1
        Note: using -1 would mean there is no allowed purchase limit for the entire collection 
     */
    function updateCollection(uint256 _id, Collection calldata _collection)
        external
        onlyRole(CREATOR_ROLE)
        returns (bool)
    {
        require(
            _collections[_id].exists == true,
            throwError(COLLECTION_DOES_NOT_EXIST)
        );
        require(
            hasRole(MAINTAINER_ROLE, msg.sender) || msg.sender == _collections[_id].owner,
            throwError(UNAUTHORISED_ACCESS)
        );

        require(
            bytes(_collection.title).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            bytes(_collection.description).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            bytes(_collection.thumbnail).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            bytes(_collection.bgImage).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            bytes(_collection.bgColor).length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            _collection.tokenIds.length > 0,
            throwError(INVALID_COLLECTION_DETAIL)
        );
        require(
            _collection.allowedPurchaseLimit > 0 ||
                _collection.allowedPurchaseLimit == -1,
            throwError(INVALID_COLLECTION_DETAIL)
        );

        _collections[_id].title = _collection.title;
        _collections[_id].description = _collection.description;
        _collections[_id].thumbnail = _collection.thumbnail;
        _collections[_id].bgImage = _collection.bgImage;
        _collections[_id].bgColor = _collection.bgColor;
        _collections[_id].tokenIds = _collection.tokenIds;
        _collections[_id].userPurchaseLimit = _collection.userPurchaseLimit;
        _collections[_id].allowedPurchaseLimit = _collection
            .allowedPurchaseLimit;
        _collections[_id].updatedAt = uint64(block.timestamp);

        uint256 len = _collection.whitelistParams.length;
        for (uint256 i = 0; i < len; i++) {
            _collections[_id].whitelistParams.push(
                _collection.whitelistParams[i]
            );
        }

        emit CollectionUpdated(_id);

        return true;
    }

    /**
     * @dev add whitelist rules to a collection
     * @param _id id of the collection
     * @param _whitelistParams the whitelist rules for a collection to be validated on sale
     * @return bool returns whether the collection is updated
     * Requirements:
     * caller must have CREATOR_ROLE role
     * {_id} must be greater than or equal to zero
     * {_id} must be associated with an existing collection
     * {_whitelistParams} length must be greater than zero
     */
    function addWhitelistToCollection(
        uint256 _id,
        WhitelistParams[] memory _whitelistParams
    ) external returns (bool) {
        require(
            _collections[_id].exists == true,
            throwError(COLLECTION_DOES_NOT_EXIST)
        );

        require(
            hasRole(MAINTAINER_ROLE, msg.sender) || msg.sender == _collections[_id].owner,
            throwError(UNAUTHORISED_ACCESS)
        );

        uint256 len = _whitelistParams.length;
        require(
            len > 0,
            throwError(INVALID_WHITELIST_PARAMS)
        );

        for (uint256 i = 0; i < len; i++) {
            _collections[_id].whitelistParams.push(_whitelistParams[i]);
        }

        emit CollectionUpdated(_id);

        return true;
    }

    /**
     * @dev transfer ownership of collection to another account
     * @param _id id of the collection
     * @param _owner the whitelist rules for a collection to be validated on sale
     * @return bool returns whether the collection is updated
     * Requirements:
     * caller must have CREATOR_ROLE role
     * {_id} must be greater than or equal to zero
     * {_id} must be associated with an existing collection
     * caller must be the current owner of the collection
     * current owner address cannot be the same as {_owner}
     */
    function transferCollectionOwnership(uint256 _id, address _owner)
        external
        onlyRole(CREATOR_ROLE)
        returns (bool)
    {
        require(
            _collections[_id].exists == true,
            throwError(COLLECTION_DOES_NOT_EXIST)
        );
        require(
            msg.sender == _collections[_id].owner,
            throwError(INVALID_OWNER)
        );
        require(_owner != msg.sender, throwError(OWNER_BUYER_CONFLICT));

        _collections[_id].owner = _owner;
        _collections[_id].updatedAt = uint64(block.timestamp);

        emit TransferredCollectionOwnership(_owner, _id);

        return true;
    }

