    COMPLETE,
    CANCELLED,
    REFUNDED
  }

  enum Op {
    INVALID,
    // off-chain
    COMPLETE_SELL_OFFER, // buyer takes order
    COMPLETE_BUY_OFFER, // seller takes offer
    CANCEL_OFFER,
    // auction
    BID,
    COMPLETE_AUCTION,
    REFUND_AUCTION,
    REFUND_AUCTION_STUCK_ITEM
  }

  enum DelegationType {
    INVALID,
    ERC721,
    ERC1155
  }
}

