        // lib.UNISWAP_V2_ROUTER = 0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff;   // UNISWAP
        lib.WETH = 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6; //Testnet
        lib.UNISWAP_V2_ROUTER = 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;    // Uniswap
        // lib.DAI = 0x8f3Cf7ad23Cd3CaDbD9735AFf958023239c6A063;
        lib.faucet1 = address(this);
    }

   

    function getAmountOutMin(address _tokenIn, address _tokenOut, uint256 _amountIn) public view returns(uint256) {
        address[] memory path = LibraryStorage.getPath(_tokenIn, _tokenOut);
        uint256[] memory amountOutMins = IUniswapV2Router01(LibraryStorage.libStorage().UNISWAP_V2_ROUTER).getAmountsOut(_amountIn,path);
        return amountOutMins[path.length -1];
    }

   function _beforeDeposit(address payable[] memory addr,uint amount, uint endDate, uint[][] memory option) view internal {
        LibraryStorage.LibStorage storage lib = LibraryStorage.libStorage();
        require(addr[1] != address(0),"Send valid beneficiary address");
        require(addr[0] != address(0),"Send valid token address");
        require(lib._tokenVsIndex[addr[0]] != 0, "Send valid token address!");
        require(endDate>=lib.minLockDate,"Send correct endDate");
        LibraryStorage.Token storage token = lib._tokens[lib._tokenVsIndex[addr[0]]];
        require(token.status == LibraryStorage.Status.OPEN, "Token is closed");
        require(amount >= token.minAmount,"Minimum amount of tokens");
        uint _totalprocent;
        for (uint i = 0; i< option.length  ; i++){
            if (_totalprocent + option[i][1] > 100) {
                revert("Ops must be equal to 100"); }
            _totalprocent +=  option[i][1];
        } 
    }

    function deposit(
        // address _token, 
        // address payable beneficiary,
        // address payable _affiliateAddress,
        address payable[] memory addr,
        uint amount,
        uint endDate,
        uint[][] memory option,
        bool isExchangable,
        bool isOwned,
        string memory uuid
    ) public payable
    {
        _beforeDeposit(addr, amount, endDate, option);
        LibraryStorage.LibStorage storage lib = LibraryStorage.libStorage();
        LibraryStorage.Token storage token = lib._tokens[lib._tokenVsIndex[addr[0]]];
        uint newAmount=LibraryStorage._calculateFee(amount,true, lib.StartFee);

        uint fee = newAmount-amount;
        token.balance += fee;
        fee = fee*lib.affiliateFee/100;

        
        if(lib.ETH == addr[0]) {
            require( msg.value >= newAmount,"Insufficient funds");
            token.balance += msg.value - newAmount;
        } else {
            IERC20(addr[0]).transferFrom(LibraryStorage._msgSender(), address(this), newAmount);
        }

        endDate += block.timestamp;
        int priceInUSD = LibraryStorage.getLatestPrice(token.priceFeedAddress);
        lib._lockId++;
        lib._idVsLockedAsset[lib._lockId] = LibraryStorage.LockedAsset({ token: addr[0], 
        beneficiary: addr[1], amount: amount, startDate: block.timestamp, endDate: endDate, id: lib._lockId,claimedAmount: 0, 
        priceInUSD:priceInUSD, option: option, isExchangable:isExchangable, isOwned:isOwned ,status: LibraryStorage.Status.OPEN });
        lib._userVsLockIds[addr[1]].push(lib._lockId);
        uint tokenId=lib._lockId;
        
        (isOwned) ? gemChest.safeMint(addr[1], tokenId) : gemChest.safeMint(address(this), tokenId);

        if (addr[2] != address(0)) {
            if (addr[0] == LibraryStorage.libStorage().ETH) {
             payable(addr[2]).transfer(fee);
        } else {
            IERC20(addr[0]).transfer(addr[2], fee);
        }
        }

        emit Deposit(uuid, tokenId, amount, priceInUSD, msg.sender, "Success");
    }

    function bulkClaim(uint[] memory ids, address SWAPTOKEN) external onlyRole(LibraryStorage.libStorage().ADMIN) {
        for(uint i=0; i < ids.length; i++){
            bool res = claimable(ids[i]);
            if (res==false) revert errClaim(ids[i], "It is wrong!");
            claim(ids[i], SWAPTOKEN);
        }
        emit BulkClaim(ids);
    }

    function claim(uint256 id, address SWAPTOKEN) public payable {
        _beforeClaim(id);
        LibraryStorage.LibStorage storage lib = LibraryStorage.libStorage();
        LibraryStorage.LockedAsset storage asset = lib._idVsLockedAsset[id];
        LibraryStorage.Token storage token = lib._tokens[lib._tokenVsIndex[asset.token]];
        uint newAmount;
        uint optionAmount;
        uint amountBeforeFee;
            if (asset.endDate <= block.timestamp) {
                for(uint i = 0; i < asset.option.length; i++) {
                    optionAmount+=asset.option[i][1];
                    delete asset.option[i];
                }
                amountBeforeFee = (asset.amount * optionAmount) / 100;
                newAmount=LibraryStorage._calculateFee(amountBeforeFee, false, lib.EndFee);
                token.balance+=amountBeforeFee-newAmount;
            } else {
                optionAmount=((asset.amount*asset.option[0][1])/100);
                newAmount=LibraryStorage._calculateFee(optionAmount, false, lib.EndFee);
                token.balance+=optionAmount-newAmount;
                for(uint i = 0; i < asset.option.length-1; i++){
                    asset.option[i] = asset.option[i+1];
                }
                asset.option.pop();
            } 
            
            if (asset.option.length==0){
                asset.status = LibraryStorage.Status.CLOSED;
                gemChest.burn(id);
            } 
            
            if(LibraryStorage.libStorage().ETH == asset.token) {          
                if (asset.isExchangable) {
                    if (!asset.isOwned) {
                        swap(asset.token, SWAPTOKEN, newAmount, address(this));
                        asset.claimedAmount += getAmountOutMin(asset.token,SWAPTOKEN,newAmount);
                    } else {
                        swap(asset.token, SWAPTOKEN, newAmount, asset.beneficiary);
                    }
                } else {
                    payable(asset.beneficiary).transfer(newAmount);
                }
            } else {
                if (asset.isExchangable) {
                    if (!asset.isOwned) {
                        swap(asset.token, SWAPTOKEN, newAmount, address(this));
                        asset.claimedAmount += getAmountOutMin(asset.token,SWAPTOKEN,newAmount);
                    } else {
                        swap(asset.token, SWAPTOKEN, newAmount, asset.beneficiary);
                    }
                } else {
                    IERC20(asset.token).transfer(asset.beneficiary, newAmount);
                }
            }
            emit Claim("Claim is done successfully");
    }

    function swap (address _tokenIn, address _tokenOut,uint _amount, address _to) public payable returns(bool success){ 
        LibraryStorage.LibStorage storage lib = LibraryStorage.libStorage();
        address[] memory path = LibraryStorage.getPath(_tokenIn,_tokenOut);
        if (_tokenIn == lib.ETH){
            uint amount = _amount;
            uint amountOutMin = getAmountOutMin(lib.WETH, _tokenOut, _amount);
            IUniswapV2Router01(lib.UNISWAP_V2_ROUTER).swapExactETHForTokens{value: amount} (
                amountOutMin,
                path,
                _to,
                block.timestamp
            );
        } else if(_tokenOut == lib.ETH){
            IERC20(_tokenIn).approve(lib.UNISWAP_V2_ROUTER, _amount);
            uint amountOutMin = getAmountOutMin(_tokenIn, lib.WETH, _amount);
            IUniswapV2Router01(lib.UNISWAP_V2_ROUTER).swapExactTokensForETH(
                _amount,
                amountOutMin,
                path,
                _to,
                block.timestamp);

        } else {
            IERC20(_tokenIn).approve(lib.UNISWAP_V2_ROUTER, _amount); 
            uint amountOutMin = getAmountOutMin(_tokenIn, _tokenOut, _amount);
            IUniswapV2Router01(lib.UNISWAP_V2_ROUTER).swapExactTokensForTokens(
                _amount,
                amountOutMin,  
                path,
                _to,
                block.timestamp
            );
        }
        return true;
    } 


    function claimable(uint256 id) public view returns(bool success){
        LibraryStorage.LockedAsset memory asset = LibraryStorage.libStorage()._idVsLockedAsset[id];
        success = (asset.endDate <= block.timestamp || _eventIs(id)) &&  asset.status == LibraryStorage.Status.OPEN ? true : false;
    }


    function _eventIs(uint id) public view returns(bool success) { 
        LibraryStorage.LockedAsset memory asset = LibraryStorage.libStorage()._idVsLockedAsset[id];
        if (asset.status == LibraryStorage.Status.CLOSED || asset.status == LibraryStorage.Status.x){
            return false;
        }
        else {
            int newAmount = asset.priceInUSD * int(asset.option[0][0])/100;   //?????
            ( /*address tokenAddress*/,
                /*uint256 minAmount*/,
                /*uint balance*/,
                address _priceFeedAddress,
                /*Status status*/
            ) = LibraryStorage.getToken(asset.token);
            int oraclePrice = LibraryStorage.getLatestPrice(_priceFeedAddress);
            success = 5*oraclePrice >= newAmount ? true : false;
        } 
    }


