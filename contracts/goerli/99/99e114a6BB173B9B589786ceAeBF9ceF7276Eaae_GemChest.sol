pragma solidity ^0.8.9;

import "./interfaces/interfaces721/ERC721.sol";
import "./interfaces/interfaces721/Ownable.sol";
import "./interfaces/interfaces721/Strings.sol";
import "./facets/Faucet1.sol";


contract GemChest is ERC721, Ownable {
    address faucet1Address;
    string extension = ".json";
    string private _baseUri = "https://server.xlock.io/metadata/";

    Faucet1 faucet1;

    constructor(address payable _contractAddress) ERC721("GemChest", "GEM") {
        faucet1 = Faucet1(_contractAddress);
        faucet1Address = _contractAddress;
    }

    /**
     * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each
     * token will be the concatenation of the `baseURI` and the `tokenId`.
     */
    function _baseURI() internal view override returns (string memory) {
        return _baseUri;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        _requireMinted(tokenId);
        string memory baseURI = _baseURI();
        return bytes(baseURI).length > 0 ? string(
            abi.encodePacked( 
                abi.encodePacked(
                    abi.encodePacked(
                        abi.encodePacked(baseURI, Strings.toString(tokenId),"/"),Strings.toString(tokenId)),extension))) : "";
    }

    function setBaseURI(string memory _newBaseUri) public onlyOwner {
        _baseUri=_newBaseUri;
    }

    function safeMint(address to, uint256 tokenId) external onlyDiamondFaucet {
        _mint(to,tokenId);
    }

    function burn(uint256 tokenId) external onlyDiamondFaucet {
        _burn(tokenId);
    }

    function transferB(address _from, address payable _to, uint _tokenId) public {
        console.log(ownerOf(_tokenId), "ownerOf(_tokenId)");
        console.log( _msgSender(), " _msgSender()");
        require (_msgSender() == faucet1Address || ownerOf(_tokenId) == _msgSender(),  "Only faucet contract");
        if (_msgSender() == faucet1Address){
            console.log(tx.origin, ownerOf(_tokenId), "TRANSFER BBBBBBBBBBBBBB");
            // require(tx.origin==ownerOf(_tokenId));
            _transfer(_from, _to, _tokenId);
        } else {
            _transfer(_msgSender(), _to, _tokenId);
            faucet1.TransferBeneficiary(_to,_tokenId);
        }
    }


    function transferFrom(address from, address payable to, uint256 tokenId) public {
        if (_msgSender() == faucet1Address){
            safeTransferFrom(from,to,tokenId);
        } else {
            safeTransferFrom(from,to,tokenId);
            faucet1.TransferBeneficiary(to,tokenId);
        }

    }

    modifier onlyDiamondFaucet() {
        require (_msgSender() == faucet1Address, "Only faucet!");
        _;
    }


}

