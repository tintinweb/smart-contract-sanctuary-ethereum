pragma solidity ^0.8.9;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";

import "./Dolar.sol";
import "./LightSpeed.sol";

import "hardhat/console.sol";

contract Casino is Pausable, Ownable, ERC1155Holder {

    Dolar dolarToken;
    LightSpeed speedToken;

    constructor(Dolar _dolarToken, LightSpeed _speedToken)
    {
        dolarToken = _dolarToken;
        speedToken = _speedToken;
    }
    
    function swap(address _to, uint256 _amount) private returns (uint256) {
        // receive dolar token
        uint256 amountInWei = _amount * 10 ** 18;
        dolarToken.transferFrom(_to, owner(), amountInWei);
        // send speed token
        speedToken.transferFrom(owner(), _to, amountInWei);
        return amountInWei;
    }
}

