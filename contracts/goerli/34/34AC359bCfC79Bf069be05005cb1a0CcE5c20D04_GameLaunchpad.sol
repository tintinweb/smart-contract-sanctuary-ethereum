  }

  function verifyERC1155Approval(
    address token,
    bytes memory params,
    uint256
  ) internal view returns (bool) {
    bool isERC1155 = verifyIsERC1155(token);
    if (isERC1155) {
      address tokenOwner = abi.decode(params, (address));
      bool isApproved = IERC1155(token).isApprovedForAll(
        tokenOwner,
        address(this)
      );
      if (isApproved) return true;
    }
    return false;
  }

