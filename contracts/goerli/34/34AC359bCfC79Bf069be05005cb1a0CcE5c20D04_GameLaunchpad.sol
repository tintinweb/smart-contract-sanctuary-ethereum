// OpenZeppelin dependencies





// Xstrela dependencies





library TransferHelper {
  using SafeERC20 for IERC20;

  function mintOrTransfer(
    TokenMintType tokenMintType,
    address token,
    address from,
    address to,
    uint256 tokenId,
    uint256 amount
  ) internal {
    if (
      tokenMintType == TokenMintType.ERC721_PUBLIC_MINTABLE ||
      tokenMintType == TokenMintType.ERC721_ROLE_BASED_MINTABLE
    ) {
      mintERC721(token, to, tokenId, uint96(amount));
    } else if (tokenMintType == TokenMintType.ERC721_TRANSFERABLE) {
      uint256 _tokenId = tokenId;
      for (uint256 i = 0; i < amount; i++) {
        _tokenId = _tokenId + i; // transfer multiple consecutive tokens
        transferERC721(token, from, to, _tokenId);
      }
    } else if (
      tokenMintType == TokenMintType.ERC1155_PUBLIC_MINTABLE ||
      tokenMintType == TokenMintType.ERC1155_ROLE_BASED_MINTABLE
    ) {
      mintERC1155(token, to, tokenId, amount);
    } else if (tokenMintType == TokenMintType.ERC1155_TRANSFERABLE) {
      transferERC1155(token, from, to, tokenId, amount);
    }
  }

  function transferFeesAndCapital(
    address currency,
    address from,
    address to,
    uint256 amount,
    uint256 launchpadFee,
    address feeRecipient
  ) internal {
    uint256 finalPublisherAmount = amount;

    // 1. Transfer fees to Xstrela fee recipient
    uint256 feeAmount = (amount * launchpadFee) / 10000;
    if (feeAmount > 0 && feeRecipient != address(0)) {
      IERC20(currency).safeTransferFrom(from, feeRecipient, feeAmount);
      finalPublisherAmount -= feeAmount;
    }

    // 2. Transfer capital to publisher
    if (finalPublisherAmount > 0)
      IERC20(currency).safeTransferFrom(from, to, finalPublisherAmount);
  }

  function mintERC721(
    address token,
    address to,
    uint256 tokenId,
    uint96 amount
  ) internal {
    if (tokenId == 0) {
      try IERC721MintConsecutive(token).mint(to, amount) {
        return;
      } catch {
        try IERC721Mint(token).mint(to) {
          if (amount > 1) {
            for (uint256 i = 1; i < amount; i++) {
              IERC721Mint(token).mint(to);
            }
          }
          return;
        } catch {
          revert MintError(UNABLE_TO_MINT);
        }
      }
    } else {
      try IERC721MintEnumerable(token).mint(to, tokenId) {
        return;
      } catch {
        revert MintError(UNABLE_TO_MINT);
      }
    }
  }

  function mintERC1155(
    address token,
    address to,
    uint256 tokenId,
    uint256 amount
  ) internal {
    try IERC1155Mintable(token).mint(to, tokenId, amount) {
      return;
    } catch {
      revert MintError(UNABLE_TO_MINT);
    }
  }

  function transferERC721(
    address token,
    address from,
    address to,
    uint256 tokenId
  ) internal {
    IERC721(token).safeTransferFrom(from, to, tokenId);
  }

  function transferERC1155(
    address token,
    address from,
    address to,
    uint256 tokenId,
    uint256 amount
  ) internal {
    IERC1155(token).safeTransferFrom(from, to, tokenId, amount, "");
  }
}

