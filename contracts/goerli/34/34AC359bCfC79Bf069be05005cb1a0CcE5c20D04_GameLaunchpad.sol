// File: contracts/launchpad/GameLaunchpad.sol


pragma solidity 0.8.17;

// OpenZeppelin dependencies



// Xstrela dependencies






contract GameLaunchpad is IGameLaunchpad {
  using EnumerableSet for EnumerableSet.AddressSet;
  using TransferHelper for ConfigData;

  address public xstrelaLaunchpad; // address of Xstrela Launchpad
  address public xstrelaAdmin; // address of Xstrela Admin
  uint256 public launchpadFee; // fee charged by Xstrela for Launchpad in basis points

  address public publisher; // address of game publisher
  address public currency; // address of currency used for capital investment

  uint256 public totalCapitalRaised; // total capital raised in currency

  // list of tokens configured for launch by publisher
  EnumerableSet.AddressSet private _tokenList;
  // token address => token configuration data
  mapping(address => ConfigData) private _tokenConfigs;
  // token address => user address => amount of token minted by user
  mapping(address => mapping(address => uint64)) private _userMinted;

  constructor(
    address _publisher,
    address _currency,
    address _xstrelaAdmin,
    uint256 _launchpadFee
  ) {
    xstrelaLaunchpad = msg.sender;
    publisher = _publisher;
    currency = _currency;
    xstrelaAdmin = _xstrelaAdmin;
    launchpadFee = _launchpadFee;
  }

  function configureTokenLaunch(
    ConfigData calldata tokenConfig
  ) external override {
    if (msg.sender != xstrelaLaunchpad) revert AccessError(FORBIDDEN);

    if (tokenConfig.token == address(0))
      revert TokenConfigError(INVALID_TOKEN_ADDRESS);
    if (tokenConfig.mintMaxSupply == 0)
      revert TokenConfigError(INVALID_MINT_MAX_SUPPLY);
    if (tokenConfig.mintPricePerToken == 0)
      revert TokenConfigError(INVALID_MINT_PRICE_PER_TOKEN);
    // mint limit per wallet should be less than uint64 max
    if (
      tokenConfig.mintLimitPerWallet == 0 ||
      tokenConfig.mintLimitPerWallet > UINT64_MAX
    ) revert TokenConfigError(INVALID_MINT_LIMIT_PER_WALLET);
    if (tokenConfig.mintStartTime == 0)
      revert TokenConfigError(INVALID_MINT_START_TIME);
    if (tokenConfig.mintedSupply != 0)
      revert TokenConfigError(CANNOT_CONFIGURE_MINTED_SUPPLY);
    if (
      !TypeChecker.verifyType(
        tokenConfig.tokenMintType,
        tokenConfig.token,
        tokenConfig.params,
        tokenConfig.mintMaxSupply
      )
    ) revert TokenConfigError(INVALID_TOKEN_MINT_TYPE);

    if (!_tokenList.contains(tokenConfig.token)) {
      _tokenList.add(tokenConfig.token);
    }
    _tokenConfigs[tokenConfig.token] = tokenConfig;
  }

  function mint(
    address to,
    address token,
    uint256 tokenId,
    uint256 amount,
    bytes32[] calldata proof
  ) external override {
    // check if token is mintable (check)
    if (msg.sender != to) revert AccessError(FORBIDDEN);
    _ensureIsMintable(token, to, amount);

    bytes32 merkleRoot = _tokenConfigs[token].whitelistMerkleRoot;
    if (merkleRoot != bytes32(0) && proof.length > 0) {
      _ensureIsWhiteListed(merkleRoot, to, proof);
    }

    // update state variables (effects)
    ConfigData storage tokenConfig = _tokenConfigs[token];

    _userMinted[token][to] += uint64(amount);
    tokenConfig.mintedSupply += amount;
    totalCapitalRaised += (amount * tokenConfig.mintPricePerToken);

    if (tokenConfig.mintedSupply >= tokenConfig.mintMaxSupply) {
      tokenConfig.mintEndTime = block.timestamp;
    }

    address from;
    if (
      tokenConfig.tokenMintType == TokenMintType.ERC721_TRANSFERABLE ||
      tokenConfig.tokenMintType == TokenMintType.ERC1155_TRANSFERABLE
    ) {
      from = abi.decode(tokenConfig.params, (address));
      from = from == address(0) ? publisher : from;
    }

    // mint or transfer tokens to minter (interactions)
    TransferHelper.mintOrTransfer(
      tokenConfig.tokenMintType,
      token,
      from,
      to,
      tokenId,
      amount
    );

    // transfer launchpad fee to xstrela admin and capital raised to publisher
    uint256 userCap = amount * tokenConfig.mintPricePerToken;
    TransferHelper.transferFeesAndCapital(
      currency,
      to,
      publisher,
      userCap,
      launchpadFee,
      xstrelaAdmin
    );

    emit TokenMinted(to, token, amount);
  }

  function updateXstrelaAdmin(address _xstrelaAdmin) external {
    if (msg.sender != xstrelaAdmin) revert AccessError(FORBIDDEN);
    xstrelaAdmin = _xstrelaAdmin;
  }

  function updateLaunchpadFee(uint256 _launchpadFeeBps) external {
    if (msg.sender != xstrelaAdmin) revert AccessError(FORBIDDEN);
    launchpadFee = _launchpadFeeBps;
  }

  function _ensureIsMintable(
    address token,
    address to,
    uint256 amount
  ) private view {
    if (token == address(0) || !_tokenList.contains(token))
      revert MintError(INVALID_TOKEN_ADDRESS);
    ConfigData memory tokenConfig = _tokenConfigs[token];
    if (tokenConfig.mintStartTime > block.timestamp)
      revert MintError(MINT_NOT_STARTED);
    if (tokenConfig.mintEndTime < block.timestamp) revert MintError(MINT_ENDED);
    if (tokenConfig.mintedSupply >= tokenConfig.mintMaxSupply)
      revert MintError(MINT_MAX_SUPPLY_REACHED);
    if (amount == 0) revert MintError(INVALID_MINT_AMOUNT);
    if (amount > tokenConfig.mintLimitPerWallet - _userMinted[token][to])
      revert MintError(MINT_LIMIT_REACHED);
  }

  function _ensureIsWhiteListed(
    bytes32 merkleRoot,
    address to,
    bytes32[] calldata proof
  ) private pure {
    bytes32 node = keccak256(abi.encodePacked(to));
    if (!MerkleProof.verify(proof, merkleRoot, node)) {
      revert MintError(INVALID_WHITELIST_PROOF);
    }
  }
}