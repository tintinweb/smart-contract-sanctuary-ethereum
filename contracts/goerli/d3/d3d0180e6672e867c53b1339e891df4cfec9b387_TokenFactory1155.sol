pragma solidity 0.8.17;

import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "./Token1155.sol";
import "./TokenWhitelist.sol";
import "./interfaces/ITokenFactory1155.sol";
import "./TokenFactory1155Storage.sol";

contract TokenFactory1155 is TokenFactory1155Storage, ITokenFactory1155, AccessControlEnumerableUpgradeable {
    /**
     * @notice Initializer, setting caller as an admin for the factory
     */
    function initialize() external initializer{
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    /**
    * @dev add other 1155 tokens or other WL contract (internal function)
    * @param _tokenAddress erc1155 token address to add
    * @param _tokenWLToAdd wl contract to add to an existing erc1155
    * @param _addingMode set the mode, 1 = token, 2 = wl
    */
    function addTokenAndWLContractAddress(address _tokenAddress, address _tokenWLToAdd, uint256 _addingMode) internal {
        if (_addingMode == 1) {
            tokens[tokenCounter] = _tokenAddress;
            tokenDeployed[_tokenAddress] = true;
        }
        if (_addingMode == 2) {
            whitelists[_tokenAddress] = _tokenWLToAdd;
            tokenWLDeployed[_tokenWLToAdd] = true;
        }
    }

    /**
    * @dev deploy a new token contract, add its address to internal variables and change the ownership to new owner address (Admin role only)
    * @param _tokenAddress, token address
    * @param _useReserveFunction, use reserve function or not
    */
    function enableReserveFunction(address _tokenAddress, bool _useReserveFunction) external onlyRole(DEFAULT_ADMIN_ROLE) {
        Token1155(_tokenAddress).enableReserveFunction(_useReserveFunction);
        emit ReserveFunctionEnabled(_tokenAddress, _useReserveFunction);
    }

    /**
    * @dev deploy a new token contract, add its address to internal variables and change the ownership to new owner address (Admin role only)
    * @param _baseTokenURI, base token URI
    * @param _creatorAddress, creator address
    * @param _feesCollectorAddress fees collector address where fees will be sent to be collected
    * @param _biPercent, BI fees percentage
    * @param _creatorPercent, creator percentage
    * @return address of the deployed token contract
    */
    function deployToken1155(string memory _name,
            string memory _baseTokenURI,
            address _creatorAddress,
            address _feesCollectorAddress,
            uint256 _biPercent,
            uint256 _creatorPercent) public override onlyRole(DEFAULT_ADMIN_ROLE) returns (address) {
        Token1155 newToken = new Token1155(); //(_baseTokenURI, _creatorAddress, _creatorCollectorAddress, _biPercent, _creatorPercent);
        newToken.initialize(_name, _baseTokenURI, _creatorAddress, _feesCollectorAddress, _biPercent, _creatorPercent);
        addTokenAndWLContractAddress(address(newToken), address(0), 1);
        newToken.transferOwnership(_creatorAddress);
        emit TokenCreated(address(newToken), tokenCounter, _creatorAddress);
        tokenCounter++;
        return address(newToken);
    }

    /**
    * @dev deploy a new token WL contract, add its address to internal variables and change the ownership to new owner address (Admin role only)
    * @param _tokenAddress, token address
    * @param _creator, creator address
    * @return address of the deployed token WL contract
    */
    function deployMintWL(address _tokenAddress, address _creator) public override onlyRole(DEFAULT_ADMIN_ROLE) returns (address) {
        require(tokenDeployed[_tokenAddress], "Token not found");
        TokenWhitelist newMintWL = new TokenWhitelist();
        newMintWL.initialize(_tokenAddress);
        Token1155(_tokenAddress).setTokenWLAddress(address(newMintWL));
        addTokenAndWLContractAddress(_tokenAddress, address(newMintWL), 2);
        newMintWL.transferOwnership(_creator);
        emit TokenWLCreated(_tokenAddress, address(newMintWL), _creator);
        return address(newMintWL);
    }

    /**
    * @dev deploy a new token and a new token WL contract calling internal functions (Admin role only)
    * @param _baseTokenURI, base token URI
    * @param _creatorAddress, creator address
    * @param _feesCollectorAddress fees collector address where fees will be sent to be collected
    * @param _biPercent, BI fees percentage
    * @param _creatorPercent, creator percentage
    * @return addresses of the deployed token and token WL contracts
    */
    function deployTokenWithWL(string memory _name,
            string memory _baseTokenURI,
            address _creatorAddress,
            address _feesCollectorAddress,
            uint256 _biPercent,
            uint256 _creatorPercent) external override onlyRole(DEFAULT_ADMIN_ROLE) returns (address, address) {
        address newTokenAddress = deployToken1155(_name, _baseTokenURI, _creatorAddress, _feesCollectorAddress, _biPercent, _creatorPercent);
        address newTokenWLaddress = deployMintWL(newTokenAddress, _creatorAddress);
        return (newTokenAddress, newTokenWLaddress);
    }
}

