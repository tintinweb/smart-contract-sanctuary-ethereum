// SPDX-License-Identifier: MIT
pragma solidity 0.8.17;


contract Token1155Storage {
    uint256 public constant PERCENT_DIVIDER = 1000000;  // percentage divider, 6 decimals

    uint256 public constant minfeesPercentage = 25000;  // min BI percentage = 2,5%

    uint256 public feesPercent;
    uint256 public creatorPercent;

    // token WL address
    address public tokenWLAddress;
    address public creatorAddress;
    address public feesCollectorAddress;
    address public multiStdFactoryAddress;

    string public baseMetadataURI;
    string public tokenName;

    // Mapping from owner to list of owned token IDs
    mapping(address => uint256[]) public ownedIDTokens;

    // totalSupply for tokenId
    mapping(uint256 => uint256) public totalSupplies;

    // maxSupply for tokenId
    mapping(uint256 => uint256) public maxSupplies;

    // startDate for tokenId
    mapping(uint256 => uint256) public startDates;

    // basePrice for tokenId
    mapping(uint256 => uint256) public basePriceForIds;

    // basePrice for tokenId
    mapping(uint256 => bool) public enabledIds;

    uint256[] ids;

    bool public useReserveFunction;
}

