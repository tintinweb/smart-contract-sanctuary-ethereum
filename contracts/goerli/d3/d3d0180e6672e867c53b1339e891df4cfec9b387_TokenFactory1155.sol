import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/common/ERC2981Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "./interfaces/ITokenWhitelist.sol";
import "./interfaces/IToken1155.sol";
import "./Token1155Storage.sol";


contract Token1155 is Token1155Storage, OwnableUpgradeable, PausableUpgradeable, ERC1155Upgradeable, ERC2981Upgradeable, IToken1155 {

    /**
     * @notice Initializer
     * @param _name token name
     * @param _baseUri token URI base
     * @param _creatorAddress creator address
     * @param _feesCollectorAddress fees collector address
     * @param _feesPercent fees collector's fees percentage
     * @param _creatorPercent creator fees percentage
     */
    function initialize(string memory _name,
            string memory _baseUri,
            address _creatorAddress,
            address _feesCollectorAddress,
            uint256 _feesPercent,
            uint256 _creatorPercent) external initializer {
        require(_feesPercent >= minfeesPercentage, "Token1155: BI precentage too low");
        require(_creatorPercent <= PERCENT_DIVIDER - _feesPercent, "Token1155: creator precentage too high"); 
        OwnableUpgradeable.__Ownable_init();
        baseMetadataURI = _baseUri;
        __ERC1155_init(baseMetadataURI);
        tokenName = _name;
        //string memory uriCompleted = string(abi.encodePacked(baseMetadataURI, "{id}.json"));
        // __ERC1155_init(uriCompleted);
        creatorAddress = _creatorAddress;
        feesCollectorAddress = _feesCollectorAddress;
        feesPercent = _feesPercent;
        creatorPercent = _creatorPercent;
        multiStdFactoryAddress = _msgSender();
    }

    /** @notice check if msg.sender is creator address */
    modifier creatorOnly() {
        require(creatorAddress == _msgSender());
        _;
    }

    /** @notice check if msg.sender is factory address */
    modifier factoryOnly() {
        require(multiStdFactoryAddress == _msgSender());
        _;
    }

    /**
     * @dev enable reserve function for creator (factoryOnly)
     * @param _useReserve true or false
     */
    function enableReserveFunction(bool _useReserve) external override factoryOnly {
        useReserveFunction = _useReserve;
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     * @return enhanced support interface
     */
    function supportsInterface(bytes4 interfaceId) public view override(ERC1155Upgradeable, ERC2981Upgradeable) returns (bool) {
        // IERC1155Upgradeable: 0xd9b67a26 || IERC2981Upgradeable: 0x2a55205a || IERC1155MetadataURIUpgradeable: 0x0e89341c
        return (
            interfaceId == type(IERC1155Upgradeable).interfaceId ||
            interfaceId == type(IERC2981Upgradeable).interfaceId ||
            interfaceId == type(IERC1155MetadataURIUpgradeable).interfaceId ||
            super.supportsInterface(interfaceId)
        );
    }

    /**
     * @dev set token whitelist address (factoryOnly)
     * @param _tokenWLAddr whitelist address
     */
    function setTokenWLAddress(address _tokenWLAddr) external factoryOnly {
        require(multiStdFactoryAddress == _msgSender() || owner() == _msgSender());
        require(_tokenWLAddr != address(0), "Token1155: WL address not allowed");
        tokenWLAddress = _tokenWLAddr;
    }

    /**
     * @dev get fees percentage for fees collector address
     * @return feesPercent fees percentage
     */
    function getFeesPercent() external view override returns (uint256) {
        return feesPercent;
    }

    /**
     * @dev Total amount of tokens in with a given id.
     * @return totalSupplies[id] 
     */
    function totalSupply(uint256 id) public view override returns (uint256) {
        return totalSupplies[id];
    }

    function uri(uint256 _tokenId) override public view returns(string memory) {
        return string(abi.encodePacked(baseMetadataURI, StringsUpgradeable.toString(_tokenId)));
    }

    /**
     * @dev Indicates weither any token exist with a given id, or not.
     */
    // function exists(uint256 id) external view override returns (bool) {
    //     return totalSupply(id) > 0;
    // }

    function enableIds(uint256[] memory _ids, 
            uint256[] memory _supplies, 
            uint256[] memory _basePrices,
            uint256[] memory _starts) external creatorOnly {
        for (uint256 i = 0; i < _ids.length; ++i) {
            require(!enabledIds[_ids[i]], "Token1155: ID already enabled");
            enabledIds[_ids[i]] = true;
            maxSupplies[_ids[i]] = _supplies[i];
            basePriceForIds[_ids[i]] = _basePrices[i];
            startDates[_ids[i]] = _starts[i];
            ids.push(_ids[i]);
        }
        emit TokenIdsEnabled();
    }

    function reserve(uint256 _id, uint256 _amount, bytes memory _data) external whenNotPaused creatorOnly {
        require(useReserveFunction, "Token1155: reserve function not allowed");
        require(block.timestamp < startDates[_id], "Token1155: Time greater than start date");
        require(totalSupplies[_id] + _amount <= maxSupplies[_id], "Token1155: max supply reached");
        totalSupplies[_id] += _amount;
        _mint(_msgSender(), _id, _amount, _data);
    }

    function isMintPossible(uint256 _id, uint256 _amount) internal view returns (bool) {
        require(enabledIds[_id], "Token1155: ID not allowed from creator");
        require(block.timestamp >= startDates[_id], "Token1155: Start Date not elapsed");
        require(totalSupplies[_id] + _amount <= maxSupplies[_id], "Token1155: max supply reached");
        return true;
    }

    /**
     * @dev Creates `amount` new tokens for `to`, of token type `id`.
     *
     * See {ERC1155-_mint}.
     *
     * Requirements:
     *
     * - the caller must have the `MINTER_ROLE`.
     */
    function mint(uint256 _id, uint256 _amount, bytes memory _data) external payable whenNotPaused {
        require(isMintPossible(_id, _amount));

        require(msg.value >= basePriceForIds[_id] * _amount, "Token1155: not enough value to mint");

        totalSupplies[_id] += _amount;
        _mint(_msgSender(), _id, _amount, _data);
    }

    /**
     * @dev xref:ROOT:erc1155.adoc#batch-operations[Batched] variant of {mint}.
     */
    function mintBatch(uint256[] memory _ids,
            uint256[] memory _amounts,
            bytes memory data) external payable whenNotPaused {
        uint256 totalValueRequired;
        for (uint256 i = 0; i < _ids.length; ++i) {
            require(isMintPossible(_ids[i], _amounts[i]));
            totalValueRequired += basePriceForIds[_ids[i]] * _amounts[i];
        }

        require(msg.value >= totalValueRequired, "Token1155: not enough value to mint all requested tokens");

        _mintBatch(_msgSender(), _ids, _amounts, data);
        for (uint256 i = 0; i < _ids.length; ++i) {
            totalSupplies[_ids[i]] += _amounts[i];
        }
    }

    /**
     * @dev See {ERC1155-_burn}.
     */
    function burn(address _account,
            uint256 _id,
            uint256 _amount) external whenNotPaused {
        require(_account == _msgSender() || isApprovedForAll(_account, _msgSender()), "Token1155: caller is not owner nor approved");
        _burn(_account, _id, _amount);
        totalSupplies[_id] -= _amount;
    }

    /**
     * @dev See {ERC1155-_burnBatch}.
     */
    function burnBatch(address _account,
            uint256[] memory _ids,
            uint256[] memory _amounts) external whenNotPaused {
        require(_account == _msgSender() || isApprovedForAll(_account, _msgSender()), "Token1155: caller is not owner nor approved");
        _burnBatch(_account, _ids, _amounts);
        for (uint256 i = 0; i < _ids.length; ++i) {
            totalSupplies[_ids[i]] -= _amounts[i];
        }
    }

    /**
     * @dev Pauses all token transfers.
     *
     * See {ERC1155Pausable} and {Pausable-_pause}.
     *
     * Requirements:
     *
     * - the caller must have the `PAUSER_ROLE`.
     */
    function pause() external creatorOnly{
        _pause();
    }

    /**
     * @dev Unpauses all token transfers.
     *
     * See {ERC1155Pausable} and {Pausable-_unpause}.
     *
     * Requirements:
     *
     * - the caller must have the `PAUSER_ROLE`.
     */
    function unpause() external creatorOnly {
        _unpause();
    }

    function _beforeTokenTransfer(address operator,
            address from,
            address to,
            uint256[] memory ids,
            uint256[] memory amounts,
            bytes memory data) internal override {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);

        require(tokenWLAddress == address(0) || (tokenWLAddress != address(0) && ITokenWhitelist(tokenWLAddress).isWhitelisted(to)), "Token1155: receiver not whitelisted");

        uint256 toIDsLen = ownedIDTokens[to].length;
        for(uint256 j = 0; j < ids.length; j++) {
            bool found = false;
            uint256 i;
            for (i = 0; i < toIDsLen; i++) {
                if (ownedIDTokens[to][i] == ids[j]) {
                    found = true;
                }
            }

            if (!found) {
                ownedIDTokens[to].push(ids[j]);
            }

            if(from != address(0) && balanceOf(from, j) == 0) {
                ownedIDTokens[to][j] = 0;
            }
        }
    }

    function getOwnerTokenIDLen(address _owner) external view override returns (uint256) {
        return ownedIDTokens[_owner].length;
    }

    // function getOwnerTokenIDValue(address _owner, uint256 index) external view override returns (uint256) {
    //     return ownedIDTokens[_owner][index];
    // }

    /** @dev EIP2981 royalties implementation. */
    function setCreatorAddress(address _newCreator) external creatorOnly {
        require(_newCreator != address(0), "Token1155: new recipient is the zero address");
        // OwnableUpgradeable.transferOwnership(_newCreator);
        creatorAddress = _newCreator;
        emit CreatorChanged(creatorAddress);
    }

    // EIP2981 standard royalties return.
    function royaltyInfo(uint256 _salePrice) external view override returns (address receiver, uint256 royaltyAmount) {
        return (creatorAddress, _salePrice * creatorPercent / PERCENT_DIVIDER);
    }

    /**
    * @dev withdraws value in contract, splitting fees
    */
    function withdraw() external payable {
        uint256 balance = address(this).balance;
        require(balance > 0, "Token1155: No ethers to withdraw");
        bool success;
        // blockInvest fees
        uint256 biAmount = balance * feesPercent / PERCENT_DIVIDER;
        (success, ) = payable(feesCollectorAddress).call{value: biAmount}("");
        require(success, "Token1155: Transfer failed.");
        // creator fees
        uint256 creatorAmount = balance - biAmount;
        (success, ) = payable(creatorAddress).call{value: creatorAmount}("");
        require(success, "Token1155: Transfer failed.");
    }

}

