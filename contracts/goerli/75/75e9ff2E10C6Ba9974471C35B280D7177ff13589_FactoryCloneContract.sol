pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "./proxy.sol";
import "hardhat/console.sol";
import "./emitter.sol";
import "./implementation.sol";

/// @title StationXFactory Cloning Contract
/// @dev Contract create proxies of DAO Token and Governor contract
contract FactoryCloneContract {

    address Implementation;
    address owner;
    address USDC;
    address emitter;

    struct DAO {
        string name; 
        string symbol; 
        uint256 _totalDeposit;
        uint256 _minDeposit;
        uint256 _maxDeposit; 
        uint256 _ownerFee; 
        uint256 _days;
        bool _feeUSDC;
        uint256 _quorum;
        uint256 _threshold;
        address _ownerAddress;
    }

    /// @dev Initial Deployment of Token and Governor contract and storing their addresses for proxies creation
    constructor (address _USDC) {       
        Implementation = address(new ERC20NonTransferable());
                console.log("Implementation contract set to:", Implementation);
        owner = msg.sender;
                console.log("Factory Owner Assign:", owner);
        USDC = _USDC;
                console.log("USDC contract set to:", USDC);
        emitter = address(new Emitter());
                console.log("Emitter address set to:", emitter);
        Emitter(emitter).Initialize(Implementation, _USDC);
    }

    /// @dev Modifier to check for Owner of contract
    modifier isOrganiser () {
        require(msg.sender == owner,"Only Owner Allowed");
        _;
    }

    /// @dev Function to assign new Owner
    /// @param _newOrganiser address of new owner
    function changeOrganiser(address _newOrganiser) external isOrganiser{
        if(_newOrganiser != address(0))
        {
            owner = _newOrganiser;
            Emitter(emitter).changeOrganiser(_newOrganiser);
        }
    }

    /// @dev Function to Get the emitter address
    function emitterAddress() public view returns(address){
        return emitter;
    }

    /// @dev Function to change base Governor contract address
    /// @param _newDAO address of new Governor contract
    function changeDAOImplementation(address _newDAO) external isOrganiser{
        if(_newDAO != address(0))
        {
            Implementation = _newDAO;            
            Emitter(emitter).changeImplementationAddress(_newDAO);
        }
    }

    /// @dev Function to create proxies and initialisation of Token and Governor contract
    function createDAO(DAO calldata params)external returns(address) {
        bytes memory data = abi.encodeWithSignature("initializeERC20(string,string,uint256,uint256,uint256,uint256,uint256,bool,uint256,uint256,address,address,address)",params.name,params.symbol,params._totalDeposit,params._minDeposit,params._maxDeposit,params._ownerFee,params._days,params._feeUSDC,params._quorum,params._threshold,params._ownerAddress,USDC,emitter);
        address proxy = address(new ERC1155TradeableProxy(Implementation,owner,data));
        Emitter(emitter).createDao(params._ownerAddress, proxy, params.name, params.symbol, params._totalDeposit, params._minDeposit, params._maxDeposit, params._ownerFee, params._days, params._feeUSDC, params._quorum, params._threshold, emitter);
        return proxy;
    }

    /// @dev Function to assign USDC address
    /// @param _USDCAddress address of USDC token
    function changeUSDCAddress(address _USDCAddress) external isOrganiser{
        if(_USDCAddress != address(0))
        {
            USDC = _USDCAddress;
            Emitter(emitter).changedUSDCAddress(_USDCAddress);
        }
    }

    /// @dev Function to get USDC address
    /// @return contract address of USDC  
    function getUSDCAddress() external view returns(address){
        return USDC;
    }
}

