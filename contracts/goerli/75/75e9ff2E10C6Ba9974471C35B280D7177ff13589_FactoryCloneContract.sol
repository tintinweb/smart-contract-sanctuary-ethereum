pragma solidity ^0.8.7;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/draft-ERC20PermitUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/ERC20VotesUpgradeable.sol";
import "./emitter.sol";

import "hardhat/console.sol";


/// @title StationX Governance Token Contract
/// @dev Base Contract as a reference for DAO Governance Token contract proxies
contract ERC20NonTransferable is
    ERC20Upgradeable,
    ERC20PermitUpgradeable,
    ERC20VotesUpgradeable
{
    using SafeMath for uint256;

    //Declarations
    bool feeUSDC;
    bool depositIntilize;
    address public ownerAddress; //Gnosis address
    uint256 public totalDeposit; //total deposit
    uint256 public minDeposit; //min amount
    uint256 public maxDeposit; //max amount
    uint256 public closeDate; //Closing date
    uint256 public ownerFee; //owner fee
    uint256 public quorum; //quorum 
    uint256 public threshold; //precentage required to pass the proposal
    bool public depositClosed; //flag to check if deposit is closed
    address public tokenAddress; //satble coin address

    //structure of proposal
    struct Proposals {
        string proposalHash;
        string status;
        uint256 proposalId;
        address customToken;
        address airDropToken;
        bool[] executionIds;
        uint256 quorum;
        uint256 threshold;
        uint256 day;
        uint256 minDeposits;
        uint256 maxDeposits;
        uint256 totalDeposits;
        uint256 airDropAmount;
        uint256[] mintGTAmounts; // amounts array to send GT to different wallets
        address[] mintGTAddresses; // wallets address array where to send GT address in sync with mintGTAmounts
        uint256[] customTokenAmounts; // amounts array to send custom token to different wallets
        address[] customTokenAddresses; // wallets address array where to send custom token in sync with customTokenAmounts
        uint256[] sendEthAmounts; // amounts array to send Eth to different wallets
        address[] sendEthAddresses; // wallets address array where to send Eth in sync with sendEthAmounts
        uint256 ownersAirdropFees;
    }

    /// @dev balances mapping of each DAO members for governance token
    mapping(address => uint256) public balances;

    /// @dev array to store proposals
    Proposals[] proposalArray;
    
    
    /// @dev counter to store number of proposals in a contract
    uint256 proposalCounter = 0;

    /// @dev array to store members addresses
    address[] members;

    ///@dev deployer/owner of the Gnosis sdk
    address deployerAddress;

    ///@dev address of the emitter contract
    address emitterContract;

    /// @dev onlyOwner modifier to allow only Owner access to functions
    modifier onlyOwner() {
        require(ownerAddress == msg.sender, "Only Owner");
        _;
    }

    /// @dev Function to update Token Address of a stable coin 
    /// @param _tokenAddress New Token Address
    function updateTokenAddress(address _tokenAddress) external onlyOwner {
        tokenAddress = _tokenAddress;
        Emitter(emitterContract).updateUSDCTokenAddress(address(this), _tokenAddress);
    }

    function getProposals() view public returns(Proposals[] memory){
        return proposalArray;
    }


    /// @dev Function to update Minimum and Maximum deposits allowed by DAO members
    /// @param _minDeposit New minimum deposit requirement amount in wei
    /// @param _maxDeposit New maximum deposit limit amount in wei
    function updateminmaxdeposit(uint256 _minDeposit, uint256 _maxDeposit)
        external
        onlyOwner
    {
        require(_minDeposit > 0, "Min deposit should be greater then 0");
        require(_maxDeposit > 0, "Max deposit should be greater then 0");
        require(_maxDeposit > _minDeposit, "Min deposit should be less than max deposit");
        require(
            !depositIntilize,
            "Failed to update due to deposit initialized"
        );
        minDeposit = _minDeposit;
        maxDeposit = _maxDeposit;
        Emitter(emitterContract).updateMinMaxDeposit(address(this), _minDeposit, _maxDeposit);
    }

    
    /// @dev Function to update DAO Owner Fee
    /// @param _ownerFee New Owner fee
    function updateOwnerFee(uint256 _ownerFee) external onlyOwner {
        require(
            !depositIntilize,
            "Failed to update due to deposit initialized"
        );
        require(block.timestamp < closeDate, "Deposit Closed");
        require(_ownerFee > 100, "Invalid argument");
        ownerFee = _ownerFee;

        Emitter(emitterContract).updateOwnerFee(address(this), _ownerFee);
    }

    /// @dev Function to close deposit
    function closeDeposit() external onlyOwner {
        closeDate = block.timestamp;
        Emitter(emitterContract).closeDeposit(address(this), block.timestamp);
    }
 
    /// @dev Function to start deposit
    /// @param _days New close date
    /// @param _totalDeposit total deposit to be conducted
    /// @param _minDeposit min deposit a user can make into DAO
    /// @param _maxDeposit max deposit a user can make into DAO
    function startDeposit(
        uint256 _days,
        uint256 _totalDeposit,
        uint256 _minDeposit,
        uint256 _maxDeposit
    ) external onlyOwner {

        require(_totalDeposit > 0, "Total deposit should be greated than 0"); //Total deposit check
        require(totalDeposit <= _totalDeposit,"new deposit amount shoudl be grater than or equal to previous total deposit");

        //Min max check
        require(_minDeposit > 0, "Min deposit should be greater then 0");
        require(_maxDeposit > 0, "Max deposit should be greater then 0");
        require(_maxDeposit > _minDeposit, "Min deposit should be less than max deposit");
        
        require(closeDate < block.timestamp, "Deposit already started"); //check if deposit is already working or not
        
        closeDate = block.timestamp.add(_days.mul(86400));
        totalDeposit = _totalDeposit;
        minDeposit = _minDeposit;
        maxDeposit = _maxDeposit;

        Emitter(emitterContract).startDeposit(address(this), block.timestamp, closeDate);
        Emitter(emitterContract).updateMinMaxDeposit(address(this), _minDeposit, _maxDeposit);
        Emitter(emitterContract).updateRaiseAmount(address(this), _totalDeposit);
    }

    /// @dev Function to deposit USDC by DAO members and assign equivalent Governance token to members after owner fee
    /// @param _token USDC Contract Address
    /// @param _amount USDC amount to deposit
    function deposit(address _token, uint256 _amount) external {
        require(closeDate > block.timestamp, "Deposit Closed");
        require(_token == tokenAddress, "Only USDC allowed to deposit");
        require(_amount >= minDeposit, "Amount less than minimum deposit");
        require(_amount <= maxDeposit, "Amount greater than max deposit limit");
        uint256 _daoBalance = ERC20(_token).balanceOf(address(this));

        require(_daoBalance < totalDeposit, "DAO exceeded total deposit limits");

        depositIntilize = true;

        uint256 amt = _amount.sub((_amount.mul(ownerFee)).div(100));

        uint256 _customTokenDecimals = ERC20(_token).decimals();
        uint256 _amountMintRatio = (_amount.mul(1*10**18)).div(1*10**_customTokenDecimals);
        uint256 _amtMintRatio = (amt.mul(1*10**18)).div(1*10**_customTokenDecimals);

        if (balances[msg.sender] == 0) {
            members.push(msg.sender);
        }
        if (feeUSDC == true) {
            ERC20(_token).transferFrom(msg.sender, address(this), amt);
            ERC20(_token).transferFrom(
                msg.sender,
                deployerAddress,
                (_amount.sub(amt))
            );
            mintToken(msg.sender, _amountMintRatio); 
            balances[msg.sender] = balances[msg.sender].add(_amountMintRatio);
        } else {
            ERC20(_token).transferFrom(msg.sender, address(this), _amount);
            mintToken(msg.sender, _amtMintRatio);
            mintToken(deployerAddress, (_amountMintRatio.sub(_amtMintRatio)));
            balances[msg.sender] = balances[msg.sender].add(_amtMintRatio);
            balances[deployerAddress] = balances[deployerAddress].add(_amountMintRatio.sub(_amtMintRatio));
        }
        Emitter(emitterContract).deposited(address(this), msg.sender, _token, _amount);
    }

    /// @dev function to execute the proposal 
    /// @param params the structure of the proposal 
    function updateProposalAndExecution(Proposals memory params)
        external
        onlyOwner
    {
        if (params.executionIds[0] == true) {
            airDropToken(params.airDropToken, params.airDropAmount, params.ownersAirdropFees);
        }
        if (params.executionIds[1] == true) {
            mintGTToAddress(params.mintGTAmounts, params.mintGTAddresses);
        }
        if (params.executionIds[2] == true) {
            updateGovernanceSettings(params.quorum, params.threshold);
        }
        if (params.executionIds[3] == true) {
            startDepositExecute(
                params.day,
                params.totalDeposits,
                params.minDeposits,
                params.maxDeposits
            );
        }
        if (params.executionIds[4] == true) {
            closeDepositExecute();
        }
        if (params.executionIds[5] == true) {
            updateRaiseAmount(params.totalDeposits);
        }
        if (params.executionIds[6] == true) {
            sendCustomToken(
                params.customToken,
                params.customTokenAmounts,
                params.customTokenAddresses
            );
        }
        if (params.executionIds[7] == true) {
            sendEth(params.sendEthAmounts, params.sendEthAddresses);
        }
        proposalArray.push(params);
        proposalCounter+=1;
    }

    /// @dev function to send ether to users
    /// @param _amount array of amount to be transfered
    /// @param _addresses array of address where the amount should be transfered 
    function sendEth(uint256[] memory _amount, address[] memory _addresses)
        internal
    {
        require(_amount.length == _addresses.length, "Invalid parameters");

        uint256 _requiredAmount;

        for (uint256 i = 0; i < _amount.length; i++) {
            _requiredAmount = _requiredAmount.add(_amount[i]); 
        }

        require(address(this).balance < _requiredAmount , "Insufficient funds");
        
        for (uint256 i = 0; i < _amount.length; i++) {
            payable(_addresses[i]).transfer(_amount[i]); // can check for total balance > needed
        }
        Emitter(emitterContract).sendEth(address(this), _amount, _addresses);
    }

    /// @dev function to send the custom token
    /// @param _token is the token contract address
    /// @param _amount array of amount to be transfered
    /// @param _addresses array of address where the amount should be transfered 
    function sendCustomToken(
        address _token,
        uint256[] memory _amount,
        address[] memory _addresses
    ) internal {
        require(_amount.length == _addresses.length, "Invalid parameters");

        uint256 _minimumRequirment = 0;
        for(uint256 j=0;j<_amount.length;j++){
            _minimumRequirment = _minimumRequirment.add(_amount[j]);
        }

        require(_minimumRequirment <= ERC20(_token).balanceOf(address(this)), "Insufficient funds");


        for (uint256 i = 0; i < _amount.length; i++) {
            ERC20(_token).transfer(
                _addresses[i],
                _amount[i]
            );
        }

        Emitter(emitterContract).sendCustomToken(address(this), _token, _amount, _addresses);
    }

    /// @dev function to mint GT token to a addresses
    /// @param _amount array of amount to be transfered
    /// @param _userAddress array of address where the amount should be transfered
    function mintGTToAddress(
        uint256[] memory _amount,
        address[] memory _userAddress
    ) internal {
        require(_amount.length == _userAddress.length, "Invalid parameters");

        for (uint256 i = 0; i < _amount.length; i++) {
            mintToken(_userAddress[i], _amount[i]);
            balances[_userAddress[i]] = balances[_userAddress[i]].add(
                _amount[i]
            );
        }

        Emitter(emitterContract).mintGTToAddress(address(this), _amount, _userAddress);
    }

    /// @dev function to update governance settings
    /// @param _quorum update quorum into the contract
    /// @param _threshold update threshold into the contract
    function updateGovernanceSettings(uint256 _quorum, uint256 _threshold)
        internal
    {

        require(_quorum > 0,"quorum should be greater than 0");
        require(_threshold > 0,"threshold should be greater than 0");
        
        require(_quorum <= 100 ,"quorum should be less then or equal to 100");
        require(_threshold <= 100 ,"threshold should be less then or equal to 100");
        
        quorum = _quorum;
        threshold = _threshold;

        Emitter(emitterContract).updateGovernanceSettings(address(this), _quorum, _threshold);
    }

    /// @dev function to perform airdrop
    /// @param _token contract address of token for which airdrop is conducted
    /// @param _amount total amount to conduct airdrop
    /// @param _ownersAirdropFees the fees owner is charging for the airdrop
    function airDropToken(address _token, uint256 _amount,uint256 _ownersAirdropFees) internal {

        require(_ownersAirdropFees<100, "_ownersAirdropFees fees needs to be in range [0,100)");
        require(0<=_ownersAirdropFees, "_ownersAirdropFees fees needs to be in range [0,100)");

        uint256 _holdings = ERC20(_token).balanceOf(address(this));

        require(_holdings >= _amount, "Insufficient funds to conduct Airdrop");

        uint256 _updatedAmount;
        if(_ownersAirdropFees == 0){
            _updatedAmount = _amount;
        }
        else{
            uint256 _deployersShare = ((_amount).mul(_ownersAirdropFees)).div(100);
            ERC20(_token).transfer(deployerAddress, _deployersShare);
            _updatedAmount = _amount.sub(_deployersShare);
        }

        uint256 _totalSupply = totalSupply();

        for (uint256 i = 0; i < members.length; i++) {
            uint256 _dropAmount = (balanceOf(members[i]).mul(_updatedAmount)).div(_totalSupply);
            ERC20(_token).transfer(members[i], _dropAmount);
        }

        Emitter(emitterContract).airDropToken(address(this), _token, _amount, _ownersAirdropFees);
    }

    /// @dev function to update governance settings
    /// @param _amount update the total amount in DAO
    function updateRaiseAmount(uint256 _amount) internal {
        require(_amount>0,"amount should be grater than 0");
        totalDeposit = _amount;

        Emitter(emitterContract).updateRaiseAmount(address(this), _amount);
    }

    /// @dev Function to close deposit
    function closeDepositExecute() internal {
        closeDate = block.timestamp;
        // emit CloseDeposit(msg.sender, block.timestamp);
        Emitter(emitterContract).closeDeposit(address(this), closeDate);
    }

    /// @dev Function to start deposit
    /// @param _days New close date
    /// @param _totalDeposit total deposit to be conducted
    /// @param _minDeposit min deposit a user can make into DAO
    /// @param _maxDeposit max deposit a user can make into DAO
    function startDepositExecute(
        uint256 _days,
        uint256 _totalDeposit,
        uint256 _minDeposit,
        uint256 _maxDeposit
    ) internal {
        require(_totalDeposit > 0, "Total deposit should be greated than 0"); //Total deposit check

        //Min max check
        require(_minDeposit > 0, "Min deposit should be greater then 0");
        require(_maxDeposit > 0, "Max deposit should be greater then 0");
        require(_maxDeposit > _minDeposit, "Min deposit should be less than max deposit");
        require(closeDate < block.timestamp , "Deposit already started");
        
        closeDate = block.timestamp.add(_days.mul(86400));
        totalDeposit = _totalDeposit;
        minDeposit = _minDeposit;
        maxDeposit = _maxDeposit;

        Emitter(emitterContract).startDeposit(address(this), block.timestamp, closeDate);
        Emitter(emitterContract).updateMinMaxDeposit(address(this), _minDeposit, _maxDeposit);
        Emitter(emitterContract).updateRaiseAmount(address(this), _totalDeposit);

        // emit StartDeposit(msg.sender, block.timestamp);
    }

    /// @dev Function to give Governance details
    /// @return  closeDate, minDeposit, maxDeposit, members count, quorum
    function getGovernorDetails()
        external
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        return (
            closeDate,
            minDeposit,
            maxDeposit,
            quorum,
            totalDeposit,
            members.length
        );
    }

    /// @dev Function to give Members details
    /// @return  members addresses list
    function getMembersDetails() external view returns (address[] memory) {
        return members;
    }

    /// @dev Function to give Members details
    /// @return  members addresses list
    function getMembersBalances() external view returns (uint256[] memory,address[] memory) {
        uint256[] memory _balances = new uint256[](members.length);

        for(uint256 i=0;i<members.length;i++){
            _balances[i] = balances[members[i]];
        }

        return (_balances,members);

    }

    /// @dev Function to give user balance
    /// @return  uint256 user balance
    function checkUserBalance(address user) external view returns (uint256) {
        return balances[user];
    }

    /// @dev initialize Function to initialize Token contract
    /// @param name reflect the name of Governance Token
    /// @param symbol reflect the symbol of Governance Token
    /// @param _totalDeposit total deposit to be conducted
    /// @param _minDeposit min deposit a user can make into DAO
    /// @param _maxDeposit max deposit a user can make into DAO
    /// @param _ownerFee fee of owner 0-100
    /// @param _days number of days DAO will be open to accept deposit
    /// @param _feeUSDC boolean to check admin takes fees in USDC or Governance token
    /// @param _quorum reflect the symbol of Governance Token
    /// @param _threshold reflect the symbol of Governance Token
    /// @param _ownerAddress reflect the symbol of Governance Token
    /// @param _USDC reflect the symbol of Governance Token
    function initializeERC20(string calldata name, string calldata symbol,
        uint256 _totalDeposit,
        uint256 _minDeposit, uint256 _maxDeposit, 
        uint256 _ownerFee, uint256 _days,
        bool _feeUSDC,
        uint256 _quorum,uint256 _threshold,address _ownerAddress, address _USDC,address _emitter
    )
        initializer public
    {
        require(_minDeposit > 0,"minDeposit should be grater than 0");
        require(_maxDeposit > 0,"maxDeposit should be grater than 0");
        require(_maxDeposit > _minDeposit,"maxDeposit should be grater than minDeposit");
 
        require(_totalDeposit > _maxDeposit, "totalDeposit should be grater than maxDeposit");

        require(_quorum > 0,"quorum should be greater than 0");
        require(_quorum <= 100 ,"quorum should be less then or equal to 100");
 
        require(_threshold > 0,"threshold should be greater than 0");
        require(_threshold <= 100 ,"threshold should be less then or equal to 100");        
 
        require(_days > 0,"days should be greater than 0");


        require(_ownerFee >= 0,"ownerFee should be greater than or equal to 0");
        require(_ownerFee < 100,"ownerFee should be less than 100");

        require(_ownerAddress != address(0),"ownerAddress cannot be null");
 
        require(_USDC != address(0),"USDC cannot be null");
        
        totalDeposit    = _totalDeposit;     
        minDeposit      = _minDeposit;
        maxDeposit      = _maxDeposit;         
        closeDate       = block.timestamp.add(_days.mul(86400));
        ownerFee        = _ownerFee;
        ownerAddress    = _ownerAddress;
        feeUSDC         = _feeUSDC;
        quorum          = _quorum;
        threshold       = _threshold;
        tokenAddress    = _USDC; 
        emitterContract = _emitter;
        deployerAddress = tx.origin;
        __ERC20_init(name, symbol);
        __ERC20Permit_init(name);
        __ERC20Votes_init();
    }

    function decimals() public pure override returns (uint8) {
        return 18;
    }

    function stableCoinDecimals() public view returns (uint8) {
        return ERC20(tokenAddress).decimals();
    }

    // The following functions are overrides required by Solidity.

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override(ERC20Upgradeable, ERC20VotesUpgradeable) {
        super._afterTokenTransfer(from, to, amount);
    }

    function _mint(address to, uint256 amount)
        internal
        override(ERC20Upgradeable, ERC20VotesUpgradeable)
    {
        super._mint(to, amount);
    }

    function _burn(address account, uint256 amount)
        internal
        override(ERC20Upgradeable, ERC20VotesUpgradeable)
    {
        super._burn(account, amount);
    }

    /// @dev Function to override transfer to restrict token transfers
    function transfer(address, uint256) public pure override returns (bool) {
        require(false, "TOKENS NON-TRANSFERABLE");
        return true;
    }

    /// @dev Function to override transferFrom to restrict token transfers
    function transferFrom(
        address,
        address,
        uint256
    ) public pure override returns (bool) {
        require(false, "TOKENS NON-TRANSFERABLE");
        return true;
    }

    /// @dev Function to mint Governance Token and assign delegate
    /// @param to Address to which tokens will be minted
    /// @param amount Value of tokens to be minted based on deposit by DAO member
    function mintToken(address to, uint256 amount) internal {
        _mint(to, amount);
        if (delegates(to) == address(0)) {
            _delegate(to, to);
        }
    }

    /// @dev Function to burn Governance Token
    /// @param account Address from where token will be burned
    /// @param amount Value of tokens to be burned
    function burnToken(address account, uint256 amount) internal {
        _burn(account, amount);
    }

    /// @dev Function to give Governance Token  details
    /// @return token name, symbol and total supply
    function tokenDetails()
        external
        view
        returns (
            string memory,
            string memory,
            uint256
        )
    {
        return (name(), symbol(), totalSupply());
    }
}

