    usedSignature[_signature] = true;
  }

  // Takes a contract and tokenID(s) and anon(es) it(them)
  function anon(
    address _contract,
    uint256 _id,
    string memory _arweaveId,
    bytes32 _seed,
    uint256 _expiry,
    bytes memory _signature
  ) private nonReentrant returns (uint256) {
    if (block.number > _expiry) revert ExpiredSignature();
    if (usedSignature[_signature]) revert ProofInvalid();
    if (_contract == address(this) && !repliesEnabled) revert RepliesDisabled();
    if (
      !AnonUtils.verify(
        keccak256(
          abi.encodePacked(
            _contract,
            _id,
            _arweaveId,
            _seed,
            _expiry,
            msg.sender
          )
        ),
        _signature,
        signer
      )
    ) revert ProofInvalid();
    totalSupply++;
    if (_contract == address(this)) {
      anones[_id].entropy = keccak256(
        abi.encodePacked(anones[_id].level, msg.sender, block.timestamp)
      );
      anones[_id].level++;
    }
    storeBasic(totalSupply, _arweaveId, _seed, _signature);
    nftChildren[_contract][_id].push(totalSupply);

    anones[totalSupply].nft.contract_ = _contract;
    anones[totalSupply].nft.id = _id;
    anones[totalSupply].anonType = AnonType.NFT;
    anones[totalSupply].nft.owned = AnonUtils.isOwned(
      _contract,
      _id,
      msg.sender
    );

    _mint(msg.sender, totalSupply);

    emit AnonCreated(totalSupply, _contract, _id, _arweaveId);
    return totalSupply;
  }

  function mintAnonEth(
    address _contract,
    uint256 _id,
    string calldata _arweaveId,
    bytes32 _seed,
    uint256 _expiry,
    bytes calldata _signature
  ) external payable returns (uint256) {
    if (!anonEthEnabled) revert EthDisabled();
    if (msg.value < priceEth) revert InsufficientPayment();
    (bool sentTreasury, ) = treasury.call{value: msg.value}("");
    if (!sentTreasury) revert TransferFailed();
    return anon(_contract, _id, _arweaveId, _seed, _expiry, _signature);
  }

  function mintAnonErc20(
    address _contract,
    uint256 _id,
    string calldata _arweaveId,
    bytes32 _seed,
    uint256 _expiry,
    bytes calldata _signature
  ) external returns (uint256) {
    if (!anonErc20Enabled) revert Erc20Disabled();
    IERC20UpgradeableV1(erc20).transferFrom(msg.sender, treasury, priceErc20);
    return anon(_contract, _id, _arweaveId, _seed, _expiry, _signature);
  }

  // Takes a string (base64 encoded JSON) and Anones it
  function custom(
    string memory _schema,
    string memory _arweaveId,
    bytes32 _seed,
    uint256 _expiry,
    bytes memory _signature
  ) private nonReentrant returns (uint256) {
    if (block.number > _expiry) revert ExpiredSignature();
    if (usedSignature[_signature]) revert ProofInvalid();
    if (
      !AnonUtils.verify(
        keccak256(
          abi.encodePacked(_schema, _arweaveId, _seed, _expiry, msg.sender)
        ),
        _signature,
        signer
      )
    ) revert ProofInvalid();
    totalSupply++;
    storeBasic(totalSupply, _arweaveId, _seed, _signature);

    customChildren[keccak256(bytes(_schema))].push(totalSupply);

    anones[totalSupply].custom.schema = _schema;
    anones[totalSupply].anonType = AnonType.CUSTOM;

    _mint(msg.sender, totalSupply);

    emit CustomCreated(totalSupply, _schema, _arweaveId);
    return totalSupply;
  }

  function mintCustomEth(
    string calldata _schema,
    string calldata _arweaveId,
    bytes32 _seed,
    uint256 _expiry,
    bytes calldata _signature
  ) external payable returns (uint256) {
    if (!customEthEnabled) revert EthDisabled();
    if (msg.value < priceEth) revert InsufficientPayment();
    (bool sentTreasury, ) = treasury.call{value: msg.value}("");
    if (!sentTreasury) revert TransferFailed();
    return custom(_schema, _arweaveId, _seed, _expiry, _signature);
  }

  function mintCustomErc20(
    string calldata _schema,
    string calldata _arweaveId,
    bytes32 _seed,
    uint256 _expiry,
    bytes calldata _signature
  ) external returns (uint256) {
    if (!customErc20Enabled) revert Erc20Disabled();
    IERC20UpgradeableV1(erc20).transferFrom(msg.sender, treasury, priceErc20);
    return custom(_schema, _arweaveId, _seed, _expiry, _signature);
  }

  function burn(uint256 _anonId) external {
    _burn(_anonId);
  }

  function _burn(uint256 _anonId) internal override(ERC721Upgradeable) {
    if (!_exists(_anonId)) revert NonexistentToken();
    if (msg.sender != ownerOf(_anonId)) revert NotOwnedToken();
    super._burn(_anonId);
  }

  function tokenURI(uint256 _anonId)
    public
    view
    override(ERC721Upgradeable)
    returns (string memory)
  {
    if (!_exists(_anonId)) revert NonexistentToken();
    return super.tokenURI(_anonId);
  }

  function _baseURI()
    internal
    view
    override(ERC721Upgradeable)
    returns (string memory)
  {
    return baseUri;
  }

  /**
   * @dev See {IERC165-supportsInterface}.
   */
  function supportsInterface(bytes4 interfaceId)
    public
    view
    virtual
    override(ERC721Upgradeable, AccessControlUpgradeable)
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }
}

