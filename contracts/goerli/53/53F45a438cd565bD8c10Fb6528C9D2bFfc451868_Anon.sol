pragma solidity ^0.8.17;

import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";

error NonContract();

interface IERC721UpgradeableV1 {
  function supportsInterface(bytes4 interfaceId) external view returns (bool);
}

library AnonUtils {
  function verify(
    bytes32 messageHash,
    bytes memory signature,
    address signer
  ) internal pure returns (bool) {
    return
      signer ==
      ECDSAUpgradeable.recover(
        ECDSAUpgradeable.toEthSignedMessageHash(messageHash),
        signature
      );
  }

  function isOwned(
    address _contract,
    uint256 _id,
    address _sender
  ) internal view returns (bool) {
    if (!AddressUpgradeable.isContract(_contract)) revert NonContract();
    if (
      !IERC721UpgradeableV1(_contract).supportsInterface(0x80ac58cd) &&
      _contract != 0x9DFE69c0C52fa76d47Eef3f5aaE3e0Bcf73F7EE1
    ) {
      return false;
    }
    address nftOwner;
    if (_contract == 0x9DFE69c0C52fa76d47Eef3f5aaE3e0Bcf73F7EE1) {
      bytes memory punkIndexToAddress = abi.encodeWithSignature(
        "punkIndexToAddress(uint256)",
        _id
      );
      (, bytes memory result) = address(_contract).staticcall(
        punkIndexToAddress
      );
      nftOwner = abi.decode(result, (address));
    } else {
      bytes memory _ownerOf = abi.encodeWithSignature("ownerOf(uint256)", _id);
      (, bytes memory result) = address(_contract).staticcall(_ownerOf);
      nftOwner = abi.decode(result, (address));
    }
    return (nftOwner == _sender);
  }
}

