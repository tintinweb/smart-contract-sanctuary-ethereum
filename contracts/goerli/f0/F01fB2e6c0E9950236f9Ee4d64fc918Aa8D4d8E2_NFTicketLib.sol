import "../interfaces/INFTServiceTypes.sol";

library NFTicketLib {
    using EnumerableSet for EnumerableSet.AddressSet;

    struct TicketLib {
        EnumerableSet.AddressSet _whitelistSet;
        mapping(string => DataSchema) _dataSchemata;
        mapping(bytes32 => DataSchema) _schemaHashes;
    }

    function addWhitelist(TicketLib storage _t, address _address) external {
        _t._whitelistSet.add(_address);
    }

    function removeWhitelist(TicketLib storage _t, address _address) public {
        _t._whitelistSet.remove(_address);
    }

    function inWhitelist(TicketLib storage _t, address _address)
        public
        view
        returns (bool)
    {
        return _t._whitelistSet.contains(_address);
    }

    function getSender() public view returns (address) {
        return (msg.sender);
    }

    function isCheckoutService(TicketLib storage , uint32 serviceDescriptor) 
        public
        pure
        returns(bool)
    {
        return( (serviceDescriptor & CHECKOUT_TICKET) == CHECKOUT_TICKET);
    }

    function isCertificate(TicketLib storage , Ticket memory _ticket) 
        public 
        pure
        returns (bool) 
    {
        return ((_ticket.serviceDescriptor & IS_CERTIFICATE) == IS_CERTIFICATE);
    }

    function isTicket(TicketLib storage, Ticket memory _ticket) 
        public 
        pure
        returns (bool) 
    {
        return ((_ticket.serviceDescriptor & IS_TICKET) == IS_TICKET);
    }

    function extractSchemaFromData(TicketLib storage _t, DataRecords calldata _calldata)
        public
        returns (bytes32)
    {
        DataSchema memory _schema;
        bytes32 schemaHash;

        _schema = _calldata._schema;

        _t._dataSchemata[_schema.name] = _schema;
        _t._schemaHashes[schemaHash] = _schema;

        // emit evtSchemaRegistered(_schema.name, _schema);
        return (schemaHash);
    }

}

