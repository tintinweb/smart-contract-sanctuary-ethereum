        assembly {
            roleManager_ := sload(_roleManager.slot)
        }
    }

    function withdrawToken(address token_, uint256 amount_) external onlyRole(Roles.OPERATOR_ROLE) {
        address sender = _msgSender();
        if (token_ == address(0)) {
