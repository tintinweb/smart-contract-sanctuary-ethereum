pragma solidity 0.8.17;

import { AddressUpgradeable } from "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import { BitMapsUpgradeable } from "@openzeppelin/contracts-upgradeable/utils/structs/BitMapsUpgradeable.sol";
import { ReentrancyGuardUpgradeable } from "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

import { IERC721Upgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import { IERC721MetadataUpgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/IERC721MetadataUpgradeable.sol";
import { IERC721ReceiverUpgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";

import { PrimarySaleUpgradeable } from "./internal-upgradeable/PrimarySaleUpgradeable.sol";
import { BaseUpgradeable } from "./BaseUpgradeable.sol";
import { IRoleManager } from "./interfaces/IRoleManager.sol";
import { IERC721Full } from "./interfaces/IERC721Full.sol";
import { IERC721Bridge } from "./interfaces/IERC721Bridge.sol";
import { IERC4494 } from "./interfaces/IERC4494.sol";
import { Roles } from "./libraries/Roles.sol";
import { Helper } from "./libraries/Helper.sol";
import { CurrencyTransferLib } from "./libraries/CurrencyTransferLib.sol";

contract ERC721Bridge is IERC721ReceiverUpgradeable, IERC721Bridge, PrimarySaleUpgradeable, BaseUpgradeable, ReentrancyGuardUpgradeable {
    using AddressUpgradeable for address payable;
    using AddressUpgradeable for address;
    using BitMapsUpgradeable for BitMapsUpgradeable.BitMap;
    using Helper for bytes32;

    BitMapsUpgradeable.BitMap private _isSupported;
    BitMapsUpgradeable.BitMap private _isCompleted;
    mapping(bytes32 => BridgeNFTInfo) private _getBridgeNFTInfo;
    mapping(address => BitMapsUpgradeable.BitMap) private _isSupportedChain;
    uint256 public bridgeFee;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(IRoleManager roleManager_) public initializer {
        __Base_init(roleManager_);
        _addBridge(5, 0x0B914A22e1e2dB46765F1740B29AF5cBa9c41F5F, 97, 0x0B914A22e1e2dB46765F1740B29AF5cBa9c41F5F); // goerli -> bsc: save on bsc
        // _addBridge(97, 0x0B914A22e1e2dB46765F1740B29AF5cBa9c41F5F, 5, 0x0B914A22e1e2dB46765F1740B29AF5cBa9c41F5F); // goerli -> bsc: save on goerli
        bridgeFee = 100000000000000000; // 0.1
    }

    function deposit(
        address collectionAddress_,
        uint256 tokenId_,
        uint256 deadline_,
        bytes calldata permitSignature_,
        uint256 chainIdTo_,
        address recipientAddress_
    ) external payable override nonReentrant whenNotPaused {
        uint256 value = msg.value;

        if (value < bridgeFee) revert InsufficientBalance();
        CurrencyTransferLib.safeTransferNativeToken(_recipient, value);

        if (!_isSupportedChain[collectionAddress_].get(chainIdTo_)) {
            revert ChainToIsNotSupported();
        }

        if (permitSignature_.length > 0) {
            IERC4494(collectionAddress_).permit(address(this), tokenId_, deadline_, permitSignature_);
        }

        _receiveNFT(collectionAddress_, tokenId_);

        emit NFTSent(collectionAddress_, tokenId_, recipientAddress_, chainIdTo_);
    }

    function claimOrMint(uint256 submissionId_, uint256 tokenId_, address receiver_, NativeNFTInfo calldata tokenInfo_) external onlyRole(Roles.OPERATOR_ROLE) whenNotPaused {
        bytes32 bridgeId = getBridgeId(tokenInfo_.chainId, tokenInfo_.tokenAddress);
        BridgeNFTInfo memory bridgeInfo = _getBridgeNFTInfo[bridgeId];
        address tokenAddress = bridgeInfo.tokenAddress;

        if (bridgeInfo.nativeChainId != getChainId()) {
            revert Unreachable();
        }

        if (_isCompleted.get(submissionId_)) revert AlreadyAdded();
        _isCompleted.setTo(submissionId_, true);

        if (bridgeInfo.nativeChainId == getChainId() && _safeOwnerOf(tokenAddress, tokenId_) == address(this)) {
            _safeTransferFrom(tokenAddress, address(this), receiver_, tokenId_);
            emit NFTClaimed(submissionId_, tokenAddress, tokenId_, receiver_);
        } else {
            IERC721Full(tokenAddress).safeMint(receiver_, tokenId_, tokenInfo_.metadata);
            emit NFTMinted(submissionId_, tokenAddress, tokenId_, receiver_, tokenInfo_.metadata);
        }
    }

    function setBridgeFee(uint256 fee_) external onlyRole(Roles.OPERATOR_ROLE) {
        bridgeFee = fee_;
    }

    function addBridge(uint256 nativeChainId_, address nativeAddress_, uint256 secondaryChaind_, address secondAddress_) external override onlyRole(Roles.OPERATOR_ROLE) {
        _addBridge(nativeChainId_, nativeAddress_, secondaryChaind_, secondAddress_);
    }

    function _addBridge(uint256 nativeChainId_, address nativeAddress_, uint256 secondaryChaind_, address secondAddress_) internal {
        if (!_isSupportedChain[nativeAddress_].get(secondaryChaind_)) {
            _isSupportedChain[nativeAddress_].setTo(secondaryChaind_, true);
        }
        bytes32 bridgeId = getBridgeId(nativeChainId_, nativeAddress_);
        _getBridgeNFTInfo[bridgeId] = BridgeNFTInfo(secondaryChaind_, secondAddress_);

        emit BridgeAdded(nativeChainId_, nativeAddress_, secondaryChaind_, secondAddress_);
    }

    function removeBridge(bytes32 bridgeId_) external override onlyRole(Roles.OPERATOR_ROLE) {
        delete _getBridgeNFTInfo[bridgeId_];
        // _isSupportedChain[collection_].setTo(chainId_, false);
        emit BridgeRemoved(bridgeId_);
    }

    function removeChainSupport(uint256 chainId_, address collection_) external override onlyRole(Roles.OPERATOR_ROLE) {
        _isSupportedChain[collection_].setTo(chainId_, false);
    }

    /// @inheritdoc IERC721ReceiverUpgradeable
    function onERC721Received(address, address, uint256, bytes calldata) external pure override returns (bytes4) {
        return IERC721ReceiverUpgradeable.onERC721Received.selector;
    }

    function _receiveNFT(address tokenAddress_, uint256 tokenId_) internal {
        _safeTransferFrom(tokenAddress_, msg.sender, address(this), tokenId_);
    }

    function _authorizeUpgrade(address newImplementation) internal override onlyRole(Roles.UPGRADER_ROLE) {}

    /// @dev Safely transfers the given object and performs additional ownership check
    function _safeTransferFrom(address tokenAddress, address from, address to, uint256 tokenId) internal {
        IERC721Upgradeable(tokenAddress).safeTransferFrom(from, to, tokenId);

        if (_safeOwnerOf(tokenAddress, tokenId) != to) revert NotReceivedERC721();
    }

    function getBridgeId(uint256 nativeChainId_, address collection_) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(nativeChainId_, collection_));
    }

    function isCompleted(bytes32 submissionId) external view returns (bool) {
        return _isCompleted.get(submissionId.toUint256());
    }

    function getChainId() public view virtual returns (uint256 cid) {
        assembly {
            cid := chainid()
        }
    }

    function _safeOwnerOf(address nft, uint256 tokenId) internal view returns (address owner) {
        if (!IERC721Full(nft).exists(tokenId)) {
            return address(0);
        }

        (bool success, bytes memory data) = nft.staticcall(abi.encodeWithSelector(IERC721Upgradeable.ownerOf.selector, tokenId));
        if (success) {
            owner = abi.decode(data, (address));
        }
    }
}