abstract contract BaseUpgradeable is Initializable, ContextUpgradeable, UUPSUpgradeable {
    event ManagerUpdated(IRoleManager indexed from, IRoleManager indexed to);

    bytes32 private _roleManager;

    function __Base_init(IRoleManager roleManager_) internal onlyInitializing {
        __Base_init_unchained(roleManager_);
    }

    function __Base_init_unchained(IRoleManager roleManager_) internal onlyInitializing {
        __updateManager(roleManager_);
    }

    modifier onlyRole(bytes32 role) {
        _checkRole(role, _msgSender());
        _;
    }

    modifier whenNotPaused() {
        _requireNotPaused();
        _;
    }

    modifier whenPaused() {
        _requirePaused();
        _;
    }

    function updateManager(IRoleManager roleManager_) external onlyRole(Roles.OPERATOR_ROLE) {
        IRoleManager old = roleManager();
        if (old == roleManager_) revert Base__AlreadySet();
        __updateManager(roleManager_);
        emit ManagerUpdated(old, roleManager_);
    }

