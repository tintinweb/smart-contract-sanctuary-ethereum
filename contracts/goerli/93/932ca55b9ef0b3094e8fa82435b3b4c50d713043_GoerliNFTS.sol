contract GoerliNFTS is ERC721URIStorage, Ownable {

    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    constructor() ERC721("Goerli_NFTS", "G_NFTS") {}

    event Mint(address _to, uint256 _tokenId);

    event Withdraw(address _to, uint256 _value);

    modifier validMintCount(uint8 count) {
        require(count <= 10, 'Can only mint up to 10 per transaction');
        _;
    }

    function totalSupply() public view returns (uint256) {
        return _tokenIds.current();
    }

    function tokenExists(uint256 tokenId) public view returns (bool) {
        return _exists(tokenId);
    }

    function withdrawFunds() external onlyOwner {
        uint256 balance = address(this).balance;
        require(balance > 0, "No ether left to withdraw");
        (bool success, ) = (msg.sender).call{value: balance}("");
        require(success, "Withdrawal Failed");
        emit Withdraw(msg.sender, balance);
    }

    function mint(uint8 count) public payable validMintCount(count) {
        for (uint8 i = 0; i < count; i++) {
            _tokenIds.increment();
            _safeMint(msg.sender, _tokenIds.current());
            emit Mint(msg.sender, _tokenIds.current());
        }
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), 'URI query for nonexistent token');

        string memory color = "deeppink";

        if (((tokenId + 6) % 6) == 0) {
            color = "deepskyblue";
        } else if (((tokenId + 6) % 6) == 1) {
            color = "springgreen";
        } else if (((tokenId + 6) % 6) == 2) {
            color = "yellow";
        } else if (((tokenId + 6) % 6) == 3) {
            color = "darkviolet";
        } else if (((tokenId + 6) % 6) == 4) {
            color = "crimson";
        }

        bytes memory attributes = generateAttributesJSON(color);

        string memory svg = string.concat(
