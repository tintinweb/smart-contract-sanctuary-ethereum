import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./interface/IZenithInitializable.sol";

contract ZenithFactory is Ownable {
    using Counters for Counters.Counter;

    event NewZenithStakeContract(address indexed zenithStake);
    event managerStatus(address indexed user,bool indexed isManagerNow);

    address private immutable implemented;
    penalty public penaltyConfig;

    mapping (address => bool) private manager;
    mapping (uint256 => address) private pairList;
    mapping (address => mapping(address => address[])) private stakePair;

    Counters.Counter private totalPair;

    struct penalty {
        uint256 minBlock;
        uint256 penaltyArg1;
        uint256 penaltyArg2;
    }

    constructor (address base, uint256 minimumBlock, uint256 stakePenaltyArg1, uint256 stakePenaltyArg2)
    {
        require(
            stakePenaltyArg1 > 0 && stakePenaltyArg2 > 0,
            "ZenithFactory : penalty fee argument must not zero"
        );
        require(
            minimumBlock > 0,
            "ZenithFactory : penalty min block argument must not zero"
        );

        unchecked {
            uint256 dummyValue = 10**18;
            uint256 dummyPenalty = ((dummyValue * stakePenaltyArg1) / stakePenaltyArg2) / 100;

            require(
                dummyValue >= dummyPenalty,
                "ZenithFactory : maximum penalty fee is `100 percent`"
            );
        }
        
        implemented = base;
        penaltyConfig = penalty(
            minimumBlock,
            stakePenaltyArg1,
            stakePenaltyArg2
        );
    }

    modifier onlyManager ()
    {
        require(
            isManager(_msgSender()) == true,
            "ZenithFactory : only stake manager allowed!"
        );
        _;
    }

    function setManager (address target, bool status)
        external virtual onlyOwner
    {
        manager[target] = status;

        emit managerStatus(target, status);
    }

    /*
     * @notice Deploy the pool
     * @param _stakedToken: staked token address
     * @param _rewardToken: reward token address
     * @param _rewardPerBlock: reward per block (in rewardToken)
     * @param _startBlock: start block
     * @param _endBlock: end block
     * @param _admin: admin address with ownership
     * @return address of new zenith stake contract
     */
    function deployPool (
        IERC20Metadata _stakedToken,
        IERC20Metadata _rewardToken,
        uint256 _rewardPerBlock,
        uint256 _startBlock,
        uint256 _bonusEndBlock
    )
        external onlyManager
    {
        require(_stakedToken.decimals() > 0);
        require(_rewardToken.decimals() > 0);

        bytes32 salt = keccak256(abi.encodePacked(_stakedToken, _rewardToken, _startBlock));
        address zenithStakeAddress = Clones.cloneDeterministic(
            implemented,
            salt
        );
        
        IZenithInitializable(zenithStakeAddress).initialize(
            address(_stakedToken),
            address(_rewardToken),
            _rewardPerBlock,
            _startBlock,
            _bonusEndBlock,
            0,
            0
        );

        {
            uint256 currentIndex = totalPair.current();
            totalPair.increment();
            stakePair[address(_stakedToken)][address(_rewardToken)].push(zenithStakeAddress);
            pairList[currentIndex] = zenithStakeAddress;
        }

        emit NewZenithStakeContract(zenithStakeAddress);
    }

    function editPenalty (uint256 minimumBlock, uint256 stakePenaltyArg1, uint256 stakePenaltyArg2)
        external onlyManager
    {
        require(
            stakePenaltyArg1 > 0 && stakePenaltyArg2 > 0,
            "ZenithFactory : penalty fee argument must not zero"
        );
        require(
            minimumBlock > 0,
            "ZenithFactory : penalty min block argument must not zero"
        );

        unchecked {
            uint256 dummyValue = 10**18;
            uint256 dummyPenalty = ((dummyValue * stakePenaltyArg1) / stakePenaltyArg2) / 100;

            require(
                dummyValue >= dummyPenalty,
                "ZenithFactory : maximum penalty fee is `100 percent`"
            );
        }

        penaltyConfig = penalty(
            minimumBlock,
            stakePenaltyArg1,
            stakePenaltyArg2
        );
    }

    function getTotalStake ()
        public view
        returns(uint256)
    { return totalPair.current(); }

    function getStakeAddressByIndex (uint256 index)
        public view
        returns(address)
    { return pairList[index]; }

    function getStakeAddressByPair (address staked, address rewarded)
        public view
        returns(address[] memory)
    { return stakePair[staked][rewarded]; }

    function getStakeAddressByRangeIndex (uint256 fromIndex, uint256 toIndex)
        public view
        returns(address[] memory)
    {
        unchecked {
            uint256 getRange = (toIndex - fromIndex) + 1;
            address[] memory list = new address[](getRange);

            for(uint256 a; a < getRange; a++){
                uint256 index = fromIndex + a;
                list[a] = pairList[index];
            }

            return list;
        }
    }

    function isManager (address user)
        public view
        returns(bool)
    { return user == owner() || manager[user] == true; }
}

