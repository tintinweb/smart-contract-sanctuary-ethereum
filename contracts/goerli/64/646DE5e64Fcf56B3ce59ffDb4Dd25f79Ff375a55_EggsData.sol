import "@openzeppelin/contracts/access/AccessControl.sol";

// Helper functions OpenZeppelin provides.
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";

interface EggCollectionInter{
    function getRevIdFromTokenId(uint eggTokenId)external view returns(uint);
}

contract EggsData is AccessControl {
    bytes32 public constant CONTRACT_ROLE = keccak256("CONTRACT_ROLE");
    bytes32 public constant UPDATER_ROLE = keccak256("UPDATER_ROLE");

    using Counters for Counters.Counter;
    Counters.Counter private _eggIds;
    
    uint _maxId = 0;
    uint _maxRevId = 0;

    struct Egg{
        uint id;
        string name;
        string description;
        string veneer;
        string durability;
        string origin;
        string temperature;
        string buoyancy;
        uint energyLevel;
        string color;
        uint procurementDifficulty;
        uint rarity;
    }

    struct Revealed {
        uint id;
        string name;
        string description;
        string weight;
        //string sound;
        string byproduct;
        string element;
        uint lethality;
        string environment;
        string diet;
        string maturityRate;
        uint adultSizeEstimation;
        string mythicalApproximation;
        uint eggId;
        string image;
        string animation;
    }

    struct EggUpload{
        uint id;
        string name;
        string description;
        string veneer;
        string durability;
        string origin;
        string temperature;
        string buoyancy;
        uint energyLevel;
        string color;
        uint procurementDifficulty;
        uint rarity;
        string eggImg;
        string eggAnni;
        // uint supply;
        // uint price;
        uint avType; //1 citizen 2 nomad 0 both
        uint randomSet;
    }

    address EGG_COLLECTION_CONTRACT;
    EggCollectionInter EggColContract;

    mapping(uint => Egg) eggIdToData;
    mapping(uint => Revealed) revIdToRevealedData;
    mapping(uint => uint[]) eggIdToPossibleReveals;
    mapping(uint => uint) idToSupply; 
    mapping(uint => uint) eggIdToPrice;
    mapping(uint => uint) revIdToSupply;
    mapping(uint => string) eggIdToHiddenImg;
    mapping(uint => string) eggIdToAnnimation;
    mapping(uint => uint[]) radomSetToEggIds; // need to add this. 
    mapping(uint => uint) eggIdToAvType;
    mapping(uint => uint) randomSetTotalSupply;

    constructor(){
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(UPDATER_ROLE, msg.sender);
        _setupRole(CONTRACT_ROLE, msg.sender);
    }

    function addContractRoles(address contractAddress)external onlyRole(UPDATER_ROLE){
        _grantRole(CONTRACT_ROLE, contractAddress);
    }

    function setUpInterfaces(address eggColl)external onlyRole(UPDATER_ROLE){
        EGG_COLLECTION_CONTRACT = eggColl;
        EggColContract = EggCollectionInter(EGG_COLLECTION_CONTRACT);
    }

    function uploadEggData(EggUpload[] calldata eggs)external onlyRole(UPDATER_ROLE){
        for (uint i; i<eggs.length;i++){
            uint _id = eggs[i].id;
            Egg memory egg = Egg({
                id: _id,
                name: eggs[i].name,
                description: eggs[i].description,
                veneer: eggs[i].veneer,
                durability: eggs[i].durability,
                origin: eggs[i].origin,
                temperature: eggs[i].temperature,
                buoyancy: eggs[i].buoyancy,
                energyLevel: eggs[i].energyLevel,
                color: eggs[i].color,
                procurementDifficulty: eggs[i].procurementDifficulty,
                rarity: eggs[i].rarity
            });
            eggIdToData[_id] = egg;
            eggIdToHiddenImg[_id] = eggs[i].eggImg;
            eggIdToAnnimation[_id] = eggs[i].eggAnni;
            eggIdToAvType[_id] = eggs[i].avType;
            radomSetToEggIds[eggs[i].randomSet].push(_id);
            if(_id > _maxId){
                _maxId = _id;
            }
        }
    }

    function setEggRandomSet(uint eggId, uint randomSet)external onlyRole(UPDATER_ROLE){
        radomSetToEggIds[randomSet].push(eggId);
    }

    function setEggToAvType(uint eggId, uint avType)external onlyRole(UPDATER_ROLE){
        eggIdToAvType[eggId] = avType;
    }

    function setEggImage(uint eggId, string calldata img)external onlyRole(UPDATER_ROLE){
        eggIdToHiddenImg[eggId] = img;
    }

    function setEggAnimation(uint eggId, string calldata ani)external onlyRole(UPDATER_ROLE){
        eggIdToHiddenImg[eggId] = ani;
    }

    function setEggPrice(uint eggId, uint price)external onlyRole(UPDATER_ROLE){
        eggIdToPrice[eggId] = price;
    }

    function setEggSupply(uint eggId, uint supply)external onlyRole(UPDATER_ROLE){
        idToSupply[eggId] = supply;
    }

    function uploadRevealData(Revealed[] calldata reveals)external onlyRole(UPDATER_ROLE){
        for(uint i; i < reveals.length; i++){
            uint _id = reveals[i].id;
            eggIdToPossibleReveals[reveals[i].eggId].push(_id);
            revIdToRevealedData[_id] = reveals[i];
            if(_id > _maxRevId){
                _maxRevId = _id;
            }
        }
    }

    function setRevealedSupply(uint revId, uint supply)external onlyRole(UPDATER_ROLE){
        revIdToSupply[revId] = supply;
    }

    function pickRevealItem(uint eggId)external view returns(uint){
        uint[] memory options = eggIdToPossibleReveals[eggId];
        
        uint random = uint(blockhash(block.number - 1)) % options.length;
        console.log(random);
        if(revIdToSupply[options[random]] > 0){
            return options[random];
        }else{
            uint i = 0;
            while(revIdToSupply[options[random]] <= 0){
                random = uint(blockhash(block.number - 1)) % options.length;
                if(i>150){
                    //to project against infinate loope
                    break;
                }
                i += 1;
            }
            return options[random];
        }
    }

    function changeRevealedImage(uint revId, string calldata img)external onlyRole(UPDATER_ROLE){
        Revealed memory rev = revIdToRevealedData[revId];
        rev.image = img; 
        revIdToRevealedData[revId] = rev;
    }

    function chnageRevealedAnnimation(uint revId, string calldata anni)external onlyRole(UPDATER_ROLE){
        Revealed memory rev = revIdToRevealedData[revId];
        rev.animation = anni; 
        revIdToRevealedData[revId] = rev;
    }

    function okToMint(uint eggId)external view returns(bool){
        if(idToSupply[eggId] > 0){
            return true;
        }else{
            return false;
        }
    }

    function okToAttach(uint eggId, uint avType)external view returns(bool){
        uint eggAv = eggIdToAvType[eggId];
        if(eggAv == 0){
            return true;
        }else if(eggAv == avType){
            return true;
        }else{
            return false;
        }
    }


    function eggIdToMint(uint randomSet)external view returns(uint){
       
        uint[] memory options = radomSetToEggIds[randomSet];
        uint random = uint(blockhash(block.number - 1)) % options.length;
        
        if(idToSupply[options[random]] > 0){
            return options[random];
        }else{
            uint i = 0; 
            while(revIdToSupply[options[random]] <= 0){
                random = uint(blockhash(block.number - 1)) % options.length;
                if (i>100){
                    //protecting aginst infinate loop if all sold out 
                    break;
                }
                i += 1;
            }
            return options[random];
        }
    }

    function eggPrice(uint eggId)external view returns(uint) {
        return eggIdToPrice[eggId];
    }

    function eggsAttachedTokenUri(uint[] memory eggsTokenIds)external view returns(bytes memory){
        bytes memory dataURI = abi.encodePacked("");
        for(uint i; i<eggsTokenIds.length;i++){
            uint egg = EggColContract.getRevIdFromTokenId(eggsTokenIds[i]);
            dataURI = abi.encodePacked(
                dataURI,
                '"},{ "trait_type": "Pet ',
                Strings.toString(i),
                '", "value": "',
                revIdToRevealedData[egg].name
            );
        }
        return dataURI;
    }

    function eggRevID(uint eggTokenId)external view returns(uint){
        
        return EggColContract.getRevIdFromTokenId(eggTokenId);
    }

    function revealedSecontOne(uint itemId)internal view returns(bytes memory){
        string memory img = revIdToRevealedData[itemId].image;
        bytes memory dataURI = abi.encodePacked(
            '{"name": "',
            revIdToRevealedData[itemId].name,
            '", "description": "',
            revIdToRevealedData[itemId].description,
            '", "image": "',
            img,
            '","animation_url:"',
            revIdToRevealedData[itemId].animation,
            '", "attributes": [{ "trait_type": "Egg", "value": "',
            eggIdToData[revIdToRevealedData[itemId].eggId].name,
            '"},{ "trait_type": "Weight", "value": "',
            revIdToRevealedData[itemId].weight);
        return dataURI;
    }

    function individualTokenUriRevealed(uint itemId)external view returns(string memory){
        bytes memory dataURI = abi.encodePacked(
            revealedSecontOne(itemId),
            // '"},{ "trait_type": "Sound", "value": "',
            // revIdToRevealedData[itemId].sound,
            '"},{ "trait_type": "Byproduct", "value": "',
            revIdToRevealedData[itemId].byproduct,
            '"},{ "trait_type": "Element", "value": "',
            revIdToRevealedData[itemId].element,
            '"},{ "trait_type": "Lethality", "value": "',
            Strings.toString(revIdToRevealedData[itemId].lethality),
            '"},{ "trait_type": "Enviroment", "value": "',
            revIdToRevealedData[itemId].environment,
            '"},{ "trait_type": "Diet", "value": "',
            revIdToRevealedData[itemId].diet,
            '"},{ "trait_type": "Maturity Rate", "value": "',
            revIdToRevealedData[itemId].maturityRate,
            '"},{ "trait_type": "Adult Size Estimation", "value": "',
            Strings.toString(revIdToRevealedData[itemId].adultSizeEstimation),
            '"},{ "trait_type": "Mythical Approximation", "value": "',
            revIdToRevealedData[itemId].mythicalApproximation,
            '"}]}'
        );
        return string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(dataURI)
                )
            );
    }

    function eggSectionOne(uint itemId)internal view returns(bytes memory){
        bytes memory dataURI = abi.encodePacked(
            '{"name": "',
            eggIdToData[itemId].name,
            '", "description": "',
            eggIdToData[itemId].description,
            '", "image": "',
            eggIdToHiddenImg[itemId],
            '","animation_url:"',
            eggIdToAnnimation[itemId],
            '", "attributes": [{ "trait_type": "Veneer", "value": "',
            eggIdToData[itemId].veneer,
            '"},{ "trait_type": "Durability", "value": "',
            eggIdToData[itemId].durability);
        return dataURI;
    }

    function individualTokenUri(uint itemId)external view returns(string memory){
        bytes memory dataURI = abi.encodePacked(
            eggSectionOne(itemId),
            
            '"},{ "trait_type": "Origin", "value": "',
            eggIdToData[itemId].origin,
            '"},{ "trait_type": "Temperature", "value": "',
            eggIdToData[itemId].temperature,
            '"},{ "trait_type": "Buoyancy", "value": "',
            eggIdToData[itemId].buoyancy,
            '"},{ "trait_type": "Energy Level", "value": "',
            Strings.toString(eggIdToData[itemId].energyLevel),
            '"},{ "trait_type": "Colour", "value": "',
            eggIdToData[itemId].color,
            '"},{ "trait_type": "Procurement Difficulty", "value": "',
            Strings.toString(eggIdToData[itemId].procurementDifficulty),
            '"},{ "trait_type": "Procurement Difficulty", "value": "',
            Strings.toString(eggIdToData[itemId].rarity),
            '"}]}'
        );
        return string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(dataURI)
                )
            );
    }
}

