    function _checkCountrys(Country[] memory _countrys, Country _champion)
        internal
        pure
    {
        require(
            _countrys.length == 7 || _countrys.length == 8,
            "Country length error"
        );
        for (uint256 i = 1; i < _countrys.length; i++) {
            uint256 temp = uint256(_countrys[i - 1]);
            uint256 country = uint256(_countrys[i]);
            require(
                temp != uint256(_champion) && country != uint256(_champion),
                "Country champion error"
            );
            require(
                temp > 0 &&
                    country > 0 &&
                    uint256(_champion) > 0 &&
                    temp < 33 &&
                    country < 33 &&
                    uint256(_champion) < 33 &&
                    country > temp,
                "Country input error"
            );
        }
    }

    function _pureLotteryPrice(Country[] memory _countrys)
        internal
        view
        returns (uint256)
    {
        uint256 total;
        for (uint256 i = 0; i < _countrys.length - 1; i++) {
            uint256 country = uint256(_countrys[i]);
            total = total.add(countryPrice[country]);
        }
        return total;
    }

    function _pureLotteryId(Country[] memory _countrys, Country _champion)
        internal
        pure
        returns (uint256)
    {
        uint256 ida;
        uint256 idb = 1;
        for (uint256 i = 0; i < _countrys.length; i++) {
            uint256 country = uint256(_countrys[i]);
            ida = ida.add(country);
            idb = idb.mul(country);
        }
        ida = ida.add(uint256(_champion));
        idb = idb.mul(uint256(_champion));
        return ida.mul(idb);
    }

    function _fillingCountrys(Country[] memory _countrys)
        internal
        pure
        returns (Country[8] memory)
    {
        Country[8] memory _co;
        for (uint256 i = 0; i < _countrys.length; i++) {
            _co[i] = _countrys[i];
        }
        return _co;
    }

    function _findLotteryId(BetInfo[] memory _infos, uint256 _id)
        internal
        pure
        returns (bool, uint256)
    {
        bool result;
        uint256 index;
        for (uint256 i = 0; i < _infos.length; i++) {
            if (_infos[i].id == _id) {
                result = true;
                index = i;
            }
        }
        return (result, index);
    }

    function _countrysToUint(Country[] memory _countrys)
        internal
        pure
        returns (uint256[8] memory)
    {
        uint256[8] memory _cous;
        for (uint256 i = 0; i < _countrys.length; i++) {
            uint256 country = uint256(_countrys[i]);
            _cous[i] = country;
        }
        return _cous;
    }

    function _pureWinningPrize(Country[8] memory _countrys, Country _champion)
        internal
        view
        returns (uint256)
    {
        uint256 temp;
        for (uint256 i = 0; i < _countrys.length; i++) {
            if (uint256(_countrys[i]) != 0) {
                uint256 country = uint256(_countrys[i]);
                if (_WorldCupMap[country]) {
                    temp = temp.add(1);
                }
            }
        }
        if (_champion != WorldCup[7] && temp == 7) {
            return 2;
        }
        if (_champion == WorldCup[7]) {
            if (temp == 7) {
                return 1;
            } else if (temp == 6) {
                return 3;
            } else if (temp == 5) {
                return 4;
            } else if (temp == 4) {
                return 5;
            } else if (temp == 3) {
                return 3;
            }
        }
        return 0;
    }

    function _isContract(address _addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(_addr)
        }
        return size > 0;
    }
