// File: contracts/cup/index.sol



pragma solidity ^0.8.0;







contract Cup is AccessControl, Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    Status public CupStatu;
    IERC20 public payToken;
    IERC20 public favToken;
    address public feeAddress;

    uint256 fee = 2000; // 20%
    uint256 multiple_count = 2;
    uint256 public expiration_time = 2 * 30 * 24 * 3600;

    uint256 public total_amount;
    mapping(uint256 => uint256) public countryPrice;
    mapping(address => uint256) public favTokens;
    mapping(uint256 => uint256) all_prize;

    struct BetInfo {
        Country[8] countrys;
        Country champion;
        uint256 id;
        uint256 prize;
        uint256 tickets;
        bool claimable;
    }

    mapping(uint256 => uint256) public prize_tickets;
    mapping(address => BetInfo[]) public Bets;

    Country[8] public WorldCup; //  Country[7], champion
    mapping(uint256 => bool) private _WorldCupMap;

    enum Status {
        Pending,
        Start,
        Close,
        Apply,
        Claim
    }

    enum Country {
        Total,
        Qatar,
        Brazil,
        Belgium,
        France,
        Argentina,
        England,
        Spain,
        Portugal,
        Mexico,
        Netherlands,
        Denmark,
        Germany,
        Uruguay,
        Switzerland,
        UnitedStates,
        Croatia,
        Senegal,
        Iran,
        Japan,
        Morocco,
        Serbia,
        Poland,
        SouthKorea,
        Tunisia,
        Cameroon,
        Canada,
        Ecuador,
        SaudiArabia,
        Ghana,
        Wales,
        CostaRica,
        Australia
    }

    event HappyCupUser(
        address indexed caller,
        uint256[8] countrys,
        uint256 indexed champion,
        uint256 count,
        address invite_code
    );

    event ApplyCupUser(
        address indexed caller,
        uint256[8] countrys,
        uint256 indexed champion,
        uint256 count
    );
    event ClaimCupUser(
        address indexed caller,
        uint256[8] countrys,
        uint256 indexed champion,
        uint256 count
    );

    modifier notContract() {
        require(!_isContract(msg.sender), "Contract not allowed");
        require(msg.sender == tx.origin, "Proxy contract not allowed");
        _;
    }

    constructor() {
        all_prize[1] = 4000; // 40%
        all_prize[2] = 2000;
        all_prize[3] = 1000;
        all_prize[4] = 500;
        all_prize[5] = 300;
        all_prize[6] = 200;
    }

    function setCupStatus(Status _status) external onlyOwner {
        CupStatu = _status;
    }

    function setPayToken(address _addr) external onlyOwner {
        payToken = IERC20(_addr);
    }

    function setFavToken(address _addr) external onlyOwner {
        favToken = IERC20(_addr);
    }

    function setFeeAddress(address _addr) external onlyOwner {
        feeAddress = _addr;
    }

    function setCountryPrice(
        uint256[] memory _countrys,
        uint256[] memory _prices
    ) external onlyOwner {
        for (uint256 i = 0; i < _countrys.length; i++) {
            countryPrice[_countrys[i]] = _prices[i];
        }
    }

    function setPrize(Country[7] memory _countrys, Country _champion)
        external
        onlyOwner
    {
        require(CupStatu == Status.Close, "World Cup is not Close");
        for (uint256 i = 0; i < _countrys.length; i++) {
            WorldCup[i] = _countrys[i];
            _WorldCupMap[uint256(_countrys[i])] = true;
        }
        WorldCup[7] = _champion;
    }

    function happyCup(
        Country[] memory _countrys,
        Country _champion,
        uint256 _multiple,
        uint256 _tokenNum,
        address _invite_code
    ) external notContract nonReentrant {
        require(CupStatu == Status.Start, "World Cup is not start");
        _checkCountrys(_countrys, _champion);

        require(_multiple >= 1, "tickets must more than one");
        uint256 piece = _pureLotteryPrice(_countrys);
        uint256 amount = piece.mul(_multiple);
        uint256 discount = _multiple.div(10);
        if (discount > 0) {
            amount = piece.mul(_multiple.sub(discount));
        }
        if (_countrys.length == 8) {
            amount = amount.mul(multiple_count);
        }
        require(
            payToken.allowance(msg.sender, address(this)) >= amount,
            "Token allowance value is below the amount"
        );
        require(
            payToken.transferFrom(msg.sender, address(this), amount),
            "Token value sent is below the amount"
        );
        uint256 treasury = amount.mul(fee).div(10000);
        payToken.safeTransfer(feeAddress, treasury);

        total_amount = total_amount.add(amount.sub(treasury));

        if (_countrys.length == 8) {
            uint256 count;
            if (_tokenNum > 0) {
                count = _tokenNum.div(10**18);
            }
            require(count >= 1, "Token value is below the min");
            require(
                favToken.allowance(msg.sender, address(this)) >= _tokenNum,
                "Token allowance value is below the amount"
            );
            require(
                favToken.transferFrom(msg.sender, address(this), _tokenNum),
                "Token value sent is below the amount"
            );
            favTokens[msg.sender] = favTokens[msg.sender].add(_tokenNum);
        }

        uint256 _id = _pureLotteryId(_countrys, _champion);
        (bool find, uint256 idx) = _findLotteryId(Bets[msg.sender], _id);
        if (find) {
            Bets[msg.sender][idx].tickets = Bets[msg.sender][idx].tickets.add(
                _multiple
            );
        } else {
            BetInfo memory bet = BetInfo({
                countrys: _fillingCountrys(_countrys),
                champion: _champion,
                id: _id,
                prize: 0,
                tickets: _multiple,
                claimable: false
            });
            Bets[msg.sender].push(bet);
        }
        emit HappyCupUser(
            msg.sender,
            _countrysToUint(_countrys),
            uint256(_champion),
            _multiple,
            _invite_code
        );
    }

    function applyCup(Country[] memory _countrys, Country _champion)
        external
        notContract
        nonReentrant
    {
        require(CupStatu == Status.Apply, "World Cup is not in apply");
        uint256 _id = _pureLotteryId(_countrys, _champion);
        (bool find, uint256 idx) = _findLotteryId(Bets[msg.sender], _id);
        require(find, "Do not own the ticket");
        BetInfo memory info = Bets[msg.sender][idx];
        require(info.prize == 0, "Have already applied");
        uint256 _prize = _pureWinningPrize(info.countrys, info.champion);
        require(_prize > 0, "Do not win");
        prize_tickets[_prize] = prize_tickets[_prize].add(info.tickets);
        Bets[msg.sender][idx].prize = _prize;

        emit ApplyCupUser(
            msg.sender,
            _countrysToUint(_countrys),
            uint256(_champion),
            info.tickets
        );
    }

    function claimCup(Country[] memory _countrys, Country _champion)
        external
        notContract
        nonReentrant
    {
        require(CupStatu == Status.Claim, "World Cup is not in Claim");
        uint256 _id = _pureLotteryId(_countrys, _champion);
        (bool find, uint256 idx) = _findLotteryId(Bets[msg.sender], _id);
        require(find, "Do not own the ticket");
        BetInfo memory info = Bets[msg.sender][idx];
        require(info.prize > 0, "Do not win");
        require(!info.claimable, "Claimable finished");
        Bets[msg.sender][idx].claimable = true;
        uint256 claim_amount = total_amount
            .mul(info.tickets)
            .div(prize_tickets[info.prize])
            .mul(all_prize[info.prize])
            .div(10000);
        payToken.safeTransfer(msg.sender, claim_amount);

        emit ClaimCupUser(
            msg.sender,
            _countrysToUint(_countrys),
            uint256(_champion),
            info.tickets
        );
    }

    function pickUpFavToken() external {
        require(
            CupStatu == Status.Apply || CupStatu == Status.Claim,
            "World Cup is not in Claim"
        );
        require(favTokens[msg.sender] > 0, "Do not own the token");
        favToken.safeTransfer(msg.sender, favTokens[msg.sender]);
        favTokens[msg.sender] = 0;
    }

