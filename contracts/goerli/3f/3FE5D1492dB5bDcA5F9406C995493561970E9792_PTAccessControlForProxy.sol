pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";

contract PTWalletManager is
    PausableUpgradeable,
    AccessControlEnumerableUpgradeable,
    UUPSUpgradeable,
    ReentrancyGuardUpgradeable
{
    using AddressUpgradeable for address;
    using SafeMathUpgradeable for uint256;
    address private _accessControl;
    address private _eventLogger;

    struct Balance {
        address _address;
        uint256 _balance;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(address admin_) public initializer {
        __UUPSUpgradeable_init();
        __ReentrancyGuard_init();
        __Pausable_init();
        __AccessControlEnumerable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, admin_);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {}

    function balancesOf(address[] memory addresses)
        public
        view
        returns (Balance[] memory balances)
    {
        balances = new Balance[](addresses.length);
        for (uint8 i = 0; i < addresses.length; ) {
            balances[i]._address = addresses[i];
            balances[i]._balance = address(addresses[i]).balance;
            unchecked {
                ++i;
            }
        }
    }

    function _totalBalanceof(address[] memory addresses)
        internal
        view
        returns (uint256 balance)
    {
        balance = 0;
        for (uint8 i = 0; i < addresses.length; ) {
            balance += address(addresses[i]).balance;
            unchecked {
                ++i;
            }
        }
    }

    function distribute(address[] memory addresses)
        external
        payable
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        uint256 totalOfCurrentBalances = _totalBalanceof(addresses);
        uint256 total = totalOfCurrentBalances.add(address(this).balance);
        uint256 toBeDistributed = total.div(addresses.length);

        for (uint8 i = 0; i < addresses.length; ) {
            address payable to = payable(addresses[i]);
            if (toBeDistributed > to.balance) {
                to.transfer(toBeDistributed.sub(to.balance));
            }
            unchecked {
                ++i;
            }
        }
    }
}

