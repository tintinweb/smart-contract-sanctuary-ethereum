        public
        view
        virtual
        override(ERC1155Upgradeable)
        returns (bool)
    {
        return
            hasRole(LibPT.PLATFORM_ROLE, msg.sender) ||
            super.isApprovedForAll(owner, operator);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(LibPT.DEPLOYER_ROLE)
    {}

