        _symbol = symbol_;
        __ERC1155_init(uri_);
        __AccessControl_init();
        __Pausable_init();
        __ERC1155Burnable_init();
        __ERC1155Supply_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(LibPT.DEPLOYER_ROLE, msg.sender);
        _grantRole(LibPT.PLATFORM_ROLE, msg.sender);
