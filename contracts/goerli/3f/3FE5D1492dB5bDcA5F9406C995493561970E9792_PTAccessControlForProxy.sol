import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/Create2Upgradeable.sol";

import "./LibPT.sol";

contract PTUtils is AccessControlEnumerableUpgradeable, UUPSUpgradeable {
    using AddressUpgradeable for address;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(address admin_) public initializer {
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, admin_);
        _grantRole(LibPT.DEPLOYER_ROLE, admin_);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(LibPT.DEPLOYER_ROLE)
    {}

    function computeAddress(
        bytes32 salt,
        bytes32 bytecodeHash,
        address deployer
    ) external pure returns (address) {
        return Create2Upgradeable.computeAddress(salt, bytecodeHash, deployer);
    }

    function tokensOwnedByAddress(address owner, LibPT.NFT[] calldata nfts)
        public
        view
        returns (LibPT.NFT[] memory)
    {
        uint256 total = 0;

        for (uint8 i = 0; i < nfts.length; ) {
            if (nfts[i].amount != 0) {
                uint256 balance = IERC1155(nfts[i].contractAddress).balanceOf(
                    owner,
                    nfts[i].tokenId
                );
                if (balance > 0) {
                    total += balance;
                }
            } else {
                uint256 balance = IERC721(nfts[i].contractAddress).balanceOf(
                    owner
                );
                total += balance;
            }

            unchecked {
                ++i;
            }
        }

        LibPT.NFT[] memory result = new LibPT.NFT[](total);
        uint256 k = 0;

        for (uint8 i = 0; i < nfts.length; ) {
            if (nfts[i].amount != 0) {
                uint256 balance = IERC1155(nfts[i].contractAddress).balanceOf(
                    owner,
                    nfts[i].tokenId
                );
                if (balance > 0) {
                    LibPT.NFT memory nft;
                    nft.contractAddress = nfts[i].contractAddress;
                    nft.tokenId = nfts[i].tokenId;
                    nft.amount = balance;
                    result[k] = nft;
                    unchecked {
                        ++k;
                    }
                }
            } else {
                uint256 balance = IERC721(nfts[i].contractAddress).balanceOf(
                    owner
                );

                if (balance > 0) {
                    for (uint256 j = 0; j < balance; ) {
                        uint256 tokenId = IERC721(nfts[i].contractAddress)
                            .tokenOfOwnerByIndex(owner, j);
                        LibPT.NFT memory nft;
                        nft.contractAddress = nfts[i].contractAddress;
                        nft.tokenId = tokenId;
                        nft.amount = 1;
                        result[k] = nft;
                        unchecked {
                            ++k;
                        }

                        unchecked {
                            ++j;
                        }
                    }
                }
            }

            unchecked {
                ++i;
            }
        }

        return result;
    }
}

