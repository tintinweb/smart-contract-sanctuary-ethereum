pragma solidity ^0.8.10;

import "./utils/ERC721Upgradeable.sol";
import "./utils/Ownable.sol";
import "./utils/PausableUpgradeable.sol";
import "./utils/ReentrancyGuardUpgradeable.sol";
import "./utils/EightyEightyMinterWhitelist.sol";
import "./utils/PaymentSplitterUpgradeable.sol";

import "./lib/CurrencyTransferLib.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";

contract EightyEightyMinterERC721 is
    ERC721Upgradeable,
    Ownable,
    PausableUpgradeable,
    Whitelist,
    ReentrancyGuardUpgradeable,
    PaymentSplitterUpgradeable
{

    struct GeneralSettings {
        uint256 maxSupply;
        string baseURI;
        uint256 ownerRemainingCap;
        uint256 revealAfter;
        bool revealed;
        address designatedSigner;
        address[] payees;
        uint256[] shares;
    }

    struct SaleCondition {
        bool isSaleConditionValid;
        uint256 buyLimitPerWallet;
        uint256 maxAllocatedSupply;
        uint256 remainingTokens;
        uint256 mintPrice;
        string groupID;
        address currency;
    }
    struct TokenGatedSaleCondition {
        bool isSaleConditionValid;
        uint256 minMintReq;
        uint256 remainingTokens;
        uint256 buyLimitPerWallet;
        uint256 maxAllocatedSupply;
        uint256 mintPrice;
        string groupID;
        address tokenGatedAddress;
        address currency;
    }

    /// @notice Mapping of groupId to sale condition
    mapping (uint256 => SaleCondition) public saleConditions;
    mapping (uint256 => TokenGatedSaleCondition) public tokenGatedSaleConditions;
    mapping (uint256 => mapping (address => uint256)) public saleMintTracker;
    mapping (uint256 => mapping (uint256 => uint256)) public tokenGatedTokenTracker;

    bool public isInitialized;

    bytes32 private constant MODULE_TYPE = bytes32("EightyEightyMinterERC721");
    uint256 private constant VERSION = 1;

    string public baseURI;
    address public designatedSigner;

    uint256 public maxSupply;
    uint256 public totalSupply;
    uint256 public ownerRemainingCap;

    uint256 public revealAfter;
    bool public revealed;

    modifier checkSupply(uint256 _amount) {
        require(_amount > 0, "Invalid Amount");
        require(_amount + totalSupply <= maxSupply - ownerRemainingCap, "Exceeding Max Supply");
        _;
    }

    /**
    @notice Event when sale condition is set
    @param sales Array of sale conditions
    */
    event SaleConditionsAdded(SaleCondition[] sales);

    /**
    @notice Event when sale condition is updated
    @param updatedSale Updated sale condition
    */
    event SaleConditionsUpdated(SaleCondition updatedSale);

    /**
    @notice Event when tokenGated sale condition is set
    @param tokenGatedSales Array of tokenGatedSale conditions
    */
    event TokenGatedSaleConditionsAdded(TokenGatedSaleCondition[] tokenGatedSales);

    /**
    @notice Event when tokenGated sale condition is updated
    @param updatedTokenGatedSale Updated tokenGatedSale conditions
    */
    event TokenGatedSaleConditionsUpdated(TokenGatedSaleCondition updatedTokenGatedSale);

    /**
    @notice Event when sale conditions are updated  
    @param saleConditionIndex Array Index for in which sale token is minted  
    @param buyer Buyer address  
    @param amount Amount of token minted    
    */
    event TokensMinted(uint256 indexed saleConditionIndex, address indexed buyer, uint256 amount);

    /**
    @notice Event for uri change when token are revealed   
    @param uri New uri of the collection   
    */
    event URI(string uri);

    /**
    @notice Event when token are revealed   
    @param time Time when token are revealed   
    */
    event TokensRevealed(uint256 time);

    /**
    @notice This is initializer function is used to initialize values of contracts  
    @param _name Collection name  
    @param _symbol Collection Symbol  
    @param _SIGNING_DOMAIN Signing domain  
    @param _SIGNATURE_VERSION Signing version   
    @param _generalSettings General Setting for Drop  
    */
    function initialize(
        GeneralSettings memory _generalSettings,
        string memory _name,
        string memory _symbol,
        string memory _SIGNING_DOMAIN,
        string memory _SIGNATURE_VERSION
    ) public initializer {
        require(
            _generalSettings.payees.length == _generalSettings.shares.length,
            "EightyEightyMinterERC721 Initialize: Invalid Input"
        );
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();
        __ERC721_init(_name, _symbol);
        __EightyEightyMinterSigner_init(_SIGNING_DOMAIN, _SIGNATURE_VERSION);
        __PaymentSplitter_init(_generalSettings.payees, _generalSettings.shares);

        designatedSigner = _generalSettings.designatedSigner;
        maxSupply = _generalSettings.maxSupply;
        baseURI = _generalSettings.baseURI;
        ownerRemainingCap = _generalSettings.ownerRemainingCap;
        totalSupply = 0;

        revealed = _generalSettings.revealed;
        revealAfter = _generalSettings.revealAfter;
    }

    /**
    @notice Returns the type of the contract   
    */
    function contractType() external pure returns (bytes32) {
        return MODULE_TYPE;
    }

    /**
    @notice Returns the version of the contract   
    */
    function contractVersion() external pure returns (uint8) {
        return uint8(VERSION);
    }

    /**
    @notice This is function is used to mint tokens for owner  
    @param _amount amount of tokens to mint  
    @param _accounts accounts on which you need to air drop  
    */
    function airDrop(uint256[] calldata _amount, address[] calldata _accounts) external onlyOwner {
        require(_amount.length == _accounts.length, "EightyEightyMinterERC721 AirDrop: Invalid Input");

        uint256 totalAmount = 0;
        for (uint256 i = 0; i < _amount.length; i++) {
            totalAmount += _amount[i];
        }

        require(totalAmount + totalSupply <= maxSupply, "EightyEightyMinterERC721 AirDrop: Exceeding Supply");
        require(totalAmount <= ownerRemainingCap, "EightyEightyMinterERC721 AirDrop: Exceeding Owner Allotment");

        ownerRemainingCap -= totalAmount;

        for (uint256 i = 0; i < _accounts.length; i++) {
            for (uint256 j = 0; j < _amount[i]; j++) {
                totalSupply++;
                _safeMint(_accounts[i], totalSupply);
            }
        }
    }

    /**
    @notice This is function is used to mint tokens   
    @param _whitelist whitelisting object which contains user address and backend signature for verification  
    @param _amount amount of tokens to mint in one transaction   
    */
    function allowListMint(whitelist memory _whitelist, uint256 _amount)
        external
        payable
        whenNotPaused
        nonReentrant
        checkSupply(_amount)
    {
        require(msg.sender == tx.origin, "EightyEightyMinterERC721 Mint: Only wallets allowed");

        verifySaleCondition(_whitelist.groupID, _whitelist, _amount, false);

        collectMintPrice(_whitelist.groupID, _amount, false);

        transferMintedTokens(_whitelist.groupID, _whitelist, _amount, false);

        emit TokensMinted(_whitelist.groupID, _msgSender(), _amount);
    }

    /**
    @notice This is function is used to mint token gated tokens       
    @param _whitelist whitelisting object which contains user address and backend signature for verification  
    @param _tokenIds Token gated tokens Ids held by the caller  
    @param _amounts No. of tokens the caller wishes to mint against the tokenIds  
    */
    function tokenGatedMint(
        whitelist memory _whitelist,
        uint256[] memory _tokenIds,
        uint256[] memory _amounts
    ) external payable whenNotPaused nonReentrant {
        require(msg.sender == tx.origin, "EightyEightyMinterERC721 Token Gated Mint: Only wallets allowed");
        require(_tokenIds.length == _amounts.length, "EightyEightyMinterERC721 Token Gated Mint: Invalid Input");
        // Get the sale conditions.
        require(
            _tokenIds.length >= tokenGatedSaleConditions[_whitelist.groupID].minMintReq,
            "EightyEightyMinterERC721 Token Gated Mint: Min requirement not satisfy"
        );
        uint256 totalAmountToMint = 0;

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            require(_tokenIds[i] != 0 && _amounts[i] != 0, "EightyEightyMinterERC721 Token Gated Mint: Null Input");
            require(
                IERC721Upgradeable(tokenGatedSaleConditions[_whitelist.groupID].tokenGatedAddress).ownerOf(
                    _tokenIds[i]
                ) == msg.sender,
                "EightyEightyMinterERC721 Token Gated Mint: !Owner"
            );
            require(
                _amounts[i] + tokenGatedTokenTracker[_whitelist.groupID][_tokenIds[i]] <=
                tokenGatedSaleConditions[_whitelist.groupID].buyLimitPerWallet,
                "EightyEightyMinterERC721 Token Gated Mint: Claimed Already"
            );
            totalAmountToMint += _amounts[i];
            tokenGatedTokenTracker[_whitelist.groupID][_tokenIds[i]] += _amounts[i];
        }

        require(totalAmountToMint + totalSupply <= maxSupply - ownerRemainingCap, "Exceeding Max Supply");

        verifySaleCondition(_whitelist.groupID, _whitelist, totalAmountToMint, true);
        collectMintPrice(_whitelist.groupID, totalAmountToMint, true);
        transferMintedTokens(_whitelist.groupID, _whitelist, totalAmountToMint, true);

        emit TokensMinted(_whitelist.groupID, _msgSender(), totalAmountToMint);
    }


    /**
    @notice This is function is used to verify sale conditions   
    @param _saleId Active sale id in saleLists array   
    @param _whitelist whitelisting object which contains user address and backend signature for verification  
    @param _amount amount of tokens to mint in one transaction   
    @param _isTokenGated Boolean value to know if sale is token gated   
    */
    function verifySaleCondition(
        uint256 _saleId,
        whitelist memory _whitelist,
        uint256 _amount,
        bool _isTokenGated
    ) internal view {
        require(getSigner(_whitelist) == designatedSigner, "!Signer");
        require(_whitelist.userAddress == msg.sender, "!Sender");

        if (_isTokenGated) {
            require(
                _amount <= tokenGatedSaleConditions[_saleId].remainingTokens,
                "EightyEightyMinterERC721 Token Gated Mint: All Sold Out"
            );

        } else {
            require(
                saleMintTracker[_whitelist.groupID][_whitelist.userAddress] + _amount <= saleConditions[_saleId].buyLimitPerWallet,
                "EightyEightyMinterERC721 Mint: Individual Cap exceeding"
            );
            require(
                _amount <= saleConditions[_saleId].remainingTokens,
                "EightyEightyMinterERC721 Mint: All Sold Out"
            );
        }
    }

    /**
    @notice This is function is used to collect mint price  
    @param _saleId Active sale id in saleLists array   
    @param _amount amount of tokens to mint in one transaction  
    @param _isTokenGated Boolean value to know if sale is token gated   
    */
    function collectMintPrice(
        uint256 _saleId,
        uint256 _amount,
        bool _isTokenGated
    ) internal {
        if (_isTokenGated) {
            require(
                msg.value >= _amount * tokenGatedSaleConditions[_saleId].mintPrice,
                "EightyEightyMinterERC721 Token Gated Mint: Pay Exact fees"
            );
            if (
                tokenGatedSaleConditions[_saleId].currency == CurrencyTransferLib.NATIVE_TOKEN
            ) {
                CurrencyTransferLib.transferCurrency(
                    tokenGatedSaleConditions[_saleId].currency,
                    _msgSender(),
                    address(this),
                    _amount
                );
                if (
                    (msg.value - (_amount * tokenGatedSaleConditions[_saleId].mintPrice)) > 0
                ) {
                    CurrencyTransferLib.transferCurrency(
                        tokenGatedSaleConditions[_saleId].currency,
                        address(this),
                        _msgSender(),
                        msg.value - (_amount * tokenGatedSaleConditions[_saleId].mintPrice)
                    );
                }
            } else {
                CurrencyTransferLib.transferCurrency(
                    tokenGatedSaleConditions[_saleId].currency,
                    _msgSender(),
                    owner(),
                    _amount
                );
                if (
                    (msg.value - (_amount * tokenGatedSaleConditions[_saleId].mintPrice)) > 0
                ) {
                    CurrencyTransferLib.transferCurrency(
                        tokenGatedSaleConditions[_saleId].currency,
                        owner(),
                        _msgSender(),
                        msg.value - (_amount * tokenGatedSaleConditions[_saleId].mintPrice)
                    );
                }
            }
        } else {
            require(
                msg.value >= _amount * saleConditions[_saleId].mintPrice,
                "EightyEightyMinterERC721 Mint: Pay exact fees"
            );

            if (saleConditions[_saleId].currency == CurrencyTransferLib.NATIVE_TOKEN) {
                CurrencyTransferLib.transferCurrency(
                    saleConditions[_saleId].currency,
                    _msgSender(),
                    address(this),
                    _amount
                );
                if ((msg.value - (_amount * saleConditions[_saleId].mintPrice)) > 0) {
                    CurrencyTransferLib.transferCurrency(
                        saleConditions[_saleId].currency,
                        address(this),
                        _msgSender(),
                        msg.value - (_amount * saleConditions[_saleId].mintPrice)
                    );
                }
            } else {
                CurrencyTransferLib.transferCurrency(
                    saleConditions[_saleId].currency,
                    _msgSender(),
                    owner(),
                    _amount
                );
                if ((msg.value - (_amount * saleConditions[_saleId].mintPrice)) > 0) {
                    CurrencyTransferLib.transferCurrency(
                        saleConditions[_saleId].currency,
                        owner(),
                        _msgSender(),
                        msg.value - (_amount * saleConditions[_saleId].mintPrice)
                    );
                }
            }
        }
    }

    /**
    @notice This is function is used to transfer minted tokens    
    @param _saleId Active sale id in saleLists array   
    @param _whitelist whitelisting object which contains user address and backend signature for verification  
    @param _amount amount of tokens to mint in one transaction   
    @param _isTokenGated Boolean value to know if sale is token gated  
    */
    function transferMintedTokens(
        uint256 _saleId,
        whitelist memory _whitelist,
        uint256 _amount,
        bool _isTokenGated
    ) internal {
        if (_isTokenGated) {
            tokenGatedSaleConditions[_saleId].remainingTokens -= _amount;
        } else {
            saleMintTracker[_whitelist.groupID][_whitelist.userAddress] += _amount;
            saleConditions[_saleId].remainingTokens -= _amount;
        }
        for (uint256 i = 0; i < _amount; i++) {
            totalSupply++;
            _safeMint(_whitelist.userAddress, totalSupply);
        }
    }

    /**
    @notice This function is used to set sales conditions  
    @param _sales Array of sales condition which need to set, update or delete  
    */
    function setSaleConditions(SaleCondition[] memory _sales, uint256[] memory encodedGroupId) external onlyOwner {
        for (uint256 i = 0; i < _sales.length; i++) {
            saleConditions[encodedGroupId[i]] = _sales[i];
        }
        emit SaleConditionsAdded(_sales);
    }

    function updateSaleCondition(SaleCondition memory _sale, uint256 _groupId) external onlyOwner {
        // to check if the given group id is an empty string or not.
        // abi.encodePacked("") == 0x. It's type is bytes memory
        // keccak256(0x) == 0xc5d2460186f7233c927e7db2dcc703c0e500b653ca82273b7bfad8045d85a470. It's type is bytes32
        // typecasting of bytes32 to uint256 is our output.
        // uint256(0xc5d2460186f7233c927e7db2dcc703c0e500b653ca82273b7bfad8045d85a470) = 89477152217924674838424037953991966239322087453347756267410168184682657981552
        require(uint256(keccak256(abi.encodePacked(tokenGatedSaleConditions[_groupId].groupID))) != 89477152217924674838424037953991966239322087453347756267410168184682657981552, "EightyEightyMinterERC721: Sale not found");
        saleConditions[_groupId] = _sale;
        emit SaleConditionsUpdated(_sale);
    }

    /**
    @notice This is function is used to set sales conditions  
    @param _sales Array of sales condition which need to set, update or delete  
    */
    function setTokenGatedSaleConditions(
        TokenGatedSaleCondition[] memory _sales,
        uint256[] calldata encodedGroupId
    ) external onlyOwner {
        for (uint256 i = 0; i < _sales.length; i++) {
            tokenGatedSaleConditions[encodedGroupId[i]] = _sales[i];
        }
        emit TokenGatedSaleConditionsAdded(_sales);
    }

    function updateTokenGatedSaleCondition(
        TokenGatedSaleCondition memory _sale,
        uint256 _groupId
    ) external onlyOwner {
        // to check if the given group id is an empty string or not.
        // abi.encodePacked("") == 0x. It's type is bytes memory
        // keccak256(0x) == 0xc5d2460186f7233c927e7db2dcc703c0e500b653ca82273b7bfad8045d85a470. It's type is bytes32
        // typecasting of bytes32 to uint256 is our output.
        // uint256(0xc5d2460186f7233c927e7db2dcc703c0e500b653ca82273b7bfad8045d85a470) = 89477152217924674838424037953991966239322087453347756267410168184682657981552
        require(uint256(keccak256(abi.encodePacked(tokenGatedSaleConditions[_groupId].groupID))) != 89477152217924674838424037953991966239322087453347756267410168184682657981552, "EightyEightyMinterERC721: Sale not found");
        tokenGatedSaleConditions[_groupId] = _sale;
        emit TokenGatedSaleConditionsUpdated(_sale);
    }

    /**
    @notice This function is used to reveal the token can only be called by owner  
    @dev TokensRevealed and URI event is emitted  
    */
    function revealTokens(string memory _uri) external onlyOwner {
        require(block.timestamp >= revealAfter, "Cannot reveal now");
        require(!revealed, "Already revealed");

        revealed = true;
        setBaseURI(_uri);
        emit TokensRevealed(block.timestamp);
        emit URI(_uri);
    }

    /**
    @notice Public function to withdraw minter funds  
    */
    function withdraw() external onlyOwner nonReentrant {
        require(msg.sender != address(0), "Withdraw: msg.sender to the zero address");
        release(payable(_msgSender()));
    }

    /**
    @notice The function is use to pause and unpause the mint   
    */
    function togglePause() external onlyOwner {
        if (paused()) {
            _unpause();
        } else {
            _pause();
        }
    }

    /**
    @notice This function is used to get amount of token bought by the address  
    @param _groupId GroupId id to get data from
    @param _userAddress User address to get about   
    */
    function getSaleTrackerValues(uint256 _groupId, address _userAddress) external view returns (uint256) {
        return saleMintTracker[_groupId][_userAddress];
    }

    /**
    @notice This function is used to get details of the sale condition of a particular group
    @param _groupId Group id to get data from
    */
    function getSaleConditions(uint256 _groupId) external view returns (SaleCondition memory) {
            return saleConditions[_groupId];
    }

    /**
    @notice This function is used to get details of the sale condition of a particular token gated group
    @param _groupId Group id to get data from
    */
    function getTokenGatedSaleConditions(uint256 _groupId) external view returns (TokenGatedSaleCondition memory) {
        return tokenGatedSaleConditions[_groupId];
    }

    /**
    @notice Returns the chain id of the current blockchain  
    @dev This is used to workaround an issue with ganache returning different 
    *    values from the on-chain chainId() function  
    */
    function getChainID() external view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    ////////////////
    ////Setters////
    //////////////

    /**
    @notice This function is used to set base URI  
    @param baseURI_ New Base URI  
    */
    function setBaseURI(string memory baseURI_) public onlyOwner {
        require(bytes(baseURI_).length > 0, "Invalid Base URI Provided");
        baseURI = baseURI_;
    }

    /**
    @notice This function is used to set designated signer  
    @param _signer New signer address    
    */
    function setDesignatedSigner(address _signer) external onlyOwner {
        require(_signer != address(0), "Invalid Address Provided");
        designatedSigner = _signer;
    }

    /**
    @notice This function is used to set max supply of collection  
    @param _supply New max supply     
    */
    function setMaxSupply(uint256 _supply) external onlyOwner {
        require(totalSupply < _supply, "Total Supply Exceeding");
        maxSupply = _supply;
    }

    /**
    @notice This function is used to set owner cap    
    @param _cap New owner cap       
    */
    function setOwnerCap(uint256 _cap) external onlyOwner {
        ownerRemainingCap = _cap;
    }

    /**
    @notice This function is used to set reveal after duration value    
    @param _revealAfter New revealAfter     
    */
    function setRevealAfter(uint256 _revealAfter) external onlyOwner {
        revealAfter = _revealAfter;
    }

    ////////////////
    ///Overridden///
    ////////////////

    /**
    @notice This function is used to get base URI value     
    */
    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }
}

