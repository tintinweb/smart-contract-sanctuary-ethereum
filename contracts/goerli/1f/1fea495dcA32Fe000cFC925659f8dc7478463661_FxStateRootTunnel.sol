pragma solidity ^0.8.0;

import {FxBaseRootTunnel} from "../../tunnel/FxBaseRootTunnel.sol";
import "../../lib/ERC721.sol";
import "../../lib/model.sol";

/**
 * @title FxStateRootTunnel
 */
 
contract FxStateRootTunnel is FxBaseRootTunnel {
    event BridgeMessage(address sender, uint tokenId);

    bytes public latestData;
    ERC721 erc;
    LogData[] logs;
    uint256[] versions = new uint256[](1000);

    constructor(
        address _checkpointManager,
        address _fxRoot,
        address _ercAddress
    ) FxBaseRootTunnel(_checkpointManager, _fxRoot) {
        erc = ERC721(_ercAddress);
    }

    function bridgeItem(uint256 tokenId, string memory guid) public {
        // inquiry erc
        InquiryData memory inquiry = erc.inquiryItem(tokenId);

        // add validation caller == has NFT token [unit test done]
        require(inquiry.owner == msg.sender, "only owner");

        //validation active item [unit test done]
        require(inquiry.is_active == true, "is_active is false");

        // use send message
        bytes memory data = erc.inquiryBytes2(tokenId);
        // remark for bypass testing
        _sendMessageToChild(data);

        // add to log include owner
        LogData memory log;
        log.data = data;
        log.guid = guid;
        log.sender = msg.sender;
        log.timestamp = block.timestamp;
        log.tokenId = tokenId;
        logs.push(log);

        // set item nft as disable
        erc.disableItem(tokenId);
        emit BridgeMessage(msg.sender, tokenId);
    }

    function _processMessageFromChild(bytes memory data) internal override {
        latestData = data;

        (address address1, bool active, uint256 level, uint256 tokenId,string memory guid) = abi
            .decode(data, (address, bool, uint256, uint256,string));

        console.log("_processMessageFromRoot item-0 address", address1);
        console.log("_processMessageFromRoot item-1 bool", active);
        console.log("_processMessageFromRoot item-2 level", level);
        console.log("_processMessageFromRoot item-3 tokenId", tokenId);
        console.log("_processMessageFromRoot item-4 guid", guid);

        erc.updateData(address1, level, tokenId);

        versions[tokenId] = versions[tokenId] + 1;

        // todo
        // add to log
    }

    // function _processMessageFromChild(bytes memory data) internal override {
    //     latestData = data;
    // }

    function sendMessageToChild(bytes memory message) public {
        _sendMessageToChild(message);
    }

    function getLogs() public view returns (LogData[] memory) {
        return logs;
    }

    function convertIntToString(uint256 input)
        internal
        pure
        returns (string memory)
    {
        string memory output = Strings.toString(input);
        return output;
    }

    function convertAddressToString(address input)
        internal
        pure
        returns (string memory)
    {
        string memory output = Strings.toHexString(uint160(input), 20);
        return output;
    }

    function convertBytes32ToString(bytes memory foo)
        internal
        pure
        returns (string memory)
    {
        string memory bar = string(abi.encodePacked(foo));
        return bar;
    }
}

