pragma solidity ^0.8.9;
// import "@openzeppelin/contracts/utils/escrow/Escrow.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "@openzeppelin/contracts/token/ERC777/IERC777Recipient.sol";
import "@openzeppelin/contracts/token/ERC777/IERC777.sol";
import "@openzeppelin/contracts/utils/introspection/IERC1820Registry.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "hardhat/console.sol";

error TestTokenEscrow__TokensStillVesting();
error TestTokenEscrow__InsufficientBalance();
error TestTokenEscrow__AddressMustNotBeZero();
error TestTokenEscrow__AmountMustNotBeZero();
error TestTokenEscrow__VestingPeriodNotPassed();
error TestTokenEscrow__AllVestedTokensReleased();
error TestTokenEscrow__TransferFailed();

contract TestTokenEscrow is Ownable, ReentrancyGuard, IERC777Recipient {
    uint256 public immutable intervalInSeconds;

    uint256 public immutable start;
    uint256 public immutable releaseAmountInPercent;
    uint256 public cumReleasedAmount;
    uint256 public LastReleaseTime;
    IERC777 public token;

    // Errors
    event Deposited(address indexed payee, uint256 weiAmount);
    event Withdrawn(address indexed payee, uint256 weiAmount);
    event NewWhitelistedAccountAdded(address indexed payee, uint256 weiAmount);
    event vestedTokensReleased(uint256 cumReleased, uint256 releaseTime);

    //Mapping
    mapping(address => mapping(IERC777 => uint256))
        public beneficiaryVestedTokens;

    mapping(address => mapping(IERC777 => uint256))
        public beneficiaryAmountAvailable;
    mapping(address => mapping(IERC777 => uint256))
        public beneficiaryAllocation;
    mapping(address => mapping(IERC777 => uint256))
        public beneficiaryCumReceived;
    mapping(address => mapping(IERC777 => uint256)) public balance;
    bytes public testStore;

    //Array
    address[] public earlyAdopters;

    // keccak256('ERC777TokensRecipient')
    bytes32 private constant TOKENS_RECIPIENT_INTERFACE_HASH =
        0xb281fc8c12954d22544db45de3159a39272895b169a852b314f9cc762e44c53b;
    // Shared registry address
    IERC1820Registry private registry =
        IERC1820Registry(0x1820a4B7618BdE71Dce8cdc73aAB6C95905faD24);

    /**
     * @param _interval: vesting schedule
     * @param  _start: time of vesting
     * @dev Initilise the contract
     */
    constructor(
        address _token,
        uint256 _interval,
        uint256 _start,
        uint256 _releaseAmountInPercent
    ) {
        token = IERC777(_token);

        // set the interval and start time
        intervalInSeconds = _interval;
        releaseAmountInPercent = _releaseAmountInPercent;
        start = _start;
        LastReleaseTime = block.timestamp + start;
    }

    /////////////////////
    // Main Functions //
    /////////////////////

    function setInterfaceImplementerInRegistry() external {
        // Registers itself as having the implementation of an ERC777TokensRecipient
        registry.setInterfaceImplementer(
            address(this),
            TOKENS_RECIPIENT_INTERFACE_HASH,
            address(this)
        );
    }

    // Commenting all input args as not used in test
    function tokensReceived(
        address, /*operator*/
        address, /*from*/
        address, /*to*/
        uint256 amount,
        bytes calldata userData,
        bytes calldata /*operatorData*/
    ) external override {
        // update balance of token for record keeping
        //balances[msg.sender] += amount;
        balance[address(this)][token] += amount;
        testStore = userData;
    }

    // Allows the stakeholders to withdraw the released tokens
    function withdraw() public nonReentrant {
        uint256 _amount = beneficiaryAmountAvailable[msg.sender][token];

        if (_amount <= 0) {
            revert TestTokenEscrow__InsufficientBalance();
        }

        beneficiaryAmountAvailable[msg.sender][token] = 0;
        token.send(msg.sender, _amount, "");
        // if (!success) {
        //     revert TestTokenEscrow__TransferFailed();
        // }

        emit Withdrawn(msg.sender, _amount);
    }

    /// Lockup a certain TestToken value.
    /// @ param stakeholder addresses and their total allocation
    /// @dev Add whitelisted addresses
    function whiteListedAccounts(
        address _whiteListedAddress,
        uint256 _totalAllocation
    ) external onlyOwner {
        //IERC777 _token = IERC777(_tokenAddress);
        if (_whiteListedAddress == address(0)) {
            revert TestTokenEscrow__AddressMustNotBeZero();
        }
        if (_totalAllocation <= 0) {
            revert TestTokenEscrow__AmountMustNotBeZero();
        }
        earlyAdopters.push(_whiteListedAddress);
        beneficiaryAllocation[_whiteListedAddress][token] =
            beneficiaryAllocation[_whiteListedAddress][token] +
            _totalAllocation;
        beneficiaryVestedTokens[_whiteListedAddress][
            token
        ] = beneficiaryAllocation[_whiteListedAddress][token];
        emit NewWhitelistedAccountAdded(_whiteListedAddress, _totalAllocation);
    }

    // Releases tokens based on the vesting schedule
    function releaseVestedToken() external onlyOwner {
        if (!vestingScheduleElapse()) {
            revert TestTokenEscrow__VestingPeriodNotPassed();
        }
        release();
        LastReleaseTime = block.timestamp;
        emit vestedTokensReleased(cumReleasedAmount, block.timestamp);
    }

    // Updates the available balances of all whitlisted when called
    function release() internal {
        if (address(this).balance <= 0) {
            revert TestTokenEscrow__AllVestedTokensReleased();
        } else {
            address[] memory addedAccounts = earlyAdopters;
            for (uint256 i = 0; i < addedAccounts.length; i++) {
                if (
                    beneficiaryCumReceived[addedAccounts[i]][token] >=
                    beneficiaryAllocation[addedAccounts[i]][token]
                ) {
                    beneficiaryAmountAvailable[addedAccounts[i]][
                        token
                    ] = beneficiaryAmountAvailable[addedAccounts[i]][token];
                } else {
                    uint256 _amount = (beneficiaryAllocation[addedAccounts[i]][
                        token
                    ] * releaseAmountInPercent) / 100;
                    beneficiaryAmountAvailable[addedAccounts[i]][token] =
                        beneficiaryAmountAvailable[addedAccounts[i]][token] +
                        _amount;
                    beneficiaryVestedTokens[addedAccounts[i]][token] =
                        beneficiaryVestedTokens[addedAccounts[i]][token] -
                        beneficiaryAmountAvailable[addedAccounts[i]][token];
                    beneficiaryCumReceived[addedAccounts[i]][token] += _amount;
                    cumReleasedAmount = cumReleasedAmount + _amount;
                }
            }
        }
    }

    // Monitors the vesting schedule
    function vestingScheduleElapse() internal view returns (bool) {
        uint256 _timeToRelease = LastReleaseTime + intervalInSeconds;

        if (block.timestamp >= _timeToRelease) {
            return true;
        }
        return false;
    }

    function spendTokens(
        address _tokenAddress,
        address _address,
        uint256 _amount
    ) external {
        // Try taking 1 unit of given token from given address
        IERC777(_tokenAddress).operatorSend(
            _address,
            address(this),
            _amount,
            "",
            ""
        );
    }

    /////////////////////
    // Getter Functions //
    /////////////////////

    // Returns the quantity of tokens of a whitlisted address still vestingÂ§
    function getVestedTokens(address _account) external view returns (uint256) {
        return beneficiaryVestedTokens[_account][token];
    }

    function getcontractBalance() external view returns (uint256) {
        return balance[address(this)][token];
    }
}

