pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

contract DDDProxy is Pausable, AccessControl, ReentrancyGuard, IERC721Receiver {
  uint256 public constant DDD_ORIGINAL_PRICE = 0.000666 ether;
  uint256 public constant LIMIT_PER_WALLET = 10;
  uint256 public constant LIMIT_PER_NFT = 5;
  uint256 public constant WHITELISTED_NFT_ID = 2826;
  uint256 public constant LIMIT_PER_CALL = LIMIT_PER_WALLET;

  uint256 public whitelistedMintPrice = 0.0001 ether;
  uint256 public nftOwnerMintPrice = 0.0002 ether;
  uint256 public publicMintPrice = 0.0005 ether;

  mapping(address => bool) public whitelisted;
  mapping(address => uint256) public mintedByWallet;
  mapping(uint256 => uint256) public mintedByNFT;

  IDDD public dddContract = IDDD(0x876A8e2d7369e2d6d343f1c6A676e1be0Dfd0b4D);

  modifier notContract() {
    require(!Address.isContract(msg.sender), "contract");
    _;
  }

  constructor() {
    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
  }

  function _walletLimit(uint256 _amount) private {
    require(
      mintedByWallet[msg.sender] + _amount <= LIMIT_PER_WALLET,
      "wallet limit reached"
    );
    mintedByWallet[msg.sender] += _amount;
  }

  function _nftLimit(uint256 _amount) private {
    uint256 freeSum = 0;
    uint256 balance = dddContract.balanceOf(msg.sender);
    for (uint256 i = 0; i < balance; i++) {
      uint256 tokenId = dddContract.tokenOfOwnerByIndex(msg.sender, i);
      require(tokenId <= WHITELISTED_NFT_ID, "id too high");
      uint256 diff = LIMIT_PER_NFT - mintedByNFT[tokenId];
      freeSum += diff;
      mintedByNFT[tokenId] += diff;
      if (freeSum >= _amount) {
        return;
      }
    }
    revert("nft limit reached");
  }

  function _mintAll(uint256 _amount) private {
    try dddContract.mint{value: DDD_ORIGINAL_PRICE * _amount}(_amount) {
      for (uint256 i = 0; i < _amount; i++) {
        uint256 tokenId = dddContract.tokenOfOwnerByIndex(address(this), 0);
        dddContract.safeTransferFrom(address(this), msg.sender, tokenId);
      }
      dddContract.emergencyWithdraw();
    } catch {
      revert("external call failed");
    }
  }

  function mint(uint256 _amount)
    public
    payable
    whenNotPaused
    nonReentrant
    notContract
  {
    require(_amount < LIMIT_PER_CALL, "amount too high");
    require(
      address(this).balance >= DDD_ORIGINAL_PRICE * _amount,
      "lower mint amount needed"
    );

    if (msg.value == _amount * whitelistedMintPrice) {
      require(whitelisted[msg.sender], "not whitelisted");
      _walletLimit(_amount);
    } else if (msg.value == _amount * nftOwnerMintPrice) {
      _walletLimit(_amount);
      _nftLimit(_amount);
    } else if (msg.value == _amount * publicMintPrice) {
      // no limits
    } else {
      revert("wrong amount/value");
    }
    _mintAll(_amount);
  }

  function pause() external onlyRole(DEFAULT_ADMIN_ROLE) {
    _pause();
  }

  function unpause() external onlyRole(DEFAULT_ADMIN_ROLE) {
    _unpause();
  }

  function whitelistToggle(address _address)
    external
    onlyRole(DEFAULT_ADMIN_ROLE)
  {
    whitelisted[_address] = !whitelisted[_address];
  }

  function setPrice(
    uint256 _whitelistedMintPrice,
    uint256 _nftOwnerMintPrice,
    uint256 _publicMintPrice
  ) external onlyRole(DEFAULT_ADMIN_ROLE) {
    whitelistedMintPrice = _whitelistedMintPrice;
    nftOwnerMintPrice = _nftOwnerMintPrice;
    publicMintPrice = _publicMintPrice;
  }

  function setDDD(IDDD _dddContract) external onlyRole(DEFAULT_ADMIN_ROLE) {
    dddContract = _dddContract;
  }

  function proxyTransferOwnership(address _newOwner)
    public
    onlyRole(DEFAULT_ADMIN_ROLE)
  {
    dddContract.transferOwnership(_newOwner);
  }

  function emergencyWithdraw() external payable onlyRole(DEFAULT_ADMIN_ROLE) {
    (bool sent, ) = payable(msg.sender).call{value: msg.value}("");
    require(sent, "failed");
  }

  function supportsInterface(bytes4 interfaceId)
    public
    view
    override(AccessControl)
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }

  function onERC721Received(
    address,
    address,
    uint256,
    bytes memory
  ) public virtual override onlyRole(DEFAULT_ADMIN_ROLE) returns (bytes4) {
    return this.onERC721Received.selector;
  }

  receive() external payable {}
}

abstract contract IDDD {
  function mint(uint256 amount) external payable virtual;

  function balanceOf(address owner) external virtual returns (uint256 balance);

  function transferOwnership(address newOwner) public virtual;

  function tokenOfOwnerByIndex(address owner, uint256 index)
    external
    view
    virtual
    returns (uint256);

  function safeTransferFrom(
    address,
    address,
    uint256
  ) external virtual;

  function emergencyWithdraw() external virtual;
}

