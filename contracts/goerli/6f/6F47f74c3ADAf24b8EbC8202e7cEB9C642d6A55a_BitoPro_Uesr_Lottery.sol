contract BitoPro_Uesr_Lottery is
    ERC1155,
    ERC1155Burnable,
    ERC1155Supply,
    Pausable,
    AccessControl,
    VRFConsumerBaseV2,
    ConfirmedOwner,
    ReentrancyGuard
{
    using Counters for Counters.Counter;
    using ECDSA for bytes32;
    using Strings for uint256;
    using SafeMath for uint256;

    IERC20 public usdt;
    IERC20 public bitoCoin;

    Counters.Counter public _tokenIdCounter;

    uint256 public USDTBalance = 0;
    uint256 public BitoCoinBalance = 0;

    /* Use AccessControl to signup some Role with admin. */
    bytes32 public constant LOTTERY_ROLE = keccak256("LOTTERY_ROLE");
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

    event RequestFulfilled(uint256 requestId, uint256[] randomWords);
    event Lottery(address Winner, string award, bool isWin);
    event AcceptAward(address Winner, uint256 tokenId, uint256 _value);

    struct Award {
        bool isUseUSDT; //lottery payment currency
        uint256 tokenId;
        uint256 lotteryPrice;
        uint256 USDTNumber; //number of USDT by one award.
        uint256 BitoCoinNumber; //number of Bito coin by one award.
        uint256 expiredTime;
        uint32 awardNumber; //number of award.
        uint16 awardProbability; // awardProbability/10000
    }

    struct WinAward {
        string awardName;
        uint256 tokenId;
    }

    struct Winner {
        string[] awards;
        mapping(string => uint256) countAwards;
    }

    struct ChainlinkParams {
        bytes32 keyHash;
        uint64 s_subscriptionId;
        uint32 callbackGasLimit;
        uint16 requestConfirms;
    }

    struct RequestStatus {
        bool fulfilled; // whether the request has been successfully fulfilled
        bool exists; // whether a requestId exists
        uint256[] randomWords;
    }

    //string => Award
    mapping(string => Award) public awards;
    //address => Winner
    mapping(address => Winner) winners;
    //tokenId => WinAward
    mapping(uint256 => WinAward) public tokens;

    string[] public allAwards;

    // Chainlink info
    VRFCoordinatorV2Interface public COORDINATOR;
    mapping(uint256 => RequestStatus)
        public s_requests; /* requestId --> requestStatus */
    uint64 s_subscriptionId; // Your subscription ID.
    uint256[] public requestIds; // past requests Id.
    uint256 public lastRequestId;
    bytes32 keyHash =
        0x79d3d8832d904592c0bf9818b621522c988bb8b0c05cdc3b15aea1b6e8db0c15;
    uint32 callbackGasLimit = 100000; // Depends on the number of requested values
    uint16 requestConfirmations = 3; // The default is 3, but you can set this higher.
    uint32 numWords = 1;
    address COORDINATOR_ADDRESS = 0x2Ca8E0C643bDe4C2E08ab1fA0da3401AdAD7734D;

    constructor(
        uint64 _subscriptionId,
        address _USDTAddress,
        address _bitoCoinAddress
    )
        VRFConsumerBaseV2(COORDINATOR_ADDRESS)
        ConfirmedOwner(msg.sender)
        ERC1155("https://bitolottery.com.tw/")
    {
        COORDINATOR = VRFCoordinatorV2Interface(COORDINATOR_ADDRESS);
        s_subscriptionId = _subscriptionId;
        usdt = IERC20(_USDTAddress);
        bitoCoin = IERC20(_bitoCoinAddress);
        _tokenIdCounter.increment(); //let token start from 1.
        /* DEFAULT_ADMIN_ROLE is 0x00... */
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(LOTTERY_ROLE, msg.sender);
        _grantRole(MANAGER_ROLE, msg.sender);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    /***************************
     * MODs setting award function here *
     ***************************/
    function setAward(
        string calldata _awardName, //Name of award.
        bool _isUseUSDT, //is use USDT or BitoCoin. true=>USDT false=>BitoCoin.
        uint16 _awardProbability, //Probability of award
        uint32 _awardNumber, //Number of award.
        uint256 _USDTNumber, //BitoCoin number.
        uint256 _BitoCoinNumber, //BitoCoin number.
        uint256 _lotteryPrice, //Price of lotter once award
        uint256 _expiredTime //Award expired time.
    ) external onlyRole(LOTTERY_ROLE) {
        Award storage award = awards[_awardName];
        require(award.awardNumber == 0, "award already exists!!");
        if (_USDTNumber > 0) {
            usdt.transferFrom(
                msg.sender,
                address(this),
                _USDTNumber * _awardNumber
            );
            USDTBalance += _USDTNumber * _awardNumber;
            award.USDTNumber = _USDTNumber;
        } else if (_BitoCoinNumber > 0) {
            bitoCoin.transferFrom(
                msg.sender,
                address(this),
                _BitoCoinNumber * _awardNumber
            );
            BitoCoinBalance += _BitoCoinNumber * _awardNumber;
            award.BitoCoinNumber = _BitoCoinNumber;
        } else {
            uint256 tokenId = _tokenIdCounter.current();
            award.tokenId = tokenId;
            _tokenIdCounter.increment();
        }
        award.awardNumber = _awardNumber;
        award.awardProbability = _awardProbability;
        award.lotteryPrice = _lotteryPrice;
        award.isUseUSDT = _isUseUSDT;
        award.expiredTime = _expiredTime;
        allAwards.push(_awardName);
    }

    function deleteAward(
        string calldata _awardName
    ) external onlyRole(LOTTERY_ROLE) {
        Award memory award = awards[_awardName];
        if (award.USDTNumber > 0) {
            usdt.transfer(msg.sender, award.awardNumber * award.USDTNumber);
            USDTBalance -= award.awardNumber * award.USDTNumber;
        }
        if (award.BitoCoinNumber > 0) {
            bitoCoin.transfer(
                msg.sender,
                award.awardNumber * award.BitoCoinNumber
            );
            BitoCoinBalance -= award.awardNumber * award.BitoCoinNumber;
        }
        for (uint i = 0; i < allAwards.length; i++) {
            if (
                keccak256(abi.encode(allAwards[i])) ==
                keccak256(abi.encode(_awardName))
            ) {
                allAwards[i] = allAwards[allAwards.length - 1];
                allAwards.pop();
            }
        }
        delete awards[_awardName];
    }

    function queryAllAwards() external view returns (string[] memory) {
        return allAwards;
    }

    /***************************
     * User function here *
     ***************************/

    //For demo use, if in product run, change to use ChainLink.
    function getRandomNumber(
        uint256 _saltForTest
    ) public view returns (uint256) {
        uint256 randomNumber = uint256(
            keccak256(abi.encode(block.timestamp, _saltForTest))
        );
        return randomNumber;
    }

    function lottery(
        string calldata _award,
        uint256 _saltForTest
    ) external payable {
        require(tx.origin == msg.sender, "EOA address only");
        require(msg.value == 0, "Only payable in USDT or BitoCoin");
        Award storage award = awards[_award];
        require(award.awardNumber > 0, "no more awards.");
        require(award.expiredTime > block.timestamp, "award was expired");
        if (award.isUseUSDT) {
            usdt.transferFrom(msg.sender, address(this), award.lotteryPrice);
        } else {
            bitoCoin.transferFrom(
                msg.sender,
                address(this),
                award.lotteryPrice
            );
        }
        uint16 ProbabilityNumber = 10000 / awards[_award].awardProbability;
        uint256 randomNumber1 = getRandomNumber(_saltForTest);
        uint256 randomNumber2 = uint(keccak256(abi.encode(randomNumber1)));
        if (
            randomNumber1 % ProbabilityNumber ==
            randomNumber2 % ProbabilityNumber
        ) {
            award.awardNumber -= 1;
            if (award.USDTNumber > 0) {
                usdt.transfer(msg.sender, award.USDTNumber);
                USDTBalance -= award.USDTNumber;
            } else if (award.BitoCoinNumber > 0) {
                bitoCoin.transfer(msg.sender, award.BitoCoinNumber);
                BitoCoinBalance -= award.BitoCoinNumber;
            } else {
                WinAward storage winAward = tokens[award.tokenId];
                winAward.awardName = _award;
                winAward.tokenId = award.tokenId;
                mint(msg.sender, award.tokenId, 1, "");
            }
            emit Lottery(msg.sender, _award, true);
        } else {
            emit Lottery(msg.sender, _award, false);
        }
    }

    function userAcceptAward(uint256 _tokenId, uint256 _value) external {
        require(
            balanceOf(msg.sender, _tokenId) >= _value,
            "no more award to accept."
        );
        burn(msg.sender, _tokenId, _value);
        emit AcceptAward(msg.sender, _tokenId, _value);
    }

    function userAcceptAwardMany(
        uint256[] memory _tokenId,
        uint256[] memory _value
    ) external {
        require(
            _tokenId.length == _value.length,
            "input data was wrong, array length not correct."
        );
        for (uint i = 0; i < _tokenId.length; i++) {
            require(
                balanceOf(msg.sender, _tokenId[i]) >= _value[i],
                "not enough award to accept."
            );
            emit AcceptAward(msg.sender, _tokenId[i], _value[i]);
        }
        burnBatch(msg.sender, _tokenId, _value);
    }

    function officialAcceptAward(
        address _winner,
        uint256[] memory _tokenId,
        uint256[] memory _value,
        bytes calldata _signature
    ) external onlyRole(MANAGER_ROLE) {
        require(
            _tokenId.length == _value.length,
            "input data was wrong, array length not correct."
        );
        require(
            isAuthorized(_winner, _tokenId, _value, _signature),
            "Authorized fail!!"
        );
        for (uint i = 0; i < _tokenId.length; i++) {
            require(
                balanceOf(_winner, _tokenId[i]) >= _value[i],
                "no more award to accept."
            );
            emit AcceptAward(_winner, _tokenId[i], _value[i]);
        }
        burnBatch(_winner, _tokenId, _value);
    }

    /***************************
     * Chain Link function here *
     ***************************/

    //Chain Link setup function
    function setChainLink(
        uint64 _subscriptionId,
        bytes32 _keyHash,
        uint32 _callbackGasLimit,
        uint16 _requestConfirmations,
        uint32 _numWords,
        address _coordinator_address
    ) external onlyRole(MANAGER_ROLE) {
        s_subscriptionId = _subscriptionId;
        keyHash = _keyHash;
        callbackGasLimit = _callbackGasLimit;
        requestConfirmations = _requestConfirmations;
        numWords = _numWords;
        COORDINATOR = VRFCoordinatorV2Interface(_coordinator_address);
    }

    function getRandomWord()
        public
        onlyRole(LOTTERY_ROLE)
        returns (uint256 requestId)
    {
        // Will revert if subscription is not set and funded.
        requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        s_requests[requestId] = RequestStatus({
            randomWords: new uint256[](0),
            exists: true,
            fulfilled: false
        });
        requestIds.push(requestId);
        lastRequestId = requestId;
        return requestId;
    }

    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {
        require(s_requests[_requestId].exists, "request not found");
        s_requests[_requestId].fulfilled = true;
        s_requests[_requestId].randomWords = _randomWords;
        emit RequestFulfilled(_requestId, _randomWords);
    }

    function getRequestStatus(
        uint256 _requestId
    ) external view returns (bool fulfilled, uint256[] memory randomWords) {
        require(s_requests[_requestId].exists, "request not found");
        RequestStatus memory request = s_requests[_requestId];
        return (request.fulfilled, request.randomWords);
    }

    //Verify Signature.
    function isAuthorized(
        address sender,
        uint256[] memory _tokenId,
        uint256[] memory _value,
        bytes calldata _signature
    ) private pure returns (bool) {
        bytes32 hashMsg = keccak256(abi.encodePacked(_tokenId, _value));
        bytes32 ethHashMessage = hashMsg.toEthSignedMessageHash();
        return ethHashMessage.recover(_signature) == sender;
    }

    function setURI(string memory newuri) public onlyRole(MANAGER_ROLE) {
        _setURI(newuri);
    }

    function mint(
        address account,
