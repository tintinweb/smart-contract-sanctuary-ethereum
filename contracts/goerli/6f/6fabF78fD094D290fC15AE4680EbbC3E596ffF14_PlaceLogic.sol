// Place logic
library PlaceLogic {
    using FixedPointMathLib for uint;
    using SafeTransferLib for IERC20;
    uint constant BPS = 10 ** 4;

    // See `IOrderPool` for descriptions
    event CurveUpdated(address indexed lender, Curve.Point[] curve);

    event Deposited(address indexed lender, uint amount);

    struct PlaceParams {
        Curve.Point[] curve;
        uint amount;
        address sender;
        bool aaveDeposit;
    }

    // @dev Main router function for the place logic. All the other functions
    // are internal.
    function place(
        mapping(address => Curve.Point[]) storage curves,
        mapping(address => uint) storage funds,
        IERC20 underlying,
        IModule aave,
        PlaceParams memory params
    ) public {
        if (params.aaveDeposit) aave.supply(params.amount, params.sender);
        else deposit(funds, underlying, params.amount, params.sender);
        updateCurve(curves, params.curve, params.sender);
    }

    // @dev Should fill the Lender's curve database with an incoming
    // sequence of curves.
    // TODO: add test
    function updateCurve(
        mapping(address => Curve.Point[]) storage curves,
        Curve.Point[] memory curve,
        address sender
    ) public {
        // @todo we could force a minimum maturity for the curve here
        uint maturity;
        // reset the lender curve
        delete curves[sender];
        for (uint i = 0; i < curve.length; i++) {
            require(curve[i].maturity > maturity, 'JL');
            maturity = curve[i].maturity;
            curves[sender].push(Curve.Point(curve[i].maturity, curve[i].rate));
        }
        emit CurveUpdated(sender, curves[sender]);
    }

    function deposit(
        mapping(address => uint) storage funds,
        IERC20 underlying,
        uint amount,
        address sender
    ) public {
        underlying.safeTransferFrom(sender, address(this), amount);
        funds[sender] += amount;

        emit Deposited(sender, amount);
    }
}

