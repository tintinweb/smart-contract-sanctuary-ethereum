import './IModule.sol';
import 'src/interfaces/IERC20.sol';

import '@aave/interfaces/IPool.sol';
import '@aave/interfaces/IScaledBalanceToken.sol';
import {WadRayMath} from '@aave/protocol/libraries/math/WadRayMath.sol';

/// @notice the aave reward module, deposit and withdraw to aave via our orderBook, keep track of the rewards for each depositor
contract AaveModule is IModule {
    using WadRayMath for uint;

    // @todo this is supposedly usdc, which is the underlying and not collateral
    IERC20 public collateral;
    IScaledBalanceToken public aToken;

    IPool public aavePool;

    /// @notice mount of atoken for each user
    mapping(address => uint) public deposits;

    constructor(
        IPool _aavePool,
        IERC20 _collateral,
        IScaledBalanceToken _aToken
    ) {
        aavePool = _aavePool;
        collateral = _collateral;
        aToken = _aToken;
        // give aavePool full access to the collateral
        collateral.approve(address(aavePool), type(uint).max);
    }

    function balanceOf(address account) external view returns (uint) {
        return
            deposits[account].rayMul(
                aavePool.getReserveNormalizedIncome(address(collateral))
            );
    }

    function supply(uint amount, address to) external override {
        collateral.transferFrom(to, address(this), amount);
        // retrieve the amount of atokens before supplying
        uint balanceBefore = aToken.scaledBalanceOf(address(this));
        aavePool.supply(address(collateral), amount, address(this), 0);
        deposits[to] += aToken.scaledBalanceOf(address(this)) - balanceBefore;
    }

    /// @notice withdraw from the module and return the amount of underlying asset withdrawn
    function withdraw(uint amount, address from, address to) external override {
        // retrieve the amount of atokens before supplying
        uint balanceBefore = aToken.scaledBalanceOf(address(this));
        deposits[from] -= balanceBefore - aToken.scaledBalanceOf(address(this));
        aavePool.withdraw(address(collateral), amount, to);
    }
}

