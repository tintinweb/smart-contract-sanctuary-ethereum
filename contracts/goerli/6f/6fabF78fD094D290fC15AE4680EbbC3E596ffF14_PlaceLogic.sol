pragma solidity ^0.8.17;

import '@solmate/utils/FixedPointMathLib.sol';
import '@solmate/utils/SignedWadMath.sol';

// import 'forge-std/console.sol';

library Curve {
    using FixedPointMathLib for uint;
    uint constant BPS = 10 ** 4;

    struct Point {
        uint128 maturity; // x axis of the curve, maturity in seconds
        uint24 rate; // y axis of the curve
    }

    // @notice Find the point range that has the maturity inside.
    // TODO: decide what to do in case maturity is out of range,
    // meanwhile it's returning the edges.
    function search(
        Point[] memory curve,
        uint128 maturity
    ) internal pure returns (Point memory, Point memory) {
        uint low = 0;
        uint high = curve.length - 1;

        while (low < high) {
            uint mid = (low + high + 1) / 2;
            if (curve[mid].maturity < maturity) {
                low = mid;
            } else {
                high = mid - 1;
            }
        }
        require(curve[high].maturity <= maturity, 'S:OR');
        return (curve[high], curve[high + 1]);
    }

    // @notice Calculate rate from desired maturity that
    // is between two points.
    function rate(
        Point memory point1,
        Point memory point2,
        uint maturity
    ) public pure returns (uint result) {
        // Maturity and rate difference between points
        uint mDiff = point2.maturity - point1.maturity;
        uint rDiff = point2.rate - point1.rate;

        // Proportion of the maturity difference for the desired maturity
        uint mDist = maturity - point1.maturity;
        uint mProp = (mDist * BPS) / mDiff;

        // Calculate the rate for the desired maturity
        result = point1.rate + (mProp * rDiff) / BPS;
    }

    function searchRate(
        Point[] memory curve,
        uint128 maturity
    ) external pure returns (uint) {
        (Point memory p1, Point memory p2) = search(curve, maturity);
        return rate(p1, p2, maturity);
    }
}

