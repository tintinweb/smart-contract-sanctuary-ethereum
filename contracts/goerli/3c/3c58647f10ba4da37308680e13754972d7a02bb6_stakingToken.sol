// File: stakeToken.sol



pragma solidity ^0.8.15;








contract stakingToken is Pausable, Ownable, ERC721Holder, ReentrancyGuard {

    ERC20 public token;

    struct optionsNFTStaking {
        uint256 amountStakedToken;
        uint256 lockDays;
    }

    struct userInfoStaking {
        bool isActive;
        uint256 amountStakedToken;
        uint256 startTime;
        uint256 endTime;
        uint256 stakeOptions;
        uint256 fullLockedDays;
        address nftAddress;
        uint256 tokenId;
    }

    mapping(uint256 => optionsNFTStaking) public optionsNFTStakingList;
    mapping(bytes32 => userInfoStaking) public userInfoStakingList;
    mapping(address => bool) public nftAddressList;
    uint256 public countIdOptions = 3;
    address public operatorAddress;
    uint256 public amountStakedToken;

    // 0xaD6855a4d7885A7882EdF0CE125C70A997e11172
    constructor(ERC20 _token) 
    {
        token = _token;
        optionsNFTStakingList[1] = optionsNFTStaking(uint256(200000000000000000000),uint256(300));
        optionsNFTStakingList[2] = optionsNFTStaking(uint256(500000000000000000000),uint256(600));
        nftAddressList[0x45158ff71FF6AF45f2d9C89b78feD8ba19ce8943]= true; 
        operatorAddress = 0x4D6fa494444A69ac8b2CEF6f521A14045c3e3fd7;
    }

    function setOperator(address _operatorAdd) nonReentrant public onlyOwner {
        operatorAddress = _operatorAdd;
    }

    function setNFTAddress(address[] memory _nftAddressInfo, bool state) nonReentrant public
    {
        for(uint256 i=0; i < _nftAddressInfo.length; i++)
        {
            nftAddressList[_nftAddressInfo[i]] = state;
        }
    }

    function setOptions(
        uint256[] memory _optionInfoAmountStakedToken,
        uint256[] memory _optionInfoDay
    ) nonReentrant public onlyOwner{
        require(_optionInfoAmountStakedToken.length == _optionInfoDay.length, "SetOptions: The inputs have the same length");
        for(uint256 i=0; i < _optionInfoDay.length; i++){
            optionsNFTStaking memory info = optionsNFTStaking(
                _optionInfoAmountStakedToken[i], 
                _optionInfoDay[i]
            );
            optionsNFTStakingList[countIdOptions] = info;
            countIdOptions+=1;
        }
    }

    function viewOptions(uint256 _id) public view returns(uint256, uint256)
    {
        optionsNFTStaking memory optionInfo = optionsNFTStakingList[_id];
        return (optionInfo.amountStakedToken, 
                optionInfo.lockDays);
    }

    function mintNFT(bool _isCheckOperator,address _nftAddress ,uint256 _optionsId,uint256 _tokenId) nonReentrant public {
        optionsNFTStaking memory options = optionsNFTStakingList[_optionsId];
        uint256 _amountStake = options.amountStakedToken;
        uint256 _lockDay =  options.lockDays;
        bytes32 _value = keccak256(abi.encodePacked(msg.sender, _nftAddress, _optionsId, _tokenId));

        if(_isCheckOperator == true)
        {
            require(operatorAddress == msg.sender, "You Are Not An Operator.");
            storageInfo( _value, _amountStake, _lockDay, _optionsId, _nftAddress, _tokenId);
            // IERC721(_nftAddress).safeTransferFrom(address(this), msg.sender, _tokenId);
            amountStakedToken+=1;
        }
        else
        {
            require(token.balanceOf(msg.sender) >= _amountStake,"Balance Does Not Have Enough.");
            token.transferFrom(msg.sender, address(this), _amountStake);
            storageInfo( _value, _amountStake, _lockDay, _optionsId, _nftAddress, _tokenId);
            IERC721(_nftAddress).safeTransferFrom(address(this), msg.sender, _tokenId);
            amountStakedToken+=1;
        }

    }

    function storageInfo(bytes32 _value,uint256 _amountStake,uint256 _lockDay,uint256 _optionsId,address _nftAddress,uint256 _tokenId) internal 
    {
        uint256 _endTime = block.timestamp + _lockDay;
        userInfoStaking memory info =
            userInfoStaking(
                true, 
                _amountStake,
                block.timestamp,
                _endTime, 
                _optionsId,
                _lockDay,
                _nftAddress,
                _tokenId
            );
        userInfoStakingList[_value] = info;
    } 

    function viewUserStakingInfo(address _nftAddress, uint256 _optionsId,uint256 _tokenId) public view 
        returns (bool, uint256, uint256, uint256, uint256, uint256, address, uint256)
    {
        bytes32 _value = keccak256(abi.encodePacked(msg.sender, _nftAddress, _optionsId, _tokenId));
        userInfoStaking memory info = userInfoStakingList[_value];
        return (
            info.isActive,
            info.amountStakedToken,
            info.startTime,
            info.endTime,
            info.stakeOptions,
            info.fullLockedDays,
            info.nftAddress,
            info.tokenId
        );
    }

}