    IOperations.CommittedBatchInfo latestCommittedBatch;
    /// @notice List of pending deposit records by Id
    mapping(uint256 => IOperations.PendingDepositRecord) pendingDepositRecord;
    /// @notice List of pending deposit records by Id
    mapping(uint32 => bool) isExecuted;
    /// @notice
