// File: NinjaData.sol


pragma solidity ^0.8.9;



contract NinjaData{

    uint256 public tokenId;
    string public ninja; // none or value 
    string public typestring; // none or honarary
    string public background; // deep alpha 3 xNST
    string public clothing;
    string public face; // wagmi, spectator 2 xNST 
    string public gender;
    string public hat;
    string public weapon;
    SVG public image;

    constructor(uint256 _tokenID, string memory _ninja, string memory _typestring, string memory _bg, string memory _clothing,
                string memory _face, string memory _gender,string memory _hat,  string memory _weapon, address _svg){

        tokenId = _tokenID;
        ninja = _ninja;
        typestring = _typestring;
        background = _bg;
        clothing = _clothing;
        face = _face;
        gender = _gender;
        hat = _hat;
        weapon = _weapon;
        image = SVG(_svg);

    }


}
