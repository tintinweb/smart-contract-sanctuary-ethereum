// File: OnChainNinja.sol


pragma solidity ^0.8.9;











contract OnChainNinja is ERC721, ERC721Enumerable, ERC721URIStorage, ERC721Burnable, Ownable {

    using Counters for Counters.Counter;
    using Strings for uint256;

    XNinjaSquadToken public xNinja;
    Counters.Counter private _tokenIdCounter;

    uint256 public constant ONE_DAY = 86400;
    string constant DEEP_ALPHA = "Deep Alpha";


    struct NinjaReward{

        uint256 dailyReward; // 100 ether, 200 ether, 300 ether, 500 ether
        uint256 lastRewardClaimedBlock;


    }

    mapping (uint256 => NinjaData) public tokenIDtoNinja;
    mapping (uint256 => NinjaReward) public tokenIDtoNinjaReward;
    mapping (uint256 => address) public allowlistByTokenID;


    constructor(address _xNST) ERC721("On Chain Ninja", "POOF") {

        xNinja = XNinjaSquadToken(_xNST);
        
    }

    function mint(address _ninjaData) public{
        NinjaData ninja = NinjaData(_ninjaData);
        require(msg.sender == allowlistByTokenID[ninja.tokenId()]);
        uint256 tokenId = ninja.tokenId();
        uint256 dReward = 100 ether;

        if(isEqual(ninja.face(), "WAGMI") || isEqual(ninja.face(), "Spectator")){

            if(isEqual(ninja.background(), DEEP_ALPHA)){

               dReward = 500 ether;

            }else{

                dReward = 200 ether;

            }

        }else{

            if(isEqual(ninja.background(), DEEP_ALPHA)){

                dReward = 300 ether;
            }


        }

        tokenIDtoNinja[tokenId] = ninja;

        tokenIDtoNinjaReward[tokenId] = NinjaReward({

            dailyReward: dReward,
            lastRewardClaimedBlock: block.timestamp

        });



        _tokenIdCounter.increment();
        _safeMint(msg.sender, tokenId);
        delete allowlistByTokenID[tokenId]; // delete for duplicate
        _setTokenURI(tokenId, getTokenURI(tokenId,ninja));

    }

    function getTokenURI(uint256 tokenId,NinjaData _ninja) public view returns (string memory){
        
        bytes memory name = abi.encodePacked("#",tokenId.toString()) ;

        bytes memory attributes = abi.encodePacked(

            '"attributes": [{"trait_type": "Background", "value": "', _ninja.background(), '"},',
                    '{"trait_type": "Clothing", "value": "', _ninja.clothing(), '"},',
                    '{"trait_type": "Face", "value": "', _ninja.face(), '"},',
                    '{"trait_type": "Gender", "value": "', _ninja.gender(), '"},',
                    '{"trait_type": "Hat", "value": "', _ninja.hat(), '"},',
                    '{"trait_type": "Weapon", "value": "', _ninja.weapon(), '"}',
                    ']'


        );

        if(isEqual(tokenIDtoNinja[tokenId].typestring(), "Honorary")){

            if(isEqual(tokenIDtoNinja[tokenId].ninja(),"none") != true){

                name = abi.encodePacked(_ninja.ninja());
                attributes = abi.encodePacked(

                '"attributes": [{"trait_type": "Type", "value": "', _ninja.typestring(), '"},',
                    '{"trait_type": "Ninja", "value": "', _ninja.ninja(), '"}',
                    ']'


                );
            }else{

                attributes = abi.encodePacked(


                '"attributes": [{"trait_type": "Type", "value": "', _ninja.typestring(), '"}',
                    ']'
 

                );
            }

        }

        bytes memory dataURI = abi.encodePacked(
            '{',
                '"name": "Ninja ', name, '",',
                '"description": "On-Chain Version of Ninja Squad Official Collection",',
                '"image": "', _ninja.image().generate(), '",',
                attributes,
            '}'
        );

        return string(
            abi.encodePacked(
                "data:application/json;base64,",
                Base64.encode(dataURI)
            )
        );
    }


    function calculateReward(uint256 _tokenID) public view returns(uint256){

        uint256 reward = 0;
        uint256 earned = 0;

        NinjaReward memory ninja = tokenIDtoNinjaReward[_tokenID];

        if(ninja.lastRewardClaimedBlock >= 0){

            reward = ninja.dailyReward * (block.timestamp - ninja.lastRewardClaimedBlock) / ONE_DAY;
            earned = reward / 100;

        }

        return earned;

        
    }

    function claimReward(uint256 _tokenID) public onlyHolder(_tokenID){

        uint256 calculated = calculateReward(_tokenID);

        if(calculated >= 0){

            tokenIDtoNinjaReward[_tokenID].lastRewardClaimedBlock = block.timestamp;
            xNinja.mint(msg.sender, calculated);
        }
        
    }

    function isEqual(string memory a, string memory b) public pure returns(bool){

        if(bytes(a).length != bytes(b).length){
            return false;
        }else{
            return keccak256(abi.encodePacked(a)) == keccak256(abi.encodePacked(b));
        }
    }
    
    function addAddressToAllowList(uint256[] memory _tokenID, address[] memory _tokenHolder ) public onlyOwner {
        require(_tokenID.length == _tokenHolder.length);
        for(uint i = 0; i < _tokenID.length; i++){

            allowlistByTokenID[_tokenID[i]] = _tokenHolder[i];
        }
        
    }


    function _tHolder(uint256 _tokenID) private view{
        require (ownerOf(_tokenID) == msg.sender);

    }

    modifier onlyHolder(uint256 _tokenID){
        _tHolder(_tokenID);
        _;

    }


    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(address from, address to, uint256 tokenId, uint256 batchSize)
        internal
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) onlyHolder(tokenId) {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

}