pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155URIStorage.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./royalties/ERC2981ContractWideRoyalties.sol";

contract SHRNFT_1155_Collection_v1_CustomInit is
    Initializable,
    ERC1155,
    Ownable,
    Pausable,
    ERC1155Burnable,
    ERC1155Supply,
    ERC1155URIStorage,
    ERC2981ContractWideRoyalties
{
    using Counters for Counters.Counter;

    mapping(uint256 => string) private _tokenURIs;
    Counters.Counter private _tokenIds;
    string public name;
    string public symbol;
    uint256 public totalSupply = 0;
    string public contractURI;

    constructor() ERC1155("") {
        _disableInitializers();
    }

    function initialize(
        string memory collectionName,
        string memory contractUri,
        string memory contractSymbol,
        address defaultRoyaltyRecipient,
        uint256 defaultRoyaltyValue
    ) public initializer {
        contractURI = contractUri;
        name = collectionName;
        symbol = contractSymbol;
        _setRoyalties(defaultRoyaltyRecipient, defaultRoyaltyValue);
        if (tx.origin != msg.sender) {
            _transferOwnership(tx.origin);
        }
    }

    function safeMint(
        address to,
        string memory tokenURI,
        uint256 amount
    ) public onlyOwner returns (uint256) {
        uint256 newItemId = _tokenIds.current();
        _mint(to, newItemId, amount, "");
        _setURI(newItemId, tokenURI);
        _tokenIds.increment();
        totalSupply += amount;
        return newItemId;
    }

    function safeMintBatch(
        address to,
        string[] memory tokenURIs,
        uint256[] memory amounts
    ) public onlyOwner returns (uint256[] memory) {
        require(
            tokenURIs.length == amounts.length,
            "ERC1155: Arrays length mismatch"
        );

        uint256[] memory ids;
        for (uint256 i; i < tokenURIs.length; i++) {
            uint256 newItemId = _tokenIds.current();
            ids[i] = newItemId;
            _setURI(newItemId, tokenURIs[i]);
            _tokenIds.increment();
            totalSupply += amounts[i];
        }

        _mintBatch(to, ids, amounts, "");
        return ids;
    }

    function safeMintMultipleReceivers(
        address[] memory tos,
        string memory tokenURI,
        uint256[] memory amounts
    ) public onlyOwner returns (uint256) {
        require(
            tos.length == amounts.length,
            "ERC1155: tos and amounts length mismatch"
        );
        uint256 newItemId = _tokenIds.current();
        for (uint256 i = 0; i < tos.length; i++) {
            _mint(tos[i], newItemId, amounts[i], "");
            totalSupply += amounts[i];
        }
        _setURI(newItemId, tokenURI);
        _tokenIds.increment();
        return newItemId;
    }

    function safeTransferMultipleReceiversSingleToken(
        address from,
        address[] memory tos,
        uint256 id,
        uint256[] memory amounts
    ) public {
        require(
            tos.length == amounts.length,
            "ERC1155: tos and amounts length mismatch"
        );
        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()),
            "ERC1155: caller is not owner nor approved"
        );
        for (uint256 i = 0; i < tos.length; i++) {
            _safeTransferFrom(from, tos[i], id, amounts[i], "");
        }
    }

    function safeMintBatchToken(
        address[] memory tos,
        uint256 id,
        uint256[] memory amounts
    ) public onlyOwner {
        require(
            tos.length == amounts.length,
            "ERC1155: tos and amounts length mismatch"
        );
        string memory tokenURI = uri(id);
        require(
            bytes(tokenURI).length != 0,
            "ERC1155: token must be minted before"
        );
        for (uint256 i = 0; i < tos.length; i++) {
            _mint(tos[i], id, amounts[i], "");
            totalSupply += amounts[i];
        }
    }

    function setRoyalties(address recipient, uint256 value) public onlyOwner {
        _setRoyalties(recipient, value);
    }

    function setContractURI(string memory contractUri) public onlyOwner {
        contractURI = contractUri;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC1155, ERC2981Base)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function uri(uint256 tokenId)
        public
        view
        override(ERC1155, ERC1155URIStorage)
        returns (string memory)
    {
        return super.uri(tokenId);
    }

    function setURI(string memory newuri) public onlyOwner {
        _setURI(newuri);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public onlyOwner {
        _mint(account, id, amount, data);
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public onlyOwner {
        _mintBatch(to, ids, amounts, data);
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) whenNotPaused {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }
}

