// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "./SHR_NFT_1155_V1_custom_init.sol";

contract SHRFactoryERC1155Clone is Initializable, OwnableUpgradeable, UUPSUpgradeable {

    SHRNFT_1155_Collection_v1_CustomInit[] public collections; //an array that contains different ERC1155 tokens deployed
    mapping(uint256 => address) public indexToContract; //index to contract address mapping
    mapping(uint256 => address) public indexToOwner; //index to ERC1155 owner address
    address erc1155Implementation;

    event ERC1155Created(address owner, address tokenContract); //emitted when ERC1155 token is deployed
    event ERC1155Minted(address owner, address tokenContract, uint amount); //emmited when ERC1155 token is minted

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() public initializer {
        __Ownable_init();
        __UUPSUpgradeable_init();
        erc1155Implementation = address(new SHRNFT_1155_Collection_v1_CustomInit());
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyOwner
        override
    {}

    function deployERC1155(string memory _contractName, string memory _uri, string memory _symbol, address _royaltyRecipient, uint256 _royaltyValue) public returns (address) {
        address clone = Clones.clone(erc1155Implementation);
        SHRNFT_1155_Collection_v1_CustomInit col = SHRNFT_1155_Collection_v1_CustomInit(clone);
        col.initialize(_contractName, _uri, _symbol, _royaltyRecipient, _royaltyValue);
        collections.push(col);
        indexToContract[collections.length - 1] = clone;
        indexToOwner[collections.length - 1] = tx.origin;
        emit ERC1155Created(msg.sender, clone);
        return clone;
    }


    function mintERC1155(uint256 _index, address _to, string memory _tokenURI, uint256 amount) public {

        collections[_index].safeMint(_to, _tokenURI, amount);
        emit ERC1155Minted(_to, address(collections[_index]), amount);
    }

    /*
    Helper functions below retrieve contract data given an ID or name and index in the tokens array.
    */
    function getCountERC1155byIndex(uint256 _index, uint256 _id) public view returns (uint amount) {
        return collections[_index].balanceOf(indexToOwner[_index], _id);
    }

    function getERC1155byIndexAndId(uint _index, uint _id)
        public
        view
        returns (
            address _contract,
            address _owner,
            string memory _uri,
            uint supply
        )
    {
        SHRNFT_1155_Collection_v1_CustomInit token = collections[_index];
        return (address(token), token.owner(), token.uri(_id), token.balanceOf(indexToOwner[_index], _id));
    }
}

