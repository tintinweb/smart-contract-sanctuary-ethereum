contract Wallet is OwnableUpgradeable {
    IERC20Upgradeable public token;

    //IUniswapV2Router02 public immutable uniswapV2Router;
    //address public immutable uniswapV2Pair;
    
    uint public liquidityBalance = 20 * 10 ** 6 * (10 ** 18); //Should be balance of Wallet contract;

    event Deposit(address indexed account, string id, uint indexed amount);
    event Withdraw(address indexed account, string id, uint indexed amount);
    
    mapping(address => uint256) private walletBalances;
    mapping(address => bool) public shouldBeApps;

    uint tokenPrice = 100000;

    // constructor(address _tokenAddress) {
    //     //IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
    //     token = IERC20Upgradeable(_tokenAddress);
    //     // uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).getPair(address(token), _uniswapV2Router.WETH());
    //     // uniswapV2Router = _uniswapV2Router;
    // }

    function initialize(address _tokenAddress) external initializer{
        token = IERC20Upgradeable(_tokenAddress);
          __Ownable_init();

    }
    

    function withdraw(address account, string memory id, uint _amount) public {
        require(shouldBeApps[msg.sender], 'Not allowed to access withdrawal');
        require(_amount > 0, 'Withdraw amount should be greated than 0');
        uint currentBalance = walletBalances[account];
        require(currentBalance >= _amount, 'Insufficient wallet balance to withdraw');
        walletBalances[account] = currentBalance-(_amount);

        emit Withdraw(account, id, _amount);
    }

    function depositTokens(address account, string memory id , uint _amount) external{
        require(token.balanceOf(msg.sender) >= _amount, "Insufficient balance to transfer");
        require(shouldBeApps[msg.sender], "Not allowed to access deposit");
         token.transferFrom(msg.sender, address(this), _amount);
        walletBalances[account] += _amount;
        emit Deposit(account, id, _amount);
        }
        
    
    function getApproval(uint amount) public {
        require(shouldBeApps[msg.sender], 'Not allowed to get approval');
        token.approve(msg.sender, amount);
    }

    function walletBalanceOf(address _addr) public view returns(uint){
        return walletBalances[_addr];
    }

    function setTokenAddress(address _addr) public onlyOwner {
        token = IERC20Upgradeable(_addr);
    }
    
    function setApps(address _addr) public onlyOwner {
        shouldBeApps[_addr] = true;
    }
    
    function setLiquidityBalance(uint _value) public onlyOwner {
        liquidityBalance = _value;
    }

    function swapEthToDoxa(string memory id) public payable {
        require(msg.value > 0, 'Amount should be greated than 0');
        
        //(,, uint amount) = getTokenPrice(uniswapV2Pair, msg.value);
        uint amount = tokenPrice * msg.value;
        require(liquidityBalance >= amount, 'Insufficient liquidity fund');
        uint currentBalance = walletBalances[msg.sender];
        liquidityBalance = liquidityBalance-(amount);
        walletBalances[msg.sender] = currentBalance+(amount);
        payable(owner()).transfer(msg.value);
    
        emit Deposit(msg.sender, id, amount);
    }

    function getTokenPrice(address pairAddress, uint amount) public view returns(uint, uint, uint) {
        IUniswapV2Pair pair = IUniswapV2Pair(pairAddress);
        //IERC20 token1 = IERC20(pair.token1());
        (uint Res0, uint Res1,) = pair.getReserves();

        return(Res0, Res1, (amount*Res0)/Res1); // return amount of token0 needed to buy token1
    }

    function changeTokenPrice(uint _value) public onlyOwner {
        tokenPrice = _value;
    }

    

}