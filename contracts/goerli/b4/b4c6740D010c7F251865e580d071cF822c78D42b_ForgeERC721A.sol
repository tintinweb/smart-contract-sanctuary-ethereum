pragma solidity ^0.8.15;

import "./utils/ERC721AUpgradeable.sol";
import "./utils/Ownable.sol";
import "./utils/PausableUpgradeable.sol";
import "./utils/ReentrancyGuardUpgradeable.sol";
import "./utils/EightyEightyMinterWhitelist.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";

contract ForgeERC721A is
    ERC721AUpgradeable,
    Ownable,
    PausableUpgradeable,
    Whitelist,
    ReentrancyGuardUpgradeable
{
    
    error InvalidMaxSupply(string reason);
    
    struct GeneralSettings {
        uint256 maxSupply;
        string baseURI;
        uint256 ownerReserve;
        address designatedSigner;
        bool reveal;
        address[] payees;
        uint256[] shares; // Multiply the share by 100 before entering the value
    }
	
	struct SaleCondition {
		bool isSaleConditionValid;
		uint256 buyLimitPerWallet;
		uint256 maxAllocatedSupply;
		uint256 totalTokenMinted;
		uint256 mintPrice;
		string groupID;
		address currency;
	}
	
	/// @notice Mapping of groupId to sale condition
	mapping (uint256 => SaleCondition) public saleConditions;
	mapping (uint256 => mapping (address => uint256)) public saleMintTracker;
	
	bool public isInitialized;
	
	bytes32 private constant MODULE_TYPE = bytes32("ForgeERC721A");
	uint256 private constant VERSION = 1;
	
	address[] public payees;
	uint256[] public shares;
	string public baseURI;
	address public designatedSigner;
	uint256 public maxSupply;
	uint256 public ownerMintLeft;
    uint256 public ownerReserve;
    bool public reveal;
	bool public withdrawn;

    modifier checkSupply(uint256 _amount) {
        require(_amount > 0, "Invalid Amount");
        require(_amount + totalSupply() <= maxSupply - ownerMintLeft, "Exceeding Max Supply");
        _;
    }
    
    /**
    @notice Event when sale conditions are updated
    @param saleConditionIndex Array Index for in which sale token is minted
    @param buyer Buyer address
    @param amount Amount of token minted
    */
    event TokensMinted(uint256 indexed saleConditionIndex, address indexed buyer, uint256 amount);
    
    /**
    @notice Event when sale condition is set
    @param sales Array of sale conditions
    */
    event SaleConditionsAdded(SaleCondition[] sales);
    
    /**
    @notice Event when sale condition is updated
    @param updatedSale Updated sale condition
    */
    event SaleConditionsUpdated(SaleCondition updatedSale);

    
    /**
    @notice This is initializer function is used to initialize values of contracts
    @param _name Collection name
    @param _symbol Collection Symbol
    @param _SIGNING_DOMAIN Signing domain
    @param _SIGNATURE_VERSION Signing version
    @param _generalSettings General Setting for Drop
    */
    function initialize(
        GeneralSettings memory _generalSettings,
        string memory _name,
        string memory _symbol,
        string memory _SIGNING_DOMAIN,
        string memory _SIGNATURE_VERSION
    ) public initializer {
        require(
            _generalSettings.payees.length == _generalSettings.shares.length,
            "EightyEightyMinterERC721A Initialize: Invalid Input"
        );
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();
        __ERC721A_init(_name, _symbol);
        __EightyEightyMinterSigner_init(_SIGNING_DOMAIN, _SIGNATURE_VERSION);
    
        payees = _generalSettings.payees;
        shares = _generalSettings.shares;
        designatedSigner = _generalSettings.designatedSigner;
        maxSupply = _generalSettings.maxSupply;
        baseURI = _generalSettings.baseURI;
        ownerReserve = _generalSettings.ownerReserve;
        ownerMintLeft = ownerReserve;
    }

    /**
    @notice Returns the type of the contract
    */
    function contractType() external pure returns (bytes32) {
        return MODULE_TYPE;
    }

    /**
    @notice Returns the version of the contract
    */
    function contractVersion() external pure returns (uint8) {
        return uint8(VERSION);
    }

    /**
    @notice This is function is used to mint tokens for owner
    @param _amount amount of tokens to mint
    @param _accounts accounts on which you need to air drop
    */
    function airDrop(uint256[] calldata _amount, address[] calldata _accounts) external onlyOwner {
        require(_amount.length == _accounts.length, "EightyEightyMinterERC721A AirDrop: Invalid Input");

        uint256 totalAmount = 0;
        for (uint256 i = 0; i < _amount.length; i++) {
            totalAmount += _amount[i];
        }

        require(totalAmount + totalSupply() <= maxSupply, "EightyEightyMinterERC721A AirDrop: Exceeding Supply");
        require(totalAmount <= ownerMintLeft, "EightyEightyMinterERC721A AirDrop: Exceeding Owner Allotment");

        ownerMintLeft -= totalAmount;

        for (uint256 i = 0; i < _accounts.length; i++) {
            _mint(_accounts[i], _amount[i]);
        }
    }

    /**
    @notice This is function is used to mint tokens
    @param _whitelist whitelisting object which contains user address and backend signature for verification
    @param _amount amount of tokens to mint in one transaction
    */
    function allowListMint(whitelist memory _whitelist, uint256 _amount)
        external
        payable
        whenNotPaused
        nonReentrant
        checkSupply(_amount)
    {
        require(msg.sender == tx.origin, "EightyEightyMinterERC721A Mint: Only wallets allowed");
    
        verifySaleCondition(_whitelist.groupID, _whitelist, _amount);
    
        collectMintPrice(_whitelist.groupID, _amount);
    
        transferMintedTokens(_whitelist.groupID, _whitelist, _amount);
    
        emit TokensMinted(_whitelist.groupID, _msgSender(), _amount);
    }
    
    /**
    @notice This is function is used to verify sale conditions
    @param _saleId Active sale id in saleLists array
    @param _whitelist whitelisting object which contains user address and backend signature for verification
    @param _amount amount of tokens to mint in one transaction
    */
    function verifySaleCondition(
        uint256 _saleId,
        whitelist memory _whitelist,
        uint256 _amount
    ) internal view {
        require(getSigner(_whitelist) == designatedSigner, "!Signer");
        require(_whitelist.userAddress == msg.sender, "!Sender");
        require(
            saleMintTracker[_whitelist.groupID][_whitelist.userAddress] + _amount <= saleConditions[_saleId].buyLimitPerWallet,
            "EightyEightyMinterERC721 Mint: Individual Cap exceeding"
        );
        require(
            saleConditions[_saleId].totalTokenMinted + _amount <= saleConditions[_saleId].maxAllocatedSupply,
            "EightyEightyMinterERC721 Mint: All Sold Out"
        );
    }
    
    /**
@notice This is function is used to collect mint price
    @param _saleId Active sale id in saleLists array
    @param _amount amount of tokens to mint in one transaction
    */
    function collectMintPrice(
        uint256 _saleId,
        uint256 _amount
    ) internal {
        
        if (saleConditions[_saleId].currency == address (0)) {
            require(
                msg.value == _amount * saleConditions[_saleId].mintPrice,
                "EightyEightyMinterERC721 Mint: Pay exact fees"
            );
        } else {
            IERC20Upgradeable(saleConditions[_saleId].currency).transferFrom(
                msg.sender,
                address(this),
                _amount * saleConditions[_saleId].mintPrice
            );
        }
    }
    
    /**
    @notice This is function is used to transfer minted tokens
    @param _saleId Active sale id in saleLists array
    @param _whitelist whitelisting object which contains user address and backend signature for verification
    @param _amount amount of tokens to mint in one transaction
    */
    function transferMintedTokens(
        uint256 _saleId,
        whitelist memory _whitelist,
        uint256 _amount
    ) internal {
        saleMintTracker[_whitelist.groupID][_whitelist.userAddress] += _amount;
        saleConditions[_saleId].totalTokenMinted += _amount;
        _safeMint(_whitelist.userAddress, _amount);
    }
    
    /**
    @notice This function is used to set sales conditions
    @param _sales Array of sales condition which need to set, update or delete
    */
    function setSaleConditions(SaleCondition[] memory _sales, uint256[] memory encodedGroupId) external onlyOwner {
        for (uint256 i = 0; i < _sales.length; i++) {
            saleConditions[encodedGroupId[i]] = _sales[i];
        }
        emit SaleConditionsAdded(_sales);
    }
    
    /**
        @notice This function is used to update sales conditions for normal sales
        @param _sale Sale condition which need to update
        @param _groupId Group id of the sale
    */
    function updateSaleCondition(SaleCondition memory _sale, uint256 _groupId) external onlyOwner {
        saleConditions[_groupId] = SaleCondition({
        isSaleConditionValid: _sale.isSaleConditionValid,
        buyLimitPerWallet: _sale.buyLimitPerWallet,
        maxAllocatedSupply: _sale.maxAllocatedSupply,
        totalTokenMinted: saleConditions[_groupId].totalTokenMinted,
        mintPrice: _sale.mintPrice,
        groupID: _sale.groupID,
        currency: _sale.currency
        });
        emit SaleConditionsUpdated(_sale);
    }
    
    function setPayeeAndShares(address[] memory _payees, uint256[] memory _shares) external onlyOwner {
        require(_payees.length == _shares.length, "EightyEightyMinterERC721: Payees and shares length mismatch");
        require(withdrawn == false, "EightyEightyMinterERC721: Cannot change payee after first withdraw");
        uint256 totalShares = 0;
        for (uint256 i = 0; i < _shares.length; i++) {
            totalShares += _shares[i];
        }
        require(totalShares == 10000, "EightyEightyMinterERC721: Total shares must be 10000");
        payees = _payees;
        shares = _shares;
    }
    
    /**
    @notice Public function to withdraw minter funds
    */
    function withdraw() external nonReentrant {
        require(msg.sender != address(0), "Withdraw: msg.sender to the zero address");
        withdrawn = true;
        uint256[] memory payments = new uint256[](payees.length);
        for (uint256 i = 0; i < payees.length; i++) {
            uint256 payment = (address(this).balance) * (shares[i]) / 10000;
            payments[i] = payment;
        }
        
        for (uint256 i = 0; i < payees.length; i++) {
            payable(payees[i]).transfer(payments[i]);
        }
    }
    
    /**
    @notice The function is use to pause and unpause the mint
    */
    function togglePause() external onlyOwner {
        if (paused()) {
            _unpause();
        } else {
            _pause();
        }
    }
    
    /**
    @notice This function is used to get amount of token bought by the address
    @param _groupId GroupId id to get data from
    @param _userAddress User address to get about
    */
    function getSaleTrackerValues(uint256 _groupId, address _userAddress) external view returns (uint256) {
        return saleMintTracker[_groupId][_userAddress];
    }
    
    /**
    @notice This function is used to get details of the sale condition of a particular group
    @param _groupId Group id to get data from
    */
    function getSaleConditions(uint256 _groupId) external view returns (SaleCondition memory) {
        return saleConditions[_groupId];
    }
    
    /**
    @notice Returns the chain id of the current blockchain
    @dev This is used to workaround an issue with ganache returning different
    *    values from the on-chain chainId() function
    */
    function getChainID() external view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    ////////////////
    ////Setters////
    //////////////
    
    /**
@notice This function is used to set base URI
    @param baseURI_ New Base URI
    */
    function setBaseURI(string memory baseURI_, bool isMetadataRevealed) public onlyOwner {
        require(bytes(baseURI_).length > 0, "Invalid Base URI Provided");
        baseURI = baseURI_;
        reveal = isMetadataRevealed;
    }

    /**
    @notice This function is used to set designated signer
    @param _signer New signer address
    */
    function setDesignatedSigner(address _signer) external onlyOwner {
        require(_signer != address(0), "Invalid Address Provided");
        designatedSigner = _signer;
    }
    
    /**
    @notice This function is used to set max supply of collection
    @param _supply New max supply
    */
    function setMaxSupply(uint256 _supply) external onlyOwner {
        if (totalSupply() < _supply) {
            revert InvalidMaxSupply("Error: Cannot set max supply less than current supply");
        }
        if(totalSupply() + ownerMintLeft < _supply) {
            revert InvalidMaxSupply("Error: Cannot set max supply less than current supply + owner mint left");
        }
        maxSupply = _supply;
    }
    
    /**
    @notice This function is used to set owner cap
    @dev Enter the amount that you want to change
    @dev If you want to increase the cap, Eg: 30 -> 50, then enter 20
    @dev If you want to decrease the cap, Eg: 50 -> 30, then enter -20
    @dev Decrease will only happen if the previous amount is not yet minted
    @dev Eg: If previous cap was 50 and 30 tokens are already minted, then you can only decrease the cap to 30
    @param _reserve New owner reserve
    */
    function setOwnerCap(int256 _reserve) external onlyOwner {
        if (_reserve < 0) {
            require(int256(ownerMintLeft) + _reserve >= 0, "Owner already minted more than the cap");
            ownerMintLeft = uint256(int256(ownerMintLeft) + _reserve);
            ownerReserve = uint256(int256(ownerReserve) + _reserve);
        } else {
            ownerReserve = uint256(int256(ownerReserve) + _reserve);
            ownerMintLeft = uint256(int256(ownerMintLeft) + _reserve);
        }
    }
    
    
    ////////////////
    ///Overridden///
    ////////////////

    /**
    @notice This function is used to get first token id
    */
    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }

    /**
    @notice This function is used to get base URI value
    */
    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function encodeID(string memory _str) public pure returns (uint256) {
        return uint256(keccak256(abi.encodePacked(_str)));
    }
    
    /**
    @notice This function is used to return all the payees and their shares
    */
    function getAllPayeesAndShares() external view returns(address[] memory, uint256[] memory) {
        return (payees, shares);
    }
}

