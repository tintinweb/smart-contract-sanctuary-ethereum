        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }
}


// File contracts/oracles/EventFeeds.sol

pragma solidity ^0.8.0;

contract EventFeeds is ReentrancyGuard {
    enum EventStatus {
        Pending,
        Opening,
        Completed,
        Canceled
    }

    struct Event {
        bytes32 id;
        uint256 startTime;
        uint8 goalTeamA;
        uint8 goalTeamB;
        EventStatus status;
    }

    mapping(bytes32 => Event) public events;
    mapping(bytes32 => mapping(bytes32 => uint256)) eventOptions;
    bytes32 public constant BET_1 = keccak256("BET_1");
    bytes32 public constant BET_X = keccak256("BET_X");
    bytes32 public constant BET_2 = keccak256("BET_2");

    function setEvent(Event calldata _event) external nonReentrant {
        events[_event.id] = _event;
        emit EventSetted(_event.id);
    }

    function getEvent(bytes32 _id) external view returns (Event memory _event) {
        _event = events[_id];
    }

    function updateEventOdds(
        bytes32 _id,
        uint256 _odds1,
        uint256 _oddsX,
        uint256 _odds2
    ) external nonReentrant {
         eventOptions[_id][BET_1]  = _odds1;
         eventOptions[_id][BET_X]  = _odds2;
         eventOptions[_id][BET_2]  = _oddsX;
        emit EventOddsUpdated(_id, _odds1, _oddsX, _odds2);
    }

    function getOdds(bytes32 _id, bytes32 _option) public view returns(uint256) {
        return eventOptions[_id][_option];
    }
    /*================================ EVENTS ==============================*/

    event EventOddsUpdated(
        bytes32 id,
        uint256 odds1,
        uint256 oddsX,
        uint256 odds2
    );
    event EventSetted(bytes32 id);
}