// SPDX-License-Identifier: MIT
pragma solidity 0.8.7;

import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {ERC1155} from "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import {AccessControl} from "@openzeppelin/contracts/access/AccessControl.sol";
import {Address} from "@openzeppelin/contracts/utils/Address.sol";
import {Strings} from "@openzeppelin/contracts/utils/Strings.sol";

import {ICollection} from "./interfaces/ICollection.sol";
import {IAISkinMasterCollection} from "./interfaces/IAISkinMasterCollection.sol";
import {IAISkinFactory} from "./interfaces/IAISkinFactory.sol";

contract AISkinMasterCollection is
    AccessControl,
    ERC1155,
    IAISkinMasterCollection
{
    using Address for address;
    using Strings for uint256;

    /// ============== ACCESS CONTROL ============= ///
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");
    bytes32 public constant FACTORY_ROLE = keccak256("FACTORY_ROLE");

    string private _name = "AISkins Master Collection";
    string private _symbol = "AISkins";

    // baseUri / collection address / tokenId
    string public _baseURI;

    address public aiskinFactory;

    // creator -> token -> sales
    mapping(address => mapping(address => CreatorSale)) _creatorSales;
    // token -> fees
    mapping(address => AISkinFee) _aiskinFees;

    /// @dev tokenId -> Aiskin
    mapping(uint256 => AISkin) private _aiskins;
    /// @dev tokenId -> drop collection address
    mapping(uint256 => address) private _dropReference;

    // creator -> tokenIds
    mapping(address => uint256[]) private _creatorItems;

    modifier onlyManager() {
        if (!hasRole(MANAGER_ROLE, _msgSender())) {
            revert Unauthorized(MANAGER_ROLE);
        }
        _;
    }

    modifier onlyFactory() {
        if (!hasRole(FACTORY_ROLE, _msgSender())) {
            revert Unauthorized(FACTORY_ROLE);
        }
        _;
    }

    modifier onlyCreator(uint256 tokenId) {
        if (_aiskins[tokenId].creator != _msgSender()) {
            revert CallerNotCreator();
        }
        _;
    }

    modifier onlyDropCollection() {
        // revert when caller is not on drop collections (whitelist)
        if (!IAISkinFactory(aiskinFactory).isValidCollection(_msgSender())) {
            revert Unauthorized(keccak256("DROP_COLLECTION"));
        }
        _;
    }

    modifier onlyMintable(uint256 tokenId, uint256 quantity) {
        if (_dropReference[tokenId] != address(0)) {
            revert MintDropCollection();
        }

        AISkin memory aiskin = _aiskins[tokenId];
        if (tokenId == 0 || aiskin.tokenId == 0) {
            revert MintNonExistingItem();
        }

        if (aiskin.cap == aiskin.minted) {
            revert SoldOut();
        }

        if (aiskin.minted + quantity > aiskin.cap) {
            revert MintQuantityExceedsLimit();
        }

        _;
    }

    constructor(address aiskinFactory_) ERC1155("") {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        aiskinFactory = aiskinFactory_;
    }

    function createItem(
        uint256 tokenId,
        address creator,
        uint256 cap,
        address tokenPayment,
        uint256 priceInWei,
        bool limited
    ) external payable override onlyFactory {
        _createItem(tokenId, creator, cap, tokenPayment, priceInWei, limited);
    }

    function registerDropItem(
        uint256 tokenId,
        address collection,
        uint256 cap,
        uint256 priceInWei,
        bool limited
    ) external payable override onlyFactory {
        if (
            _aiskins[tokenId].tokenId != 0 ||
            _dropReference[tokenId] != address(0)
        ) {
            revert TokenIdAlreadyExist();
        }
        _dropReference[tokenId] = collection;

        emit CreateItem(tokenId, collection, cap, priceInWei, limited);
    }

    function mintItem(
        address to,
        uint256 tokenId,
        uint256 quantity
    ) external payable override onlyMintable(tokenId, quantity) {
        AISkin memory aiskin = _aiskins[tokenId];
        uint256 amount = aiskin.priceInWei * quantity;

        uint256 fee = IAISkinFactory(aiskinFactory).calculateCommission(
            address(this),
            amount
        );

        _creatorSales[aiskin.creator][aiskin.tokenPayment].totalSales =
            _creatorSales[aiskin.creator][aiskin.tokenPayment].totalSales +
            (amount - fee);

        _aiskinFees[aiskin.tokenPayment].totalFees =
            _aiskinFees[aiskin.tokenPayment].totalFees +
            fee;
        _aiskins[tokenId].minted = aiskin.minted + quantity;

        if (aiskin.tokenPayment == address(0) && amount != msg.value) {
            revert InvalidMintAmount();
        } else if (aiskin.tokenPayment != address(0)) {
            require(
                IERC20(aiskin.tokenPayment).transferFrom(
                    _msgSender(),
                    address(this),
                    amount
                )
            );
        }

        emit MintItem(
            _msgSender(),
            to,
            aiskin.creator,
            tokenId,
            aiskin.priceInWei,
            quantity
        );

        _mint(to, tokenId, quantity, "");
    }

    function emitTransferSingle(
        address operator,
        address from,
        address to,
        uint256 id,
        uint256 amount
    ) external override onlyDropCollection {
        emit TransferSingle(operator, from, to, id, amount);
    }

    function emitTransferBatch(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts
    ) external override onlyDropCollection {
        emit TransferBatch(operator, from, to, ids, amounts);
    }

    /// @dev Limited collection cannot increase cap
    function enableLimited(
        uint256 tokenId
    ) external override onlyCreator(tokenId) {
        if (_aiskins[tokenId].limited) {
            revert AlreadyLimitedCollection();
        }

        _aiskins[tokenId].limited = true;

        emit EnableLimitedCollection(tokenId);
    }

    function increaseCap(
        uint256 tokenId,
        uint256 cap
    ) external override onlyCreator(tokenId) {
        if (_aiskins[tokenId].limited && _aiskins[tokenId].minted > cap) {
            revert UnableToIncreaseCap();
        }

        emit IncreaseCap(tokenId, _aiskins[tokenId].cap, cap);

        _aiskins[tokenId].cap = cap;
    }

    function updateBaseURI(string calldata _uri) external onlyManager {
        _baseURI = _uri;
    }

    function withdrawCreatorSale(address tokenPayment) external {
        uint256 amount = _getCreatorWithdrawableAmount(
            _msgSender(),
            tokenPayment
        );
        require(amount > 0);

        _creatorSales[_msgSender()][tokenPayment].totalSalesClaimed =
            _creatorSales[_msgSender()][tokenPayment].totalSalesClaimed +
            amount;

        if (tokenPayment == address(0)) {
            Address.sendValue(payable(msg.sender), amount);
        } else {
            require(IERC20(tokenPayment).transfer(msg.sender, amount));
        }
    }

    function withdrawAISkinFee(address tokenPayment) external onlyManager {
        uint256 amount = _getAISkinWithdrawableAmount(tokenPayment);
        require(amount > 0);

        _aiskinFees[tokenPayment].totalFeesClaimed =
            _aiskinFees[tokenPayment].totalFeesClaimed +
            amount;
        if (tokenPayment == address(0)) {
            Address.sendValue(
                payable(IAISkinFactory(aiskinFactory).getTreasury()),
                amount
            );
        } else {
            require(
                IERC20(tokenPayment).transfer(
                    IAISkinFactory(aiskinFactory).getTreasury(),
                    amount
                )
            );
        }
    }

    function getBaseUri() external view override returns (string memory) {
        return _baseURI;
    }

    function getCreatorItemIds(
        address creator
    ) external view override returns (uint256[] memory) {
        return _creatorItems[creator];
    }

    function balanceOf(
        address account,
        uint256 tokenId
    ) public view virtual override returns (uint256) {
        address _dropAddress = _dropReference[tokenId];
        if (_dropAddress != address(0)) {
            return ERC1155(_dropAddress).balanceOf(account, tokenId);
        }

        return super.balanceOf(account, tokenId);
    }

    function getAISkinInfo(
        uint256 tokenId
    ) external view override returns (AISkin memory) {
        return _aiskins[tokenId];
    }

    function getCreatorClaimableAmount(
        address creator,
        address tokenPayment
    ) external view override returns (uint256) {
        return _getCreatorWithdrawableAmount(creator, tokenPayment);
    }

    function getAISkinClamaibleFees(
        address tokenPayment
    ) external view override returns (uint256) {
        return _getAISkinWithdrawableAmount(tokenPayment);
    }

    /// @dev Publish new item
    function _createItem(
        uint256 tokenId,
        address creator,
        uint256 cap,
        address tokenPayment,
        uint256 priceInWei,
        bool limited
    ) internal {
        // check if tokenId exist
        if (
            _aiskins[tokenId].tokenId != 0 ||
            _dropReference[tokenId] != address(0)
        ) {
            revert TokenIdAlreadyExist();
        }

        _creatorItems[creator].push(tokenId);
        _aiskins[tokenId] = AISkin({
            tokenId: tokenId,
            creator: creator,
            cap: cap,
            tokenPayment: tokenPayment,
            priceInWei: priceInWei,
            minted: 0,
            limited: limited
        });

        emit CreateItem(tokenId, address(this), cap, priceInWei, limited);
    }

    function _getCreatorWithdrawableAmount(
        address creator,
        address tokenPayment
    ) internal view returns (uint256) {
        return
            _creatorSales[creator][tokenPayment].totalSales -
            _creatorSales[creator][tokenPayment].totalSalesClaimed;
    }

    function _getAISkinWithdrawableAmount(
        address tokenPayment
    ) internal view returns (uint256) {
        return
            _aiskinFees[tokenPayment].totalFees -
            _aiskinFees[tokenPayment].totalFeesClaimed;
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal virtual override {
        // revert when ids is not on master collection
        if (ids.length == 1 && _dropReference[ids[0]] != address(0)) {
            revert();
        } else {
            uint256 idsSize = ids.length;
            for (uint256 n = 0; n < idsSize; ++n) {
                require(_dropReference[ids[n]] == address(0));
            }
        }
    }

    function uri(
        uint256 _id
    ) public view virtual override returns (string memory) {
        address _uriAddrsPath = _dropReference[_id] != address(0)
            ? _dropReference[_id]
            : address(this);
        return
            string(
                abi.encodePacked(_baseURI, _uriAddrsPath, "/", _id.toString())
            );
    }

    function name() external view override returns (string memory) {
        return _name;
    }

    function symbol() external view override returns (string memory) {
        return _symbol;
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view override(ERC1155, AccessControl) returns (bool) {
        return
            ERC1155.supportsInterface(interfaceId) ||
            AccessControl.supportsInterface(interfaceId) ||
            super.supportsInterface(interfaceId);
    }
}

