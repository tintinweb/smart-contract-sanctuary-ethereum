            );
        } else if (_slot0.tick < tickUpper) {
            amountAstra = getAmount0Delta(
                sqrtPriceX96,
                TickMath.getSqrtRatioAtTick(tickUpper),
                liquidityDelta
            );
        }
        return uint256(amountAstra);
    }

