pragma solidity 0.8.17;

/**
 * @title AirdropConfig
 */
abstract contract AirdropConfig {
    struct AirdropSettings {
        bool enabled;
        bool isRange;
        uint256 quantity;
        uint256 lowerReserved;
        uint256 upperReserved;
    }

    uint256 public airdropTotalSend;

    AirdropSettings public airdropSettings =
        AirdropSettings(false, false, 0, 0, 0);

    modifier noAirdropClosed() {
        require(airdropSettings.enabled, "Airdrops are closed");
        _;
    }

    function _openAirdrop() internal virtual {
        require(!airdropSettings.enabled, "Airdrop are open");
        airdropSettings.enabled = true;
    }

    function _closeAirdrop() internal virtual {
        require(airdropSettings.enabled, "Airdrop are closed");
        airdropSettings.enabled = false;
    }

    function _isNotAirdropId(uint256 _id) internal virtual returns (bool) {
        if (airdropSettings.lowerReserved == 1) {
            return (_id > airdropSettings.upperReserved);
        }
        return (_id < airdropSettings.lowerReserved ||
            _id > airdropSettings.upperReserved);
    }

    function _resetAirdrop() internal virtual {
        airdropSettings = AirdropSettings(false, false, 0, 0, 0);
    }

    function _setAirdropSettings(
        bool _enabled,
        bool _isRange,
        uint256 _quantity,
        uint256 _lowerReserved,
        uint256 _upperReserved
    ) internal virtual {
        if (_isRange) {
            require(_lowerReserved > 0, "Lower can not be zero");
            require(_upperReserved > _lowerReserved, "Upper must be higher");
            airdropSettings = AirdropSettings(
                _enabled,
                true,
                0,
                _lowerReserved,
                _upperReserved
            );
        } else {
            require(_quantity > 0, "Quantity can not be zero");
            airdropSettings = AirdropSettings(_enabled, false, _quantity, 0, 0);
        }
    }

    function airdropRemains() public view returns (uint256) {
        if (
            airdropSettings.quantity == 0 && airdropSettings.lowerReserved == 0
        ) {
            return 0;
        }

        if (airdropSettings.isRange) {
            return airdropSettings.upperReserved - airdropTotalSend;
        }

        return airdropSettings.quantity - airdropTotalSend;
    }
}

