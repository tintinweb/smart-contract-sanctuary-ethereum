contract ERC721ABasic is
    ERC721A,
    Pausable,
    AccessControl,
    ERC721ABurnable,
    Modifiers,
    AirdropConfig,
    ReentrancyGuard,
    PlatformRoyalties,
    DefaultOperatorFilterer
{
    using Strings for uint256;
    uint256 public mintPrice;
    uint256 public maxSupply;
    uint256 public mintMax;
    uint256 public holdMax;
    string public baseURI;
    bool public open;
    address public withdrawalAddress;
    uint public immutable contractType;

    mapping(uint256 => string) private _tokenURIs;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    event PaymentReceived(address, uint256);
    event WithdrawalSuccess(uint256 amount);

    struct SettingsStruct {
        string name;
        string symbol;
        uint256 maxSupply;
        uint256 mintPrice;
        uint256 mintMax;
        uint256 holdMax;
        string baseURI;
        bool open;
        bool paused;
        uint256 totalMinted;
        uint256 totalBurned;
        address withdrawalAddress;
    }

    constructor(
        string memory name,
        string memory symbol,
        string memory _uri,
        uint256 _mintPrice,
        uint256 _maxSupply,
        uint256 _mintMax,
        uint256 _holdMax,
        address _withdrawalAddress
    ) ERC721A(name, symbol) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);

        baseURI = _uri;
        mintPrice = _mintPrice;
        maxSupply = _maxSupply;
        mintMax = _mintMax;
        holdMax = _holdMax;
        open = false;
        contractType = 0x0102;
        withdrawalAddress = _withdrawalAddress != address(0)
            ? _withdrawalAddress
            : _msgSender();
