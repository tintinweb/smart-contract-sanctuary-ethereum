        Types.TokenGatedMintArgs[] memory tmpArgs = args;

        uint256 totalTokenPrice = 0;
        for (uint i = 0; i < tmpArgs.length; ) {
            totalTokenPrice += totalPriceInTokens(
                tmpArgs[i].tokenIds,
                tmpArgs[i].amounts
            );
            unchecked {
                ++i;
            }
        }

        if (!isAcceptableEthValue(msg.value, totalTokenPrice))
            revert Errors.InsufficientFunds();
