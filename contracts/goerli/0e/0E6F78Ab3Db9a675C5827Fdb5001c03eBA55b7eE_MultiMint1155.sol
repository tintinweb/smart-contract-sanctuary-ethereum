                revert Errors.OneTokenPerPass();
            }
            unchecked {
                ++i;
            }
        }

        _;
    }

    modifier onlyWithinDeadline(uint256[] calldata deadlines) {
        for (uint256 i; i < deadlines.length; ) {
            if (block.timestamp > deadlines[i]) {
                revert Errors.MintDeadlinePassed();
            }
            unchecked {
                ++i;
            }
        }

        _;
    }

    modifier onlyValidTokens(Types.TokenGatedMintArgs[] calldata args) {
        for (uint256 i; i < args.length; ) {
            Types.TokenGatedMintArgs memory tmpArgs = args[i];

            if (
                tokenTypes[tmpArgs.pass] != TokenType.Genesis &&
                tokenTypes[tmpArgs.pass] != TokenType.Infinity
            ) {
                revert Errors.TokenNotSupported();
            }

            if (block.timestamp > tokenDeadlines[tmpArgs.tokenIds[0]][0]) {
                revert Errors.TokenSaleClosed(tmpArgs.tokenIds[0]);
            }

            unchecked {
                ++i;
            }
        }

        _;
    }

