        return stableMintPrices[tokenId] * amount;
    }

    function minimalEthPrice(uint256 tokenId) public view returns (uint256) {
        if (stableMintPrices[tokenId] == 0) return 0;
        (, int256 price, , , ) = priceAggregator.latestRoundData();
        uint256 ethPrice = uint256(price);

        return (1 ether * stableMintPrices[tokenId] * 100) / ethPrice;
    }

    function minimalEthPrice(
        uint256[] memory tokenIds,
        uint256[] memory amounts
    ) public view returns (uint256 price_) {
        for (uint256 i; i < tokenIds.length; ) {
            price_ += minimalEthPrice(tokenIds[i]) * amounts[i];
            unchecked {
                i++;
            }
        }
    }

    function totalPriceInTokens(
        uint256[] memory tokenIds,
        uint256[] memory amounts
    ) public view returns (uint256 price_) {
        for (uint256 i; i < tokenIds.length; ) {
            price_ += stableMintPrices[tokenIds[i]] * amounts[i];
            unchecked {
                i++;
            }
        }
    }

    function isAcceptableEthValue(
        uint256 msgValue,
        uint256 requiredValue // total value in Stable tokens
    ) public view returns (bool) {
        uint80 roundId;
        int256 price;
        uint256 startedAt;

        (roundId, price, startedAt, , ) = priceAggregator.latestRoundData();
        while (true) {
            uint256 ethToUSDPrice = uint256(price);
            uint256 ethPrice = (1 ether * requiredValue * 100) / ethToUSDPrice;

            if (msgValue >= ethPrice) {
                return true;
            }
            if (block.timestamp - priceAggregatorStep > startedAt) {
                break;
            }

            (roundId, price, startedAt, , ) = priceAggregator.getRoundData(
                roundId - 1
            );
        }

        return false;
    }

    function isFeatureEnabled(Feature feature) public view returns (bool) {
        return (featureEnabledBitMap & (1 << uint256(feature))) != 0;
    }

    function uri(
        uint256 tokenId_
    ) public view override returns (string memory) {
        return tokenURIs[tokenId_];
    }

    /*//////////////////////////////-////////////////////////////////
                            Private functions
    ////////////////////////////////-//////////////////////////////*/

    //every token once in a season
    //Every Inspired By Ledger token must be claimable once for every Genesis Pass token that exists in a season
    function _processGenesisPass(
        address pass,
        uint256 tokenGatedId,
        uint256 tokenId
    ) private {
        if (usedGenesisPasses[pass][tokenGatedId][seasonId][tokenId]) {
            revert Errors.TokenGatedIdAlreadyUsed(tokenGatedId);
        }

        usedGenesisPasses[pass][tokenGatedId][seasonId][tokenId] = true;
    }

    //TODO Each Infinity Pass can be used to claim one token, per wallet, from Inspired By Ledger in a given time period
    //1 token per season
    function _processInfinityPass(
        address pass,
        uint256 tokenGatedId,
        uint256 tokenId
    ) private {
        if (usedInfinityPasses[pass][tokenGatedId][msg.sender][seasonId]) {
            revert Errors.TokenGatedIdAlreadyUsedInSeason(
                tokenGatedId,
                seasonId
            );
        }
        usedInfinityPasses[pass][tokenGatedId][msg.sender][tokenId] = true;
    }

    function _mintLogic(address to, Types.MintArgs memory args) private {
        if (args.tokenIds.length != args.amounts.length) {
            revert Errors.MismatchLengths();
        }

        for (uint256 i; i < args.tokenIds.length; ) {
            if (
                currentSupplyPerToken[args.tokenIds[i]] + args.amounts[i] >
                maxSupplyPerToken[args.tokenIds[i]]
            ) {
                revert Errors.SupplyLimitReached();
            }

            super._mint(to, args.tokenIds[i], args.amounts[i], args.data);

            unchecked {
                minted[to][args.tokenIds[i]] += args.amounts[i];
                currentSupplyPerToken[args.tokenIds[i]] += args.amounts[i];
                ++i;
            }
        }
    }

    function _permit(Types.PermitSig calldata permitSig) private {
        stableCoin.permit(
            permitSig.owner,
            address(this),
            permitSig.value,
            permitSig.deadline,
            permitSig.v,
            permitSig.r,
            permitSig.s
        );
        stableCoin.transferFrom(
            permitSig.owner,
            address(this),
            permitSig.value
        );
    }
}

