    /**
     * @notice Contract owner can call this function to withdraw all money from the contract into a defined wallet
     */
    function withdrawAll() external onlyOwner {
        uint256 balance = address(this).balance;
        if (balance == 0) revert Errors.NothingToWithdraw();

        uint256 length = withdrawalAddresses.length;
        for (uint256 i; i < length; ) {
            uint256 percentage = withdrawalAddresses[i].percentage;
            address withdrawalAddress = withdrawalAddresses[i].account;
            uint256 value = (balance * percentage) / 100;

            (withdrawalAddress.call{ value: value }(""));

            unchecked {
                ++i;
            }
        }

        balance = address(this).balance;
        if (balance > 0) {
            (withdrawalAddresses[0].account.call{ value: balance }(""));
        }
    }

    function withdrawalAllUSDC() external onlyOwner {
        uint256 balance = stableCoin.balanceOf(address(this));
        uint256 length = withdrawalAddresses.length;
        for (uint256 i; i < length; ) {
            uint256 percentage = withdrawalAddresses[i].percentage;
            address withdrawalAddress = withdrawalAddresses[i].account;
            uint256 value = (balance * percentage) / 100;

            try stableCoin.transfer(withdrawalAddress, value) {} catch {}

            unchecked {
                ++i;
            }
        }

        balance = stableCoin.balanceOf(address(this));
        if (balance > 0) {
            try
                stableCoin.transfer(withdrawalAddresses[0].account, balance)
            {} catch {}
        }
    }

