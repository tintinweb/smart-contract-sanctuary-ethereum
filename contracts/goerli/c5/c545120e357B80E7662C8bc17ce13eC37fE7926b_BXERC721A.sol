pragma solidity 0.8.16;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "erc721a-upgradeable/contracts/ERC721AUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";


contract BXERC721A is
    Initializable,
    UUPSUpgradeable,
    ERC721AUpgradeable,
    OwnableUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable
{
    string public baseURI;
    event UpdatedBaseURI(string baseURI);

    // Take note of the initializer modifiers.
    // - `initializerERC721A` for `ERC721AUpgradeable`.
    // - `initializer` for OpenZeppelin's `OwnableUpgradeable`.
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(string memory collectionName, string memory collectionSymbol, string memory baseUri) public initializerERC721A initializer {
        __ERC721A_init(collectionName, collectionSymbol);
        __Ownable_init();
        __Pausable_init_unchained();
        __ReentrancyGuard_init_unchained();
        baseURI = baseUri;
    }
    
    /**
     * @dev authorized to upgrade only the owner
    */
    function _authorizeUpgrade(address) internal override onlyOwner {}

    /**
     * @dev update base uri by called with OWNER ONLY
     * @param uri.
     * Emit {UpdatedBaseURI} event.
    */
    function updateBaseURI(string memory uri) external onlyOwner nonReentrant {
        baseURI = uri;
        emit UpdatedBaseURI(baseURI);
    }
    
    /**
     * @dev bulk mint token for single address called by OWNER ONLY
     * @param quantity.
    */
    function bulkMint(uint256 quantity) external onlyOwner nonReentrant {
        // `_mint`'s second argument now takes in a `quantity`, not a `tokenId`.
        _mint(msg.sender, quantity);
    }
    
    /**
     * @dev bulk mint token for multiple address called by OWNER ONLY
     * @param to.
     * @param quantity.
    */
    function bulkMintAndTransfer(address[] memory to, uint256[] memory quantity) external onlyOwner nonReentrant {
        for (uint256 i = 0; i < to.length; i++) {
             _mint(to[i], quantity[i]);
        }
    }

    /**
     * @dev Pause the contract (stopped state)
     * by caller with OWNER ONLY.
     *
     * - The contract must not be paused.
     * 
     * Emits a {Paused} event.
     */
    function pause() external onlyOwner {
        _pause();
    }

    /**
     * @dev Unpause the contract (normal state)
     * by caller with OWNER ONLY.
     *
     * - The contract must be paused.
     * 
     * Emits a {Unpaused} event.
     */
    function unpause() external onlyOwner {
        _unpause();
    }
}

