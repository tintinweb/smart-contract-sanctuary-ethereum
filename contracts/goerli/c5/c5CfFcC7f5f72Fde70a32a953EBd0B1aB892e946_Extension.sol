interface IOwnable {
  function owner() external view returns (address);

  function renounceOwnership() external;
  
  function transferOwnership( address newOwner_ ) external;
}

contract Ownable is IOwnable {
    
  address internal _owner;

  event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

  constructor () {
    _owner = msg.sender;
    emit OwnershipTransferred( address(0), _owner );
  }

  function owner() public view override returns (address) {
    return _owner;
  }

  modifier onlyOwner() {
    require( _owner == msg.sender, "Ownable: caller is not the owner" );
    _;
  }

  function renounceOwnership() public virtual override onlyOwner() {
    emit OwnershipTransferred( _owner, address(0) );
    _owner = address(0);
  }

  function transferOwnership( address newOwner_ ) public virtual override onlyOwner() {
    require( newOwner_ != address(0), "Ownable: new owner is the zero address");
    emit OwnershipTransferred( _owner, newOwner_ );
    _owner = newOwner_;
  }
}


contract Extension is usingProvable, Ownable{
   using SafeMath for uint256;
  
   uint256 public amountOfSCondorPerDollar = 0;
   uint256 public multiplierOfScondorForCondorNFTHolders; // 
   string  public apiURL;
   uint256 public burnRate = 50; 
   mapping(uint256 => uint256) tier; //tier[collectionID] = multiplier
   mapping(address => uint256) claimedAmountForDollar;
   mapping(address => uint256) totalPurchasedAmountForDollar;
   address ntfAdress;
   address CondorAddress;
   address ScondorAddress;
   address public constant deadAddress = address(0xdead);
   event _setAmountOfSCondorPerDollar(uint256 amountOfSCondorPerDollar);
   event _setMultiplierOfScondorForCondorNFTHolders(uint256 multiplierOfScondorForCondorNFTHolders);
   event _setTier(uint256 collectionID, uint256 multiplier, string eventName);
   event _claimedScondor(address claimedAdress, uint256 claimedScondor);
   event _setApiURL(string apiURL);
   event _setAddress(address NFTAddress, address CondorAddress, address ScondorAddress);
   event LogResult(string result, bytes32 _queryId);
   event LogNewProvableQuery(string description);
   event NewWithdrawl(uint256 amount);
   event _setBurnRate(uint256 _burnRate);
    bool initialized;
    modifier initialization() {
        require(!initialized);
        _;
        initialized = true;
    }

    
   function setAmountOfSCondorPerDollar(uint256 _amount) public  onlyOwner {
       amountOfSCondorPerDollar = _amount;
       emit _setAmountOfSCondorPerDollar(_amount);
   }

   function setApiURL (string memory url) public onlyOwner {
       apiURL = url;
       emit _setApiURL(url);
   }
   function setMultiplierOfScondorForCondorNFTHolders(uint256 _multiplier) public onlyOwner {
       multiplierOfScondorForCondorNFTHolders = _multiplier;
       emit _setMultiplierOfScondorForCondorNFTHolders(_multiplier);
   }

   function setTier(uint256 _collectionId, uint256 _multiplier) public onlyOwner {
        string memory eventName;
        if(_multiplier==0){
            eventName = "delete";
        }else{
            if(tier[_collectionId]>0){
                eventName = "edit";
            }else{
                eventName = "add";
            }
        }
        tier[_collectionId] = _multiplier;// add, edit, delete=> when multiplier = 0 -> remove
        emit _setTier(_collectionId, _multiplier, eventName);
   }

   function setAddrss(address _nftaddress, address _condorAddress, address _scondorAddress) public onlyOwner {
       ntfAdress = _nftaddress;
       CondorAddress = _condorAddress;
       ScondorAddress = _scondorAddress;
       emit _setAddress(_nftaddress, _condorAddress, _scondorAddress);
   }

   function setBurnRate(uint256 _burnRate) public onlyOwner {
       burnRate = _burnRate;
       emit _setBurnRate(_burnRate);
   }

    function __callback( bytes32 _queryId,string memory _result) public {
        require(msg.sender == provable_cbAddress());

        totalPurchasedAmountForDollar[msg.sender] = stringToUint(_result) * 10 ** 18;
        emit LogResult(_result, _queryId);
    }

    function stringToUint(string memory s) private pure returns (uint) {
        bytes memory b = bytes(s);
        uint result = 0;
        for (uint256 i = 0; i < b.length; i++) {
            uint256 c = uint256(uint8(b[i]));
            if (c >= 48 && c <= 57) {
                result = result * 10 + (c - 48);
            }
        }
        return result;
    }
   function addressToString(address _addr) private pure returns(string memory) {
    bytes32 value = bytes32(uint256(uint160(_addr)));
    bytes memory alphabet = "0123456789abcdef";

    bytes memory str = new bytes(51);
    str[0] = "0";
    str[1] = "x";
    for (uint i = 0; i < 20; i++) {
        str[2+i*2] = alphabet[uint(uint8(value[i + 12] >> 4))];
        str[3+i*2] = alphabet[uint(uint8(value[i + 12] & 0x0f))];
    }
    return string(str);
}
  
   function claimScondor() public payable {
       //get collection id according to msg.sender
       //get amount $ using Oracle api
        uint256 claimScondorAmount = 0;
        string memory api; 
        uint256 mulVal = 0;
        uint256 condorNFTBalance = 0; 
        uint256[] memory collectionIDsbyAddress;
        api = string.concat("json(",apiURL); // "/:" + addressToString(msg.sender) +").purchased";
        api = string.concat(api, "/:");
        api = string.concat(api,addressToString(msg.sender));
        api = string.concat(api, ").purchased");

        if (provable_getPrice("computation") > address(this).balance) {
            emit LogNewProvableQuery("Provable query was NOT sent, please add some ETH to cover for the query fee");
        } else {
            emit LogNewProvableQuery("Provable query was sent, standing by for the answer...");
            provable_query("URL", api);
        }
        condorNFTBalance = IERC721CUSTOM(ntfAdress).getCondorNFTBalance(msg.sender);
        mulVal += condorNFTBalance * multiplierOfScondorForCondorNFTHolders;
        collectionIDsbyAddress = IERC721CUSTOM(ntfAdress).getCollectionIDs(msg.sender);

        uint i;
        uint j;
        uint tierBalance=0;
        for(i=0; i < collectionIDsbyAddress.length; i++){
            j = collectionIDsbyAddress[i];
            tierBalance = IERC721CUSTOM(ntfAdress).getBalanceByIdAndAddress(msg.sender, j);
            mulVal += tierBalance * tier[j];
        }
        claimScondorAmount = (totalPurchasedAmountForDollar[msg.sender].sub(claimedAmountForDollar[msg.sender])) * amountOfSCondorPerDollar * mulVal;
        //IERC20(ScondorAddress).transfer(msg.sender, claimScondorAmount);
        ISCONDOR(ScondorAddress).sendToken(msg.sender, claimScondorAmount);
        ICONDOR(CondorAddress).burn(address(this), claimScondorAmount.mul(burnRate).div(100));
        claimedAmountForDollar[msg.sender] = totalPurchasedAmountForDollar[msg.sender];

        emit _claimedScondor(msg.sender, claimScondorAmount);

   }
   
    function withdraw() public onlyOwner {
		uint256 amount = address(this).balance; // get the amount of ether in the contract
		require(amount > 0, 'You have no ether to withdraw');
		(bool success, ) = payable(msg.sender).call{value: amount}('');
		require(success, 'Withdraw failed');
		emit NewWithdrawl(amount);
	}
    receive() external payable { }
    
}