    FunctionInterface(aegAddress).transferFrom(
      msg.sender,
      address(this),
      assetPrice
    ); //CHECK TO BE SURE THIS WORKS -----------------------------IT DOESN"T
    FunctionInterface(_address).fpMint(
      msg.sender,
      s_requests[requests[0]].randomWords[0] %
        FunctionInterface(_address).totalTokens(),
      1
    );
    console.log(
      "FINSHED MINTING RANDOM ETHERNAL ID: ",
      s_requests[requests[0]].randomWords[0] %
        FunctionInterface(_address).totalTokens()
    );
  }

  /** GETTERS **/
  // frontend can use this to display how many packs are available to open
  function getFulfilledByAddressAndType(address _address, AssetType _assetType)
    public
    view
    returns (uint256[] memory)
  {
    //get all s_userRequests for this address
    uint256[] memory requestIds = s_userRequests[_address];
    uint256[] memory fulfilledRequestIds = new uint256[](requestIds.length);
    uint256 fulfilledRequestIdsCount = 0;
    for (uint256 i = 0; i < requestIds.length; i++) {
      if (
        s_requests[requestIds[i]].fulfilled &&
        !s_requests[requestIds[i]].used &&
        s_requests[requestIds[i]].assetType == _assetType
      ) {
        fulfilledRequestIds[fulfilledRequestIdsCount] = requestIds[i];
        fulfilledRequestIdsCount++;
      }
    }
    //remove empty elements from array
    uint256[] memory returnArray = new uint256[](fulfilledRequestIdsCount);
    for (uint256 i = 0; i < fulfilledRequestIdsCount; i++) {
      returnArray[i] = fulfilledRequestIds[i];
    }

    return returnArray;
  }

  /** ACTIVATION **/

  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  function requestRandomWords(AssetType _assetType)
    internal
    returns (uint256 requestId)
  {
    //get number of words to return from asset type
    uint32 numWords = 3; //default it's a fp
    if (
      _assetType == AssetType.Ethernal ||
      _assetType == AssetType.Adventurer ||
      _assetType == AssetType.CardBack
    ) {
      numWords = 1;
    }

    //TEST REQUEST ID
    // requestId = 83132445710567745731713214319446805456689061274219614556348005953705680908145; // <----------------TESTING
    // Will revert if subscription is not set and funded. <------------------PROD: NEED TO UNCOMMENT FOR VRF
    requestId = COORDINATOR.requestRandomWords(
      keyHash,
      s_subscriptionId,
      requestConfirmations,
      callbackGasLimit,
      numWords //number of words to return
    );
    console.log("msg.sender in requestRandomWords", msg.sender);
    s_requests[requestId] = RequestStatus({
      opener: msg.sender,
      randomWords: new uint256[](0),
      exists: true,
      fulfilled: false,
      used: false,
      assetType: _assetType
    });
    s_userRequests[msg.sender].push(requestId);
    // console.log(s_userRequests[msg.sender][0]);
    // emit RequestSent(requestId, numWords);
    return requestId;
  }

  function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords)
    internal
    override
  {
    // console.log("IN FULFILLRANDOMWORDS");
    require(s_requests[_requestId].exists, "request not found");
    s_requests[_requestId].fulfilled = true;
    s_requests[_requestId].randomWords = _randomWords;

    // console.log("fulfillRandomWords DONE", _requestId, _randomWords.length);
    emit RequestFulfilled(
      s_requests[_requestId].opener,
      s_requests[_requestId].assetType
    );
  }
}

