      0
    );
    return tokenId;
  }

  /** FOUNDER PACK OPENING **/

  function startOpenFP() public whenNotPaused {
    //require user has a token to burn
    require(
      FunctionInterface(burnTokenAddress).tokenOfOwnerByIndex(msg.sender, 0) >
        0,
      "No token to burn."
    );
    console.log("STARTING OPENING FP");
    requestRandomWords(AssetType.All);
  }

