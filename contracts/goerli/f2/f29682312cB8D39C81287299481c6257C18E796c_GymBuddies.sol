pragma solidity ^0.8.13;
import "ERC721URIStorage.sol";
import "ReentrancyGuard.sol";
import "Pausable.sol";
import "Ownable.sol";
import {ERC20Spendable} from "BDO.sol";

contract GymBuddies is ERC721URIStorage, Ownable, ReentrancyGuard, Pausable {
    address public _token;
    string public baseURI;
    //Counters
    uint256 public tokenCounter;
    uint256 public listForRentCounter;
    uint256 public rentedCounter;
    uint256 public activityCounter;
    uint256 public minRentalDays;
    uint256 public blacklistedCount;
    //Mapping Objects
    mapping(uint256 => NFT) private _idToNFT;
    mapping(uint256 => ListedForRentNFT) private _idToListedForRentNFT;
    mapping(uint256 => RentedNFT) private _idToRentedNFT;
    mapping(uint256 => Activity) private _idToActivity;
    mapping(address => bool) blacklist;
    //Structs
    struct NFT {
        uint256 tokenId;
        uint256 price;
        string tokenUri;
        bool listedForSale;
    }
    struct NFTs {
        string tokenUri;
        uint256 price;
    }
    struct AppNFT {
        NFT nft;
        uint256 listedForRentNFT;
        uint256 rentalFeesPerDay;
        uint256 rented;
        uint256 expireOn;
        address beneficiary;
        address owner;
    }
    struct ListedForRentNFT {
        uint256 tokenId;
        uint256 rentalFeesPerDay;
    }
    struct RentedNFT {
        uint256 tokenId;
        address beneficiary;
        uint256 expireOn;
    }
    struct Activity {
        uint256 activityId;
        string name;
        uint256 weight;
    }
    //Events
    event NFTSold(
        uint256 indexed tokenId,
        address indexed from,
        address indexed to,
        uint256 timestamp
    );
    event ListedForSale(
        uint256 indexed tokenId,
        uint256 price,
        uint256 timestamp
    );
    event ListedForRent(
        uint256 indexed tokenId,
        uint256 rentalFeesPerDay,
        address indexed from,
        uint256 timestamp
    );
    event UnListedFromRent(
        uint256 indexed tokenId,
        address indexed from,
        uint256 timestamp
    );
    event NFTRent(
        uint256 indexed tokenId,
        address indexed beneficiary,
        address indexed owner,
        uint256 rentalFees,
        uint256 expireOn,
        uint256 rentalDays,
        uint256 timestamp
    );
    event BlackListed(address[] indexed buyer);

    constructor(string memory baseURI_, address token)
        ERC721("Gym Buddies", "GB")
    {
        baseURI = baseURI_;
        _token = token;
        tokenCounter = 0;
        listForRentCounter = 0;
        rentedCounter = 0;
        blacklistedCount = 0;
        activityCounter = 0;
        minRentalDays = 14;
    }

    //Set ipfs Url
    function _setBaseURI(string memory baseURI_) public nonReentrant onlyOwner {
        baseURI = baseURI_;
    }

    function getbaseURI() private view returns (string memory) {
        return baseURI;
    }

    //Mint New NFTs
    function multiMint(NFTs[] memory _NFTs)
        public
        nonReentrant
        onlyOwner
        whenNotPaused
    {
        if (0 != _NFTs.length) {
            for (uint256 index = 0; index < _NFTs.length; index++) {
                uint256 newTokenId = tokenCounter;
                _safeMint(msg.sender, newTokenId);
                _setTokenURI(
                    newTokenId,
                    string.concat(getbaseURI(), _NFTs[index].tokenUri)
                );
                //Add NFT To List
                _idToNFT[newTokenId] = NFT(
                    newTokenId,
                    _NFTs[index].price,
                    _NFTs[index].tokenUri,
                    true
                );
                tokenCounter = tokenCounter + 1;
            }
        }
    }

    //List For Rent New NFTs
    function multiRent(ListedForRentNFT[] memory _NFTs)
        public
        nonReentrant
        onlyOwner
        whenNotPaused
    {
        if (0 != _NFTs.length) {
            for (uint256 index = 0; index < _NFTs.length; index++) {
                require(
                    msg.sender == owner() ||
                        (msg.sender == ownerOf(_NFTs[index].tokenId) &&
                            !isBlacklisted(ownerOf(_NFTs[index].tokenId))),
                    "Can't List NFT For Rent"
                );
                //Require Token Is Minted
                require(
                    tokenisMinted(_NFTs[index].tokenId) == 1,
                    "NFT Not Minted Yet"
                );
                //Require Token Is Not Listed Before
                require(
                    tokenListedForRent(_NFTs[index].tokenId) == 0,
                    "NFT Already Listed For Rent"
                );
                //Require Token Is Not Rented Now
                require(
                    tokenisRented(_NFTs[index].tokenId) == 0,
                    "Token Already Rented"
                );
                require(
                    tokenlistedForSale(_NFTs[index].tokenId) == 0,
                    "Cann't List For Rent While Listed For Buy"
                );
                _idToListedForRentNFT[listForRentCounter] = ListedForRentNFT(
                    _NFTs[index].tokenId,
                    _NFTs[index].rentalFeesPerDay
                );
                emit ListedForRent(
                    _NFTs[index].tokenId,
                    _NFTs[index].rentalFeesPerDay,
                    ownerOf(_NFTs[index].tokenId),
                    block.timestamp
                );
                listForRentCounter = listForRentCounter + 1;
            }
        }
    }

    //Buy NFT
    function buyNft(uint256 tokenId, address buyer)
        public
        nonReentrant
        onlyOwner
        whenNotPaused
    {
        //Check If Buyer Is Black Listed Or Not
        require(!isBlacklisted(buyer), "Address Is Black Listed");
        //Check If NFT Is Minted Or Not
        require(tokenisMinted(tokenId) == 1, "NFT Not Minted");
        NFT storage nft = _idToNFT[tokenId];
        //Check If Buyer Balance More Than NFT price Or Not
        require(
            ERC20Spendable(_token).balanceOf(buyer) >= nft.price,
            "Tokens transferred must be less or equal to account balance"
        );
        address _owner = ownerOf(tokenId);
        //Check If Buyer Is Owner Or Not
        require(ownerOf(tokenId) != buyer, "Owner Can't Be Buyer");
        //Check If NFT Is Rented Or Not
        require(tokenisRented(tokenId) == 0, "NFT is Rented");
        //Check If NFT Is For Sale Or Not
        require(tokenlistedForSale(tokenId) == 1, "NFT Not For Sale");
        //Transfer Price From Buyer To Owner
        ERC20Spendable(_token).transfer(buyer, ownerOf(tokenId), nft.price);
        //Transfer NFT From Owner To Buyer
        safeTransferFrom(ownerOf(tokenId), buyer, nft.tokenId);
        //UnList NFT From Sale
        _idToNFT[tokenId].listedForSale = false;
        //UnList NFT From Rent
        if (tokenListedForRent(tokenId) == 1) {
            for (uint256 i = 0; i < listForRentCounter; i++) {
                if (_idToListedForRentNFT[i].tokenId == tokenId) {
                    delete _idToListedForRentNFT[i];
                }
            }
        }
        emit NFTSold(nft.tokenId, buyer, _owner, block.timestamp);
    }

    //List NFT For Sale
    function listForSale(uint256 tokenId, uint256 price)
        public
        nonReentrant
        whenNotPaused
    {
        require(
            msg.sender == owner() ||
                (msg.sender == ownerOf(tokenId) &&
                    !isBlacklisted(ownerOf(tokenId))),
            "Can't List NFT For Sale"
        );
        //Require Token Is Minted
        require(tokenisMinted(tokenId) == 1, "NFT Not Minted Yet");
        //Require Token Is Not Listed Before
        require(
            tokenListedForRent(tokenId) == 0,
            "Cann't List NFT For Sale While Listed For Rent"
        );
        //Require Token Is Not Rented Now
        require(tokenisRented(tokenId) == 0, "Token Already Rented");
        _idToNFT[tokenId].price = price;
        _idToNFT[tokenId].listedForSale = true;

        emit ListedForSale(tokenId, price, block.timestamp);
    }

    //List NFT For Rent
    function listForRent(uint256 tokenId, uint256 feesPerDay)
        public
        nonReentrant
        whenNotPaused
    {
        require(
            msg.sender == owner() ||
                (msg.sender == ownerOf(tokenId) &&
                    !isBlacklisted(ownerOf(tokenId))),
            "Can't List NFT For Rent"
        );
        //Require Token Is Minted
        require(tokenisMinted(tokenId) == 1, "NFT Not Minted Yet");
        //Require Token Is Not Listed Before
        require(
            tokenListedForRent(tokenId) == 0,
            "NFT Already Listed For Rent"
        );
        //Require Token Is Not Rented Now
        require(tokenisRented(tokenId) == 0, "Token Already Rented");
        require(
            tokenlistedForSale(tokenId) == 0,
            "Cann't List For Rent While Listed For Buy"
        );
        _idToListedForRentNFT[listForRentCounter] = ListedForRentNFT(
            tokenId,
            feesPerDay
        );
        emit ListedForRent(
            tokenId,
            feesPerDay,
            ownerOf(tokenId),
            block.timestamp
        );
        listForRentCounter = listForRentCounter + 1;
    }

    //Change NFT Rental Fees Per Day
    function changeNFTRentalFeesPerDay(uint256 tokenId, uint256 newprice)
        public
        nonReentrant
        whenNotPaused
    {
        require(
            msg.sender == owner() ||
                (msg.sender == ownerOf(tokenId) &&
                    !isBlacklisted(ownerOf(tokenId))),
            "Can't Change NFT Rental Fees"
        );

        require(tokenisMinted(tokenId) == 1, "Token Not Minted Yet");
        require(tokenListedForRent(tokenId) == 1, "NFT Not Listed For Rent");
        require(tokenisRented(tokenId) == 0, "Token Already Rented");
        for (uint256 i = 0; i < listForRentCounter; i++) {
            if (_idToListedForRentNFT[i].tokenId == tokenId) {
                _idToListedForRentNFT[i] = ListedForRentNFT(tokenId, newprice);
            }
        }
    }

    //Un List From Rent List
    function UnlistFromRent(uint256 tokenId) public nonReentrant whenNotPaused {
        require(
            msg.sender == owner() ||
                (msg.sender == ownerOf(tokenId) &&
                    !isBlacklisted(ownerOf(tokenId))),
            "Can't UnList NFT From Rent"
        );
        require(tokenListedForRent(tokenId) == 1, "NFT Not Listed For Rent");
        require(tokenisRented(tokenId) == 0, "Token is Rented");
        for (uint256 i = 0; i < listForRentCounter; i++) {
            if (_idToListedForRentNFT[i].tokenId == tokenId) {
                delete _idToListedForRentNFT[i];
            }
        }
        emit UnListedFromRent(tokenId, ownerOf(tokenId), block.timestamp);
    }

    //Rent NFT
    function rentNFT(
        uint256 tokenId,
        uint256 rentalDays,
        address beneficiary
    ) public nonReentrant whenNotPaused {
        require(!isBlacklisted(beneficiary), "Beneficiary is BlackListed");
        require(tokenisMinted(tokenId) == 1, "Token Not Minted Yet");
        require(tokenListedForRent(tokenId) == 1, "NFT Not Listed For Rent");
        require(tokenisRented(tokenId) == 0, "Token Already Rented");
        require(
            rentalDays >= minRentalDays,
            "Renting Days Must Be At Least 7 Days"
        );
        require(ownerOf(tokenId) != beneficiary, "Beneficiary Can't Be Owner");
        ListedForRentNFT memory listedForRentNFT;
        for (uint256 i = 0; i < listForRentCounter; i++) {
            if (_idToListedForRentNFT[i].tokenId == tokenId) {
                listedForRentNFT = _idToListedForRentNFT[i];
            }
        }
        uint256 rentalFees = listedForRentNFT.rentalFeesPerDay * rentalDays;
        require(
            ERC20Spendable(_token).balanceOf(beneficiary) >= rentalFees,
            "Tokens transferred must be less or equal to account balance"
        );

        ERC20Spendable(_token).transfer(
            beneficiary,
            ownerOf(tokenId),
            rentalFees
        );

        uint256 rentalExpireOn = block.timestamp + (rentalDays * 24 * 60 * 60);
        _idToRentedNFT[rentedCounter] = RentedNFT(
            tokenId,
            beneficiary,
            rentalExpireOn
        );
        emit NFTRent(
            tokenId,
            beneficiary,
            ownerOf(tokenId),
            rentalFees,
            rentalExpireOn,
            rentalDays,
            block.timestamp
        );
        rentedCounter = rentedCounter + 1;
    }

    //Get Listed NFTS
    function getmintedNfts(uint256[] memory tokenIds)
        public
        view
        returns (AppNFT[] memory)
    {
        uint256 nftCount = tokenIds.length;
        AppNFT[] memory nfts = new AppNFT[](nftCount);
        uint256 nftsIndex = 0;
        for (uint256 i = 0; i < nftCount; i++) {
            nfts[nftsIndex].nft = _idToNFT[tokenIds[i]];
            nfts[nftsIndex].listedForRentNFT = tokenListedForRent(tokenIds[i]);
            nfts[nftsIndex].rentalFeesPerDay = tokenFeesPerDay(tokenIds[i]);
            nfts[nftsIndex].rented = tokenisRented(tokenIds[i]);
            nfts[nftsIndex].expireOn = rentedTokenExpiry(tokenIds[i]);
            nfts[nftsIndex].beneficiary = rentedTokenbeneficiary(tokenIds[i]);
            nfts[nftsIndex].owner = ownerOf(tokenIds[i]);
            nftsIndex++;
        }

        return nfts;
    }

    //Get All Listed NFTS
    function getmintedNft() public view returns (AppNFT[] memory) {
        uint256 nftCount = tokenCounter;
        AppNFT[] memory nfts = new AppNFT[](nftCount);
        uint256 nftsIndex = 0;
        for (uint256 i = 0; i < nftCount; i++) {
            nfts[nftsIndex].nft = _idToNFT[i];
            nfts[nftsIndex].listedForRentNFT = tokenListedForRent(
                _idToNFT[i].tokenId
            );
            nfts[nftsIndex].rentalFeesPerDay = tokenFeesPerDay(
                _idToNFT[i].tokenId
            );
            nfts[nftsIndex].rented = tokenisRented(_idToNFT[i].tokenId);
            nfts[nftsIndex].expireOn = rentedTokenExpiry(_idToNFT[i].tokenId);
            nfts[nftsIndex].beneficiary = rentedTokenbeneficiary(
                _idToNFT[i].tokenId
            );
            nfts[nftsIndex].owner = ownerOf(_idToNFT[i].tokenId);
            nftsIndex++;
        }
        return nfts;
    }

    //Get NFT By TokenId
    function getmintedNftbyId(uint256 _tokenId)
        public
        view
        returns (AppNFT memory)
    {
        NFT memory _nft = _idToNFT[_tokenId];
        AppNFT memory _NFT = AppNFT(
            _nft,
            tokenListedForRent(_nft.tokenId),
            tokenFeesPerDay(_nft.tokenId),
            tokenisRented(_nft.tokenId),
            rentedTokenExpiry(_nft.tokenId),
            rentedTokenbeneficiary(_nft.tokenId),
            ownerOf(_nft.tokenId)
        );

        return _NFT;
    }

    //Get NFT Owned By Address
    function getMyNfts(address _nftowner)
        public
        view
        returns (AppNFT[] memory)
    {
        uint256 nftCount = tokenCounter;
        uint256 myNftCount = 0;
        for (uint256 i = 0; i < nftCount; i++) {
            if (ownerOf(_idToNFT[i].tokenId) == _nftowner) {
                myNftCount++;
            }
        }
        AppNFT[] memory nfts = new AppNFT[](myNftCount);
        uint256 nftsIndex = 0;
        for (uint256 i = 0; i < nftCount; i++) {
            if (ownerOf(_idToNFT[i].tokenId) == _nftowner) {
                nfts[nftsIndex].nft = _idToNFT[i];
                nfts[nftsIndex].listedForRentNFT = tokenListedForRent(
                    _idToNFT[i].tokenId
                );
                nfts[nftsIndex].rentalFeesPerDay = tokenFeesPerDay(
                    _idToNFT[i].tokenId
                );
                nfts[nftsIndex].rented = tokenisRented(_idToNFT[i].tokenId);
                nfts[nftsIndex].expireOn = rentedTokenExpiry(
                    _idToNFT[i].tokenId
                );
                nfts[nftsIndex].beneficiary = rentedTokenbeneficiary(
                    _idToNFT[i].tokenId
                );
                nfts[nftsIndex].owner = ownerOf(_idToNFT[i].tokenId);
                nftsIndex++;
            }
        }
        return nfts;
    }

    //Get NFT Rented By Address
    function getMyRentedNfts(address beneficiary)
        public
        view
        returns (AppNFT[] memory)
    {
        uint256 myrentedNftCount = 0;
        for (uint256 i = 0; i < rentedCounter; i++) {
            if (_idToRentedNFT[i].beneficiary == beneficiary) {
                myrentedNftCount++;
            }
        }
        AppNFT[] memory nfts = new AppNFT[](myrentedNftCount);
        uint256 nftsIndex = 0;
        for (uint256 i = 0; i < rentedCounter; i++) {
            if (
                _idToRentedNFT[i].beneficiary == beneficiary &&
                _idToRentedNFT[i].expireOn > block.timestamp
            ) {
                //nfts[_idToRentedNFT[i].tokenId].nft = _idToNFT[i];
                nfts[nftsIndex].nft = _idToNFT[_idToRentedNFT[i].tokenId];
                nfts[nftsIndex].listedForRentNFT = tokenListedForRent(
                    _idToNFT[_idToRentedNFT[i].tokenId].tokenId
                );
                nfts[nftsIndex].rentalFeesPerDay = tokenFeesPerDay(
                    _idToNFT[_idToRentedNFT[i].tokenId].tokenId
                );
                nfts[nftsIndex].rented = tokenisRented(
                    _idToNFT[_idToRentedNFT[i].tokenId].tokenId
                );
                nfts[nftsIndex].expireOn = rentedTokenExpiry(
                    _idToNFT[_idToRentedNFT[i].tokenId].tokenId
                );
                nfts[nftsIndex].beneficiary = rentedTokenbeneficiary(
                    _idToNFT[_idToRentedNFT[i].tokenId].tokenId
                );
                nfts[nftsIndex].owner = ownerOf(
                    _idToNFT[_idToRentedNFT[i].tokenId].tokenId
                );
                nftsIndex++;
            }
        }
        return nfts;
    }

    //Check If token Is Minted Or Not
    function tokenisMinted(uint256 tokenId) public view returns (uint256) {
        uint256 tokenIsMinted = 0;
        uint256 nftCount = tokenCounter;
        if (nftCount > 0) {
            for (uint256 i = 0; i < nftCount; i++) {
                if (_idToNFT[i].tokenId == tokenId) {
                    tokenIsMinted = 1;
                }
            }
        } else {
            tokenIsMinted = 0;
        }
        return tokenIsMinted;
    }

    //Check If token Is For Sale Or Not
    function tokenlistedForSale(uint256 tokenId) public view returns (uint256) {
        uint256 tokenListedForSale = 0;
        uint256 nftCount = tokenCounter;
        if (nftCount > 0) {
            if (_idToNFT[tokenId].listedForSale == true) {
                tokenListedForSale = 1;
            }
        } else {
            tokenListedForSale = 0;
        }
        return tokenListedForSale;
    }

    //Check If token Is Listed For Rent Or Not
    function tokenListedForRent(uint256 tokenId) public view returns (uint256) {
        uint256 tokenIsListedForRent = 0;
        uint256 listForRentnftCount = listForRentCounter;
        if (listForRentnftCount > 0) {
            for (uint256 i = 0; i < listForRentnftCount; i++) {
                if (
                    _idToListedForRentNFT[i].tokenId == tokenId &&
                    _idToListedForRentNFT[i].rentalFeesPerDay != 0
                ) {
                    tokenIsListedForRent = 1;
                }
            }
        } else {
            tokenIsListedForRent = 0;
        }
        return tokenIsListedForRent;
    }

    //Check If token Is Rented Or Not
    function tokenisRented(uint256 tokenId) public view returns (uint256) {
        uint256 tokenIsRented = 0;
        uint256 rentednftCount = rentedCounter;
        if (rentednftCount > 0) {
            for (uint256 i = 0; i < rentednftCount; i++) {
                if (
                    _idToRentedNFT[i].tokenId == tokenId &&
                    _idToRentedNFT[i].expireOn > block.timestamp
                ) {
                    tokenIsRented = 1;
                }
            }
        } else {
            tokenIsRented = 0;
        }
        return tokenIsRented;
    }

    //Get Listed Token Fees Per Day
    function tokenFeesPerDay(uint256 tokenId) public view returns (uint256) {
        uint256 fees = 0;
        uint256 listForRentnftCount = listForRentCounter;
        if (listForRentnftCount > 0) {
            for (uint256 i = 0; i < listForRentnftCount; i++) {
                if (_idToListedForRentNFT[i].tokenId == tokenId) {
                    fees = _idToListedForRentNFT[i].rentalFeesPerDay;
                }
            }
        } else {
            fees = 0;
        }
        return fees;
    }

    //Get Rented Token Expiration Day
    function rentedTokenExpiry(uint256 tokenId) public view returns (uint256) {
        uint256 tokenexpireOn = 0;
        uint256 rentednftCount = rentedCounter;
        if (rentednftCount > 0) {
            for (uint256 i = 0; i < rentednftCount; i++) {
                if (
                    _idToRentedNFT[i].tokenId == tokenId &&
                    _idToRentedNFT[i].expireOn > block.timestamp
                ) {
                    tokenexpireOn = _idToRentedNFT[i].expireOn;
                }
            }
        } else {
            tokenexpireOn = 0;
        }
        return tokenexpireOn;
    }

    //Get Rented Token Beneficiary
    function rentedTokenbeneficiary(uint256 tokenId)
        public
        view
        returns (address)
    {
        address tokenbeneficiary = address(0);
        uint256 rentednftCount = rentedCounter;
        if (rentednftCount > 0) {
            for (uint256 i = 0; i < rentednftCount; i++) {
                if (
                    _idToRentedNFT[i].tokenId == tokenId &&
                    _idToRentedNFT[i].expireOn > block.timestamp
                ) {
                    tokenbeneficiary = address(_idToRentedNFT[i].beneficiary);
                }
            }
        } else {
            tokenbeneficiary = address(0);
        }
        return tokenbeneficiary;
    }

    //Add New Activities
    function addActivities(Activity[] memory _activities)
        public
        nonReentrant
        onlyOwner
        whenNotPaused
    {
        if (0 != _activities.length) {
            for (uint256 index = 0; index < _activities.length; index++) {
                uint256 newActivityId = activityCounter;
                _idToActivity[activityCounter] = Activity(
                    newActivityId,
                    _activities[index].name,
                    _activities[index].weight
                );
                activityCounter = activityCounter + 1;
            }
        }
    }

    //Get All Activities
    function getAllActivities() public view returns (Activity[] memory) {
        Activity[] memory activities = new Activity[](activityCounter);
        uint256 Index = 0;
        for (uint256 i = 0; i < activityCounter; i++) {
            activities[Index] = _idToActivity[i];
            Index++;
        }
        return activities;
    }

    //Get Activity By Id
    function getActivitybyId(uint256 _activityId)
        public
        view
        returns (Activity memory)
    {
        Activity memory _activity = _idToActivity[_activityId];
        return _activity;
    }

    function changeActivityweight(uint256 _activityId, uint256 new_weight)
        public
        nonReentrant
        onlyOwner
        whenNotPaused
    {
        Activity memory _activity = _idToActivity[_activityId];
        _idToActivity[_activityId] = Activity(
            _activityId,
            _activity.name,
            new_weight
        );
    }

    function pause() public onlyOwner nonReentrant {
        _pause();
    }

    function unpause() public onlyOwner nonReentrant {
        _unpause();
    }

    //Add Address To Black List
    function addToBlacklist(address[] memory _address)
        public
        nonReentrant
        onlyOwner
    {
        require(_address.length <= 100, "More than 100 addresses");
        uint256 j = 0;
        for (j; j < _address.length; j++) {
            blacklist[_address[j]] = true;
        }
        emit BlackListed(_address);
    }

    //Remove Address From Black List
    function removeFromBlacklist(address[] memory _address)
        public
        nonReentrant
        onlyOwner
    {
        require(_address.length <= 100, "More than 100 addresses");
        uint256 j = 0;
        for (j; j < _address.length; j++) {
            blacklist[_address[j]] = false;
        }
    }

    //Check If Address Black Listed
    function isBlacklisted(address _address) public view returns (bool) {
        return (blacklist[_address]);
    }

    //Change NFT Price
    function changeNFTPrice(uint256 _tokenId, uint256 newprice)
        public
        nonReentrant
        onlyOwner
        whenNotPaused
    {
        require(
            msg.sender == ownerOf(_tokenId),
            "Can't Change NFT Rental Fees"
        );
        NFT memory _nft = _idToNFT[_tokenId];
        _idToNFT[_tokenId] = NFT(_tokenId, newprice, _nft.tokenUri, true);
    }

