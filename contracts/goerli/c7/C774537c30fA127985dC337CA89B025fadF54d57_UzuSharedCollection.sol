contract UzuSharedCollection is
  Initializable,
  ERC165Upgradeable,
  IUzuSharedCollection,
  ERC1155URIStorageUpgradeable,
  ERC2981Upgradeable,
  EIP712Upgradeable,
  AdminPool,
  UUPSUpgradeable
{
  using ECDSAUpgradeable for bytes32;

  /**
   * @dev Manage whether the NFT is transferable or not. Only updated by the NFT creators via `approveTransferForEveryone()`
   */
  mapping(uint256 => bool) private _isNftTransferable;

  /**
   * @dev Manage the Creator for each NFT.
   */
  mapping(uint256 => address) private _creatorOf;

  /**
   * @dev NFT Transfer Proxy. All request from NFT Transfer proxy should be OK.
   */
  mapping(INftTransferProxy => bool) private _trustedNftTransferProxy;

  /**
   * @dev constructor to initialize Trusted Forwarder(Meta Tx)
   */
  constructor(address _trustedForwarder)
    ERC2771ContextUpgradeable(_trustedForwarder)
  {}

  /**
   * @dev Initializer(Must be called only once)
   */
  function __UzuSharedCollection_init() external initializer {
    __ERC165_init();
    __AdminPool_init();
    __EIP712_init("UzuSharedCollection", "1.0");
    __UUPSUpgradeable_init();
    __ERC1155URIStorage_init();
    __ERC2981_init();
  }

  /**
   * @dev See {UUPSUpgradeable._authorizeUpgrade()}
   *
   * @param newImplementation address
   *
   * Requirements:
   * - onlyAdmin can call
   */
  function _authorizeUpgrade(address newImplementation)
    internal
    virtual
    override
    onlyAdmin
  {}

  /**
   * @dev See {IERC165Upgradeable-supportsInterface}.
   *
   * @param interfaceId bytes4
   */
  function supportsInterface(bytes4 interfaceId)
    public
    view
    virtual
    override(
      ERC1155Upgradeable,
      ERC165Upgradeable,
      ERC2981Upgradeable,
      IERC165Upgradeable
    )
    returns (bool)
  {
    return
      interfaceId == type(IUzuSharedCollection).interfaceId ||
      super.supportsInterface(interfaceId);
  }

  /**
   * @dev override _msgSender in order to use ERC2771ContextUpgradeable._msgSender()
   */
  function _msgSender()
    internal
    view
    override(ContextUpgradeable, ERC2771ContextUpgradeable)
    returns (address)
  {
    return ERC2771ContextUpgradeable._msgSender();
  }

  /**
   * @dev override _msgData in order to use ERC2771ContextUpgradeable._msgData()
   */
  function _msgData()
    internal
    view
    override(ContextUpgradeable, ERC2771ContextUpgradeable)
    returns (bytes calldata)
  {
    return ERC2771ContextUpgradeable._msgData();
  }

  /**
   * @dev See {IUzuSharedCollection.addTrustedProxy}.
   *
   * Requirements:
   * - onlyAdmin can call
   */
  function addTrustedNftTransferProxy(INftTransferProxy operator)
    external
    virtual
    override
    onlyAdmin
  {
    require(
      operator.supportsInterface(type(INftTransferProxy).interfaceId),
      "IUzuSharedCollection: unsupported NftTransferProxy"
    );

    _trustedNftTransferProxy[operator] = true;

    emit TrustedNftTransferProxyAdded(operator);
  }

  /**
   * @dev See {IUzuSharedCollection.deleteTrustedProxy}.
   *
   * Requirements:
   * - onlyAdmin can call
   */
  function deleteTrustedNftTransferProxy(INftTransferProxy operator)
    external
    virtual
    override
    onlyAdmin
  {
    require(
      _trustedNftTransferProxy[operator],
      "UzuSharedCollection:deleteTrustedNftTransferProxy trying to delete non-existing operator"
    );

    delete _trustedNftTransferProxy[operator];

    emit TrustedNftTransferProxyDeleted(operator);
  }

  /**
   * @dev Throws if called by any account other than Trusted Proxy.
   */
  modifier onlyTrustedProxy() {
    require(
      _trustedNftTransferProxy[INftTransferProxy(_msgSender())],
      "UzuSharedCollection: only Trusted Proxy can call"
    );
    _;
  }

  /**
   * @dev Check validity of MintData and
   * Verify signature for mint (must be signed by Admin)
   *
   * @param mintData Domain.MintData calldata
   * @param mintSign bytes calldata
   */
  function _checkMintDataAndVerifySignaturesForMint(
    Domain.MintData calldata mintData,
    bytes calldata mintSign
  ) private view {
    require(
      mintData.creator != address(0),
      "UzuSharedCollection: mintData.creator is invalid"
    );
    require(
      mintData.nftId != 0,
      "UzuSharedCollection: mintData.nftId is invalid"
    );
    require(
      mintData.amount != 0,
      "UzuSharedCollection: mintData.amount is invalid"
    );
    require(
      bytes(mintData.uri).length > 0,
      "UzuSharedCollection: mintData.uri is invalid"
    );

    // Recoverd address of ERC712 signed data
    address recoverdAddress = _hashTypedDataV4(Domain._hashMintData(mintData))
      .recover(mintSign);

    // Make sure its signed by Uzu Admin
    require(
      isAdmin(recoverdAddress),
      "UzuSharedCollection: not signed by Admin"
    );
  }

  /**
   * @dev See {IUzuSharedCollection.transferFromOrLazyMint}.
   *
   * Requirements:
   * - onlyTrustedProxy can call
   */
  function transferFromOrLazyMint(
    address to,
    uint256 amount,
    Domain.MintData calldata mintData,
    bytes calldata mintSign
  ) external virtual override onlyTrustedProxy {
    require(to != address(0), "UzuSharedCollection: to address is invalid");
    require(amount != 0, "UzuSharedCollection: amount is invalid");

    _checkMintDataAndVerifySignaturesForMint(mintData, mintSign);

    uint256 amountToMint = amount;
    // check creator balance to figure out how much nfts needs to be minted or transferred
    uint256 creatorbalance = balanceOf(mintData.creator, mintData.nftId);
    if (creatorbalance > 0) {
      // creator has some already minted nft, so try to transfer
      uint256 amountTotransfer = amount;
      if (amount > creatorbalance) {
        // creatorbalance is not enough, so transfer the available balance.
        amountTotransfer = creatorbalance;
      }

      safeTransferFrom(
        mintData.creator,
        to,
        mintData.nftId,
        amountTotransfer,
        ""
      );
      amountToMint = amount - amountTotransfer;
    }

    // mint and transfer the needed amount
    if (amountToMint > 0) {
      _lazyMintAndTransfer(to, amountToMint, mintData);
    }
  }

  /**
   * @dev [Internal]
   */
  function _lazyMintAndTransfer(
    address to,
    uint256 amount,
    Domain.MintData calldata mintData
  ) internal {
    // mint NFT
    _mintWithURI(
      mintData.creator,
      to,
      mintData.nftId,
      amount,
      mintData.uri,
      mintData.data,
      mintData.royaltyReceiver,
      mintData.royaltyFeeNumerator
    );

    // transfer NFT
    safeTransferFrom(mintData.creator, to, mintData.nftId, amount, "");
  }

  /**
   * @dev [Private] Mint, set it URI and store the creator
   *
   * @param creator address
   * @param to address
   * @param nftId uint256
   * @param amount uint256
   * @param uri_ string calldata
   * @param data bytes calldata
   * @param royaltyReceiver address
   * @param royaltyFeeNumerator uint96
   */
  function _mintWithURI(
    address creator,
    address to,
    uint256 nftId,
    uint256 amount,
    string calldata uri_,
    bytes calldata data,
    address royaltyReceiver,
    uint96 royaltyFeeNumerator
  ) private {
    // mint NFT
    ERC1155Upgradeable._mint(to, nftId, amount, data);

    // set NFT URI
    ERC1155URIStorageUpgradeable._setURI(nftId, uri_);

    // Assign the creator of the NFT
    _creatorOf[nftId] = creator;

    // setTokenRoyalty
    ERC2981Upgradeable._setTokenRoyalty(
      nftId,
      royaltyReceiver,
      royaltyFeeNumerator
    );
  }

  /**
   * @dev See {IUzuSharedCollection.mint}.
   *
   * Requirements:
   * - mintSign must be signed by Admin
   */
  function mint(Domain.MintData calldata mintData, bytes calldata mintSign)
    external
    virtual
    override
  {
    _checkMintDataAndVerifySignaturesForMint(mintData, mintSign);

    _mintWithURI(
      mintData.creator,
      mintData.creator,
      mintData.nftId,
      mintData.amount,
      mintData.uri,
      mintData.data,
      mintData.royaltyReceiver,
      mintData.royaltyFeeNumerator
    );
  }

  /**
   * @dev See {ERC2981Upgradeable._setDefaultRoyalty}
   *
   * Requirements:
   * - must be called by Admin
   */
  function setDefaultRoyalty(address receiver, uint96 feeNumerator)
    external
    onlyAdmin
  {
    ERC2981Upgradeable._setDefaultRoyalty(receiver, feeNumerator);
  }

  /**
   * @dev See {ERC2981Upgradeable._setTokenRoyalty}
   *
   * Requirements:
   * - must be called by the owner of the tokenId and must have some balance
   */
  function setTokenRoyalty(
    uint256 tokenId,
    address receiver,
    uint96 feeNumerator
  ) external {
    require(
      _creatorOf[tokenId] == _msgSender(),
      "UzuSharedCollection: caller should be creator"
    );

    ERC2981Upgradeable._setTokenRoyalty(tokenId, receiver, feeNumerator);
  }

  /**
   * @dev See {IUzuSharedCollection.approveByNftCreatorAndTransfer}.
   */
  function approveByNftCreatorAndTransfer(
    Domain.OwnershipTransfer calldata ot,
    bytes calldata signature
  ) external virtual override {
    uint256 nftId = ot.nftId;

    require(
      _creatorOf[nftId] != address(0),
      "UzuSharedCollection: nftId does not exists"
    );

    // Verify signature
    // Recoverd address of ERC712 signed data
    address recoverdAddress = _hashTypedDataV4(Domain._hashOwnershipTranfer(ot))
      .recover(signature);
    // Make sure its signed by Creator
    require(
      _creatorOf[nftId] == recoverdAddress,
      "UzuSharedCollection: not signed by Creator"
    );

    // Check whether the NFT is approved for transfer for everyone
    if (_isNftTransferable[nftId] == false) {
      // Not approved yet, make it approved
      _isNftTransferable[nftId] = true;

      emit NftTransferApprovedForAll(nftId, _msgSender());
    }

    // Transfer NFT.
    _safeTransferFrom(_creatorOf[nftId], ot.to, nftId, ot.amount, "");
  }

  /**
   * @dev See {IUzuSharedCollection.approveTransferForEveryoneByNftCreator}.
   */
  function approveTransferForEveryoneByNftCreator(uint256 nftId)
    external
    virtual
    override
  {
    require(
      _creatorOf[nftId] != address(0),
      "UzuSharedCollection: nftId does not exists"
    );

    address msgSender = _msgSender();
    require(
      msgSender == _creatorOf[nftId],
      "UzuSharedCollection: caller must be creator"
    );

    _isNftTransferable[nftId] = true;

    emit NftTransferApprovedForAll(nftId, msgSender);
  }

  /**
   * @dev Return true if the operator is approved by the account OR operator is one of the trusted proxies
   *
   * @param _account address
   * @param _operator address
   */
  function isApprovedForAll(address _account, address _operator)
    public
    view
    virtual
    override(ERC1155Upgradeable, IERC1155Upgradeable)
    returns (bool)
  {
    return
      _trustedNftTransferProxy[INftTransferProxy(_operator)] ||
      super.isApprovedForAll(_account, _operator);
  }

  /**
   * @dev See {ERC1155Upgradeable._beforeTokenTransfer}
   */
  function _beforeTokenTransfer(
    address operator,
    address from,
    address to,
    uint256[] memory ids,
    uint256[] memory amounts,
    bytes memory data
  ) internal virtual override {
    // If the NFT exists
    //   - Cannot transfer the NFT if:
    //     - Transfer is NOT from NFT Transfer Proxy (_trustedNftTransferProxy[operator] != true)
    //     - Transfer for one of NFTs is not approved yet by the NFT Creator. (_isNftTransferable[nftId] != true)
    bool fromTrustedProxy = _trustedNftTransferProxy[
      INftTransferProxy(operator)
    ];
    if (fromTrustedProxy == false) {
      for (uint256 idx = 0; idx < ids.length; idx++) {
        uint256 nftId = ids[idx];
        // Make sure NFT exits, this is needed to allow minting as _beforeTokenTransfer is called when minting as well
        if (_creatorOf[nftId] != address(0)) {
          require(
            _isNftTransferable[nftId],
            "UzuSharedCollection: nft id not allowed to transfer"
          );
        }
      }
    }

    super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
  }
}

