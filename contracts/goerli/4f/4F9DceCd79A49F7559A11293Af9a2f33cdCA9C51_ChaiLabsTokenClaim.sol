pragma solidity ^0.8.9;

// Uncomment this line to use console.log
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";

contract ChaiLabsTokenClaim {
    using SafeERC20 for IERC20;

    struct TokenClaim {
        // admin of the tokenClaim, admin can change the merkle root
        address admin;
        // token of sale
        address token;
        // merkle root of the tokenClaim
        bytes32 merkleRoot;
        // the start time for users to claim tokens
        uint256 startTime;
        // if 0, there is no deadline
        uint256 deadline;
        // total amount deposited for tokenClaim
        uint256 totalAmount;
        // amount remaining to be claimed
        uint256 amountRemaining;
        // if true, merkle root cannot be changed later
        bool merkleRootFrozen;
    }

    uint256 public nextTokenClaimId;

    // tokenClaimId => tokenClaim
    mapping(uint256 => TokenClaim) public tokenClaims;
    // user => tokenClaimId => claimed
    mapping(address => mapping(uint256 => bool)) userTokenClaimed;

    event TokenClaimCreated(uint256 tokenClaimId, TokenClaim tokenClaim);
    event TokenClaimed(uint256 indexed tokenClaimId, address indexed user, uint256 amount);
    event TokenClaimMerkleRootUpdated(uint256 indexed tokenClaimId, bytes32 indexed merkleRoot);
    event TokenClaimMerkleRootFreezed(uint256 indexed tokenClaimId, bytes32 indexed merkleRoot);
    event TokenClaimTokenDeposit(uint256 indexed tokenClaimId, uint256 amount);

    function _validationTokenClaim(TokenClaim memory tokenClaim) internal view {
        require(tokenClaim.token != address(0), "ChaiLabs: token zero address");
        require(tokenClaim.admin != address(0), "ChaiLabs: admin zero address");
        require(tokenClaim.merkleRoot.length != 0, "ChaiLabs: invalid merkle root");
        require(tokenClaim.startTime > block.timestamp, "ChaiLabs: invalid startTime");
        require(tokenClaim.deadline > tokenClaim.startTime || tokenClaim.deadline == 0, "ChaiLabs: invalid deadline");
        require(tokenClaim.totalAmount > 0, "ChaiLabs: invalid totalAmount");
    }

    function createTokenClaim(TokenClaim memory tokenClaim) external {
        _validationTokenClaim(tokenClaim);

        tokenClaim.amountRemaining = tokenClaim.totalAmount;
        tokenClaims[nextTokenClaimId] = tokenClaim;

        // transfer tokens to contract
        IERC20(tokenClaim.token).safeTransferFrom(msg.sender, address(this), tokenClaim.totalAmount);

        if (tokenClaim.merkleRootFrozen) {
            emit TokenClaimMerkleRootFreezed(nextTokenClaimId, tokenClaim.merkleRoot);
        }
        emit TokenClaimCreated(nextTokenClaimId, tokenClaim);
        emit TokenClaimTokenDeposit(nextTokenClaimId, tokenClaim.totalAmount);

        nextTokenClaimId++;
    }

    function claim(
        uint256 tokenClaimId,
        uint256 amount,
        bytes32[] memory merkleProof
    ) external {
        TokenClaim memory tokenClaim = tokenClaims[tokenClaimId];

        require(!userTokenClaimed[msg.sender][tokenClaimId], "ChaiLabs: already claimed");
        require(tokenClaim.amountRemaining >= amount, "ChaiLabs: not enough tokens");
        require(tokenClaim.startTime <= block.timestamp, "ChaiLabs: too soon");
        require(tokenClaim.deadline > block.timestamp, "ChaiLabs: tokenClaim ended");

        bytes32 node = keccak256(abi.encodePacked(msg.sender, amount));
        require(MerkleProof.verify(merkleProof, tokenClaim.merkleRoot, node), "ChaiLabs: invalid proof");

        userTokenClaimed[msg.sender][tokenClaimId] = true;
        tokenClaim.amountRemaining -= amount;
        IERC20(tokenClaim.token).safeTransfer(msg.sender, amount);

        emit TokenClaimed(tokenClaimId, msg.sender, amount);
    }

    function setMerkleRoot(uint256 tokenClaimId, bytes32 merkleRoot) external {
        TokenClaim memory tokenClaim = tokenClaims[tokenClaimId];

        require(tokenClaim.admin == msg.sender, "ChaiLabs: not admin");
        require(!tokenClaim.merkleRootFrozen, "ChaiLabs: merkle root frozen");
        require(tokenClaim.merkleRoot.length != 0, "ChaiLabs: invalid merkle root");
        require(tokenClaim.deadline > block.timestamp, "ChaiLabs: tokenClaim ended");

        tokenClaim.merkleRoot = merkleRoot;

        emit TokenClaimMerkleRootUpdated(tokenClaimId, merkleRoot);
    }

    function depositTokens(uint256 tokenClaimId, uint256 amount) external {
        TokenClaim memory tokenClaim = tokenClaims[tokenClaimId];

        require(tokenClaim.admin == msg.sender, "ChaiLabs: not admin");
        require(tokenClaim.totalAmount > 0, "ChaiLabs: zero amount");
        require(tokenClaim.deadline > block.timestamp, "ChaiLabs: tokenClaim ended");

        tokenClaim.amountRemaining += amount;
        tokenClaim.totalAmount += amount;

        // transfer tokens to contract
        IERC20(tokenClaim.token).safeTransferFrom(msg.sender, address(this), amount);
        emit TokenClaimTokenDeposit(tokenClaimId, amount);
    }

    function freezeMerkleRoot(uint256 tokenClaimId) external {
        TokenClaim memory tokenClaim = tokenClaims[tokenClaimId];

        require(tokenClaim.admin == msg.sender, "ChaiLabs: not admin");
        require(!tokenClaim.merkleRootFrozen, "ChaiLabs: already frooze");
        require(tokenClaim.deadline > block.timestamp, "ChaiLabs: tokenClaim ended");

        tokenClaim.merkleRootFrozen = true;

        emit TokenClaimMerkleRootFreezed(tokenClaimId, tokenClaim.merkleRoot);
    }
}

