pragma solidity ^0.8.0;

import "./IUpsidedownEngineerFactory.sol";

contract UpsidedownEngineerFactory is IUpsidedownEngineerFactory {

    event DeployInstance(address indexed);

    constructor() {}

    fallback() external payable {}
    receive() external payable {}

    function deploy() external returns(address addr) {

        // Bytecode. Result of solc contracts/UpsidedownEngineer.sol --bin --optimize --optimize-runs=200
        bytes memory bytecode = hex"";
        
        assembly {
            addr := create(0, add(bytecode, 0x20), mload(bytecode))
            if iszero(extcodesize(addr)) {
                revert(0, 0)
            }
        }
        emit DeployInstance(addr);
    }

    function hint() external {
        revert("Try out https://library.dedaub.com/decompile");
    }

}

