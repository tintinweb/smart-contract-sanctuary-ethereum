pragma solidity ^0.8.0;




import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
//import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/utils/cryptography/EIP712.sol";


import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

//import "./SignedMinting.sol";

interface NftContract {
    function mintStore(address, uint256) external;
    function balanceOf(address) external view returns (uint256);
    function totalSupply() external view returns (uint256);
}

interface NapContract {
    function stakeManual(address, uint256[] calldata, address, bool) external;
}

contract NFTStore is ReentrancyGuard, Ownable, EIP712 {

    address NFT_CONTRACT_ADDRESS = 0x74B95c89551650956F6B643ab143debf39d44928;
    NftContract public hkcNft =
//        NftContract(0x592f6585dcaaf7524a7c5b17e9c1c80a917ac94c);
        NftContract(NFT_CONTRACT_ADDRESS);

    address NAP_CONTRACT_ADDRESS = 0xd1b27f86617f8D5a3B7B07d1929912f63a20F757;
    NapContract public hkcNap =
        NapContract(NAP_CONTRACT_ADDRESS);


    // EIP712 Feature
    bytes32 public constant TYPEHASH =
        keccak256("PassReq(address receiver,uint256 amount)");
    struct PassReq {
        address receiver;
        uint256 amount;
    }


//bitiocracy
    address private constant core1Address =
        0xa2B9f8336a3923291249EA7E19f03ef727cD8f1E;
    

    /**
        Numbers for Hidden Kitten NftContract
     */
    mapping(address => uint256) private claimedAmount; 


    /**
        Set Current Claim Limits
     */
    uint256 public LIMIT_PER_WALLET = 1;
    uint256 public LIMIT_PER_WALLET_PLUS_ONE = 2;


    uint256 public MAX_SUPPLY = 5000;
    uint256 public MAX_SUPPLY_PLUS_ONE = 5001;    



    event SetHiddenKittenNftContract(address hkcNft);
    event SetHiddenKittenNapContract(address napNft);

    event ClaimHiddenKittenSignature(address account, uint256 amount);   

    event SendStakedKitten (address recipient, uint256 amount);   


    event Withdraw(address to);


    bool public claimOn = false;


    constructor(
        string memory __name
    ) EIP712(__name, "1") 
    {
    }

    modifier claimOpened() {
        require( claimOn, "Claim is not open" );

        _;
    }

    modifier onlyOwnerOrTeam() {
        require(
            core1Address == msg.sender || owner() == msg.sender,
            "caller is neither Team Wallet nor Owner"
        );
        _;
    }


    function setMaxClaimWallet(uint256 _newMaxClaim) external onlyOwner {
        LIMIT_PER_WALLET = _newMaxClaim;
        LIMIT_PER_WALLET_PLUS_ONE = _newMaxClaim + 1;
    }


    function toggleClaim() external onlyOwner {
        claimOn = !claimOn;
    }

    function setHiddenKittenNftContract(NftContract _hkcNft) external onlyOwner {
        hkcNft = _hkcNft;
        emit SetHiddenKittenNftContract(address(_hkcNft));
    }

    function setHiddenKittenNapContract(NapContract _hkcNap) external onlyOwner {
        hkcNap = _hkcNap;
        emit SetHiddenKittenNapContract(address(_hkcNap));
    }


    /**
        Claim. Set by Wallet. 
     */
    function claimKittenSignature(
        uint256 amount,
        uint256 _totalClaimAmount,
        uint8 vSig,
        bytes32 rSig,
        bytes32 sSig
    ) external payable {

        require(claimOn, "Presale Not Live");

        uint256 _totalSupply = hkcNft.totalSupply(); 
        //Just checking against total meowlist numbers.
        require(_totalSupply + amount < MAX_SUPPLY_PLUS_ONE, "Exceeds max supply");


        uint256 totalClaim = claimedAmount[msg.sender] + amount;
        require(totalClaim <= LIMIT_PER_WALLET_PLUS_ONE, "Too Many For Now");
        require(totalClaim <= _totalClaimAmount, "Exceed Allocation");
        
        

        // hash verification
        bytes32 digest = _hashTypedDataV4(
            keccak256(abi.encode(TYPEHASH, msg.sender, _totalClaimAmount))
        );

        address signer = ecrecover(digest, vSig, rSig, sSig);
        require(signer == owner(), "Signature is not from the owner");


        claimedAmount[msg.sender] += amount;

        hkcNft.mintStore(NAP_CONTRACT_ADDRESS, amount);

        //uint256[] memory _tokenIds = new uint256[amount];
        uint256[] memory _tokenIds;

        for(uint256 i=0; i < amount; )
        {          
            _tokenIds[i] = _totalSupply + i;  
            unchecked {
                i++;
            }
        }

        hkcNap.stakeManual( NFT_CONTRACT_ADDRESS, _tokenIds, msg.sender, true );


        emit ClaimHiddenKittenSignature(msg.sender, amount);

    }

    
    
    function sendStakedKitten(
        address _to, 
        uint256 amount
    ) external onlyOwnerOrTeam {
        uint256 _totalSupply = hkcNft.totalSupply(); 
        require(_totalSupply + amount < MAX_SUPPLY_PLUS_ONE, "Exceeds max supply");

        hkcNft.mintStore(NAP_CONTRACT_ADDRESS, amount);
        uint256[] memory _tokenIds;

        for(uint256 i=0; i < amount; )
        {          
            _tokenIds[i] = _totalSupply + i;  
            unchecked {
                i++;
            }
        }

        hkcNap.stakeManual( NFT_CONTRACT_ADDRESS, _tokenIds, _to, true );        

        emit SendStakedKitten(_to, amount);

    }


    function claimKittenTEST(
        uint256 amount,
        uint256 _totalClaimAmount

    ) external payable onlyOwnerOrTeam {

        require(claimOn, "Presale Not Live");

        uint256 _totalSupply = hkcNft.totalSupply(); 
        //Just checking against total meowlist numbers.
        require(_totalSupply + amount < MAX_SUPPLY_PLUS_ONE, "Exceeds max supply");


        uint256 totalClaim = claimedAmount[msg.sender] + amount;
        require(totalClaim <= LIMIT_PER_WALLET_PLUS_ONE, "Too Many For Now");
        require(totalClaim <= _totalClaimAmount, "Exceed Allocation");
        
        

        claimedAmount[msg.sender] += amount;

        hkcNft.mintStore(NAP_CONTRACT_ADDRESS, amount);

        uint256[] memory _tokenIds;

        for(uint256 i=0; i < amount; )
        {          
            _tokenIds[i] = _totalSupply + i;  
            unchecked {
                i++;
            }
        }

        hkcNap.stakeManual( NFT_CONTRACT_ADDRESS, _tokenIds, msg.sender, true );


        emit ClaimHiddenKittenSignature(msg.sender, amount);

    }


    function withdraw() external onlyOwnerOrTeam {
        uint256 balance = address(this).balance;
        require(balance > 0);

        _withdraw(core1Address, balance);
    }

    function _withdraw(address _address, uint256 _amount) private {
        payable(_address).transfer(_amount);
    }    

}

