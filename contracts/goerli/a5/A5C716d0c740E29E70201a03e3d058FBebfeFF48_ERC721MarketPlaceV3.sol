    {}

    function mintWithPermit(
        address to,
        uint256 value,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public virtual {
        require(totalSupply() < maxSupply(), "All tokens have been minted");
        _mint(_mintPrice, to, value, deadline, v, r, s);
    }

