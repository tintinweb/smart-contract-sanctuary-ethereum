        _settlementToken.transfer(bidder, amount);
        emit TokenBidWithdrawn(tokenId, bid.value, bidder);
    }

    function _transferWithRoyalties(address to, uint256 amount)
        internal
        virtual
    {
        uint256 royaltyAmount = 0;
        if (_royalties > 0) {
            royaltyAmount = (amount * _royalties) / 100;
            _settlementToken.transfer(_beneficiary, royaltyAmount);
        }
        _settlementToken.transfer(to, amount - royaltyAmount);
    }

    function _receiveSettlementToken(
        address from,
        uint256 value,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal virtual returns (bool) {
        _settlementToken.permit(from, address(this), value, deadline, v, r, s);
        return _settlementToken.transferFrom(from, address(this), value);
    }

    function _requireTokenOwner(address caller, uint256 tokenId) internal view {
        require(
            ownerOf(tokenId) == caller,
            "The caller is not the owner of the token!"
        );
    }

    function _requireNotTokenOwner(address caller, uint256 tokenId)
        internal
        view
    {
        require(
            ownerOf(tokenId) != caller,
            "The caller is the owner of the token!"
        );
    }

    function _requireExistingToken(uint256 tokenId) internal view {
        require(
            _exists(tokenId),
            "The token either doesn't exist or has not been minted yet!"
        );
    }
}

