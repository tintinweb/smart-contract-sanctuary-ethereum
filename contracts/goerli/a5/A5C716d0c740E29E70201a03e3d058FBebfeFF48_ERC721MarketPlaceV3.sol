//SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/interfaces/IERC2981.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "./ISettlementToken.sol";
import "../v3/ERC721i.sol";

abstract contract AbstractERC721MarketPlace is ERC721i, IERC2981 {
    address private immutable _beneficiary;
    uint256 private immutable _royalties;
    ISettlementToken internal _settlementToken;

    struct Offer {
        bool isForSale;
        uint256 tokenId;
        address seller;
        uint256 minValue; // in wei
        address onlySellTo; // optional
    }

    struct Bid {
        bool hasBid;
        uint256 tokenId;
        address bidder;
        uint256 value;
    }

    // A record of tokens that are offered for sale at a specific minimum value, and optionally a specific person
    mapping(uint256 => Offer) public offers;

    // A record of the highest token bid
    mapping(uint256 => Bid) public bids;

    event TokenOffered(
        uint256 indexed tokenId,
        uint256 minValue,
        address indexed toAddress
    );

    event TokenBidEntered(
        uint256 indexed tokenId,
        uint256 value,
        address indexed fromAddress
    );

    event TokenBidWithdrawn(
        uint256 indexed tokenId,
        uint256 value,
        address indexed fromAddress
    );

    event TokenBought(
        uint256 indexed tokenId,
        uint256 value,
        address indexed fromAddress,
        address indexed toAddress
    );

    event TokenNoLongerForSale(uint256 indexed tokenId);

    constructor(
        string memory name,
        string memory symbol,
        uint256 royalties_,
        address beneficiary_,
        address settlementToken_
    ) ERC721i(name, symbol) {
        _royalties = royalties_;
        _beneficiary = beneficiary_;
        _settlementToken = ISettlementToken(settlementToken_);
    }

    function beneficiary() public view virtual returns (address) {
        return _beneficiary;
    }

    function royalties() public view virtual returns (uint256) {
        return _royalties;
    }

    function totalSupply() public view virtual returns (uint256) {
        return _totalSupply;
    }

    function royaltyInfo(uint256 tokenId, uint256 salePrice)
        external
        view
        returns (address receiver, uint256 royaltyAmount)
    {
        return (_beneficiary, ((salePrice * _royalties) / 100));
    }

    function _mint(
        uint256 mintPrice,
        address minter,
        uint256 value,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal virtual {
        require(value >= mintPrice, "The caller did not send enough token");

        require(
            _receiveSettlementToken(minter, value, deadline, v, r, s),
            "The minter is unable to transfer the token"
        );

        _settlementToken.transfer(_beneficiary, value);
        _totalSupply += 1;
        _safeMint(minter, totalSupply());
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        if (offers[tokenId].isForSale) _tokenNoLongerForSale(from, tokenId);

        Bid memory bid = bids[tokenId];
        if (bid.bidder == to) {
            _settlementToken.transfer(to, bid.value);
            bids[tokenId] = Bid(false, tokenId, address(0), 0);
        }
    }

    function _tokenNoLongerForSale(address from, uint256 tokenId)
        internal
        virtual
    {
        _requireTokenOwner(from, tokenId);

        offers[tokenId] = Offer(false, tokenId, from, 0, address(0));
        emit TokenNoLongerForSale(tokenId);
    }

    function _offerTokenForSale(
        address from,
        uint256 tokenId,
        uint256 minSalePriceInWei,
        address toAddress
    ) internal virtual {
        _requireTokenOwner(from, tokenId);

        offers[tokenId] = Offer(
            true,
            tokenId,
            from,
            minSalePriceInWei,
            toAddress
        );
        emit TokenOffered(tokenId, minSalePriceInWei, toAddress);
    }

    function _buyToken(
        address buyer,
        uint256 tokenId,
        uint256 value,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal virtual {
        _requireExistingToken(tokenId);
        _requireNotTokenOwner(buyer, tokenId);

        Offer memory offer = offers[tokenId];
