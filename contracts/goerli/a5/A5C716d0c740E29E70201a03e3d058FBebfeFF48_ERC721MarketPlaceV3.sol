        uint256 tokenId,
        uint256 minPrice,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public virtual {
        address owner = ownerOf(tokenId);

        requireAcceptTokenPermit(tokenId, owner, minPrice, deadline, v, r, s);

        _acceptTokenBid(owner, tokenId, minPrice);
    }

    function withdrawTokenBidWithPermit(
        uint256 tokenId,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public virtual {
        address bidder = bids[tokenId].bidder;

        requireWithdrawTokenPermit(tokenId, bidder, deadline, v, r, s);

        _withdrawTokenBid(bidder, tokenId);
    }

    function transferFromWithPermit(
        address from,
        address to,
        uint256 tokenId,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public virtual {
        requirePermit(tokenId, from, address(this), deadline, v, r, s);

        _safeTransfer(from, to, tokenId, "");
    }
}

