            deadline,
            v,
            r,
            s
        );

        _offerTokenForSale(owner, tokenId, minSalePriceInWei, address(0));
    }

    function offerTokenForSaleToAddressWithPermit(
        uint256 tokenId,
        uint256 minSalePriceInWei,
        address toAddress,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public virtual {
        address owner = ownerOf(tokenId);

        requireOfferTokenForSalePermit(
            tokenId,
            owner,
            minSalePriceInWei,
            toAddress,
            deadline,
            v,
            r,
            s
        );

        _offerTokenForSale(owner, tokenId, minSalePriceInWei, toAddress);
    }

