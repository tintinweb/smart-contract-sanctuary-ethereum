        require(bid.value > 0, "The bid value has to be more than 0");
        require(
            bid.value >= minPrice,
            "The bid value has to be more than the minPrice"
        );

        uint256 amount = bid.value;
        bids[tokenId] = Bid(false, tokenId, address(0), 0);

