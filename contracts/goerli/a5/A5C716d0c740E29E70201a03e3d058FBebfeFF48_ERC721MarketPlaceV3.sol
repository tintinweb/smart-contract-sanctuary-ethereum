        address from,
        uint256 tokenId,
        uint256 value,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public virtual {
        _enterTokenBid(from, tokenId, value, deadline, v, r, s);
    }

