pragma solidity ^0.8.13;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./EMRD.sol";

contract TheVillageShop is ERC1155, Ownable, ERC1155Supply, ERC1155Burnable {

    EMRD token;

    uint256 public mintPrice = 5;

    // a mapping from an address to whether or not it can burn
    mapping(address => bool) controllers;

    constructor(EMRD _token) ERC1155("https://ipfsURI") {
        token = _token;
    }

    function mint(uint256 amount) external payable {
        // Check if sufficient $EMERALD balance
        require(token.balanceOf(msg.sender) >= mintPrice * amount, "Insufficient $EMERALD balance");
        // Check if correct EMRD value is sent
        require(msg.value == mintPrice * amount, "Incorrect ether value sent");
        _mint(msg.sender, 1, amount, " ");
        token.burn(msg.sender, msg.value);
    }

    function uri(uint _tokenId) override public pure returns (string memory) {
        return string(abi.encodePacked(
                "ipfs://myIPFS",
                Strings.toString(_tokenId),
                ".json"
            ));
    }

    /**
     * burns $EMRD from a holder
     * @param from the holder of the $EMRD
   * @param amount the amount of $EMRD to burn
   */
    function burn(address from, uint256 amount) external {
        require(controllers[msg.sender], "Only controllers can burn");
        _burn(from, 1, amount);
    }

    /**
     * enables an address to mint / burn
     * @param controller the address to enable
   */
    function addController(address controller) external onlyOwner {
        controllers[controller] = true;
    }

    /**
     * disables an address from minting / burning
     * @param controller the address to disbale
   */
    function removeController(address controller) external onlyOwner {
        controllers[controller] = false;
    }

    // The following functions are overrides required by Solidity.
    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data) internal override(ERC1155, ERC1155Supply) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }
}