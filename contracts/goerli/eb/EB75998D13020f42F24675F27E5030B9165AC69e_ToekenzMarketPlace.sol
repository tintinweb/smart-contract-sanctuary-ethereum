pragma solidity ^0.8.0;

/// @title NFT  built on Ethereum, Polygon and Binance blockchain network.
/// @notice This is a smart contract for NFT minting
/// built on three blockchain networks. This is buit as part of Toekenz Collectibles
/// creator can receive distributed royalties into different addresses
/// contracts are made upgradeable for future changes in royalty


import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./lib/BasicMetaTransaction.sol";
import "./ERC1155MetaDataURI.sol";

//ERC1155Upgradeable
contract Toekenz is
    ERC1155Upgradeable,
    ERC1155MetadataURI,
    AccessControlUpgradeable,
    OwnableUpgradeable,
    BasicMetaTransaction
{
    string public constant name = "TOEKENZ";
    string public constant symbol = "TOEKENZ";

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    event Mint(
        address indexed creator,
        uint256 id,
        uint256 quantity,
        uint256 creatorRoyalty,
        address[] royaltyAddress,
        uint256[] royaltyAmount,
        string uri
    );
    event GiftNft(
        address sourceNftWallet,
        address destinationNftWallet,
        uint256 tokenId,
        uint256 tokenQuantity
    );

    mapping(uint256 => address) public creator; //creator address
    mapping(uint256 => uint256) public creatorRoyalty; //creator royalty amount
    mapping(uint256 => address[]) internal tokenRoyaltyAddress; // array of addresses in which creator wants royalty
    mapping(uint256 => uint256[]) internal tokenRoyaltyAmount; // array of royalty amount  divison

// function to initialize the contract 
    function initialize() public initializer {
        __ERC1155_init("");
        _setupRole(DEFAULT_ADMIN_ROLE, msgSender());
        _setupRole(MINTER_ROLE, msgSender());
        _setupRole(ADMIN_ROLE, msgSender());
    }

    // adding a new role
    function addAdminRole(address _admin)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(_admin != address(0), "Cannot be zero address.");
        grantRole(ADMIN_ROLE, _admin);
    }

    function addMinterRole(address _minter) external onlyRole(ADMIN_ROLE) {
        require(
            _minter != address(0),
            "Cannot be zero address."
        );
        _setupRole(MINTER_ROLE, _minter);
    }

    /**
     * @dev function to check if caller has minter role
     */
    function isMinter(address account) external view returns (bool) {
        return hasRole(MINTER_ROLE, account);
    }

    /**
     * @dev function to check if caller has admin role
     */
    function isAdmin(address account) external view returns (bool) {
        return hasRole(ADMIN_ROLE, account);
    }

    /**
     * @dev function to check if caller has default admin role
     */
    function isDefaultAdmin(address account) external view returns (bool) {
        return hasRole(DEFAULT_ADMIN_ROLE, account);
    }

    /**
     * @dev function for minting NFT
     * tokenId, numberOfItems, creator royalty, array of addresses in which creator wants royalty,
     * array of amounts where royalty will be distributed as per user input and
     * token URI are the parameters
     * @dev we need to check the following conditions
     * token with same id cannot be minted
     * item quanity cannot be zero
     * token uri cannot be empty
     * creator royalty must be between 0 and 100
     * creator royalty address cannot be zero
     * number of addresses and number of royalties distributed must be same
     * sum of distributed royalties should be equal to royalty input by creator
     */
    function mint(
        uint256 id,
        uint256 quantity,
        uint256 _creatorRoyalty,
        address[] memory royaltyAddresses,
        uint256[] memory royaltyAmount,
        string memory _tokenURI
    ) external onlyRole(MINTER_ROLE) {
       tokenRoyaltyAddress[id] = royaltyAddresses;

        _beforeTokenMint(
            id,
            quantity,
            royaltyAddresses,
            royaltyAmount,
            _creatorRoyalty,
            _tokenURI
        );

        creator[id] = msgSender();
        tokenRoyaltyAmount[id] = royaltyAmount;
        creatorRoyalty[id] = _creatorRoyalty;
        _setTokenURI(id, _tokenURI);

        // emit the event for notifying the minting of item
        emit Mint(
            msgSender(),
            id,
            quantity,
            _creatorRoyalty,
            royaltyAddresses,
            royaltyAmount,
            _tokenURI
        );
        _mint(msgSender(), id, quantity, "");

    }

    /**
     * @dev function for gifting an nft
     * @dev if nft is a gift, there will be no creator and admin royalties.
     * direct wallet to wallet transfer
     */
    function giftNft(
        uint256 tokenId,
        uint256 tokenQuantity,
        address receiver
    ) external {

        // emit the event for notifying the gifting of item
        emit GiftNft(msgSender(), receiver, tokenId, tokenQuantity);

        safeTransferFrom(msgSender(), receiver, tokenId, tokenQuantity, "");
    }

    /**
     * @dev function for making the contract access controllable
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155Upgradeable, AccessControlUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /**
     * @dev function for returning royalty amount array
     */
    function getRoyaltyAmount(uint256 id)
        external
        view
        returns (uint256[] memory)
    {
        return tokenRoyaltyAmount[id];
    }

    /**
     * @dev function for returning royalty address array
     */
    function getRoyaltyAddresses(uint256 id)
        external
        view
        returns (address[] memory)
    {
        return tokenRoyaltyAddress[id];
    }

    /**
     * @dev internal function for checking conditions before minting
     */

    function _beforeTokenMint(
        uint256 id,
        uint256 quantity,
        address[] memory royaltyAddresses,
        uint256[] memory royaltyAmount,
        uint256 _creatorRoyalty,
        string memory _tokenURI
    ) internal view {
        require(creator[id] == address(0), "Token already minted");
        require(quantity != 0, "Quantity should be positive");

        uint256 length = royaltyAddresses.length;
        uint256 amountLen = royaltyAmount.length;
        uint256 totalRoyaltyAmount;
        require(
            amountLen == length,
            "length of arrays not same"
        );
        require(bytes(_tokenURI).length != 0, "Cannot be empty");
        require(
            _creatorRoyalty <= 100,
            "Must be between 0 and 100"
        );

        for (uint256 i = 0; i < length; ) {
            require(
                royaltyAddresses[i] != address(0),
                "Cannot be zero address"
            );
            totalRoyaltyAmount += royaltyAmount[i];
            unchecked {
                ++i;
            }
        }

        require(
            totalRoyaltyAmount == _creatorRoyalty,
            "Total Royalty amount must be equal to creator royalty"
        );
    }

    // override functions for meta transactions
    function _msgSender() internal view virtual override returns (address) {
        return msgSender();
    }
}

