pragma solidity ^0.8.16;

/// @title NFT market place built on Ethereum, Polygon and Binance blockchain network.
/// @notice This is a smart contract for NFT market place
/// built on three blockchain networks. This is buit as part of Toekenz Collectibles
/// the royalties are static  0-100% for creator on all sales

import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "./lib/BasicMetaTransaction.sol";
import "./Toekenz.sol";

//ERC1155HolderUpgradeable 
contract ToekenzMarketPlace is
    ERC1155HolderUpgradeable,
    AccessControlUpgradeable,
    ReentrancyGuardUpgradeable,
    BasicMetaTransaction
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    Toekenz public nft;
    IERC20Upgradeable public purchaseToken;

    address[] public ngoAddresses; // address list of ngos for donation
    uint256 constant MAX_INT = 3650 days;


    struct SaleItemDetails {
        address owner; //address of the nft owner 
        bool onSale;
        bool toDonate;
        uint256 ngoId;
        uint256 quantity;
        uint256 price;
        uint256 expireTime;
        address destinationCurrencyWallet; // address of creator wallet or address of seller child's parent wallet
    }

    struct OrderItemDetails {
        address buyer; //buyer parent
        address seller; //seller child
        address destination; //nft destination child wallet
        uint256 quantity;
    }

    mapping(address => mapping(uint256 => SaleItemDetails)) public saleItems; 
    mapping(uint256 => OrderItemDetails) public orderItems;
    mapping(address => uint256) public creatorRewards;

    event PutTokenOnSale(uint256 tokenId, SaleItemDetails newSale);

    event RemoveTokenFromSale(
        uint256 tokenId,
        address sourceNftWallet, //child having nft
        bool isOnSale
    );

    event BuyToken(uint256 tokenId, uint256 price, OrderItemDetails newOrder);

    event DonateNft(
        address indexed ngoWalletAddress,
        uint256 donationAmount,
        address sourceCurrencyWallet, //buyer parent wallet
        uint256 creatorRoyalty
    );

    event RoyaltyDistributed(address royaltyAddress, uint256 royaltyAmount);

    modifier checkTokenOwner(uint256 _tokenId) {
        require(
            nft.balanceOf(msgSender(), _tokenId) != 0,
            "You are not the token owner."
        );
        _;
    }

    modifier tokenOnSale(address seller, uint256 _tokenId) {
        require(saleItems[seller][_tokenId].onSale, "Token not on sale.");
        _;
    }

    // function to initialize the contract 
        function initialize(address _token, address _NFT) external initializer{
        require(
            _token != address(0),
            "Address cannot be zero address"
        );
        require(
            _NFT != address(0),
            "Address cannot be zero address"
        );

        purchaseToken = IERC20Upgradeable(_token);
        nft = Toekenz(_NFT);

        _setupRole(DEFAULT_ADMIN_ROLE, msgSender());
    }

    /**
     * @dev function for removing the market item for listing.
     * tokenId is the parameter
     * @dev Only the wallet address that put item on sale can remove the item. This is checked in modifier checkTokenOwner.
     */
    function removeTokenFromSale(uint256 _tokenId)
        external
        nonReentrant
        tokenOnSale(msgSender(), _tokenId)
        returns (bool)
    {
        SaleItemDetails storage saleDetails = saleItems[msgSender()][_tokenId];

        uint256 _quantity = saleDetails.quantity;

        delete saleItems[msgSender()][_tokenId];

        // emit the event for notifying the renoval of item from sale
        emit RemoveTokenFromSale(_tokenId, msgSender(), false);

        nft.safeTransferFrom(
            address(this), //from - contract
            msgSender(), //to - destination nft wallet (child)
            _tokenId,
            _quantity,
            ""
        );
        return true;
    }

    /**
     * @dev function for putting market item for sale on Marketplace.
     * tokenId, numberOfItems, price per quantity in USDC , expiryTime in days (only if the caller is not a creator) and
     * ngoId if item is true for donation are the parameters
     * @dev we need to check the following conditions
     * expiry time cannot be less than current time
     * expiry time cannot be more than 30 days
     * item already on sale cannot be put on sale again by same address without removing from sale
     * caller should have enough nft balance for listing
     * price of item cannot be zero
     */
    function putTokenOnSale(
        uint256 _tokenId,
        uint256 _tokenQuantity,
        uint256 _priceAmount,
        uint256 _expireTime,
        uint256 _ngoId,
        bool toDonate,
        address _destinationCurrencyWallet
    ) external nonReentrant checkTokenOwner(_tokenId) {
        if (nft.creator(_tokenId) == msgSender()) {
            _expireTime = block.timestamp + MAX_INT;
        } else {
             
            require(
                _expireTime > block.timestamp,
                "Cannot be less than current time."
            );
            require(
                block.timestamp + 30 days >= _expireTime,
                "Cannot be more than 30 days."
            );
        }
        require(
             !saleItems[msgSender()][_tokenId].onSale,
             "Token already in sale."
            );
        require(
            nft.balanceOf(msgSender(), _tokenId) >= _tokenQuantity, //seller child address
            "Not enough NFT balance."
        );
        require(_priceAmount != 0, "Amount cannot be 0.");

        //store the data to the sale items
        SaleItemDetails memory newItem = SaleItemDetails({
            owner: msgSender(),
            onSale: true,
            toDonate: toDonate,
            ngoId: _ngoId,
            quantity: _tokenQuantity,
            price: _priceAmount,
            expireTime: _expireTime,
            destinationCurrencyWallet: _destinationCurrencyWallet
        });
        saleItems[msgSender()][_tokenId] = newItem;

        // emit the event for notifying that item is put to sale 
        emit PutTokenOnSale(_tokenId, newItem);

        nft.safeTransferFrom(
            msgSender(), //from -child
            address(this), //to - this contract
            _tokenId,
            _tokenQuantity,
            ""
        );
    }

    /**
     * @dev function for buying the items
     * upon buying the items the creator would be getting the royalty
     * based on the percentage set by the creator the royalty will be deducted from
     * the price and transferred to the creator
     * @dev we should check the following criteria
     * if the item is still on sale
     * if there is enough nfts to buy
     * if the buyer has enough balance for buying nft
     * @dev we need to find the royalty fee by calling an internal function
     * royalty fees will be deducted from the seller's payment
     */
    function buyToken(
        uint256 _tokenId,
        uint256 _tokenQuantity,
        address destinationNftWallet, //child receiver
        address sourceNftWallet // seller child
    ) external nonReentrant tokenOnSale(sourceNftWallet, _tokenId) {
        SaleItemDetails memory saleDetails = saleItems[sourceNftWallet][
            _tokenId
        ];
        uint256 finalPrice = (saleDetails.price * _tokenQuantity);

        require(
            saleDetails.expireTime > block.timestamp,
            "Token sale expired."
        );
        require(
            saleDetails.quantity >= _tokenQuantity,
            "Not enough NFTs on sale."
        );
        require(
            purchaseToken.balanceOf(msgSender()) >= finalPrice,
            "Not enough purchasing token balance."
        );

        // store the data to the order items
        OrderItemDetails memory newOrder = OrderItemDetails({
            buyer: msgSender(), //parent
            seller: saleDetails.owner,
            destination: destinationNftWallet, //child receiver
            quantity: _tokenQuantity
        });

        address creator = nft.creator(_tokenId);
        saleDetails.quantity -= _tokenQuantity;

        if (saleDetails.quantity == 0) {
            delete saleItems[saleDetails.owner][_tokenId];
        }
        // emit the event for notifying the buying of item
        emit BuyToken(_tokenId, finalPrice, newOrder);

        _splitRoyalties(
            _tokenId,
            finalPrice,
            saleDetails.ngoId,
            creator,
            msgSender(),
            saleDetails.toDonate,
            saleDetails.destinationCurrencyWallet
        );

        nft.safeTransferFrom(
            address(this),
            destinationNftWallet, //to buyer child   // doubt!
            _tokenId,
            _tokenQuantity,
            ""
        );
    }

    /**
     * @dev function for adding ngo address for donation
     * possible only by the default admin
     */
    function addNgo(address _ngoAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 length = ngoAddresses.length;
        for(uint256 i=0; i < length;){
            require(_ngoAddress != ngoAddresses[i],'Ngo already listed');
            unchecked {
                ++i;
            }
        }
        ngoAddresses.push(_ngoAddress);
    }

    /**
     * @dev function for removing ngo address from donation list
     * possible only by the default admin
     */
    function removeNgo(uint256 id) external onlyRole(DEFAULT_ADMIN_ROLE) {
        ngoAddresses[id] = ngoAddresses[ngoAddresses.length - 1];
        ngoAddresses.pop();
    }

    function getNgoId(address ngoAddress) external view returns (uint256 id) {
        for (uint256 i = 0; i < ngoAddresses.length; i++) {
            if (ngoAddress == ngoAddresses[i]) {
                return i;
            }
        }
    }

    /**
     * @dev function for making the contract ERC1155 receiver upgradeable
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155ReceiverUpgradeable, AccessControlUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /**
     * @dev internal function for spliting royalties
     * if item is for donation, only creator royalty will receive royalty and price amount will be
     * sent to the selected ngo address
     * if item is not for donation, creator royalties will be
     * transffered to their respective address.
     */
    function _splitRoyalties(
        uint256 id,
        uint256 price,
        uint256 ngoId,
        address creator,
        address buyer,
        bool toDonate,
        address destinationCurrencyWallet
    ) internal {
        uint256 tokenAmountForCreator = (price * nft.creatorRoyalty(id)) / 100;

        if (toDonate) {
            creatorRewards[creator] += tokenAmountForCreator;

            // emit the event for notifying the donation of item
            emit DonateNft(
                ngoAddresses[ngoId],
                (price - tokenAmountForCreator),
                creator,
                tokenAmountForCreator
            );
            // transfer funds to the ngo
            purchaseToken.safeTransferFrom(
                buyer,
                ngoAddresses[ngoId],
                (price - tokenAmountForCreator)
            );
            
            //transfer creator royalty
            purchaseToken.safeTransferFrom(
                buyer,
                creator,
                tokenAmountForCreator
            );
        } else {
            address[] memory royaltyAddress = nft.getRoyaltyAddresses(id);
            uint256 addrLen = royaltyAddress.length;
            uint256[] memory royaltyAmount = nft.getRoyaltyAmount(id);

            for (uint256 i = 0; i != addrLen; ) {
                royaltyAmount[i] =
                    (tokenAmountForCreator * royaltyAmount[i]) /
                    nft.creatorRoyalty(id);
                creatorRewards[royaltyAddress[i]] += tokenAmountForCreator;

                // emit the event for notifying the royalty distribution after sale  of item
                emit RoyaltyDistributed(royaltyAddress[i], royaltyAmount[i]);
                
                purchaseToken.safeTransferFrom(
                    buyer,
                    royaltyAddress[i],
                    royaltyAmount[i]
                );
                unchecked {
                    ++i;
                }
            }

            // transfer sale amount to the seller
            purchaseToken.safeTransferFrom(
                buyer,
                destinationCurrencyWallet, //needed from backend
                price - tokenAmountForCreator //- tokenAmountForAdmin
            );
            
        }
    }

    // override functions for meta transactions
    function _msgSender() internal view virtual override returns (address) {
        return msgSender();
    }
}

