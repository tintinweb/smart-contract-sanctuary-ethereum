/**
@notice MP_NftMarketplace contract imported functions
@dev getListing function returns mapping of NftAddress[Token ID][Listing]
*/
interface IMP_NftMarketplace {
    /// @notice Parameters of a Listing; as defined in NftMarketplace
    struct Listing {
        uint256 price;
        address seller;
    }

    function getListing(address, uint256) external view returns (Listing memory);

    function addProceeds(address) external payable;
}

/**
 * @notice MP_AddressRegistry contract imported functions
 * @dev Contract is a directory containing important contract addresses
 * @dev Functions are GETTERS returning contract addresses
 */
interface IMP_AddressRegistry {
    function s_marketplace_Contract() external view returns (address);

    function s_feeRecipient() external view returns (address);
}

/////////////////////
//     Errors      //
/////////////////////

error MP_NFTAuction__AddressInvalid(address _newAddressRegistry);
error MP_NFTAuction__AuctionAlreadyExists(address nftAddress, uint256 tokenId);
error MP_NFTAuction__AuctionAlreadyEnded(address nftAddress, uint256 tokenId);
error MP_NFTAuction__AuctionAlreadyResulted(address nftAddress, uint256 tokenId);
error MP_NFTAuction__AuctionDoesNotExist(address nftAddress, uint256 tokenId);
error MP_NFTAuction__AuctionNotEnded(address nftAddress, uint256 tokenId);
error MP_NFTAuction__AuctionOwner(address nftAddress, uint256 tokenId);
error MP_NFTAuction__BidOutsideWindow();
error MP_NFTAuction__BidTooLow(uint256 _bidAmount);
error MP_NFTAuction__EndTimeInvalid();
error MP_NFTAuction__FunctionPaused();
error MP_NFTAuction__InvalidAmount(uint256 amount);
error MP_NFTAuction__MarketplaceListed(address nftAddress, uint256 tokenId);
error MP_NFTAuction__NoContracts();
error MP_NFTAuction__NotApprovedForToken();
error MP_NFTAuction__NotAuctionOwner(address nftAddress, uint256 tokenId);
error MP_NFTAuction__NotHighestBidder();
error MP_NFTAuction__NotTokenOwner(address nftAddress, uint256 tokenId);
error MP_NFTAuction__NoWinningBid();
error MP_NFTAuction__StartTimeInvalid();
error MP_NFTAuction__TooLateToWithdraw();
error MP_NFTAuction__TransferFailed();

/////////////////////
//     Contract    //
/////////////////////

contract MP_NFTAuction is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using Address for address payable;

    /////////////////////
    //     Events      //
    /////////////////////

    event AddressRegistryUpdated(address _newAddressRegistry);

    event AuctionCancelled(
        address indexed nftAddress,
        uint256 indexed tokenId,
        address indexed seller
    );

    event AuctionCreated(
        address indexed nftAddress,
        uint256 indexed tokenId,
        address indexed seller,
        uint256 reservePrice,
        uint256 startTime,
        uint256 endTime
    );

    event AuctionResulted(
        address seller,
        address indexed nftAddress,
        uint256 indexed tokenId,
        address indexed winner,
        uint256 winningBid
    );

    event BidPlaced(
        address indexed nftAddress,
        uint256 indexed tokenId,
        address indexed highestBidder,
        uint256 highestBid
    );

    event BidRefunded(
        address indexed nftAddress,
        uint256 indexed tokenId,
        address indexed bidder,
        uint256 bid
    );

    event BidWithdrawn(
        address indexed nftAddress,
        uint256 indexed tokenId,
        address indexed bidder,
        uint256 bid
    );

    event PauseToggled(bool s_isPaused);

    event RoyaltiesPaid(uint256 tokenId, uint value);

    event UpdateMinBidIncrement(uint256 s_minBidIncrement);

    event UpdateAuctionReservePrice(
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 indexed reservePrice
    );

    event UpdateAuctionStartTime(
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 indexed startTime
    );

    event UpdateAuctionEndTime(
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 indexed endTime
    );

    event UpdatePlatformFee(uint256 s_platformFee);

    /////////////////////
    //     Types       //
    /////////////////////

    /// @notice Parameters of an auction
    struct Auction {
        address owner;
        uint256 minBid;
        uint256 reservePrice;
        uint256 startTime;
        uint256 endTime;
        bool resulted;
    }

    /// @notice Parameters of the highest bid received
    struct HighestBid {
        address payable bidder;
        uint256 bid;
        uint256 lastBidTime;
    }

    // ERC2981 interface ID
    bytes4 private constant C_INTERFACE_ID_ERC2981 = 0x2a55205a;

    /// @notice ERC721 Address -> Token ID -> Auction Parameters (struct)
    mapping(address => mapping(uint256 => Auction)) public s_auctions;

    /// @notice ERC721 Address -> Token ID -> highest bidder info (if a bid has been received)
    mapping(address => mapping(uint256 => HighestBid)) public s_highestBids;

    /// @notice globally and across all auctions, the amount by which a bid has to increase e.g: 1 wei
    uint256 public s_minBidIncrement = 1;

    /// @notice global platform fee, assumed to always be to 1 decimal place i.e. 25 = 2.5%
    uint256 public s_platformFee = 25;

    /// @notice for switching off auction creations, bids and withdrawals
    bool public s_isPaused;

    // Initialize interface to addressRegistry contract
    IMP_AddressRegistry public s_addressRegistry;

    // // Initialize interface to marketplace contract
    // IMP_NftMarketplace public s_marketplace;

    /////////////////////
    //    Modifiers    //
    /////////////////////

    modifier whenNotPaused() {
        if (s_isPaused) {
            revert MP_NFTAuction__FunctionPaused();
        }
        _;
    }

    modifier whenAuctionExists(address nftAddress, uint256 tokenId) {
        Auction memory auction = s_auctions[nftAddress][tokenId];

        // Ensure the auction exists
        if (auction.endTime == 0) {
            revert MP_NFTAuction__AuctionDoesNotExist(nftAddress, tokenId);
        }
        _;
    }

    modifier whenAuctionNotResulted(address nftAddress, uint256 tokenId) {
        Auction memory auction = s_auctions[nftAddress][tokenId];

        // Ensure the auction not already resulted
        if (auction.resulted) {
            revert MP_NFTAuction__AuctionAlreadyResulted(nftAddress, tokenId);
        }
        _;
    }

    modifier whenAuctionOwner(address nftAddress, uint256 tokenId) {
        Auction memory auction = s_auctions[nftAddress][tokenId];

        // Ensure the auction not already resulted
        if (_msgSender() != auction.owner) {
            revert MP_NFTAuction__NotAuctionOwner(nftAddress, tokenId);
        }
        _;
    }

    modifier whenNotAuctionOwner(address nftAddress, uint256 tokenId) {
        Auction memory auction = s_auctions[nftAddress][tokenId];

        // Ensure the bidder is not the auction owner
        if (_msgSender() == auction.owner) {
            revert MP_NFTAuction__AuctionOwner(nftAddress, tokenId);
        }
        _;
    }

    /////////////////////
    //   Constructor   //
    /////////////////////

    // constructor(
    //     address _addressRegistry
    // ) {
    //     s_addressRegistry = _addressRegistry;
    // }

    ////////////////////////////////
    //  Internal & Private Funcs  //
    ////////////////////////////////

    /// @notice Returns the current timestamp
    function _getNow() internal view virtual returns (uint256) {
        return block.timestamp;
    }

    function _createAuction(
        address nftAddress,
        uint256 tokenId,
        uint256 _reservePrice,
        uint256 _startTimestamp,
        bool minBidReserve,
        uint256 _endTimestamp
    ) private {
        // Ensure token not already listed e.g. ".endTime" will return "0" if token not listed already
        if (s_auctions[nftAddress][tokenId].endTime != 0) {
            revert MP_NFTAuction__AuctionAlreadyExists(nftAddress, tokenId);
        }

        // Ensure end time is a minimum of five minutes after start time
        if (_endTimestamp <= _startTimestamp + 300) {
            revert MP_NFTAuction__StartTimeInvalid();
        }

        // Ensure start time not in the past
        if (_startTimestamp <= _getNow()) {
            revert MP_NFTAuction__StartTimeInvalid();
        }

        // Ensure end time is less than two weeks from now
        if (_endTimestamp > (_getNow() + 1209600)) {
            revert MP_NFTAuction__EndTimeInvalid();
        }

        uint256 minimumBid = 0;

        if (minBidReserve) {
            minimumBid = _reservePrice;
        }

        // Setup the auction and add to s_auctions mapping
        s_auctions[nftAddress][tokenId] = Auction({
            owner: _msgSender(),
            minBid: minimumBid,
            reservePrice: _reservePrice,
            startTime: _startTimestamp,
            endTime: _endTimestamp,
            resulted: false
        });

        emit AuctionCreated(
            nftAddress,
            tokenId,
            _msgSender(),
            _reservePrice,
            _startTimestamp,
            _endTimestamp
        );
    }

    /**
     @notice Used for refunding escrowed funds from a previous bid
     @param _currentHighestBidder Address of the last highest bidder
     @param _currentHighestBid Ether or Mona amount in WEI that the bidder sent when placing their bid
     */
    function _refundHighestBidder(
        address nftAddress,
        uint256 tokenId,
        address payable _currentHighestBidder,
        uint256 _currentHighestBid
    ) private {
        // refund previous winning bid (if bid exists)
        (bool successRefund, ) = payable(_currentHighestBidder).call{value: _currentHighestBid}(
            ""
        );
        require(successRefund, "failed to refund previous bidder");

        // if(!successRefund) {
        //     revert NFTAuction__TransferFailed();
        // }

        emit BidRefunded(nftAddress, tokenId, _currentHighestBidder, _currentHighestBid);
    }

    /////////////////////
    //    Functions    //
    /////////////////////

    /**
     @notice Creates a new auction for a given item
     @dev Only the owner of item can create an auction and must have approved the FantomAuction contract for the item
     @dev End time for the auction must be in the future.
     @dev Heavy lifting done by the private function _createAuction
     @param nftAddress ERC 721 Address
     @param tokenId Token ID of the item being auctioned
     @param _reservePrice Item cannot be sold for less than this or s_minBidIncrement, whichever is higher
     @param _startTimestamp Unix epoch in seconds for the auction start time
     @param _endTimestamp Unix epoch in seconds for the auction end time.
     */
    function createAuction(
        address nftAddress,
        uint256 tokenId,
        uint256 _reservePrice,
        uint256 _startTimestamp,
        bool minBidReserve,
        uint256 _endTimestamp
    ) external whenNotPaused {
        // Ensure msgSender is the owner of the token
        // Ensure this contract is approved to move the token - isApprovedForAll checks auction is approved for all user's tokens in the nft contract
        // The approving itself will be done on the FE when user clicks to Create Auction
        if (
            IERC721(nftAddress).ownerOf(tokenId) != _msgSender() ||
            IERC721(nftAddress).isApprovedForAll(_msgSender(), address(this)) != true
        ) {
            revert MP_NFTAuction__NotApprovedForToken();
        }

        // Initialize interface to marketplace contract
        IMP_NftMarketplace marketplace = IMP_NftMarketplace(
            s_addressRegistry.s_marketplace_Contract()
        );

        // Ensure token not listed for fixed price sale
        uint256 listingPrice = marketplace.getListing(nftAddress, tokenId).price;
        if (listingPrice > 0) {
            revert MP_NFTAuction__MarketplaceListed(nftAddress, tokenId);
        }

        _createAuction(
            nftAddress,
            tokenId,
            _reservePrice,
            _startTimestamp,
            minBidReserve,
            _endTimestamp
        );
    }

    /**
     @notice Places a new bid, out-bidding the existing bidder if found and criteria is reached
     @dev Only callable when the auction is open
     @dev Bids from smart contracts are prohibited
     @param nftAddress ERC 721 Address
     @param tokenId Token ID of the item being auctioned
     */
    function placeBid(
        address nftAddress,
        uint256 tokenId
    ) external payable nonReentrant whenNotPaused whenNotAuctionOwner(nftAddress, tokenId) {
        // if(_msgSender() != tx.origin) {
        //     revert NFTAuction__NoContracts();
        // }

        if (payable(_msgSender()).isContract() == true) {
            revert MP_NFTAuction__NoContracts();
        }

        // Return existing auction
        Auction memory auction = s_auctions[nftAddress][tokenId];

        // Check auction exists and is live
        if (_getNow() < auction.startTime || _getNow() > auction.endTime) {
            revert MP_NFTAuction__BidOutsideWindow();
        }

        if (auction.minBid == auction.reservePrice && msg.value < auction.reservePrice) {
            revert MP_NFTAuction__BidTooLow(msg.value);
        }

        // Ensure bid adheres to outbid increment and threshold
        HighestBid storage highestBid = s_highestBids[nftAddress][tokenId];
        // console.log("HIGHEST BID: ", highestBid.bid);

        // Highest bid will be "0" if none has yet been received
        uint256 minBidRequired = highestBid.bid.add(s_minBidIncrement);
        // console.log("MIN BID REQUIRED: ", minBidRequired);

        if (msg.value < minBidRequired) {
            revert MP_NFTAuction__BidTooLow(msg.value);
        }

        // Refund old top bidder if there is one
        if (highestBid.bidder != address(0)) {
            _refundHighestBidder(nftAddress, tokenId, highestBid.bidder, highestBid.bid);
        }

        /** Assign new top bidder & store bid money in this contract
         *  When auction resulted the bid money is sent to s_proceeds in the marketplace contract.
         *  So users can only withdraw from marketplace
         */
        highestBid.bidder = payable(_msgSender());
        highestBid.bid = msg.value;
        highestBid.lastBidTime = _getNow();
        // console.log("SAVED HIGHEST BID: ", highestBid.bid, highestBid.bidder);

        emit BidPlaced(nftAddress, tokenId, highestBid.bidder, highestBid.bid);
    }

    /**
     @notice Allows the hightest bidder to withdraw the bid up until 1 hour post auction's end
     @dev Only callable by the existing top bidder
     @dev Cannot be called if auction has been resulted
     @param nftAddress ERC 721 Address
     @param tokenId Token ID of the item being auctioned
     */
    function withdrawBid(address nftAddress, uint256 tokenId) external nonReentrant whenNotPaused {
        // Return existing top bidder entry
        HighestBid storage highestBid = s_highestBids[nftAddress][tokenId];

        // Ensure caller is the highest bidder. Only bid we store is the highestBid so only highestBidder can withdraw
        // This alsoc ensures that an auction exists. If highestBid.bidder=(0) then either the auction doesn't exist or user isn't the Highest Bidder
        if (highestBid.bidder != _msgSender()) {
            revert MP_NFTAuction__NotHighestBidder();
        }

        // Return auction endTime
        uint256 _endTime = s_auctions[nftAddress][tokenId].endTime;

        // Highest Bidder can withdraw bid up until 1 hour after auction end
        if (_getNow() > (_endTime + 3600)) {
            revert MP_NFTAuction__TooLateToWithdraw();
        }

        // The withdrawn bid amount
        uint256 previousBid = highestBid.bid;

        // Delete highest bid from mapping
        delete s_highestBids[nftAddress][tokenId];

        // Refund the top bidder
        _refundHighestBidder(nftAddress, tokenId, payable(_msgSender()), previousBid);

        emit BidWithdrawn(nftAddress, tokenId, highestBid.bidder, highestBid.bid);
    }

    /**
     @notice Closes a finished auction and rewards the highest bidder
     @dev _msgSender() must own the token and the auction
     @dev Only admin or smart contract
     @dev Auction can only be resulted if there has been a bidder and reserve met.
     @dev If there have been no bids, the auction needs to be cancelled instead using `cancelAuction()`
     @param nftAddress ERC 721 Address
     @param tokenId Token ID of the item being auctioned
     */
    function resultAuction(
        address nftAddress,
        uint256 tokenId
    )
        external
        nonReentrant
        whenAuctionExists(nftAddress, tokenId)
        whenAuctionNotResulted(nftAddress, tokenId)
        whenAuctionOwner(nftAddress, tokenId)
    {
        // Get the Auction instance from the "s_auctions" mapping for the provided nftAddress & tokenId
        Auction memory auction = s_auctions[nftAddress][tokenId];

        // _msgSender() must own the token and the auction. Ownership check makes sure the token hasn't be transferred already
        // This means user manually triggers auction resolution on the FE; user is under no obligation to accept highest bid
        // Otherwise we would need to use a timed trigger of some sort e.g. Chainlink
        if (IERC721(nftAddress).ownerOf(tokenId) != _msgSender()) {
            revert MP_NFTAuction__NotTokenOwner(nftAddress, tokenId);
        }

        // Check the auction has ended
        if (_getNow() < auction.endTime) {
            revert MP_NFTAuction__AuctionNotEnded(nftAddress, tokenId);
        }

        _resultAuction(nftAddress, tokenId);
    }

    function _resultAuction(address nftAddress, uint256 tokenId) private {
        // Get the Auction instance from the "s_auctions" mapping for the provided nftAddress & tokenId
        Auction storage auction = s_auctions[nftAddress][tokenId];

        // Get highest bidder info
        HighestBid storage highestBid = s_highestBids[nftAddress][tokenId];
        address winner = highestBid.bidder;
        uint256 winningBid = highestBid.bid;

        // Ensure there is a winner
        if (winner == address(0)) {
            revert MP_NFTAuction__NoWinningBid();
        }

        // If user updatesReserve above the highestBid while bid is live, that bid will be rejected here
        if (winningBid < auction.reservePrice) {
            revert MP_NFTAuction__NoWinningBid();
        }

        // Ensure this contract is approved to move the token
        if (IERC721(nftAddress).isApprovedForAll(_msgSender(), address(this)) != true) {
            revert MP_NFTAuction__NotApprovedForToken();
        }

        // Set to true so auction can't be amended while being resulted
        // This also protects against reentrant attack; if auction never deleted from s_auctions
        auction.resulted = true;

        // Remove highest bid from the highest bid mapping
        delete s_highestBids[nftAddress][tokenId];

        uint256 payAmount;
        uint256 royaltiesAmount;

        // A Platform Fee is calculated on the Winning Bid
        uint256 platformCharge = winningBid.mul(s_platformFee).div(1000);

        // check whether royalties are due and deduct if true
        if (checkRoyalties(nftAddress)) {
            royaltiesAmount = calculateRoyalties(nftAddress, tokenId, winningBid);
        }

        // Define seller proceeds after platformCharge substracted from Winning Bid amount
        payAmount = winningBid.sub(platformCharge).sub(royaltiesAmount);

        // (bool platformTransferSuccess, ) = s_platformFeeRecipient.call{
        //     value: platformCharge
        // }("");
        // if(!platformTransferSuccess) {
        //     revert NFTAuction__TransferFailed();
        // }

        // Initialize interface to marketplace contract
        IMP_NftMarketplace marketplace = IMP_NftMarketplace(
            s_addressRegistry.s_marketplace_Contract()
        );

        // Send auction proceeds to marketplace contract
        if (payAmount > 0) {
            marketplace.addProceeds{value: payAmount}(auction.owner);
        }

        // Transfer the token to the auction winner
        IERC721(nftAddress).safeTransferFrom(
            IERC721(nftAddress).ownerOf(tokenId),
            winner,
            tokenId
        );

        emit AuctionResulted(_msgSender(), nftAddress, tokenId, winner, winningBid);

        // Delete auction from mapping
        delete s_auctions[nftAddress][tokenId];
        // console.log(s_auctions[nftAddress][tokenId]);
    }

    /**
     @notice Cancels an inflight and un-resulted auction, returning the funds to the top bidder if there is one
     @dev Only auction owner can call this
     @param nftAddress ERC 721 Address
     @param tokenId Token ID of the NFT being auctioned
     */
    function cancelAuction(
        address nftAddress,
        uint256 tokenId
    )
        external
        nonReentrant
        whenAuctionExists(nftAddress, tokenId)
        whenAuctionNotResulted(nftAddress, tokenId)
        whenAuctionOwner(nftAddress, tokenId)
    {
        // return existing top bidder entry
        HighestBid storage highestBid = s_highestBids[nftAddress][tokenId];
        // if top bidder exists; refund them
        if (highestBid.bidder != address(0)) {
            _refundHighestBidder(nftAddress, tokenId, payable(highestBid.bidder), highestBid.bid);

            // Delete highest bid from mapping
            delete s_highestBids[nftAddress][tokenId];
        }

        // Delete auction from mapping
        delete s_auctions[nftAddress][tokenId];

        emit AuctionCancelled(nftAddress, tokenId, _msgSender());
    }

    /// @notice Checks if contract implements the ERC-2981 interface
    /// @return true if ERC-2981 interface is supported, false otherwise
    function checkRoyalties(address nftAddress) internal view returns (bool) {
        // NFT contract complying with 2981 must have supportsInterface implemented
        bool success = IERC2981(nftAddress).supportsInterface(C_INTERFACE_ID_ERC2981);
        return success;
    }

    /// @notice Transfers royalties to the rightsowner if applicable
    /// @param tokenId - the NFT token queried for royalties
    /// @param saleValue - the price at which the asset will be sold
    /// @return _royaltiesAmount - the value that will go to the seller after deducting royalties
    function calculateRoyalties(
        address nftAddress,
        uint256 tokenId,
        uint256 saleValue
    ) private returns (uint256 _royaltiesAmount) {
        // Get amount of royalties to pays and recipient
        (address royaltiesReceiver, uint256 royaltiesAmount) = IERC2981(nftAddress).royaltyInfo(
            tokenId,
            saleValue
        );

        // not using a "success" bool here results in a warning when contract comiled but we can ignore it
        if (royaltiesAmount > 0 && royaltiesReceiver != address(0)) {
            (bool successRoyalty, ) = payable(royaltiesReceiver).call{value: royaltiesAmount}("");
            require(successRoyalty, "failed to send royalty to royalty receiver");

            // Broadcast royalties payment
            emit RoyaltiesPaid(tokenId, royaltiesAmount);

            _royaltiesAmount = royaltiesAmount;
            return _royaltiesAmount;
        } else {
            return 0;
        }
    }

    /**
     @notice Toggling the pause flag
     @dev Only admin
     */
    function toggleIsPaused(bool _paused) external onlyOwner {
        s_isPaused = _paused;
        emit PauseToggled(s_isPaused);
    }

    /**
     @notice Purely used for testing
     */
    function updateAuctionResulted(
        address nftAddress,
        uint256 tokenId,
        bool _resulted
    )
        external
        whenAuctionExists(nftAddress, tokenId)
        whenAuctionNotResulted(nftAddress, tokenId)
        whenAuctionOwner(nftAddress, tokenId)
    {
        Auction storage auction = s_auctions[nftAddress][tokenId];

        auction.resulted = _resulted;
    }

    /**
     @notice Update the current reserve price for an auction
     @dev Only auction owner
     @dev Auction must exist
     @param nftAddress ERC 721 Address
     @param tokenId Token ID of the NFT being auctioned
     @param _reservePrice New Ether reserve price (WEI value)
     */
    function updateAuctionReservePrice(
        address nftAddress,
        uint256 tokenId,
        uint256 _reservePrice
    )
        external
        whenAuctionExists(nftAddress, tokenId)
        whenAuctionNotResulted(nftAddress, tokenId)
        whenAuctionOwner(nftAddress, tokenId)
    {
        Auction storage auction = s_auctions[nftAddress][tokenId];

        auction.reservePrice = _reservePrice;
        emit UpdateAuctionReservePrice(nftAddress, tokenId, _reservePrice);
    }

    /**
     @notice Update the current start time for an auction
     @dev Only auction owner
     @dev Auction must exist
     @param nftAddress ERC 721 Address
     @param tokenId Token ID of the NFT being auctioned
     @param _startTime New start time (unix epoch in seconds)
     */
    function updateAuctionStartTime(
        address nftAddress,
        uint256 tokenId,
        uint256 _startTime
    )
        external
        whenAuctionExists(nftAddress, tokenId)
        whenAuctionNotResulted(nftAddress, tokenId)
        whenAuctionOwner(nftAddress, tokenId)
    {
        Auction storage auction = s_auctions[nftAddress][tokenId];

        if (_startTime == 0) {
            revert MP_NFTAuction__StartTimeInvalid();
        }

        // New Start Time must not be in the past
        if (_getNow() >= _startTime) {
            revert MP_NFTAuction__StartTimeInvalid();
        }

        // New Start Time must be 5 mins less than End Time
        if (_startTime + 300 >= auction.endTime) {
            revert MP_NFTAuction__StartTimeInvalid();
        }

        auction.startTime = _startTime;
        emit UpdateAuctionStartTime(nftAddress, tokenId, _startTime);
    }

    /**
     @notice Update the current end time for an auction
     @dev Only auction owner
     @dev Auction must exist
     @param nftAddress ERC 721 Address
     @param tokenId Token ID of the NFT being auctioned
     @param _endTimestamp New end time (unix epoch in seconds)
     */
    function updateAuctionEndTime(
        address nftAddress,
        uint256 tokenId,
        uint256 _endTimestamp
    ) external whenAuctionExists(nftAddress, tokenId) whenAuctionOwner(nftAddress, tokenId) {
        Auction storage auction = s_auctions[nftAddress][tokenId];

        // Ensure the auction has not ended
        if (_getNow() >= auction.endTime) {
            revert MP_NFTAuction__EndTimeInvalid();
        }

        if (auction.startTime >= _endTimestamp) {
            revert MP_NFTAuction__EndTimeInvalid();
        }

        // New End Time should be at least 5 mins from now
        if (_endTimestamp <= _getNow() + 300) {
            revert MP_NFTAuction__EndTimeInvalid();
        }

        auction.endTime = _endTimestamp;
        emit UpdateAuctionEndTime(nftAddress, tokenId, _endTimestamp);
    }

    /**
     @notice Update the amount by which bids have to increase, across all auctions
     @dev Only marketplace contract owner
     @param _minBidIncrement New bid step in WEI
     */
    function updateMinBidIncrement(uint256 _minBidIncrement) external onlyOwner {
        s_minBidIncrement = _minBidIncrement;
        emit UpdateMinBidIncrement(_minBidIncrement);
    }

    /**
     @notice Method for updating platform fee
     @dev Only marketplace owner
     @param _platformFee uint256 the platform fee to set
     */
    function updatePlatformFee(uint256 _platformFee) external onlyOwner {
        s_platformFee = _platformFee;
        emit UpdatePlatformFee(_platformFee);
    }

    /**
     *  @notice Method for getting contract's balance
     */
    function getContractBalance() external onlyOwner nonReentrant returns (uint256) {
        uint256 balance = address(this).balance;
        return balance;
    }

    /**
     * @notice Method for owner withdrawing contract funds
     */
    function withdrawContractBalance(uint256 amount) external onlyOwner {
        if (amount <= 0 || amount > address(this).balance) {
            revert MP_NFTAuction__InvalidAmount(amount);
        }

        (bool success, ) = payable(msg.sender).call{value: amount}("");
        require(success, "Transfer failed");
        // bool withdrawSuccess = payable(msg.sender).send(address(this).balance);
        // require(withdrawSuccess, "Withdraw Failed");
    }

    /**
     *  @notice Method for updating address registry
     *  @dev Only auction contract owner
     *  @param _newAddressRegistry is the updated address for the contract
     */
    function updateAddressRegistry(address _newAddressRegistry) external onlyOwner {
        if (_newAddressRegistry == address(0)) {
            revert MP_NFTAuction__AddressInvalid(_newAddressRegistry);
        }

        s_addressRegistry = IMP_AddressRegistry(_newAddressRegistry);
        emit AddressRegistryUpdated(_newAddressRegistry);
    }
}

