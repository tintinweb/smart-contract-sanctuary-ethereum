import {IERC1271} from "@openzeppelin/contracts/interfaces/IERC1271.sol";
import {ERC721} from "@openzeppelin/contracts/token/ERC721/ERC721.sol";

import {LibModels} from "../library/LibModels.sol";
import {IERC721Mintable} from "../token/IERC721Mintable.sol";
import {BaseOrderProcessor} from "../base/BaseOrderProcessor.sol";

contract AssetExchange is BaseOrderProcessor, ReentrancyGuardUpgradeable {
    using LibModels for LibModels.Order;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    /**
     * @dev Initializes OfferManager contract
     *
     */
    function initialize() external initializer {
        __ReentrancyGuard_init();
        __Ownable_init();
        __EIP712_init("PaycheckMarketplace", "1");
    }

    function matchOrders(LibModels.Order calldata makerOrder, LibModels.Order calldata takerOrder) internal pure {
        require(LibModels.hash(makerOrder.makerAsset) == LibModels.hash(takerOrder.takerAsset), "Maker asset mismatch");
        require(LibModels.hash(makerOrder.takerAsset) == LibModels.hash(takerOrder.makerAsset), "Taker asset mismatch");
    }

    function processOrder(LibModels.Order calldata makerOrder, LibModels.Order calldata takerOrder)
        external
        override
        nonReentrant
    {
        // Calculate the hash of the maker order
        bytes32 orderHash = LibModels.hash(makerOrder);

        // Verify whether the order already processed, cancelled or expired
        require(!_processedOrders[orderHash], "Order executed or cancelled");

        // solhint-disable-next-line not-rely-on-time
        require(
            block.timestamp >= makerOrder.startTime && block.timestamp <= makerOrder.endTime,
            "Maker order expired"
        );

        validate(orderHash, makerOrder.owner, makerOrder.signature);

        matchOrders(makerOrder, takerOrder);

        if (makerOrder.takerAsset.assetType == LibModels.AssetType.ERC721) {
            emit ProcessOrder(
                orderHash,
                makerOrder.orderSide,
                takerOrder.owner,
                makerOrder.owner,
                makerOrder.takerAsset.assetContract,
                makerOrder.takerAsset.assetId,
                makerOrder.makerAsset.assetValue
            );
        } else if (makerOrder.makerAsset.assetType == LibModels.AssetType.ERC721) {
            emit ProcessOrder(
                orderHash,
                makerOrder.orderSide,
                makerOrder.owner,
                takerOrder.owner,
                makerOrder.makerAsset.assetContract,
                makerOrder.makerAsset.assetId,
                makerOrder.takerAsset.assetValue
            );
        }

        transferAssets(takerOrder, makerOrder);
        _processedOrders[orderHash] = true;
        // overflow is not likely possible
        unchecked {
            _userNonces[makerOrder.owner]++;
        }
    }
}

