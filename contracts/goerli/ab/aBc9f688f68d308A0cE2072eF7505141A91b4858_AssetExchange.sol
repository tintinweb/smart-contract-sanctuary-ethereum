import {AddressUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import {ContextUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import {OwnableUpgradeable} from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import {ECDSAUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import {EIP712Upgradeable} from "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import {IERC1271} from "@openzeppelin/contracts/interfaces/IERC1271.sol";
import {IERC20Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import {IERC721Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import {IERC1155Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import {SafeERC20Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

import {IERC721Mintable} from "../token/IERC721Mintable.sol";
import {IOrderProcessor} from "./IOrderProcessor.sol";
import {LibModels} from "../library/LibModels.sol";

abstract contract BaseOrderProcessor is IOrderProcessor, ContextUpgradeable, OwnableUpgradeable, EIP712Upgradeable {
    bytes4 internal constant MAGICVALUE = 0x1626ba7e;
    using AddressUpgradeable for address;
    using ECDSAUpgradeable for bytes32;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    // user orders hashes which already executed or cancelled
    mapping(bytes32 => bool) internal _processedOrders;

    // user order nonces
    mapping(address => uint256) internal _userNonces;

    // project service fee
    uint256 internal _serviceFee;

    /// Project fee receiver
    address internal _projectFeeReceiver;

    /// @notice Cancel order
    /// @param orderHash The order hash
    event CancelOrder(bytes32 orderHash);

    /// @param from Seller of an asset
    /// @param to Buyer of an asset
    /// @param token Token smart contract
    /// @param tokenId Token ID
    /// @param price The price of the sale
    event ProcessOrder(
        bytes32 orderHash,
        LibModels.OrderSide side,
        address indexed from,
        address indexed to,
        address token,
        uint256 tokenId,
        uint256 price
    );

    /// @param previousFee Previouse service fee
    /// @param newFee New service fee
    event ChangeServiceFee(uint256 previousFee, uint256 newFee);

    function _validate(
        address signer,
        address creator,
        bytes32 hash,
        bytes calldata signature,
        string memory message
    ) private view {
        if (signer != creator) {
            if (creator.isContract()) {
                require(IERC1271(creator).isValidSignature(_hashTypedDataV4(hash), signature) == MAGICVALUE, message);
            } else {
                revert(message);
            }
        }
    }

    function updateServiceFee(uint256 serviceFee_) external onlyOwner {
        require(serviceFee_ < 1000, "Fee should be less than 10%");
        emit ChangeServiceFee(_serviceFee, serviceFee_);        
        _serviceFee = serviceFee_;
    }

    function updateProjectFeeReceiver(address projectFeeReceiver) external onlyOwner {
        require(projectFeeReceiver != address(0), "no fees receiver");
        _projectFeeReceiver = projectFeeReceiver;
    }

    function domainSeparator() external view returns (bytes32) {
        return _domainSeparatorV4();
    }

    function getUserNonce(address account) external view returns (uint256) {
        return _userNonces[account];
    }

    function hashKey(LibModels.Order calldata order) public pure returns (bytes32) {
        return LibModels.hash(order);
    }

    function serviceFee() public view returns (uint256) {
        return _serviceFee;
    }

    function cancelOrder(LibModels.Order calldata order) external {
        require(_msgSender() == order.owner, "Wrong maker");
        bytes32 orderHash = LibModels.hash(order);
        _processedOrders[orderHash] = true;
        // overflow is not likely possible
        unchecked {
            _userNonces[order.owner]++;
        }
        emit CancelOrder(orderHash);
    }

    function transferAssets(LibModels.Order calldata takerOrder, LibModels.Order calldata makerOrder) internal {
        mintAndTransferAsset(makerOrder.takerAsset, takerOrder.owner, makerOrder.owner);
        mintAndTransferAsset(makerOrder.makerAsset, makerOrder.owner, takerOrder.owner);
    }

    function mintAndTransferAsset(
        LibModels.Asset calldata asset,
        address from,
        address to
    ) internal {
        if (asset.assetType == LibModels.AssetType.ERC20) {
            uint256 fee = 0;
            if (_serviceFee > 0) {
                fee = asset.assetValue * _serviceFee / 10000;
            }

            IERC20Upgradeable(asset.assetContract).safeTransferFrom(from, address(this), asset.assetValue);
            IERC20Upgradeable(asset.assetContract).safeTransfer(to, asset.assetValue);
        } else if (asset.assetType == LibModels.AssetType.ERC721) {
            if (!IERC721Mintable(asset.assetContract).exists(asset.assetId)) {
                LibModels.AssetInfo memory assetInfo = abi.decode(asset.metadata, (LibModels.AssetInfo));
                IERC721Mintable(asset.assetContract).mint(to, asset.assetId);
                IERC721Mintable(asset.assetContract).setTokenUri(asset.assetId, assetInfo.tokenUri);
            } else {
                IERC721Upgradeable(asset.assetContract).safeTransferFrom(from, to, asset.assetId);
            }
        }
    }

    function validate(bytes32 orderHash, address orderOwner, bytes calldata signature) internal view {
        // Verify the signer is not address(0)
        require(orderOwner != address(0), "Invalid maker");

        if (_msgSender() != orderOwner) {
            address signer = _hashTypedDataV4(orderHash).recover(signature);
            _validate(signer, orderOwner, orderHash, signature, "Order invalid signature");
        }
    }
}

