pragma solidity ^0.8.4;

library LibModels {
    bytes32 internal constant ASSET_TYPEHASH =
        keccak256("Asset(uint8 assetType,address assetContract,uint256 assetId,uint256 assetValue)");

    bytes32 internal constant ROYALTY_TYPEHASH = keccak256("Royalty(address account,uint256 value)");

    bytes32 internal constant MAKER_ORDER_TYPEHASH =
        keccak256(
            "Order(uint8 orderSide,address owner,Asset makerAsset,Asset takerAsset,uint256 nonce,uint256 startTime,uint256 endTime,bytes params)Asset(uint8 assetType,address assetContract,uint256 assetId,uint256 assetValue)"
        );

    bytes32 internal constant ASSET_INFO_TYPEHASH =
        keccak256(
            "AssetInfo(address creator,uint256 tokenId,string tokenUri,uint256 supply,Royalty[] royalties)Royalty(address account,uint256 value)"
        );

    // EIP1271 magic value
    bytes4 public constant MAGIC_VALUE_EIP1271 = 0x1626ba7e;

    struct RoyaltyPart {
        address account;
        uint256 value;
    }

    struct AssetInfo {
        // creator address
        address creator;
        // Token ID of NFT
        uint256 tokenId;
        // NFT URL (stored either on IPFS or S3)
        string tokenUri;        
        // Total supply of tokens (for ERC1155, for ERC721 - might be 0)
        uint256 supply;
        // NFT royalties info
        RoyaltyPart[] royalties;
    }

    /// Generic asset type
    enum AssetType {
        // Native token
        NATIVE,
        // ERC20 token
        ERC20,
        // ERC721 token
        ERC721,
        // ERC1155 token
        ERC1155
    }

    /// Generic asset
    struct Asset {
        // Token type
        AssetType assetType;
        // Address of smart contract for token
        address assetContract;
        // Asset ID (tokenID)
        uint256 assetId;
        // Asset value
        uint256 assetValue;
        // Token data
        bytes metadata;
    }

    /// Order side
    enum OrderSide {
        // Ask order
        ASK,
        // Bid order
        BID
    }

    struct Order {
        // Order side (ASK or BID)
        OrderSide orderSide;
        // Order owner
        address owner;
        // Maker asset (either NFT for listing or for token amount for make offer)
        Asset makerAsset;
        // Taker asset (either token for purchase or NFT to make offer)
        Asset takerAsset;
        // unique nonce
        uint256 nonce;
        // start time for to be valid
        uint256 startTime;
        // end time for order to be valid
        uint256 endTime;
        // additional parameters
        bytes params;
        // signature
        bytes signature;
    }

    function hash(Asset memory asset) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(ASSET_TYPEHASH, asset.assetType, asset.assetContract, asset.assetId, asset.assetValue)
            );
    }

    function hash(Order memory order) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    MAKER_ORDER_TYPEHASH,
                    order.orderSide,
                    order.owner,
                    hash(order.makerAsset),
                    hash(order.takerAsset),
                    order.nonce,
                    order.startTime,
                    order.endTime,
                    keccak256(order.params)
                )
            );
    }

    function hash(RoyaltyPart memory royalty) internal pure returns (bytes32) {
        return keccak256(abi.encode(ROYALTY_TYPEHASH, royalty.account, royalty.value));
    }

    function hash(AssetInfo memory assetInfo) internal pure returns (bytes32) {
        bytes32[] memory royaltiesData = new bytes32[](assetInfo.royalties.length);
        for (uint256 i = 0; i < assetInfo.royalties.length; i++) {
            royaltiesData[i] = hash(assetInfo.royalties[i]);
        }

        return
            keccak256(
                abi.encode(
                    ASSET_INFO_TYPEHASH,
                    assetInfo.creator,
                    assetInfo.tokenId,
                    keccak256(bytes(assetInfo.tokenUri)),                    
                    assetInfo.supply,                    
                    keccak256(abi.encodePacked(royaltiesData))
                )
            );
    }
}

