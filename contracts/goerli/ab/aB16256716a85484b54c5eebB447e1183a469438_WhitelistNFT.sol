    function burn(uint256[] memory tokenIds) external nonReentrant onlyAdmin {
        require(tokenIds.length > 0, "Invalid length");

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(tokenIds[i] > 0 && tokenIds[i] <= _tokenCounter.current(), "Invalid tokenId");
            _burn(tokenIds[i]);
        }

        emit Burnt(_msgSender(), tokenIds);
    }

    /**
     *  @notice Return current base URI.
     */
    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    /**
     *  @notice Replace current base URI by new base URI.
     *
     *  @dev    Only owner can call this function.
     */
    function setBaseURI(string memory _newURI) external onlyOwner {
        baseURI = _newURI;
    }

    /**
     *  @notice Mapping token ID to base URI in ipfs storage
     *
     *  @dev    All caller can call this function.
     */
    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token.");
        return _baseURI();
    }

    function getTokenCounter() public view returns (uint256) {
        return _tokenCounter.current();
    }

    /**
