 */
contract WhitelistNFT is Validatable, ERC721EnumerableUpgradeable, ReentrancyGuardUpgradeable {
    using CountersUpgradeable for CountersUpgradeable.Counter;

    CountersUpgradeable.Counter private _tokenCounter;

    /**
     *  @notice baseURI store the value of the ipfs url of NFT images
     */
    string public baseURI;

    event MintedWhitelistNFT(address[] users, uint256[] tokenIds);
    event Burnt(address indexed caller, uint256[] tokenIds);

    /**
     * @notice Init contract
     * @dev    Replace for contructor
     * @param _admin Address of admin
     * @param _name Name of NFT
     * @param _symbol Symbol of NFT
     * @param baseUri_ BaseURI of NFT
     */
    function initialize(
        IAdmin _admin,
        string memory _name,
        string memory _symbol,
        string memory baseUri_
    ) public initializer {
        __Validatable_init(_admin);
        __ReentrancyGuard_init();
        __ERC721_init(_name, _symbol);

        baseURI = baseUri_;
    }

    /**
     * @notice Mint NFT for user
     * @dev    Only admin can call this function and user must have a NFT
     * @param users List address of the user
     *
     * emit {MintedWhitelistNFT} events
     */
    function mint(address[] memory users) public onlyAdmin {
        require(users.length > 0, "Invalid length");
        uint256[] memory tokenIds = new uint256[](users.length);
        for (uint256 i = 0; i < users.length; i++) {
            require(users[i] != address(0), "Invalid address");
            require(balanceOf(users[i]) == 0, "Only own a NFT");

            _tokenCounter.increment();
            tokenIds[i] = _tokenCounter.current();
            _mint(users[i], _tokenCounter.current());
        }

        emit MintedWhitelistNFT(users, tokenIds);
    }

