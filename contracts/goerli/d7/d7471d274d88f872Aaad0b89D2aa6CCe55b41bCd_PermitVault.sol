pragma solidity 0.8.13;

import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/Multicall.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./Permit20.sol";
import "./Permit721.sol";
import "./Permit1155.sol";

contract PermitVault is
    Initializable,
    UUPSUpgradeable,
    Permit20,
    Permit721,
    Permit1155,
    PausableUpgradeable,
    Multicall
{
    address constant ETH_ADDRESS = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;

    using AddressUpgradeable for address payable;
    using SafeERC20 for IERC20;

    /// Events
    event PlatformFeeTransferred(address indexed previousFeeAddress, address indexed newReceiverAddress);
    event TransferPermit20(address token, address indexed owner, uint256 platformFee, uint32 metadata);

    event TransferPermit721(
        AssetIdentifierData asset,
        address token,
        address indexed receiverAddress,
        uint256 ownerFee,
        uint256 platformFee,
        address indexed ownVaultApprovalAddr,
        uint256 ownVaultApprovalFee,
        uint32 metadata
    );
    event TransferPermit1155(
        AssetIdentifierData asset,
        address token,
        address indexed receiverAddress,
        uint256 ownerFee,
        uint256 platformFee,
        address indexed ownVaultApprovalAddr,
        uint256 ownVaultApprovalFee,
        uint256 value,
        uint32 metadata
    );

    address private _platformFeeAddr;

    /**
     * Initializes the contract
     * @param   name The contract name.
     * @param   version The contract Address.
     */
    function initialize(string memory name, string memory version) public initializer {
        __UUPSUpgradeable_init();
        __Ownable_init();
        __Pausable_init();
        __EIP712_init(name, version);
        _platformFeeAddr = owner();
    }

    /**
     * @notice Function that should revert when msg.sender is not authorized to upgrade the contract.
     *          Called by upgradeTo and upgradeToAndCall.
     * @param   newImplementation new implamentation Address.
     */
    function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}

    /**
     * @notice Transfer ETH or ERC20 to "to" param if the value is greater than 0 and "to" is not a Zero address
     * @param   token Token Address.
     * @param   from Sender Address.
     * @param   to Receiver Address.
     * @param   value Amount.
     */
    function transfer(
        address token,
        address from,
        address to,
        uint256 value
    ) private {
        require(to != address(0), "PermitVault: zero address is not authorized");
        if (value == 0) {
            return;
        }

        if (token == ETH_ADDRESS) {
            payable(to).sendValue(value);
        } else {
            IERC20(token).safeTransferFrom(from, to, value);
        }
    }

    /**
     * @notice Returns the address of the current platform fee address.
     * @return  address Plaform Fee Address.
     */
    function platformFeeAddr() public view returns (address) {
        return _platformFeeAddr;
    }

    /**
     * @notice External method to transfer erc20 token with signature and fees.
     * @dev The signature can be provisioned by owner and GOTCHA.
     * @param   registry Address
     * @param   from Address.
     * @param   to Address.
     * @param   value Amount.
     * @param   platformFee Amount.
     * @param   nonce Nonce Value.
     * @param   deadline Deadline Timestamp.
     * @param   metadata PublicationId and actionType.
     *           - PublicationId (8 digits)
     *           - ActionType Internal Action Identifier (2 digits)
     * @param   signatures  Signatures. position0 owner.
     *                                  position1 gotcha.
     */
    function transfer20(
        address registry,
        address from,
        address to,
        uint256 value,
        uint256 platformFee,
        uint256 nonce,
        uint256 deadline,
        uint32 metadata,
        bytes[2] calldata signatures
    ) external whenNotPaused {
        super.transfer20WithSign(registry, from, to, value, platformFee, nonce, deadline, signatures);

        transfer(registry, from, _platformFeeAddr, platformFee);

        emit TransferPermit20(registry, _platformFeeAddr, platformFee, metadata);
    }

    /**
     * @notice External method to transfer erc721 token with signature and fees.
     * @dev The signature can be provisioned by the nft owner or GOTCHA.
     * @param   assetIdentifierData Asset Identifier Data.
     * @param   token Token Address.
     * @param   to Address.
     * @param   feeData Fee Data.
     * @param   nonce Nonce Value.
     * @param   deadline Deadline Timestamp.
     * @param   metadata PublicationId and actionType.
     *           - PublicationId (8 digits)
     *           - ActionType Internal Action Identifier (2 digits)
     * @param   signature  Signature.
     */
    function transfer721(
        AssetIdentifierData memory assetIdentifierData,
        address token,
        address to,
        FeeData memory feeData,
        uint256 nonce,
        uint256 deadline,
        uint32 metadata,
        bytes memory signature
    ) external payable whenNotPaused {
        revertIfWrongPayableValue(token, feeData);

        super.transfer721WithSign(assetIdentifierData, to, feeData, nonce, deadline, signature);

        address sender = msg.sender;
        address ownVaultApprovalFeeAddr = feeData.ownVaultApprovalFeeAddr;
        uint256 ownerFee = feeData.ownerFee;
        uint256 ownVaultApprovalFee = feeData.ownVaultApprovalFee;
        uint256 platformFee = feeData.platformFee;

        transfer(ETH_ADDRESS, sender, ownVaultApprovalFeeAddr, ownVaultApprovalFee);
        transfer(token, sender, feeData.receiverFeeAddr, ownerFee);
        transfer(token, sender, _platformFeeAddr, platformFee);

        emit TransferPermit721(
            assetIdentifierData,
            token,
            feeData.receiverFeeAddr,
            ownerFee,
            platformFee,
            ownVaultApprovalFeeAddr,
            ownVaultApprovalFee,
            metadata
        );
    }

    /**
     * @notice External method to transfer erc1155 token with signature and fees.
     * @dev The signature can be provisioned by the nft owner or GOTCHA.
     * @param   assetIdentifierData Asset Identifier Data.
     * @param   token Token Address.
     * @param   from Address.
     * @param   to Address.
     * @param   value Amount.
     * @param   feeData Fee Data.
     * @param   nonce Nonce Value.
     * @param   deadline Deadline Timestamp.
     * @param   metadata PublicationId and actionType.
     *           - PublicationId (8 digits)
     *           - ActionType Internal Action Identifier (2 digits)
     * @param   data Data.
     * @param   signature Signature.
     */
    function transfer1155(
        AssetIdentifierData memory assetIdentifierData,
        address token,
        address from,
        address to,
        uint256 value,
        FeeData memory feeData,
        uint256 nonce,
        uint256 deadline,
        uint32 metadata,
        bytes memory data,
        bytes memory signature
    ) external payable whenNotPaused {
        revertIfWrongPayableValue(token, feeData);

        super.transfer1155WithSign(assetIdentifierData, from, to, value, feeData, nonce, deadline, data, signature);

        address sender = msg.sender;
        transfer(ETH_ADDRESS, sender, feeData.ownVaultApprovalFeeAddr, feeData.ownVaultApprovalFee);
        transfer(token, sender, feeData.receiverFeeAddr, feeData.ownerFee);
        transfer(token, sender, _platformFeeAddr, feeData.platformFee);

        emit TransferPermit1155(
            assetIdentifierData,
            token,
            feeData.receiverFeeAddr,
            feeData.ownerFee,
            feeData.platformFee,
            feeData.ownVaultApprovalFeeAddr,
            feeData.ownVaultApprovalFee,
            value,
            metadata
        );
    }

    /**
     * @notice Replaces the contract platform fee address.
     * @dev Can only be called by the current owner.
     * @param   newPlatformFeeAddress New Platform Fee Address.
     */
    function transferPlatformFeeAddress(address newPlatformFeeAddress) external whenNotPaused onlyOwner {
        require(newPlatformFeeAddress != address(0), "PermitVault:: new platform fee address is the zero address");
        require(
            newPlatformFeeAddress != _platformFeeAddr,
            "PermitVault:: newPlatformFeeAddress is the same as the current one"
        );
        address oldPlatformFeeAddr = _platformFeeAddr;
        _platformFeeAddr = newPlatformFeeAddress;
        emit PlatformFeeTransferred(oldPlatformFeeAddr, newPlatformFeeAddress);
    }

    /**
     * @notice revert if payable value is not valid
     * @dev Internal method.
     * @param   token Token Address
     * @param   feeData Fee Data.
     */
    function revertIfWrongPayableValue(address token, FeeData memory feeData) internal {
        if (token == ETH_ADDRESS) {
            require(
                feeData.platformFee + feeData.ownerFee + feeData.ownVaultApprovalFee == msg.value,
                "PermitVault:: The ETH amount sent is wrong"
            );
        } else {
            require(feeData.ownVaultApprovalFee == msg.value, "PermitVault:: The ETH amount sent is wrong");
        }
    }

    /**
     * @notice The owner revoke all approvals.
     * @dev
     *  1- Pause the contract, the functions transfer721, transfer1155 and transferPlatformFeeAddress stops working.
     *  2- Renounce ownership.
     *  Can only be called by the current owner.
     */
    function revokeApprovals() external onlyOwner {
        _pause();
        _transferOwnership(address(0));
    }
}

