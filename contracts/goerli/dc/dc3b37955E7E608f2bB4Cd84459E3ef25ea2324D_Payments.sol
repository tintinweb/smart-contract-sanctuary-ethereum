pragma solidity ^0.8.1;
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

interface Parent {
    struct RequestStatus {
        uint256 requestId;
        uint256 paid; // amount paid in link
        bool fulfilled; // whether the request has been successfully fulfilled
        uint256[] randomWords;
    }
    struct PacksBought {
        uint256 projectID;
        uint256 packSoldID;
        address purchasedBy;
        address openedBy;
        address ownedBy;
        uint256[] wonItems;
        uint256 totalPaid;
        address currencyPaid;
        uint256 ownerRecieved;
        address ownerCurrency;
        address seapadCurrency;
        uint256 seapadRecieved;
        bool minted;
        RequestStatus requestStatus;
    }

    function openPack(uint256 projectid, uint256 packSoldID_)
        external
        payable
        returns (uint256[] calldata wonItems);

    function getPacksSold(uint256 projectid, uint256 packSoldID_)
        external
        view
        returns (PacksBought memory);

    function updatePackOwner(
        uint256 projectid,
        uint256 packSoldID_,
        address newOwner,
        address from
    ) external;

    function packRecievedImages(uint256 projectId)
        external
        view
        returns (bool recieved);

    function getProjectUris(uint256 projectId)
        external
        view
        returns (string memory unopenedUri, string memory openedUri);
}

contract Pack is ERC721, Ownable {
    address public seapadAddress;
    address public seapadOwner;
    address public packCollectionOwner;

    uint256 public projectid;
    uint256 public networkid;
    string private baseExtension = ".json";
    string pendingUri = "https://cdn.seapad.io/pending.json";

    uint256 public amountSeapadWithdrawable = 0;
    uint256 public amountOwnerWithdrawable = 0;
    uint256 public seapadRoyaltyPercentage = 5000; //default 50% goes to seapad, can add 2 commas.

    // SPLIT INCOMING ROYALTIES AMONGST THE OWNERS OF SEAPAD AND OWNER OF THE COLLECTION.
    receive() external payable {
        //calculate 19% to seapad and rest to owner in solidity
        uint256 seapadAmount = (msg.value * seapadRoyaltyPercentage) / 10000;
        uint256 ownerAmount = msg.value - seapadAmount;
        amountSeapadWithdrawable += seapadAmount;
        amountOwnerWithdrawable += ownerAmount;
    }

    // ONLY SEAPAD  OR SEAPAD OWNER CAN CAN FUNCTIONS WITH THIS MODIFIER
    modifier onlySeapad() {
        _;
        require(
            msg.sender == seapadAddress || msg.sender == seapadOwner,
            "Only seapadAddress contract can call this (Pack.sol)."
        );
    }

    // ONLY COLLECTION OWNER CAN CALL FUNCTIONS WITH THIS MODIFIER
    modifier onlyCollectionOwner() {
        _;
        require(
            msg.sender == packCollectionOwner,
            "Only contract owner can call this (Pack.sol)."
        );
    }
    modifier onlyCollectionOwnerOrSeapad() {
        _;
        require(
            msg.sender == packCollectionOwner || msg.sender == seapadOwner,
            "Only contract owner or Seapad Owner can call this (Pack.sol)."
        );
    }

    function uint2str(uint256 _i)
        public
        pure
        returns (string memory _uintAsString)
    {
        if (_i == 0) {
            return "0";
        }
        uint256 j = _i;
        uint256 len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint256 k = len;
        while (_i != 0) {
            k = k - 1;
            uint8 temp = (48 + uint8(_i - (_i / 10) * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }

    // WITHDRAW FUNCTION. (Remains public in case no funds for owner, he can request hes buddy to withdraw but to owners wallet.)
    function WithdrawAndSplitETHFunds() public {
        payable(packCollectionOwner).transfer(amountSeapadWithdrawable);
        payable(seapadAddress).transfer(amountOwnerWithdrawable);
        amountSeapadWithdrawable = 0;
        amountOwnerWithdrawable = 0;
    }

    // WITHDRAW FUNCTION. (Remains public in case no funds for owner, he can request hes buddy to withdraw but to owners wallet.)
    function WithdrawAndSplitTokenFunds(address tokenAddress) public {
        IERC20 token = IERC20(tokenAddress);
        //calculate seapad profit
        uint256 seapadAmount = (token.balanceOf(address(this)) *
            seapadRoyaltyPercentage) / 10000;
        uint256 ownerAmount = token.balanceOf(address(this)) - seapadAmount;
        token.transfer(packCollectionOwner, ownerAmount);
        token.transfer(seapadAddress, seapadAmount);
    }

    // UPDATE ROYALTY PERCENTAGE
    function updateRoyaltyPercentage(uint256 newPercentage) public onlySeapad {
        seapadRoyaltyPercentage = newPercentage;
    }

    // PACKS ARE ONLY TRANSFARABLES IF THEY ARE UNOPENED
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override {
        Parent.PacksBought memory pack = Parent(seapadAddress).getPacksSold(
            projectid,
            tokenId
        );
        //if openRequested then packs are not transfarabele, only if to is the dead wallet
        if (pack.requestStatus.requestId != 0) {
            require(to == address(0), "Pack is open, cannot transfer");
        }

        Parent p = Parent(seapadAddress);
        p.updatePackOwner(projectid, tokenId, to, from);
    }

    constructor(
        address _packCollectionOwner,
        address _seapadAddress,
        address _seapadOwner,
        uint256 _projectid,
        string memory _name,
        string memory _symbol,
        uint256 _networkid
    ) ERC721(_name, _symbol) {
        seapadAddress = _seapadAddress;
        packCollectionOwner = _packCollectionOwner;
        seapadOwner = _seapadOwner;
        projectid = _projectid;
        transferOwnership(_seapadOwner);
        networkid = _networkid;
    }

    // RETURNS IF PACK HAS RECEIVED IMAGES
    function packRecievedImages() public view returns (bool recieved) {
        Parent parentContract = Parent(seapadAddress);
        return parentContract.packRecievedImages(projectid);
    }

    // BURNS THE PACK
    function burnPack(uint256 tokenId) public {
        require(
            ownerOf(tokenId) == msg.sender ||
                msg.sender == seapadAddress ||
                msg.sender == owner(),
            "you cant burn this pack"
        );
        _burn(tokenId);
    }

    // RETURNS IF PACK HAS BEEN OPENED FROM MAINCONTRACT
    function packOpened(uint256 packid) public view returns (bool) {
        Parent parentContract = Parent(seapadAddress);
        Parent.PacksBought memory pack = parentContract.getPacksSold(
            projectid,
            packid
        );
        return pack.minted;
    }

    // OPENS THE PACK VIA THE MAIN SEAPAD CONTRACT (NEED ETH TO OPEN)
    function openPack(uint256 packid) public payable {
        Parent parentContract = Parent(seapadAddress);

        parentContract.openPack{value: msg.value}(projectid, packid);
    }

    // RETURNS THE TOKEN URI OF THIS PACK.
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        Parent parentContract = Parent(seapadAddress);
        (string memory unopenedUri, ) = parentContract.getProjectUris(
            projectid
        );

        if (bytes(unopenedUri).length > 0) {
            return
                string(
                    abi.encodePacked(
                        unopenedUri,
                        uint2str(tokenId),
                        baseExtension
                    )
                );
        } else {
            return pendingUri;
        }
    }

    // UPDATE THE PARENTS ADDRESS AND SEAPAD OWNER
    function updateParentContract(address _seapadAddress, address _seapadOwner)
        public
        onlySeapad
    {
        seapadAddress = _seapadAddress;
        seapadOwner = _seapadOwner;
    }

    // RETURN THE STOREFRONT URI (USED FOR TRADING WEBSITES)
    function contractURI() public view returns (string memory) {
        return
            string(
                abi.encodePacked(
                    "https://cdn.seapad.io/collections/",
                    uint2str(networkid),
                    "/",
                    uint2str(projectid),
                    "/storefront.json"
                )
            );
    }

    // ONLY ALLOW SEAPAD CONTRACTS TO MINT NEW NFT's
    function mintNewToken(address owner, uint256 idToMint) public onlySeapad {
        _mint(owner, idToMint);
