pragma solidity ^0.8.1;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract NFT is ERC721, Ownable {
    address public seapadOwner;
    address public seapadAddress;
    bool public isJson = false;
    string public baseURI;

    receive() external payable {}

    fallback() external payable {}

    // ONLY SEAPAD OR SEAPADS OWNER CAN CALL FUNCTIONS WITH THIS MODIFIER
    modifier onlySeapad() {
        _;
        require(
            msg.sender == seapadOwner || msg.sender == seapadAddress,
            "Only SeaPad parent/owner can call this (NFT.sol)"
        );
    }
    // ONLY SEAPAD OR SEAPADS OWNER CAN CALL FUNCTIONS WITH THIS MODIFIER
    modifier onlySeapadOrOwner() {
        _;
        require(
            msg.sender == seapadOwner ||
                msg.sender == seapadAddress ||
                msg.sender == owner(),
            "Only SeaPad parent/owner can call this (NFT.sol)"
        );
    }

    // UPDATE SEAPAD OWNER AND SEAPADS ADDRESS
    function updateSeapadAddress(address seapadOwner_, address seapadAddress_)
        public
        onlySeapad
    {
        seapadOwner = seapadOwner_;
        seapadAddress = seapadAddress_;
    }

    constructor(
        string memory thisUri,
        address _seapadOwner,
        address _seapadAddress,
        string memory _name,
        string memory _symbol,
        address collectionOwner,
        bool _isJson
    ) ERC721(_name, _symbol) {
        seapadOwner = _seapadOwner;
        seapadAddress = _seapadAddress;
        baseURI = thisUri;
        isJson = _isJson;
        transferOwnership(collectionOwner);
    }

    //UPDATE THE BASEURI FOR METADATA
    function setBaseURI(string memory uri) public onlySeapadOrOwner {
        baseURI = uri;
    }

    //RETURNS THE TOKEN URI FOR THIS TOKEN.
    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    baseURI,
                    uint2str(_tokenId),
                    isJson ? ".json" : ""
                )
            );
    }

    // HELPER FUNCTION
    function uint2str(uint256 _i) internal pure returns (string memory str) {
        if (_i == 0) {
            return "0";
        }
        uint256 j = _i;
        uint256 length;
        while (j != 0) {
            length++;
            j /= 10;
        }
        bytes memory bstr = new bytes(length);
        uint256 k = length;
        j = _i;
        while (j != 0) {
            bstr[--k] = bytes1(uint8(48 + (j % 10)));
            j /= 10;
        }
        str = string(bstr);
    }

    // ONLY SEAPAD CAN MINT TOKENS (WHEN A PACK IS OPENED).
    function mintNewToken(address owner, uint256 idToMint)
        public
        onlySeapad
        returns (bool)
    {
        _mint(owner, idToMint);
        return true;
    }

    // WITHDRAW THE PROFITS FROM THE CONTRACT (ONLY CALLABLE BY OWNER OF THE COLLECTION)
    function withdrawTokens(address tokenAddress) public onlyOwner {
        require(
            msg.sender == owner(),
            "Only owner can withdraw tokens (NFT.sol)"
        );
        IERC20 token = IERC20(tokenAddress);
        token.transfer(msg.sender, token.balanceOf(address(this)));
    }

    // WITHDRAW THE PROFITS FROM THE CONTRACT (ONLY CALLABLE BY OWNER OF THE COLLECTION)
    function withdrawETH(uint256 amount) public onlyOwner {
        require(msg.sender == owner(), "Only owner can withdraw ETH (NFT.sol)");
        // address payable owner = owner();
        payable(msg.sender).transfer(amount);
    }
}

