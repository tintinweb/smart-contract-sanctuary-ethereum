pragma solidity ^0.8.4;
pragma experimental ABIEncoderV2;

import "../tokens/ERC721/royaltyNftBase.sol";

contract ERC721RoyaltyFactory{
    event NewRoyaltyTokenCreated(address indexed tokenAddress, address indexed creator, uint indexed time);
    address immutable public NFT_MARKET_AFRICA_HUB;
    /**
   @dev RoyaltyTokenInfo struct holds information about each created royalty token*/
    
    struct RoyaltyTokenInfoERC721 {
        address creator;
        address gallery;
        address tokenAddress;
        uint96 royaltyPercentage;
        uint tokenId;
        uint amount;
        uint itemId;
        string tokenName;
        string symbol;
    }


    /** 
    @dev tokenInfo mapping takes the address of the creator and points to another 
    mapping that takes the token address and points to the RoyaltyTokenInfo struct
    */
    mapping (address => mapping (uint => RoyaltyTokenInfoERC721)) public tokenInfoERC721;
    mapping (address => address[]) creatorTokens;
    mapping (address => bool) isCreator;
    constructor(address _hub){
        NFT_MARKET_AFRICA_HUB = _hub;
    }

    function createERC721RoyaltyToken(address _gallery, uint _amount, uint96 _percentage, string memory _tokenName, string memory _symbol, string memory _uri) public returns(address){
        require(_gallery != address(0), "Gallery Cannot Be Address ZERO");
        require(_amount >= 1, "Amount must be grater than zero");
        
        address[] storage tokens = creatorTokens[msg.sender];
        uint nextItemId = tokens.length+1;
        
        RoyaltyTokenBaseERC721 royaltyToken = new RoyaltyTokenBaseERC721(msg.sender, _gallery, _amount, _percentage, _tokenName, _symbol, _uri);
        address tokenAddress = address(royaltyToken);
        
        RoyaltyTokenInfoERC721 memory royaltyTokenInfo = tokenInfoERC721[msg.sender][nextItemId];
        tokens.push(tokenAddress);
        royaltyTokenInfo = RoyaltyTokenInfoERC721(msg.sender, _gallery, tokenAddress, _percentage, 1, _amount, nextItemId, _tokenName, _symbol);

        emit NewRoyaltyTokenCreated(tokenAddress, msg.sender, block.timestamp);
        return tokenAddress;
    }

    function getTokenDetails(address _creator, uint _itemId) public view returns(address, uint96, uint, uint ,string memory, string memory){
        RoyaltyTokenInfoERC721 memory royaltyTokenInfo = tokenInfoERC721[_creator][_itemId];
        address creator = royaltyTokenInfo.creator;
        uint96 royaltyPercentage = royaltyTokenInfo.royaltyPercentage;
        uint tokenId = royaltyTokenInfo.tokenId;
        uint amount = royaltyTokenInfo.amount;
        string memory tokenName = royaltyTokenInfo.tokenName;
        string memory symbol = royaltyTokenInfo.symbol;
        return (creator,royaltyPercentage,tokenId,amount,tokenName,symbol);
    }

    function getTokenAddress(address _creator, uint _itemId) public view returns(address){
        require(_itemId >= 1);
        uint index = _itemId-1;
        address[] memory addresses = creatorTokens[_creator];
        address tokenAddress = addresses[index];

        return tokenAddress;
    } 
}

interface IERC721RoyaltyFactory{
    function createERC721RoyaltyToken(address _gallery, uint _amount, uint96 _percentage, string memory _tokenName, string memory _symbol, string calldata _uri) external returns(address);
    function getTokenDetails(address _gallery, address _tokenAddress) external view returns(address, uint96, uint, uint ,string calldata, string calldata);
    function getTokenAddress(address _creator, uint _itemId) external view returns(address);
}

