pragma solidity ^0.8.4;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import { IERC721RoyaltyFactory } from "./factory/ERC721RoyaltyFactory.sol";
import { IERC1155RoyaltyFactory } from "./factory/ERC1155RoyaltyFactory.sol";

contract NftMarketAfricaHub is AccessControl, Pausable {
   event NewRoyaltyTokenCreated(address indexed RoyaltyToken, address indexed creator, uint indexed time);

    address public ERC1155RoyaltyFactoryAddress;
    address public ERC721RoyaltyFactoryAddress;
    
    constructor (address _deafultAdmin) {
        _grantRole(DEFAULT_ADMIN_ROLE, _deafultAdmin);
    }

    function updateERC1155RoyaltyFactoryAddress(address newERC1155RoyaltyFactoryAddress) public onlyRole(DEFAULT_ADMIN_ROLE){
        ERC1155RoyaltyFactoryAddress = newERC1155RoyaltyFactoryAddress;
    }

    function updateERC721RoyaltyFactoryAddress(address newERC721RoyaltyFactoryAddress) public onlyRole(DEFAULT_ADMIN_ROLE){
        ERC721RoyaltyFactoryAddress = newERC721RoyaltyFactoryAddress;
    }

    /**
    @dev Creates a new royalty token and adds it to the tokenInfo mapping
     */
    function createRoyaltyNftERC1155 (
        address _gallery, 
        uint _amount, 
        uint96 _percentage, 
        string memory _uri
        ) public returns(address){
        require(_gallery != address(0), "Gallery Cannot Be Address ZERO");
        require(_amount >= 1, "Amount must be grater than zero");

        address tokenAddress = IERC1155RoyaltyFactory(ERC1155RoyaltyFactoryAddress).createERC1155RoyaltyToken(_gallery, _amount, _percentage, _uri);
        emit NewRoyaltyTokenCreated(tokenAddress, msg.sender, block.timestamp);
    
        return tokenAddress;   
    }

    function createRoyaltyNftERC721 (
        address _gallery, 
        uint _amount, 
        uint96 _percentage,
        string memory _tokenName, 
        string memory _symbol, 
        string memory _uri
        ) public returns(address){
        require(_gallery != address(0), "Gallery Cannot Be Address ZERO");
        require(_amount >= 1, "Amount must be grater than zero");

        address tokenAddress = IERC721RoyaltyFactory(ERC721RoyaltyFactoryAddress).createERC721RoyaltyToken(_gallery, _amount, _percentage,_tokenName, _symbol, _uri);
        emit NewRoyaltyTokenCreated(tokenAddress, msg.sender, block.timestamp);
    
        return tokenAddress;   
    }

    function pause() public onlyRole(DEFAULT_ADMIN_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

