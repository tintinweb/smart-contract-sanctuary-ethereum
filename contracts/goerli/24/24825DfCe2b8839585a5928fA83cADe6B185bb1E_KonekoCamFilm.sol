pragma solidity ^0.8.17;

import "./BaseWhitelist.sol";
import "hardhat/console.sol";

// ホワイトリストを購入できるプライベートセール機能。
abstract contract BasePurchaseWhitelist is BaseWhitelist{
    uint internal _secondaryWhitelistSalePrice;
    bool internal _isSecondaryWhitelistSale = false;
    bytes32 private SecondaryWhitelistRoot;
    uint internal _limitedWLAmount;
    mapping(address => uint) private secondaryWhitelistMap;
    uint internal _secondaryWhitelistAmount = 0;
    
    //
    constructor(uint limitedWLAmount, uint secondaryWhitelistSalePrice){
        _limitedWLAmount = limitedWLAmount;
        _secondaryWhitelistSalePrice = secondaryWhitelistSalePrice;
    }

    // whitelist権限購入機能(whitelist抽選外れた方への販売)
    function _secondaryWhitelistSale(address account, uint allowedAmount, bytes32[] calldata proofData) internal{
        //
        require( _isSecondaryWhitelistSale == true, "Invalid Flag");
        //
        require( _msgSender() == account, "Invalid Account");
        //
        require(msg.value == _secondaryWhitelistSalePrice, "Invalid Value");
        // 購入制限 
        require( _secondaryWhitelistAmount < _limitedWLAmount, "Limited" );
        // 1人付き1つ。
        require( _getAllowedAmountOf2ndWL(_msgSender()) < 1, "Not Allowed");
        //
        require(SecondaryWhitelistRoot != "", "Enpty");
        //
        require(_verifyWithAddressAmount(account, allowedAmount, proofData, SecondaryWhitelistRoot) == true, "Invalid Proof");
        // 権利付与
        secondaryWhitelistMap[account] = allowedAmount;
        _secondaryWhitelistAmount++;
    }

    // secondaryWhitelistの権利でmintする機能
    function _privateSaleWith2ndWL(address account, uint tokenId, uint requestedAmount) internal virtual{
        require( _isPrivateSale == true, "Invalid Flag.");
        require( _getAllowedAmountOf2ndWL(account) > 0, "Not Allowed");
        _sale(account,  tokenId, _privateSalePrice, requestedAmount);
    }

    // 
    function toggleIsSecondaryWhitelistSale() external onlyOwner{
        _isSecondaryWhitelistSale = !_isSecondaryWhitelistSale;
    }

    //
    function setSecondaryWhitelistRoot(bytes32 newRoot) external onlyOwner{
        SecondaryWhitelistRoot = newRoot;
    }

    // 購入可能数
    function _getAllowedAmountOf2ndWL(address account) internal view returns(uint){
        return secondaryWhitelistMap[account];
    }

}

