//
abstract contract BaseSaleERC1155 is ERC1155, Ownable{

    //
    function _sale(address account,  uint tokenId, uint salePrice, uint requestedAmount) internal{
        require(msg.value == salePrice * requestedAmount, "Invalid Value.");
        require( _msgSender() == account, "Ivalid Account.");
        //
        _mint(_msgSender(), tokenId, requestedAmount, "");
    }

    // To transport ETH out of the smart contract
    function withdrawFunds() public virtual onlyOwner{
        payable(_msgSender()).transfer(address(this).balance);
    }

}

