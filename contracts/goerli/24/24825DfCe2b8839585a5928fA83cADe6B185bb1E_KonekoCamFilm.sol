pragma solidity ^0.8.10;
//
import "./BaseSaleERC1155.sol";
import "../interface/IExchangedItem.sol";

//
abstract contract BaseTicket is BaseSaleERC1155{
    //
    bool internal _isMigrationAllowed = false;
    uint internal _migratedAmount = 0;
    IExchangedItem public DestContract;

    // 交換したい。マークルプルーフを交換先に渡したい。
    function _migrateToken(
        uint ownedTokenId, uint requestedTokenId
    ) internal virtual returns(uint){
        //
        require(_isMigrationAllowed == true, "Invalid Flag");
        // TODO: 交換元のアドレスがセットされているか？この書き方あってる？
        require(address(DestContract) != address(0),  "Invalid dest");
        // 所有数確認
        require(balanceOf(_msgSender(), ownedTokenId) > 0, "Invalid Balance");
        // チケットの消費
        _burn(_msgSender(), ownedTokenId, 1);
        // メインコントラクト側でのmint
        uint mintedId = DestContract.receiveExchange(_msgSender(), requestedTokenId);
        //
        _migratedAmount++;
        //
        return mintedId;
    }

    //
    function setDestContract(address destContract) external onlyOwner{
        DestContract = IExchangedItem(destContract);
    }

    //
    function toggleIsMigrationAllowed() external onlyOwner{
        _isMigrationAllowed = !_isMigrationAllowed;
    }

}

