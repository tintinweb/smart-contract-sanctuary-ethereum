//
abstract contract BaseWhitelist is BaseSaleERC1155{
    //
    bool internal _isPrivateSale = false;
    uint internal _privateSalePrice;
    bytes32 internal _WhitelistRoot;
    
    constructor(uint privateSalePrice){
        _privateSalePrice = privateSalePrice;
    }

    //
    function _privateSaleMint(
        address account, uint tokenId, uint allowedAmount, bytes32[] calldata proofData , uint requestedAmount
    ) internal virtual{
        require( _isPrivateSale == true, "Invalid Flag.");
        require(_WhitelistRoot != "", "Enpty");
        require(_verifyWithAddressAmount(account, allowedAmount, proofData, _WhitelistRoot), "Invalid Proof");
        //
        _sale(account,  tokenId, _privateSalePrice, requestedAmount);
    }

    //
    function toggleIsPrivateSale() external onlyOwner{
        _isPrivateSale = !_isPrivateSale;
    }

    // マークルプルーフ
    function _verifyWithAddressAmount(
        address account,  uint allowedAmount, bytes32[] calldata proofData, bytes32 root
    ) internal pure returns (bool){
        bytes32 leaf = keccak256(abi.encodePacked(account , allowedAmount));
        return MerkleProof.verify(proofData, root, leaf);
    }

    //
    function setWhitelistRoot(bytes32 newRoot) external onlyOwner{
        _WhitelistRoot = newRoot;
    }

}

