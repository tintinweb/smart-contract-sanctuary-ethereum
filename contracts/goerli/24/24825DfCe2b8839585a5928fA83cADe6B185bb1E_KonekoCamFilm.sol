import "./library/Strings.sol";
import "./library/Convert.sol";
import "./abstract/BaseTicket.sol";
import "./abstract/BasePurchaseWhitelist.sol";
import "hardhat/console.sol";
//
contract KonekoCamFilm is BaseTicket, BasePurchaseWhitelist{
    //
    string public constant name = "takechyFilm";
    uint private constant tokenId = 1; // 固定
    uint private constant limitedAmount = 200; // 有限NFT
    uint private constant teamAmount = 88; // 運営分
    uint private constant totalLotteryWLAmount = 33; // 抽選WLミント数
    uint private constant total2ndWLAmount = 30; // 購入WLミント数
    // CHECK: WL購入リミット人数 = (フィルム総数- 運営保留数 - トータル抽選WLミント数 ) / 2ndWLミント可能数
    // WARNING: 端数の場合の処理
    uint public constant limited2WL = total2ndWLAmount / 3;
    uint public magnification = 1; // ミント可能数の倍率
    uint private constant publicSalePrice = 0.003 ether;
    bool private isPublicSale = false;
    mapping( address => uint ) public addressMintedMap;
    uint private  mintedAmount = 0;

    constructor()
        ERC1155("ipfs://QmRhg78X5sL127UV8pqbd8axJo2mnRhf2hYeFLbngvNtPA/1")
        BasePurchaseWhitelist(limited2WL, 0.001 ether)
        BaseWhitelist(0.002 ether){
            // 運営保留分
            _teamMint(_msgSender(), teamAmount);
        }

    //
    function migrateToken(uint requestedTokenId) external {
        _migrateToken(tokenId, requestedTokenId);
    }

    //
    function secondaryWhitelistSale(address account, uint allowedAmount, bytes32[] calldata proofData) external payable{
        _secondaryWhitelistSale(account, allowedAmount, proofData);
    }

    //
    function privateSaleMint(
        address account, uint allowedAmount, bytes32[] calldata proofData , uint requestedAmount
    ) external payable {
        //
        require(getStockOfFilms() >= requestedAmount, "out of stock");
        // 
        require(isWLAccount(account, allowedAmount, proofData) == true, "is not WL");
        // リクエスト数と許可数の確認。
        require(getWLMintableAmount(account, allowedAmount, proofData) >= requestedAmount, "requestedAmount over");
        
        // 抽選WL・購入WL分岐
        if(isWLRaffled(account,  allowedAmount, proofData, _WhitelistRoot)){
            _privateSaleMint(account, tokenId, allowedAmount, proofData, requestedAmount);
        }else if(isWLParchased(account)){
            _privateSaleWith2ndWL(account, tokenId, requestedAmount);
        }
        //
        addressMintedMap[account] += requestedAmount;
        mintedAmount += requestedAmount;
    }

    //
    function publicSaleMint(address account, uint requestedAmount) external payable{
        require( isPublicSale == true, "Invalid Flag.");
        require(getStockOfFilms() >= requestedAmount, "out of stock");
        _sale(account,  tokenId, publicSalePrice, requestedAmount);
        addressMintedMap[account] += requestedAmount;
        mintedAmount += requestedAmount;
    }
    
    //
    function toggleIsPublicSale() external onlyOwner{
        isPublicSale = !isPublicSale;
    }

    //
    function setMagnification(uint _magnification) external{
        magnification = _magnification;
    }

    //
    function getStockOfFilms() internal view returns(uint){
        return limitedAmount - mintedAmount;
    }

    //
    function fetchProperties() external view returns(string[11] memory){
        return [
                Convert.boolToString(_isPrivateSale), 
                Convert.boolToString(isPublicSale),
                Convert.boolToString(_isMigrationAllowed),
                Convert.boolToString(_isSecondaryWhitelistSale),
                Strings.toString(_migratedAmount),
                Strings.toString(mintedAmount),
                Strings.toString(limitedAmount), 
                Strings.toString(_privateSalePrice),
                Strings.toString(publicSalePrice),
                Strings.toString(_secondaryWhitelistSalePrice),
                Strings.toString(getStockOfFilms())
            ];
    }

    // 運営用ミントgiveaway用
    function _teamMint(address account, uint requestedAmount) internal onlyOwner{
        require(getStockOfFilms() >= requestedAmount, "out of stock");
        _mint(account, tokenId, requestedAmount, "");
        addressMintedMap[account] += requestedAmount;
        mintedAmount += requestedAmount;
    }

    // WL所有者のmint可能数を取得する関数
    function getWLMintableAmount(address account, uint allowedAmount, bytes32[] calldata proofData) public view returns(uint){
        uint minted_amount = addressMintedMap[account];
        // WL購入者か？
        if(isWLParchased(account)){
            // underflow対策
            if(_getAllowedAmountOf2ndWL(account) *magnification > minted_amount){
                return _getAllowedAmountOf2ndWL(account) *magnification - minted_amount;
            }else{
                return 0;
            }
        }
        // 抽選か？
        else if(isWLRaffled(account, allowedAmount, proofData, _WhitelistRoot)){
            // underflow対策
            if(allowedAmount *magnification > minted_amount){
                return allowedAmount *magnification  - minted_amount;
            }else{
                return 0;
            }
        }
        else{
            return 0;
        }
    }

    // ホワイトリスト所有者と未所有の区別。
    function isWLAccount(address account, uint allowedAmount, bytes32[] calldata proofData) public view returns(bool){
        // 抽選WLと購入WL
        if(isWLRaffled(account,  allowedAmount, proofData, _WhitelistRoot)){
            return true;
        }else if(isWLParchased(account)){
            return true;
        }else{
            return false;
        }
    }

    //
    function isWLParchased(address account) private view returns(bool){
        return _getAllowedAmountOf2ndWL(account) > 0;
    }

    //
    function isWLRaffled(address _account, uint _allowedAmount, bytes32[] calldata _proofData, bytes32 _root) private pure returns(bool){
        return _verifyWithAddressAmount(_account,  _allowedAmount, _proofData, _root);
    }

}

