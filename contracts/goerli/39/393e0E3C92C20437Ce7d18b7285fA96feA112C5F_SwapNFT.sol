// File contracts/Swap/SwapNFT.sol

pragma solidity ^0.8.9;





contract SwapNFT is Ownable {
    using SafeMath for uint256;
    VerifySignature private signatureVerifier = new VerifySignature();
    address private swapSigner;
    address private NFTAddress;
    address mainTokenAddress;
    address taxTokenAddress;
    ERC721 tokenInstance = ERC721(NFTAddress);
    uint256 exchangeRate = 1000;
    uint256 swapFees = 10;
    uint256 swapFeesDivider = 1000;

    constructor() {}

    function setSwapSigner(address _swapSigner) public onlyOwner {
        swapSigner = _swapSigner;
    }

    function setSwapFees(uint256 _swapFees) public onlyOwner {
        swapFees = _swapFees;
    }

    function setSwapFeesDivider(uint256 _swapFeesDivider) public onlyOwner {
        swapFeesDivider = _swapFeesDivider;
    }

    function setExchangeRate(uint256 _exchangeRate) public onlyOwner {
        exchangeRate = _exchangeRate;
    }

    function getSwapSigner() public view onlyOwner returns (address) {
        return swapSigner;
    }

    function setNFTToken(address _NFTAddress) public onlyOwner {
        tokenInstance = ERC721(_NFTAddress);
    }

    function swapTokenForNFT(
        uint256 value, // Price of the NFT
        bytes32 r, // r
        bytes32 s, // s
        string memory _uri, // URI of the NFT to Mint
        uint8 v, // v
        uint256 _tokenId, // Token Id to Mint
        bytes32 messageHash, // Message Hash Calculate using uri,msg.sender,tokenId
        uint256 _nonce // Nonce OF the account, or a mathematically random Generated Number
    ) public {
        uint256 taxAmount = value.mul(swapFees).div(swapFeesDivider);
        address signer = ecrecover(messageHash, v, r, s);
        require(
            signer == swapSigner,
            "Only Authorized Swap Transfer Authorizer is acceptable"
        );
        bytes32 calculatedMessageHash = signatureVerifier.getMessageHash(
            msg.sender,
            value,
            string(abi.encodePacked(_uri, msg.sender, _tokenId)),
            _nonce
        );
        require(
            calculatedMessageHash == messageHash,
            "Unauthenticated Swap Request"
        );
        require(
            IERC20(mainTokenAddress).allowance(msg.sender, address(this)) >
                value,
            "Insufficient ALlowance"
        );
        if (tokenInstance.isTokenMinted(_tokenId)) {
            tokenInstance.transferFrom(address(this), msg.sender, _tokenId);
        } else {
            require(
                tokenInstance.mint(msg.sender, _uri, _tokenId),
                "Could Not Mint Token"
            );
        }
        require(
            IERC20(mainTokenAddress).transferFrom(
                msg.sender,
                address(this),
                value
            ),
            "Could Not Transfer Payment"
        );
        require(
            IERC20(mainTokenAddress).transferFrom(
                msg.sender,
                owner(),
                taxAmount
            ),
            "Could Not Transfer Payment"
        );
    }

    function swapNFTForToken(
        uint256 value, // Price of the NFT
        bytes32 r, // r
        bytes32 s, // s
        string memory _uri, // URI of the NFT to Mint
        uint8 v, // v
        uint256 _tokenId, // Token Id to Mint
        bytes32 messageHash, // Message Hash Calculate using uri,msg.sender,tokenId
        uint256 _nonce // Nonce OF the account, or a mathematically random Generated Number
    ) public {
        uint256 taxAmount = value.mul(swapFees).div(swapFeesDivider);
        address signer = ecrecover(messageHash, v, r, s);
        require(
            signer == swapSigner,
            "Only Authorized Swap Transfer Authorizer is acceptable"
        );
        bytes32 calculatedMessageHash = signatureVerifier.getMessageHash(
            msg.sender,
            value,
            string(abi.encodePacked(_uri, msg.sender, _tokenId)),
            _nonce
        );
        require(
            calculatedMessageHash == messageHash,
            "Unauthenticated Swap Request"
        );

        tokenInstance.transferFrom(msg.sender, address(this), _tokenId);
        require(
            IERC20(mainTokenAddress).transfer(msg.sender, value.sub(taxAmount)),
            "Could Not Transfer Payment"
        );
        require(
            IERC20(mainTokenAddress).transferFrom(
                msg.sender,
                owner(),
                taxAmount
            ),
            "Could Not Transfer Payment"
        );
    }

    function swapTaxTokenForToken(uint256 amountOfTokenToSwap) public {
        uint256 taxAmount = amountOfTokenToSwap.mul(swapFees).div(
            swapFeesDivider
        );
        uint256 convertedAmount = amountOfTokenToSwap.div(exchangeRate);
        require(
            IERC20(taxTokenAddress).allowance(msg.sender, address(this)) >
                amountOfTokenToSwap,
            "Insufficient ALlowance"
        );
        require(
            IERC20(taxTokenAddress).transferFrom(
                msg.sender,
                address(this),
                amountOfTokenToSwap.sub(taxAmount)
            ),
            "Could Not Transfer Main Tokens"
        );
        require(
            IERC20(taxTokenAddress).transferFrom(
                msg.sender,
                address(this),
                taxAmount
            ),
            "Could Not Transfer Main Tokens"
        );
        require(
            IERC20(mainTokenAddress).transfer(msg.sender, convertedAmount),
            "Could Not Transfer User Token Balance"
        );
    }

    function swapTokenForTaxToken(uint256 amountOfTokenToSwap) public {
        uint256 taxAmount = amountOfTokenToSwap.mul(swapFees).div(
            swapFeesDivider
        );
        uint256 convertedAmount = amountOfTokenToSwap.mul(exchangeRate);
        require(
            IERC20(mainTokenAddress).allowance(msg.sender, address(this)) >
                amountOfTokenToSwap,
            "Insufficient ALlowance"
        );
        require(
            IERC20(mainTokenAddress).transferFrom(
                msg.sender,
                address(this),
                amountOfTokenToSwap.sub(taxAmount)
            ),
            "Could Not Transfer Main Tokens"
        );
        require(
            IERC20(mainTokenAddress).transferFrom(
                msg.sender,
                owner(),
                taxAmount
            ),
            "Could Not Transfer Main Tokens"
        );
        require(
            IERC20(taxTokenAddress).transfer(msg.sender, convertedAmount),
            "Could Not Transfer User Token Balance"
        );
    }

    function getNFTToken() public view returns (address) {
        return NFTAddress;
    }

    function extractBEP20(address _tokenAddress, address _recipient)
        public
        onlyOwner
    {
        IERC20 t = IERC20(_tokenAddress);
        t.transfer(_recipient, t.balanceOf(address(this)));
    }

    function extractBNB(address _recipient) public onlyOwner {
        require(payable(_recipient).send(address(this).balance));
    }
}