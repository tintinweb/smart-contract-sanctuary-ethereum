pragma solidity 0.8.17;



library SigningUtils {

    function isValidLenderSignature(
        TermData.OfferTerm calldata _offer,
        address _lender,
        bytes calldata _signature
    ) external view returns (bool) {
        return
            SignatureChecker.isValidSignatureNow(
                _lender,
                ECDSA.toEthSignedMessageHash(_getOfferMessageHash(_offer)),
                _signature
            );
    }

    function _getOfferMessageHash(TermData.OfferTerm calldata _offer)
        internal
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encodePacked(
                    _offer.collateralId,
                    _offer.principalAmount,
                    _offer.totalReturn,
                    _offer.loanCurrencyAddress,
                    _offer.collateralAddress,
                    _offer.duration,
                    _offer.nonce,
                    _offer.collateralType,
                    _offer.offerType
                )
            );
    }
}

