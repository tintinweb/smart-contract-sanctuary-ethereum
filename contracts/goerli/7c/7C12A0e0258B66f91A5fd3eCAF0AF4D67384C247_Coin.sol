import "./interfaces/ICoin.sol";
import "./interfaces/ISamuraiPizzaCat.sol";
import "./interfaces/IMurakamiLuckyCatCoinBank.sol";
import {CoinStorage} from "./libraries/CoinStorage.sol";

contract Coin is
    OwnableRoles,
    Initializable,
    ERC2981Upgradeable,
    ERC1155Upgradeable,
    ERC1155SupplyUpgradeable,
    ICoin
{
    using CoinStorage for CoinStorage.Layout;
    using StringsUpgradeable for uint256;

    uint256 public constant ADMIN_ROLE = 1 << 0;
    uint256 public constant MANAGER_ROLE = 1 << 1;
    uint256 public constant MINTER_ROLE = 1 << 2;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        address royalty_,
        uint96 royaltyFee_,
        string memory baseUri_
    ) public initializer {
        _initializeOwner(_msgSender());
        __ERC1155_init("");
        __ERC1155Supply_init();
        __ERC2981_init();

        CoinStorage.layout().name = "Coin";
        CoinStorage.layout().symbol = "Coin";
        CoinStorage.layout().baseURI = baseUri_;

        _setDefaultRoyalty(royalty_, royaltyFee_);
    }

    function setBaseURI(string memory newuri)
        external
        onlyRolesOrOwner(ADMIN_ROLE)
    {
        CoinStorage.layout().baseURI = newuri;
    }

    function mint(
        address to,
        uint256 id,
        uint256 amount,
        uint256 exp
    ) external onlyRolesOrOwner(MINTER_ROLE) {
        _mint(to, id, amount, "");
        CoinStorage.layout().coinEXP[id] = exp;
    }

    function reveal(
        address user,
        uint256 tokenId,
        uint256 exp
    ) external onlyRolesOrOwner(MANAGER_ROLE) {
        CoinStorage.layout().coinEXP[tokenId] = exp;
        _burn(user, 0, 1);
        _mint(user, tokenId, 1, "");

        emit CoinRevealed(tokenId, user);
    }

    function setMoneyCatCoinBank(address moneyCatCoinBankAddress)
        external
        onlyRolesOrOwner(ADMIN_ROLE)
    {
        CoinStorage.layout().moneyCatCoinBank = ISamuraiPizzaCat(
            moneyCatCoinBankAddress
        );
    }

    function setLuckyCatCoinBank(address luckyCatCoinBankAddress)
        external
        onlyRolesOrOwner(ADMIN_ROLE)
    {
        CoinStorage.layout().luckyCatCoinBank = IMurakamiLuckyCatCoinBank(
            luckyCatCoinBankAddress
        );
    }

    function setDefaultRoyalty(address receiver, uint96 feeNumerator)
        external
        onlyRolesOrOwner(ADMIN_ROLE)
    {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    function breakLuckyCatCoinBank(uint256 tokenId) external {
        require(
            CoinStorage.layout().luckyCatCoinBank.ownerOf(tokenId) ==
                _msgSender(),
            "Not owner"
        );
        CoinStorage.layout().luckyCatCoinBank.burn(tokenId);
        CoinStorage.layout().moneyCatCoinBank.mint(_msgSender());
        _mint(_msgSender(), 0, 1, "");
        emit LuckyCatCoinBankBroken(tokenId, _msgSender());
    }

    function getEXP(uint256 tokenId) external view returns (uint256) {
        return CoinStorage.layout().coinEXP[tokenId];
    }

    function name() public view returns (string memory) {
        return CoinStorage.layout().name;
    }

    function symbol() public view returns (string memory) {
        return CoinStorage.layout().symbol;
    }

    function uri(uint256 tokenId) public view override returns (string memory) {
        return
            bytes(CoinStorage.layout().baseURI).length > 0
                ? string(
                    abi.encodePacked(
                        CoinStorage.layout().baseURI,
                        tokenId.toString()
                    )
                )
                : CoinStorage.layout().baseURI;
    }

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
