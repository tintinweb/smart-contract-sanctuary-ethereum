pragma solidity ^0.8.9;

import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

import "hardhat/console.sol";

/** ERRORS */
error NFT__NeedMoreETH();
error NFT__TransferFailed();
error NFT__RangeOut();
error NFT__ArraysNotEquals();
error NFT__InputOutOfRange();
error NFT__MaxSupplyReached();

contract BoosterPackRare is ERC721, ERC721URIStorage, Ownable, VRFConsumerBaseV2 {
    /** DECLARATIONS */
    enum RARITY{
        COMMON,
        RARE,
        SUPER
    }

    /** EVENTS */
    event PackageRequested(uint indexed requestId, address indexed requester);
    event PackageMinted(address indexed to, uint indexed packId);
    event SetCollection(uint nrTokens, uint maxPackages);

    /** TYPES */
    /* CHAINLINK VARIABLES */
    VRFCoordinatorV2Interface private immutable i_COORDINATOR;
    uint64 private immutable i_subscriptionId;
    bytes32 private immutable i_gasLane;
    uint32 private immutable i_callbackGasLimit;
    uint16 private constant REQUEST_CONFIRMATIONS = 3;
    uint32 private NUM_WORDS;

    /* VRF HELPER */
    mapping (uint => address) public s_requestIdToSender;

    /* NFT VARIABLES*/
    uint public mintFee; //Mint fee
    uint public tokenCounter; //Counter of minted tokens 
    string[] public s_tokenURIs; //URIs of the tokens to been distributed
    uint public s_tokenURIsCounter; //Counter of the tokens to be distributed
    mapping(string => RARITY) public rarities; //Rarities mapped with tokenURI
    /*PACKAGES VARIABLES FOR DESTRIBUITE THAT*/
    uint public packageSize; //Size of the packs
    uint public packageMintedCounter; //Counter of the packs minted
    uint public maxPackages; //Max packs supply of the collection                                 
    mapping(uint => uint[]) public packagesMinted; //Packages Minted 
    /* CHANCES VARIABLES*/
    uint internal s_decimals = 1;                                      //The higher you raise the more you minimize the error for division
    uint internal MAX_CHANCE_VALUE= uint(100) * (10 ** s_decimals); 
    uint public COMMON_CHANCE = uint(75);                            //75%
    uint public RARE_CHANCE = uint(15);                              //15%
    uint public SUPER_RARE_CHANCE = uint(10);                        //10%
    uint[] public chanceArray;                                       

    /** MAIN FUNCTIONS */
    /**
    @param vrfCoordinator Coordinator VRF Chainlink
    @param subscriptionId Subscription ID Chainlink
    @param gasLane Gas lane of chain variable for Chainlink
    @param callbackGasLimit Gas limit variable for Chainlink
    @param _tokenURIs Initialize TokenURIs array, will be map with rarity tokenURI => rarity, es. URI=>RARITY it will be mapped in the struct of NFT[0] 
    @param _rarities Initialize Rarieties array, mapped with tokenURIs[] in NFT[]
    @param _mintFee Mint fee
    @param _maxPackages Max packages mintable
    @param _packageSize Size of the package
    */
    constructor(
                address vrfCoordinator
                ,uint64 subscriptionId
                ,bytes32 gasLane 
                ,uint32 callbackGasLimit
                ,string[] memory _tokenURIs
                ,uint[] memory _rarities    
                ,uint _mintFee
                ,uint _maxPackages
                ,uint32 _packageSize
               ) VRFConsumerBaseV2(vrfCoordinator) ERC721("Booster Pack Rare", "BPKR"){

        maxPackages=_maxPackages;
        packageSize= _packageSize;
        NUM_WORDS = _packageSize; //Setting how many words ChainLink will return
        mapNfts(_tokenURIs, _rarities);
        i_COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        i_subscriptionId = subscriptionId;
        i_gasLane=gasLane;
        i_callbackGasLimit=callbackGasLimit;
        mintFee=_mintFee;
    }

    /** 
        @notice Will be called on the contructor and can be called for reset the nfts
        @param _tokenURIs The array of tokens URIs to be mapped in the array "nfts"
        @param _rarities The array of rarities to be mapped in the array "nfts"
        @dev only owner can call this function
    */

    function mapNfts(string[] memory _tokenURIs, uint[] memory _rarities) public onlyOwner{
        if(_tokenURIs.length == _rarities.length && _tokenURIs.length < 0 ){
            revert NFT__ArraysNotEquals();
        }

        s_tokenURIsCounter = _tokenURIs.length;
        for(uint k; k < _tokenURIs.length; k++){
            if(uint(_rarities[k]) > uint8(RARITY.SUPER)){
                revert NFT__InputOutOfRange();
            }
            s_tokenURIs.push(_tokenURIs[k]);
            RARITY rarity = RARITY(_rarities[k]);
            rarities[_tokenURIs[k]] = rarity;
        }
        //Must reset the chances array
        setCanches();
        emit SetCollection(s_tokenURIsCounter, maxPackages);
    }

    /** 
        @notice This function will be called for request a pack
        @dev this function will call fulfillRandomWords by ChainLink oracle
        @dev will release PackageRequested event
    */
    function requestPack() public payable returns(uint requestId){
        if(msg.value < mintFee){
            revert NFT__NeedMoreETH();
        }
        if(packageMintedCounter >= maxPackages){
            revert NFT__MaxSupplyReached(); 
        }
        requestId = i_COORDINATOR.requestRandomWords(
                        i_gasLane,
                        i_subscriptionId,
                        REQUEST_CONFIRMATIONS,
                        i_callbackGasLimit,
                        NUM_WORDS
                    );
        s_requestIdToSender[requestId]=msg.sender;
        emit PackageRequested(requestId, msg.sender);
    }

    /** 
        @notice This function can be called for withdraw
        @dev Only owner can withdraw
    */
    function withdraw() public onlyOwner{
        uint amount = address(this).balance;
        (bool success, ) = payable(msg.sender).call{value: amount}("");
        if(!success)
        {
            revert NFT__TransferFailed();
        }
    }

    /** 
        @notice This function can be called for change the Max Supply
        @dev Only owner can call this function
        @param _maxSupply New maximum value of supply 
    */
    /*function changeMaxTokens(uint _maxSupply) external onlyOwner{
        s_maxSupply = _maxSupply;
    }
    */

    /** INTERNALS */
    /** 
        @notice This function will be called every time change of nfts
    */
    function setCanches() internal {
        uint counterCommon=0;
        uint counterRare=0;
        uint counterSuperRare=0;

        //Loop for searching how much of tipologies there are
        for(uint k=0; k< s_tokenURIs.length ; k++){
            RARITY rarity = rarities[s_tokenURIs[k]];
            if(rarity == RARITY.COMMON){
                counterCommon++;
            }
            if(rarity == RARITY.RARE){
                counterRare++;
            }
            if(rarity == RARITY.SUPER){
                counterSuperRare++;
            }
        }

        //Calc the percentage of eachone group and divide that for the counter
        uint valueCommon = (MAX_CHANCE_VALUE * COMMON_CHANCE) / 100 / counterCommon; 
        uint valueRare= (MAX_CHANCE_VALUE  * RARE_CHANCE / 100) / counterRare;
        uint valueSuperRare= (MAX_CHANCE_VALUE * SUPER_RARE_CHANCE / 100) / counterSuperRare;

        //Push in chanceArray the ranges es [10, 50, 100]
        //will be see like ranges [1-10, 11-50, 51-100] 
        uint chance = 0;
        for (uint k = 0; k < s_tokenURIs.length; k++) {
            RARITY rarity = rarities[s_tokenURIs[k]];
            if (rarity == RARITY.COMMON) {
                chance += valueCommon;
            } else if (rarity == RARITY.RARE) {
                chance += valueRare;
            } else if (rarity == RARITY.SUPER) {
                chance += valueSuperRare;
            }
            chanceArray.push(chance);
        }
    }

    /** OVERRIDES */
    /** 
        @notice This function will be called after the response from chainlink oracle
        @dev internal function will release PackageMinted event
    */
    function fulfillRandomWords(uint requestId, uint [] memory randomWords) internal override{
        address owner;
        for(uint k=0; k<randomWords.length; k++){
            uint random = randomWords[k] % MAX_CHANCE_VALUE;
            owner = s_requestIdToSender[requestId];
            uint tokenId = tokenCounter;
            tokenCounter += 1;
            string memory _tokenURI = getNftFromRandom(random);
            _safeMint(owner,tokenId);
            _setTokenURI(tokenId, _tokenURI);
            packagesMinted[packageMintedCounter].push(tokenId);
        }
        emit PackageMinted(owner, packageMintedCounter);
        packageMintedCounter++;
    }
    /**
        @dev Required
    */
    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    /** VIEWS */
    /** 
        @notice Helper for fullfillRandom it will give the token uri to be used for a mint
    */
    function getNftFromRandom(uint _random) internal view returns (string memory){
        require(_random <= MAX_CHANCE_VALUE && _random > 0,"Random exceeds");

        for(uint i=0; i < chanceArray.length-1;i++){
            if(_random >= chanceArray[i] && _random < chanceArray[i+1]){
                return s_tokenURIs[i];
            }
        }
        revert NFT__RangeOut(); 
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }
}

