// File: contracts/TransferHelpers.sol


pragma solidity =0.8.10;







contract TransferHelpers {
    using SafeERC20 for IERC20;
    address public constant ETHADDRESS =
        0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;

    /**
     * transfer tokens into Offer
     * EOA -> Offer
     */
    function _batchTransferIn(
        address _from,
        address _to,
        Token[] memory _tokens
    ) internal {
        _batchSafeTransferFrom(_from, _to, _tokens, true);
    }

    /**
     * transfer tokens out of Offer
     * EOA <- Offer
     */
    function _batchTransferOut(
        address _from,
        address _to,
        Token[] memory _tokens
    ) internal {
        _batchSafeTransferFrom(_from, _to, _tokens, false);
    }

    function _batchSafeTransferFrom(
        address _from,
        address _to,
        Token[] memory _tokens,
        bool _tokenIn
    ) private {
        for (uint256 i = 0; i < _tokens.length; i++) {
            TokenType t = _tokens[i].tokenType;
            address addr = _tokens[i].tokenAddr;
            uint256 amount = _tokens[i].tokenAmounts;
            uint256 id = _tokens[i].tokenId;

            if (t == TokenType.NATIVE) {
                console.log("this is ETH transfer!");
                (bool success, ) = _to.call{value: amount}("");

                require(success, "Bisaar: transfer ETH failed!");
            } else if (t == TokenType.ERC20) {
                // tokens transfer flow: from Caller into Offer
                if (_tokenIn) {
                    IERC20(addr).safeTransferFrom(_from, _to, amount);
                } else {
                    // tokens transfer flow: from Offer to Caller
                    IERC20(addr).safeTransfer(_to, amount);
                }
            } else if (t == TokenType.ERC721) {
                IERC721(addr).safeTransferFrom(_from, _to, id);
            } else if (t == TokenType.ERC1155) {
                IERC1155(addr).safeTransferFrom(_from, _to, id, amount, "");
            } else {
                revert("invalid token type input!");
            }
        }
    }
}

// File: contracts/OfferFactory.sol


pragma solidity =0.8.10;









contract OfferFactory is Ownable, TransferHelpers {
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeERC20 for IERC20;

    // wallet address -> offer address
    mapping(address => EnumerableSet.AddressSet) private _accountToOfferArray;

    // allowed loan tokens
    mapping(address => bool) public loanTokensList;

    // used by clone
    IOffer public offerTemplate;

    // price oracle
    IPriceOracle public priceOracle;

    // featured offer setting
    IERC20 public featuredToken;
    uint256 public featuredFee;
    address public featuredFeeReceiver;

    address public lastOffer;

    event CreateOffer(address _eoa, address _offer);

    event SetOfferTemplate(address _pre, address _curr);

    event SetPriceOracle(address _pre, address _curr);

    event SetFeaturedFeeToken(address _pre, address _curr);

    event SetFeaturedFee(uint256 _pre, uint256 _curr);

    event SetFeaturedFeeReceiver(address _pre, address _curr);

    event SetSupportLoanToken(address _token, bool _flag);

    constructor(
        IOffer _offerTemplate,
        IPriceOracle _priceOracle,
        IERC20 _featuredToken,
        uint256 _featuredFee,
        address _featuredFeeRec
    ) {
        offerTemplate = _offerTemplate;
        priceOracle = _priceOracle;
        featuredToken = _featuredToken;
        featuredFee = _featuredFee;
        featuredFeeReceiver = _featuredFeeRec;
    }

    /**
     * @notice this is the portal for the borrower to create a original offer
     * @param _tokens tokens that user selected
     * @param _general general info (will check parameters inside Offer instance)
     */
    function createOffer(Token[] memory _tokens, GeneralInfo memory _general)
        external
        payable
    {
        require(
            loanTokensList[_general.loanToken],
            "Bisaar: not supported loand token selected!"
        );

        // create offer
        lastOffer = _cloneProxy(address(offerTemplate));
        require(lastOffer != address(0), "Bisaar: create offer failed!");

        // initialize offer
        IOffer(lastOffer).initialize(
            msg.sender,
            address(this),
            IPriceOracle(priceOracle)
        );

        // need user to approve first for all selected tokens
        _batchTransferIn(msg.sender, lastOffer, _tokens);

        if (_general.featuredFlag) {
            IERC20(featuredToken).safeTransferFrom(
                msg.sender,
                featuredFeeReceiver,
                featuredFee
            );
        }

        // save configure, emit event
        IOffer(lastOffer).createOffer(_tokens, _general);

        // save offer array
        EnumerableSet.AddressSet storage offerArry = _accountToOfferArray[
            msg.sender
        ];
        offerArry.add(lastOffer);

        // emit event
        emit CreateOffer(msg.sender, lastOffer);
    }

    /**
    @notice set offer template
    @param _offer new offer template
    */
    function setOfferTemplate(IOffer _offer) external onlyOwner {
        IOffer prev = offerTemplate;
        offerTemplate = _offer;
        emit SetOfferTemplate(address(prev), address(_offer));
    }

    /**
    @notice set new price oracle
    @param _priceOracle new price oracle
    */
    function setPriceOracle(IPriceOracle _priceOracle) external onlyOwner {
        IPriceOracle prev = priceOracle;
        priceOracle = _priceOracle;
        emit SetPriceOracle(address(prev), address(priceOracle));
    }

    /**
    @notice set featuredToken
    @param _token feeToken
    */
    function setFeaturedFeeToken(IERC20 _token) external onlyOwner {
        IERC20 prev = featuredToken;
        featuredToken = _token;
        emit SetFeaturedFeeToken(address(prev), address(featuredToken));
    }

    /**
    @notice set featured fee amount
    @param _feeAmt fee amount
    */
    function setFeaturedFee(uint256 _feeAmt) external onlyOwner {
        uint256 prev = featuredFee;
        featuredFee = _feeAmt;
        emit SetFeaturedFee(prev, featuredFee);
    }

    /**
    @notice set featured fee receiver
    @param _feeReceiver fee receiver
    */
    function setFeaturedFeeReceiver(address _feeReceiver) external onlyOwner {
        address prev = featuredFeeReceiver;
        featuredFeeReceiver = _feeReceiver;
        emit SetFeaturedFeeReceiver(prev, featuredFeeReceiver);
    }

    /**
    @notice set supported loan tokens 
    @param _token loan token
    @param _flag flag 
    */
    function setSupportLoanToken(address _token, bool _flag)
        external
        onlyOwner
    {
        loanTokensList[_token] = _flag;
        emit SetSupportLoanToken(_token, _flag);
    }

    function _cloneProxy(address _template)
        internal
        returns (address newOffer)
    {
        bytes20 targetBytes = bytes20(_template);

        assembly {
            let clone := mload(0x40)
            mstore(
                clone,
                0x3d602d80600a3d3981f3363d3d373d3d3d363d73000000000000000000000000
            )
            mstore(add(clone, 0x14), targetBytes)
            mstore(
                add(clone, 0x28),
                0x5af43d82803e903d91602b57fd5bf30000000000000000000000000000000000
            )
            newOffer := create(0, clone, 0x37)
        }
    }
}