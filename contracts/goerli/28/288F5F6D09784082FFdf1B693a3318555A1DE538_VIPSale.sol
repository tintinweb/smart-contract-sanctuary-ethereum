pragma solidity ^0.8.15;

import "../LABZ.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../Activators/ServiceActivator.sol";
import "../VipNfts/VipNft.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "../Formulas/Interest.sol";

contract VIPWallet is InterestFormula {

    LABZ internal token;
    uint256 internal _tokenId;


    uint256 public immutable maxBalance = 1000000 ether; // can hold up to 1 million labz as per the VIP Sale token limit this does not include rewards

    event VIPWalletInitialized(address indexed owner, address indexed vipwallet);
    event Staked(address indexed owner, address indexed wallet, uint256 amount, uint256 bn);
    event Claimed(address indexed owner, address indexed wallet, uint256 amount);

    Staker internal staker;

    uint256 public unlockTime;
    uint256 public rewardsCalcLastBlock;
    uint256 public lastUpdateTime;
    uint256 public unclaimed;


    struct Staker {
        uint256 labzStaked; // if vip sale = 100% 
        uint256 unclaimedRewards; // will accumulate rewards until unlock time
        uint256 lastUpdated; // last action performed
        uint256 canClaimOnlyFromTime; // for vip sale, this is when the user can start claiming its rewards
        uint256 lockedOnBlock; // when this wallet was created 
       
    }

    // stakes amount per block number as the rate is calculated based on (staked on block number - current block number)
    mapping(uint256 => mapping(uint => uint256)) private _stakesAmtPerBlock;
    mapping(uint256 => mapping(uint => Stake)) private _stakes;
    mapping(uint256 => uint256) private _indexToBlockNum;

    uint256 private stakeIndex = 0;

    struct Stake {
        uint256 blockNum;
        uint256 blockTime;
        uint256 origAmount;
        uint256 calcRewards;
        uint8 claimable;
    }
   
    uint8 initialized = 0;

    event ReceiveToken(address indexed from, address indexed token, uint256 amount);
    event ReceiveNFT(address indexed from, address indexed nft, uint256 tokenId, string url);

    constructor() {
        
    }

    function initAfterClone(address _token, uint256 tokenId, address walletOwner) public   {
        require(initialized == 0, "already initialized");
        token = LABZ(_token);
        _tokenId = tokenId;
    
        uint256 bal = LabzTokenBalance();
        if (bal == 0) {
            revert("cannot initialize an empty wallet");
        }
        uint256 ts = block.timestamp;
        unlockTime = ts + 30 days;
        staker = Staker(bal, 0, ts, unlockTime, block.number-1);
        stake(bal);
        rewardsCalcLastBlock = block.number;
        
        initialized = 1;
        emit VIPWalletInitialized(walletOwner, address(this));

    }

    function LabzTokenBalance() public view returns(uint256) {
        return token.balanceOf(address(this));
    }

    function GetClaimableBalance() public  returns(uint256) {

        return calcClaimable();
        
    }

    function GetLockedBalance() public view returns(uint256) {
        return calcLocked();
    }

    function GetRewardsAccrued() public returns(uint256) {
        return GetClaimableBalance() + GetLockedBalance();
    }

    function _calculateRewards() internal  returns(uint256) {

        if(rewardsCalcLastBlock <= block.number || lastUpdateTime > block.timestamp || lastUpdateTime < block.timestamp + 5 minutes) {
            return 0; // already calculated for this block or invalid time or not enough time passed since last update (prevent dos)
        }
        uint256 bal = LabzTokenBalance();
        rewardsCalcLastBlock = block.number;
        lastUpdateTime = block.timestamp;
        
        return calculateCompoundRate(bal, staker.lockedOnBlock);

    }

   
    function stake(uint256 value) internal {
        require(value > 0, "cannot stake 0");
        // staking is automatic on this wallet no need to put it external
        // triggered upon receiving new token from the vip sale contract
        uint256 r = _calculateRewards(); 
        uint256 bn = block.number;
        uint256 __ts = block.timestamp;
        Stake memory __stake = Stake(bn, __ts, value, r, 0);
        _stakesAmtPerBlock[bn][stakeIndex] = value;
        _stakes[bn][stakeIndex] = __stake;
        _indexToBlockNum[stakeIndex] = bn;
        stakeIndex += 1;
        emit Staked(address(this), address(this), value, bn);
    }

 

    function withdraw(address sender, uint256 value) internal  {
        require(GetClaimableBalance() > 0 && unclaimed > value, "invalid amount to withdraw");
        uint256 r = calcClaimableAndDelete();
        token.transferFrom(address(this), sender, r);
        lastUpdateTime = block.timestamp;
        unclaimed = 0;
        emit Claimed(address(this), address(this), value);
    }

       function claim(uint256 value) external {
        withdraw(msg.sender, value);
       
        }

    function calcClaimable() internal returns(uint256) {
        uint256 claimable = 0;
        unclaimed = 0;
        for(uint si = 0; si < stakeIndex; si++ ) {
            uint256 bn = _indexToBlockNum[si];
            Stake memory s = _stakes[bn][si];
            if(s.claimable == 1) {
                claimable += s.calcRewards;
                unclaimed += s.calcRewards;
            }

        }
        return claimable;
    }

    function calcClaimableAndDelete() internal returns(uint256) {
          uint256 claimable = 0;
           unclaimed = 0;
        for(uint si = 0; si < stakeIndex; si++ ) {
            uint256 bn = _indexToBlockNum[si];
            Stake memory s = _stakes[bn][si];
            if(s.claimable == 1) {
                claimable += s.calcRewards;
                 unclaimed += s.calcRewards;
                delete _stakes[bn][si];
                delete _indexToBlockNum[si];
            }

        }
        return claimable;
    }

    function calcLocked() internal view returns(uint256) {
         uint256 unclaimable = 0;
        for(uint si = 0; si < stakeIndex; si++ ) {
            uint256 bn = _indexToBlockNum[si];
            Stake memory s = _stakes[bn][si];
            if(s.claimable != 1) {
                unclaimable += s.calcRewards;
            }

        }
        return unclaimable;
    }


    receive() external payable {
        if(this.LabzTokenBalance() == maxBalance) {
            revert("you've reached the maximum this wallet can hold for now.");
        }
        stake(msg.value);
        emit ReceiveToken(msg.sender, address(token), msg.value);
    }

}

