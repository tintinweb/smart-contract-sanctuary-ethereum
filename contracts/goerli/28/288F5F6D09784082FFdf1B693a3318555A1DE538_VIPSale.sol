import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./VIPWallet.sol";

contract VIPWalletFactory is Ownable {

    address internal vipWalletImpl;
    address internal token;

    using Clones for address;

    constructor(address __token, address _impl) {
        vipWalletImpl = _impl;
        token = __token;
    }

    function grantOwnership(address __owner) public onlyOwner {
        transferOwnership(__owner);
    }

    function create(address _wOwner, uint256 tokenId) public  returns(address) {
        bytes32 salt = keccak256(abi.encode(_wOwner, tokenId));
        address addr = Clones.predictDeterministicAddress(vipWalletImpl,salt);

        Clones.cloneDeterministic(vipWalletImpl, salt);
        VIPWallet(payable(addr)).initAfterClone(token, tokenId, _wOwner);

        return addr;
    }



}

