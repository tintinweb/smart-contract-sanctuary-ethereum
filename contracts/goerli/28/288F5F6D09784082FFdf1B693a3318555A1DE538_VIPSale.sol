pragma solidity ^0.8.15;

/**
 * Soulbound token abstract implementation of ISBT interface
 */

import "./ISBT.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";



abstract contract SBT is ISBT,  IERC165, ReentrancyGuard {

     mapping(bytes4 => bool) private _supportedInterfaces;

     mapping(uint256 => address) private _sbtOperators;
     mapping(uint256 => address) private _approvals;
     mapping(uint256 => bytes32) private _dna;
     mapping(address => bool) public _hasSoul;
     mapping(address => uint256) private _souls;
     mapping(uint256 => bytes) private _signatures;

     using Address for address;
     using Counters for Counters.Counter;

     Counters.Counter private soulTracker;

     string public baseURI;

     bytes32 public immutable SOUL_PREFIX = keccak256(abi.encodePacked("_mintSbt(address) returns(uint256)"));

     function ownerOf(address _owner) public returns(uint256) {
        require(_hasSoul[_owner] != false, "does not have soul");
        return _souls[_owner];
     }

     function _mintSbt(address from, address _to) internal {
        _beforeCreate();
        require(_hasSoul[from] != true, "cannot have more than one soul");
        require(_to.isContract() != true, "a contract cannot have a soul");
        uint256 soulId =  soulTracker.current();
        _souls[_to] = soulId;
        _hasSoul[_to] = true;
        _approvals[soulId] = _to;
        _sbtOperators[soulId] = address(this);
        bytes memory dna = abi.encode(Strings.toString(uint256(SOUL_PREFIX)), soulId, _to);
        _dna[soulId] = keccak256(dna);
        _signatures[soulId] = dna;
        emit SoulMinted(from, _to, soulId);
        _afterCreate();
    }

    function mint(address _to) public  {
        _mintSbt(msg.sender, _to);
    }

    function burn(uint256 sbtId) public  {
        _burnSbt(msg.sender, sbtId);
    }   

    
    function _burnSbt(address _owner, uint256 sbtId) internal {
        require(_hasSoul[_owner] == true, "cannot burn your soul you have none!");
        require(_approvals[sbtId] == _owner, "you are not approved to do this");
        require(_souls[_owner] == sbtId, "you do not own this soul");
        delete _souls[_owner];
        delete _hasSoul[_owner];
        delete _dna[sbtId];
        delete _signatures[sbtId];
        delete _approvals[sbtId];
        delete _sbtOperators[sbtId];
        // yay (or ouch?) our soul was burned ! 
    }

    
    function _updateSbt(uint256 sbtId, bytes calldata newData) external returns(uint8 success) {
        // not sure I will keep this... sbt should be immutable I think
    }

    function sbtURI(uint256 tokenId) public view override returns(string memory) {
        string memory urisep = abi.decode("/sbt/", (string));
        string memory token = _uint2str(tokenId);
        string memory uri =  _strConcat(_sbtBaseURI(), urisep, token, "", "");
        return uri;
    }


    function _sbtBaseURI() internal view returns(string memory) {
        return baseURI;
    }

    function setBaseURI(string calldata uri) public override {
        baseURI = uri;
    }


    function _sbtDNA(uint256 sbtId) external view returns(bytes32) {
        return _dna[sbtId];
    }


    function _approve(address to, uint256 sbtId) internal {
        require(_souls[to] != sbtId, "you cannot approve yourself");
        _approvals[sbtId] = to;
    } 

    function _onReceive() external {

    }
    function _onTransfer() external pure {
        revert("souls are non transferable");

    }
    function _beforeCreate() internal {

    }
    function _afterCreate() internal {
        soulTracker.increment();
    }



    function _registerInterface(bytes4 interfaceId) internal virtual {
        require(interfaceId != 0xffffffff, "ERC165: invalid interface id");
        _supportedInterfaces[interfaceId] = true;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        return (interfaceId == type(ISBT).interfaceId  || _supportedInterfaces[interfaceId] == true);
    }

   
 /**
     * @dev Function to convert uint to string
     * Taken from https://github.com/oraclize/ethereum-api/blob/master/oraclizeAPI_0.5.sol
     */
    function _uint2str(uint _i) internal pure returns (string memory _uintAsString) {
        if (_i == 0) {
            return "0";
        }
        uint j = _i;
        uint len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint k = len - 1;
        while (_i != 0) {
            bstr[k--] = bytes1(uint8(48 + _i % 10));
            _i /= 10;
        }
        return string(bstr);
    }

    /**
     * @dev Function to concat strings
     * Taken from https://github.com/oraclize/ethereum-api/blob/master/oraclizeAPI_0.5.sol
     */
    function _strConcat(string memory _a, string memory _b, string memory _c, string memory _d, string memory _e)
    internal pure returns (string memory _concatenatedString)
    {
        bytes memory _ba = bytes(_a);
        bytes memory _bb = bytes(_b);
        bytes memory _bc = bytes(_c);
        bytes memory _bd = bytes(_d);
        bytes memory _be = bytes(_e);
        string memory abcde = new string(_ba.length + _bb.length + _bc.length + _bd.length + _be.length);
        bytes memory babcde = bytes(abcde);
        uint k = 0;
        uint i = 0;
        for (i = 0; i < _ba.length; i++) {
            babcde[k++] = _ba[i];
        }
        for (i = 0; i < _bb.length; i++) {
            babcde[k++] = _bb[i];
        }
        for (i = 0; i < _bc.length; i++) {
            babcde[k++] = _bc[i];
        }
        for (i = 0; i < _bd.length; i++) {
            babcde[k++] = _bd[i];
        }
        for (i = 0; i < _be.length; i++) {
            babcde[k++] = _be[i];
        }
        return string(babcde);
    }

}

