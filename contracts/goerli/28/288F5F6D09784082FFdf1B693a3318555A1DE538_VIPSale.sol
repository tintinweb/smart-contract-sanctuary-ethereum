pragma solidity ^0.8.15;


contract UserMetadata {

    event MetadataCreated(uint256 indexed tokenid, bytes32 profileId, string name, string symbol, address owner);
    event MetadataCustomEntry(address indexed tokenid, bytes32 key, uint8 valuetype);

    mapping(uint256 => bytes32) private _tokenIdToMetadataId;
    mapping(bytes32 => bytes) private _idTorawBytes;
    mapping(uint256 => mapping(uint => bytes)) private _customEntries;
    mapping(bytes32 => uint) private _metaEntryKeyIndex;
    mapping(address => bytes32) private _userMetas;

    bytes32 immutable META_ID_PREFIX = keccak256(abi.encodePacked("METAIDAKX_"));

    uint internal nonce = 0;

    address private _metaSuperOp;

    enum MetaEntryValueType {
        STRING,
        ADDRESS,
        BYTES32,
        NUMBER
    }

   struct Metadata {
        bytes32 id;
        string name;
        string symbol;
        address owner;
        uint256 tokenId;
      
    }

    struct MetaEntry {
        bytes32 key;
        MetaEntryValueType valueType;
        bytes value;
    }

    constructor() {
        _metaSuperOp = msg.sender;
    }

    function _calculateMetaId(uint256 tokenId) internal  {
        bytes memory mb = abi.encode(keccak256(abi.encode(nonce, msg.sender, META_ID_PREFIX)));
        nonce += 1;
        bytes32 id = keccak256(abi.decode(mb, (bytes)));
        _tokenIdToMetadataId[tokenId] = id;
    
    }

    function addMetadata(string memory name, string memory symbol, address owner, uint256 tokenId) public onlySuperOp {
        _calculateMetaId(tokenId);
        Metadata memory mtd = Metadata(_tokenIdToMetadataId[tokenId], name, symbol, owner, tokenId);
        _idTorawBytes[_tokenIdToMetadataId[tokenId]] = abi.encode(mtd);
        emit MetadataCreated(tokenId, mtd.id, mtd.name, mtd.symbol, mtd.owner);
    }

    function addCustomEntryTo(bytes32 id, string calldata key, string calldata value) public onlySuperOp {

    }

    function _decodeRawMetadata(uint256 tokenId) internal view returns(Metadata memory) {
        Metadata memory decoded = abi.decode(_idTorawBytes[_tokenIdToMetadataId[tokenId]], (Metadata));
        return decoded;
    }

    function _decodeRawMetaEntry(uint256 tokenId, bytes32 key) internal view returns(MetaEntry memory) {
        MetaEntry memory decoded = abi.decode(_customEntries[tokenId][_metaEntryKeyIndex[key]], (MetaEntry));
        return decoded;

    }

    function _getValueType(uint256 tokenId, bytes32 key) internal view returns(MetaEntryValueType vt, MetaEntry memory decoded) {
        decoded = _decodeRawMetaEntry(tokenId, key);
         vt = decoded.valueType;
    }

    function _decodeValue(uint256 tokenId, bytes32 key) internal view {
       (MetaEntryValueType vt, MetaEntry memory decoded) = _getValueType(tokenId, key);
       if(vt == MetaEntryValueType.STRING) {
        _decodeString(decoded.value);
       }
        else if(vt == MetaEntryValueType.ADDRESS) {
        _decodeAddress(decoded.value);
       }
        else if(vt == MetaEntryValueType.BYTES32) {
        _decodeBytes32(decoded.value);
       }
        else  if(vt == MetaEntryValueType.NUMBER) {
        _decodeNumber(decoded.value);
       } else {
        revert("invalid value format");
       }

    }

    function _decodeString(bytes memory _val) internal pure returns(string memory) {
        return abi.decode(_val, (string));
    }

  function _decodeAddress(bytes memory _val) internal pure returns(address) {
        return abi.decode(_val, (address));
    }

      function _decodeBytes32(bytes memory _val) internal pure returns(bytes32) {
        return abi.decode(_val, (bytes32));
    }

     function _decodeNumber(bytes memory _val) internal pure returns(uint256) {
        return abi.decode(_val, (uint256));
    }

    function strToKey(string memory key) internal pure returns(bytes32 _bKey) {
        _bKey = keccak256(abi.encode(key));
    }

    function name(uint256 tokenId) public view returns(string memory _name)  {
        bytes memory _raw = _idTorawBytes[_tokenIdToMetadataId[tokenId]];
        _name = abi.decode(_raw, (Metadata)).name;
    }

     function symbol(uint256 tokenId) public view returns(string memory _symbol)  {
        bytes memory _raw = _idTorawBytes[_tokenIdToMetadataId[tokenId]];
        _symbol = abi.decode(_raw, (Metadata)).symbol;
    }

    function owner(uint256 tokenId) public view returns(address _owner)  {
        bytes memory _raw = _idTorawBytes[_tokenIdToMetadataId[tokenId]];
        _owner = abi.decode(_raw, (Metadata)).owner;
    }

    function id(uint256 tokenId) public view returns(bytes32 _id) {
         bytes memory _raw = _idTorawBytes[_tokenIdToMetadataId[tokenId]];
        _id = abi.decode(_raw, (Metadata)).id;
    }

    function metas() public view returns(Metadata memory) {
        bytes memory raw = _idTorawBytes[_userMetas[msg.sender]];
        return abi.decode(raw, (Metadata));
    }


    modifier onlySuperOp() {
        if(msg.sender != _metaSuperOp) {
            revert("only akx super op can do that");
        }
        _;
    }

}

