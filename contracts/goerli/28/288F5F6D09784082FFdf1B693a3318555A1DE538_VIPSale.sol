pragma solidity ^0.8.15;

contract ServiceActivator {

    address public operator;

    enum Services {
        VIP_SALE,
        DAO,
        DEX,
        L_TOKENS,
        STAKING,
        VESTING,
        UNISWAP,
        COMPOUND_INTEREST,
        SELLING
    }

    mapping(Services => bool) public _activated;

    event ServiceActivated(address indexed operator, Services service);
        event ServiceDeactivated(address indexed operator, Services service);

    constructor(address _operator) {
        operator = _operator;
        _activated[Services.VIP_SALE] = false;
        _activated[Services.DAO] = false;
        _activated[Services.DEX] = false;
        _activated[Services.L_TOKENS] = false;
        _activated[Services.STAKING] = false;
        _activated[Services.VESTING] = false;
        _activated[Services.UNISWAP] = false;
        _activated[Services.COMPOUND_INTEREST] = false;
        _activated[Services.SELLING] = false;
    }

    modifier onlyOperator() {
        require(msg.sender == operator, "unauthorized");
        _;
    }

    modifier onlyServiceActivated(Services service) {
        require(_activated[service] == true, "cannot while inactive service");
        _;
    }

    modifier onlyServiceDeactivated(Services service) {
        require(_activated[service] != true, "cannot while active service");
        _;
    }

    function Activate(Services service) internal {
        if(_activated[service] == true) {
            revert("already activated");
        }
        _activated[service] = true;
       emit ServiceActivated(operator, service);
    }

    function ActivateVIP() public {
        Activate(Services.VIP_SALE);
    }
    
    function Deactivate(Services service) public onlyOperator {
        if(_activated[service] == false) {
            revert("already deactivated");
        }
        _activated[service] = false;
       emit ServiceDeactivated(operator, service);
    }


}

