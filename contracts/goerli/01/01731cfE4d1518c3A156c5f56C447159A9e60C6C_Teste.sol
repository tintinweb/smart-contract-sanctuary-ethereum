pragma solidity 0.8.17; 

import "./ERC721PsiUpgradeable.sol";
import "./extension/ERC721PsiBurnableUpgradeable.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import "@openzeppelin/contracts/utils/Strings.sol";

contract Teste is ERC721PsiBurnableUpgradeable, OwnableUpgradeable, ReentrancyGuard {
  using Strings for uint256;
  using Counters for Counters.Counter;

  // Genesis Counter control
  Counters.Counter private _tokenIdCounter;

  // Mapping user that already claimed a NFT
  mapping(address => bool) public claimed;

  // Reference to image and metadata storage
  string public gallery;

  // Merkle Tree Mint Root
  bytes32 public merkleRoot;

  // Contract to receieve ETH raised in sales
  address public vault;

  // Control for public sale
  bool public isMintActive;

  // Control for  allowlist mint
  bool public isAllowlistMintActive;

  // Transfer Control 
  bool public transferControl;

  // Actual cost to mint
  uint256 public price;

  // Actual cost to allowlistMint
  uint256 public allowlistPrice;

  // Max supply
  uint256 public maxSupply;

  // Sets all variables for a safe and clear deploy
  constructor(
    uint256 _maxSupply,  
    uint256 _price,  
    string memory _gallery) ERC721PsiBurnableUpgradeable() {
    maxSupply = _maxSupply;
    price = _price;
    gallery = _gallery;
  }

  // - XXXXXXXXXXXXXXXXXXXXXXX - Internal Overrides - XXXXXXXXXXXXXXXXXXXXXXX - //

  // Override of `supportsInterface` for security
  function supportsInterface(bytes4 interfaceId) public view override(ERC721PsiUpgradeable) returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }

   // Override of `_baseURI()` that returns `gallery`
  function _baseURI() internal view virtual override returns (string memory) {
    return gallery;
  }

  // - XXXXXXXXXXXXXXXXXXXXXXX - Global Writes - XXXXXXXXXXXXXXXXXXXXXXX - // 

  // Sets `gallery` to be returned by `_baseURI()`
  function setGallery(string calldata _gallery) external onlyOwner {
    gallery = _gallery;
  }

  // Sets `vault` to receieve ETH from sales and used within `withdraw()`
  function setVault(address _vault) external onlyOwner {
    vault = _vault;
  }

  // Sets `maxSupply` of the collection
  function setMaxSupply(uint256 _maxSupply) external onlyOwner {
    maxSupply = _maxSupply;
  }

  // Sets bool value for Transfer Control
  function setTransferControl(bool _transferControl) external onlyOwner {
    transferControl = _transferControl;
  }

  // - XXXXXXXXXXXXXXXXXXXXXXX - AllowlistMint - XXXXXXXXXXXXXXXXXXXXXXX - // 

  // Sets `isMintAllowlistActive` to turn on/off allowlistMint.
  function setAllowlistMintActive(bool _isAllowlistMintActive) external onlyOwner {
    isAllowlistMintActive = _isAllowlistMintActive;
  }

    // Sets `Allowlist Price` of the collection
  function setAllowlistPrice(uint256 _allowlistPrice) external onlyOwner {
    allowlistPrice = _allowlistPrice;
  }

  // Sets `merkleRoot` to be used in `allowlistMint()`
  function setMerkleRoot(bytes32 _merkleRoot) public onlyOwner {
    merkleRoot = _merkleRoot;
  }

  // - XXXXXXXXXXXXXXXXXXXXXXX - Mint - XXXXXXXXXXXXXXXXXXXXXXX - // 

  // Sets `isMintActive` to turn on/off mint.
  function setMintActive(bool _isMintActive) external onlyOwner {
    isMintActive = _isMintActive;
  }

  // Sets `Price` of the collection
  function setPrice(uint256 _price) external onlyOwner {
    price = _price;
  
}

  // - XXXXXXXXXXXXXXXXXXXXXXX - Owner Functions - XXXXXXXXXXXXXXXXXXXXXXX - //

  // Send balance of contract to address referenced in `vault`
  function withdraw() external payable onlyOwner {
    require(vault != address(0), 'Vault Invalid');
    require(payable(vault).send(address(this).balance));
  }

  // Genesis airdrop minting function 
  function genesisMultiMint(address[] memory _multipleReceiver) public onlyOwner{
    require(totalSupply() + _multipleReceiver.length <= maxSupply, 'Max supply exceeded!');
      
    for(uint256 i = 0; i < _multipleReceiver.length ; i++){
      uint256 tokenId = _tokenIdCounter.current();
      _tokenIdCounter.increment();
      _safeMint(_multipleReceiver[i], tokenId);
    }
  }

  // - XXXXXXXXXXXXXXXXXXXXXXX - GENESIS PUBLIC WRITES  - XXXXXXXXXXXXXXXXXXXXXXX - //

  // Minting function used in the private sale
  function allowlistMint(bytes32[] calldata _merkleProof) nonReentrant payable external {
    require(tx.origin == _msgSender(), 'Contract Denied');
    require(totalSupply() <= maxSupply, 'Supply Denied');
    require(msg.value >= allowlistPrice, 'Ether Amount Denied');
    require(isAllowlistMintActive == true, 'The whitelist sale is not active!'); 
    require(claimed[_msgSender()] == false, 'Address already claimed!');
      bytes32 leaf = keccak256(abi.encodePacked(_msgSender()));
    require(MerkleProof.verify(_merkleProof, merkleRoot, leaf), 'Invalid proof!');

      claimed[_msgSender()] = true; 
      _tokenIdCounter.increment();
      _safeMint( _msgSender(), 1);

  }


  // Minting function used in the public sale
  function mint() nonReentrant external payable {
    require(totalSupply() <= maxSupply, 'Supply Denied');
    require(tx.origin == _msgSender(), 'Contract Denied');
    require(isMintActive == true, 'The mint sale is not active!');
    require(msg.value >= price, 'Ether Amount Denied');
      
        claimed[_msgSender()] = true; 
        _tokenIdCounter.increment();
       _safeMint( _msgSender(), 1);

  }




}

