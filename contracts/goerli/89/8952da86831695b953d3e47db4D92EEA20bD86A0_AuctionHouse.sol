pragma solidity ^0.8.13;

import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/utils/Strings.sol';
import '@openzeppelin/contracts/token/ERC721/ERC721.sol';
import './Metadata.sol';
import './AlgaerithmController.sol';

contract Algaerithm is ERC721, Ownable {
  address public admin;
  address public metadata;
  address public controller;
  address public auctionHouse;
  uint256 private lastMintedTokenId = 0;

  modifier onlyAuthorized() {
    require(
      (msg.sender == admin ||
        msg.sender == controller ||
        msg.sender == auctionHouse),
      'You are not authorized'
    );
    _;
  }

  modifier onlyAuthorizedAndOwner(uint256 tokenId) {
    require(
      (msg.sender == admin ||
        msg.sender == controller ||
        msg.sender == auctionHouse ||
        msg.sender == ownerOf(tokenId)),
      'You are not authorized'
    );
    _;
  }

  modifier onlyFrozenOrAuthorized(uint256 tokenId) {
    require(
      AlgaerithmController(controller).isFrozen(tokenId) ||
        msg.sender == controller ||
        msg.sender == auctionHouse,
      'specimen is not frozen and sender is not controller'
    );
    _;
  }

  function withdraw(address payable to) public onlyOwner {
    to.transfer(address(this).balance);
  }

  constructor(
    string memory name,
    string memory symbol,
    address _metadata,
    address _auctionHouse
  ) ERC721(name, symbol) {
    metadata = _metadata;
    auctionHouse = _auctionHouse;
    admin = msg.sender;
  }

  function mint(address recipient, uint256 tokenId) public onlyAuthorized {
    _mint(recipient, tokenId);
    lastMintedTokenId = lastMintedTokenId + 1;
  }

  function burn(uint256 tokenId) public onlyAuthorizedAndOwner(tokenId) {
    _burn(tokenId);
  }

  function updateMetadata(address _metadata) public onlyAuthorized {
    metadata = _metadata;
  }

  function updateAuctionHouse(address _auctionHouse) public onlyAuthorized {
    auctionHouse = _auctionHouse;
  }

  function updateController(address _controller) public onlyAuthorized {
    controller = _controller;
  }

  function updateAdmin(address _admin) public onlyOwner {
    admin = _admin;
  }

  function tokenURI(
    uint256 _tokenId
  ) public view override returns (string memory _infoUrl) {
    require(ownerOf(_tokenId) != address(0), 'token is probably burned');
    return Metadata(metadata).tokenURI(_tokenId);
  }

  function totalSupply() public view returns (uint256) {
    return lastMintedTokenId;
  }

  function transferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public virtual override onlyFrozenOrAuthorized(tokenId) {
    super.transferFrom(from, to, tokenId);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId,
    bytes memory data
  ) public virtual override onlyFrozenOrAuthorized(tokenId) {
    super.safeTransferFrom(from, to, tokenId, data);
  }

  function approve(
    address to,
    uint256 tokenId
  ) public virtual override onlyFrozenOrAuthorized(tokenId) {
    super.approve(to, tokenId);
  }
}

