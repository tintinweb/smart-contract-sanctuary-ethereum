pragma solidity ^0.8.13;

import './Algaerithm.sol';
import './AuctionHouse.sol';
import './utils.sol';
import './helpers/math.sol';

import '@openzeppelin/contracts/utils/math/SafeMath.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/utils/Strings.sol';
import '@openzeppelin/contracts/token/ERC721/ERC721.sol';

enum SpecimenState {
  READY,
  UP_FOR_ADOPTION,
  FROZEN
}

struct TokenInfo {
  uint256 seedHash;
  uint256 breedPrice;
  uint256 parentA;
  uint256 parentB;
  uint8 eggs;
  uint birth;
  uint menopause;
  SpecimenState state;
  uint256 adoptionPrice;
}

contract AlgaerithmController is
  Ownable // todo: check if this needs a "ReentrancyGuard"
{
  uint256 public genZeroPrice = 0.1 ether;
  uint256 public crossbreedMinAdminPrice = 0.1 ether;
  uint256 public adminCut = 10; // percentage
  uint256 public minimumAdoptionPrice = 0.1 ether;
  bool public mintingPaused = false;
  uint256 public defaultCrossbreedPrice = 0.1 ether;
  uint16 public maxGenZeroTokens = 128;

  mapping(uint256 => uint256) public tokenIdBySeed;
  mapping(uint256 => TokenInfo) public tokenInfo;

  event TokenMinted(
    uint256 tokenId,
    address recipient,
    uint256 parentA,
    uint256 parentB
  );

  using SafeMath for uint256;

  address payable public adminWallet =
    payable(0x023C51Db075f4703Ee306ddB65852904ba9568C1);
  Algaerithm public algaerithm;
  AuctionHouse public auctionHouse;

  modifier notPaused() {
    require(!mintingPaused, 'Gen zero mint controller is paused');
    _;
  }

  modifier canCrossbreed(uint256 a, uint256 b) {
    // todo: check if token is not burned (?)
    require(tokenInfo[a].seedHash > 0, 'Parent A needs to exist');
    require(tokenInfo[b].seedHash > 0, 'Parent B needs to exist');
    require(a != b, 'A parent cannot breed with itself');
    require(a < b, 'Token ids need to be sorted small to large');
    _;
  }

  modifier onlyAuthorizedAndTokenOwner(uint256 tokenId) {
    require(
      msg.sender == address(auctionHouse) ||
      msg.sender == address(algaerithm) ||
      msg.sender == algaerithm.ownerOf(tokenId)  ||
      msg.sender == owner(),
      'only authorized contracts can call this'
    );
    _;
  }

  modifier onlyAuthorized() {
    require(
      msg.sender == owner() ||
      msg.sender == address(auctionHouse) ||
      msg.sender == address(algaerithm),
      'only authorized contracts can call this'
    );
    _;
  }

  modifier mustOwnToken(uint256 tokenId) {
    require(algaerithm.ownerOf(tokenId) == msg.sender, 'Sender must own token');
    _;
  }

  modifier recentBlockOnly(uint256 blockNumber) {
    require(
      blockNumber < block.number,
      'Cannot not know a block hash in the future'
    );
    require(
      blockNumber > block.number - 256,
      'Can only retrieve the last 256 blocks'
    );
    _;
  }

  modifier isUpForAdoption(uint256 tokenId) {
    require(
      tokenInfo[tokenId].state == SpecimenState.UP_FOR_ADOPTION,
      'Token not up for adoption'
    );
    _;
  }

  constructor(
    Algaerithm _algaerithm,
    AuctionHouse _auctionHouse,
    address payable _adminWallet
  ) {
    algaerithm = _algaerithm;
    adminWallet = _adminWallet;
    auctionHouse = _auctionHouse;
  }

  function updateGenZeroPrice(uint256 _genZeroPrice) public onlyOwner {
    genZeroPrice = _genZeroPrice;
  }

  function updateMinimumAdoptionPrice(
    uint256 _minimumAdoptionPrice
  ) public onlyOwner {
    minimumAdoptionPrice = _minimumAdoptionPrice;
  }

  function updateBreedPrice(uint256 tokenId, uint256 newBreedPrice) public {
    require(
      algaerithm.ownerOf(tokenId) == msg.sender,
      'Can only update breed price for a token you own'
    );
    tokenInfo[tokenId].breedPrice = newBreedPrice;
  }

  function updateMaxGenZeroTokens(uint16 _maxGenZeroTokens) public onlyOwner {
    maxGenZeroTokens = _maxGenZeroTokens;
  }

  function updateAdminWallet(address payable _adminWallet) public onlyOwner {
    adminWallet = _adminWallet;
  }

  function updateMintingPaused(bool _mintingPaused) public onlyOwner {
    mintingPaused = _mintingPaused;
  }

  function getCrossoverHash(
    uint256 parentA,
    uint256 parentB,
    uint blockNumber,
    uint256 siblingIndex
  ) public view recentBlockOnly(blockNumber) returns (uint256) {
    return
      uint256(
        keccak256(
          abi.encodePacked(
            uint256(blockhash(blockNumber)),
            siblingIndex,
            parentA,
            parentB
          )
        )
      );
  }

  function putUpForAdoption(
    uint256 tokenId,
    uint256 price
  ) public mustOwnToken(tokenId) {
    require(
      tokenInfo[tokenId].state == SpecimenState.READY,
      'Token must be ready and available'
    );
    require(price >= minimumAdoptionPrice, 'price must be at least 0.1 eth');
    tokenInfo[tokenId].adoptionPrice = price;
    tokenInfo[tokenId].state = SpecimenState.UP_FOR_ADOPTION;
  }

  function cancelAdoption(
    uint256 tokenId
  ) public mustOwnToken(tokenId) isUpForAdoption(tokenId) {
    tokenInfo[tokenId].state = SpecimenState.READY;
  }

  function acceptAdoption(
    uint256 tokenId
  ) public payable isUpForAdoption(tokenId) {
    require(
      msg.value >= tokenInfo[tokenId].adoptionPrice,
      'You did not send enough ether'
    );
    require(
      algaerithm.ownerOf(tokenId) != msg.sender,
      'Cannot adopt when you already own'
    );

    uint256 adminReceives = msg.value.mul(adminCut).div(100);
    uint256 resellerReceives = msg.value.sub(adminReceives);

    address currentOwner = algaerithm.ownerOf(tokenId);

    _safeTransferETH(adminWallet, adminReceives);
    _safeTransferETH(payable(currentOwner), resellerReceives);

    algaerithm.safeTransferFrom(currentOwner, msg.sender, tokenId);
  }

  function _safeTransferETH(address to, uint256 value) internal returns (bool) {
    (bool success, ) = to.call{value: value, gas: 30_000}(new bytes(0));
    return success;
  }

  function getGenZeroHash(
    uint256 blockNumber
  ) public view recentBlockOnly(blockNumber) returns (uint256) {
    return uint256(blockhash(blockNumber));
  }

  function isFrozen(uint256 tokenId) public view returns (bool) {
    return tokenInfo[tokenId].state == SpecimenState.FROZEN;
  }

  function freezeSpecimen(uint256 tokenId) public mustOwnToken(tokenId) {
    require(
      tokenInfo[tokenId].state == SpecimenState.READY,
      'cannot freeze token in this state'
    );
    tokenInfo[tokenId].state = SpecimenState.FROZEN;
  }

  function _crossbreedCosts(uint256 tokenId) internal view returns (uint256) {
    return
      algaerithm.ownerOf(tokenId) == msg.sender
        ? 0
        : tokenInfo[tokenId].breedPrice;
  }

  function getCrossbreedAdminCut(
    uint256 totalPrice
  ) public view returns (uint256) {
    return max(totalPrice.mul(adminCut).div(100), crossbreedMinAdminPrice);
  }

  function getCrossbreedPrice(
    uint256 parentA,
    uint256 parentB
  ) public view canCrossbreed(parentA, parentB) returns (uint256) {
    uint256 totalPrice = _crossbreedCosts(parentA) + _crossbreedCosts(parentB);
    return totalPrice + getCrossbreedAdminCut(totalPrice);
  }

  function mintGenZero() public onlyAuthorized returns (uint256) {
    return doMint(getGenZeroHash(block.number - 1), 0, 0);
  }

  function mintByCrossbreeding(
    uint256 parentATokenId,
    uint256 parentBTokenId,
    uint blockNumber,
    uint8 siblingIndex
  )
    public
    payable
    canCrossbreed(parentATokenId, parentBTokenId)
    returns (uint256)
  {
    uint256 totalCrossbreedPrice = getCrossbreedPrice(
      parentATokenId,
      parentBTokenId
    );
    require(msg.value >= totalCrossbreedPrice, 'You did not send enough ether');
    uint256 seed = getCrossoverHash(
      parentATokenId,
      parentBTokenId,
      blockNumber,
      siblingIndex
    );
    require(isFertile(parentATokenId), 'Parent A infertile');
    require(isFertile(parentBTokenId), 'Parent B infertile');

    distributeBreedingProceeds(parentATokenId, parentBTokenId);
    return doMint(seed, parentATokenId, parentBTokenId);
  }

  function withdraw(address payable to) public onlyOwner {
    to.transfer(address(this).balance);
  }

  function isFertile(uint256 tokenId) public view returns (bool) {
    return
      block.timestamp < tokenInfo[tokenId].menopause &&
      tokenInfo[tokenId].eggs > 0;
  }

  function distributeBreedingProceeds(
    uint256 parentA,
    uint256 parentB
  ) internal {
    TokenInfo memory A = tokenInfo[parentA];
    TokenInfo memory B = tokenInfo[parentB];

    A.eggs--;
    B.eggs--;

    address ownerA = algaerithm.ownerOf(parentA);
    address ownerB = algaerithm.ownerOf(parentB);
    uint256 totalPrice = 0 ether;
    if (ownerA != msg.sender) {
      // no need to pay for breeding if you own the parent
      _safeTransferETH(payable(ownerA), A.breedPrice);
      totalPrice += A.breedPrice;
    }
    if (ownerB != msg.sender) {
      // no need to pay for breeding if you own the parent
      _safeTransferETH(payable(ownerA), B.breedPrice);
      totalPrice += B.breedPrice;
    }

    _safeTransferETH(adminWallet, getCrossbreedAdminCut(totalPrice));
  }

  function doMint(
    uint256 seed,
    uint256 parentA,
    uint256 parentB
  ) internal returns (uint256) {
    require(
      (parentA == 0 && parentB == 0) || (parentA != 0 && parentB != 0),
      'Either both parents need to be 0, or neither'
    );
    require(tokenIdBySeed[seed] == 0, 'Cannot mint existing seed twice');

    uint256 tokenId = algaerithm.totalSupply() + 1;
    uint8 eggs = uint8(utils.randomUint8(seed + 10) / 32) + 1; // 0 - 7
    uint menopause = block.timestamp + 52 weeks; // todo: make this fluctuate across specimen

    tokenInfo[tokenId] = TokenInfo(
      seed,
      defaultCrossbreedPrice,
      parentA,
      parentB,
      eggs,
      block.timestamp,
      menopause,
      SpecimenState.READY,
      0
    );

    tokenIdBySeed[seed] = tokenId;
    address recipient = msg.sender;
    // todo: implement some re-entry-guard here (?)
    algaerithm.mint(recipient, tokenId);

    emit TokenMinted(tokenId, recipient, parentA, parentB);
    return tokenId;
  }
}

pragma solidity ^0.8.13;

import {PausableUpgradeable} from '@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol';
import {ReentrancyGuardUpgradeable} from '@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol';
import {OwnableUpgradeable} from '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';

import './AlgaerithmController.sol';
import './Algaerithm.sol';
import './utils.sol';
import './interfaces/IAuctionHouse.sol';

// SPDX-License-Identifier: UNLICENSED

contract AuctionHouse is
  IAuctionHouse,
  PausableUpgradeable,
  ReentrancyGuardUpgradeable,
  OwnableUpgradeable
{
  Algaerithm algaerithm;
  AlgaerithmController algaerithmController;

  IAuctionHouse.Settings public settings;
  IAuctionHouse.Auction public auction;

  function initialize(
    Algaerithm _algaerithm,
    AlgaerithmController _algaerithmController
  ) public initializer {
    __Pausable_init();
    __ReentrancyGuard_init();
    __Ownable_init();
    _pause();

    algaerithm = _algaerithm;
    algaerithmController = _algaerithmController;

    settings = IAuctionHouse.Settings({
      timeBuffer: 1 hours,
      reservePrice: 0.1 ether,
      minBidIncrementPercentage: 10,
      duration: 1 days
    });
  }

  function _safeTransferETH(address to, uint256 value) internal returns (bool) {
    (bool success, ) = to.call{value: value, gas: 30_000}(new bytes(0));
    return success;
  }

  function createBid(uint256 tokenId) public payable {
    IAuctionHouse.Auction memory _auction = auction;
    require(_auction.tokenId == tokenId, 'Token not up for auction');
    require(block.timestamp < _auction.endTime, 'Auction expired');
    require(msg.value >= settings.reservePrice, 'Must send at least reserve');
    require(
      msg.value >=
        _auction.amount +
          ((_auction.amount * settings.minBidIncrementPercentage) / 100),
      'Must send more than last bid by settings.minBidIncrementPercentage amount'
    );

    if (_auction.bidder != address(0)) {
      _safeTransferETH(_auction.bidder, _auction.amount);
    }

    auction.amount = msg.value;
    auction.bidder = payable(msg.sender);

    bool extended = _auction.endTime - block.timestamp < settings.timeBuffer;
    if (extended) {
      auction.endTime = _auction.endTime =
        block.timestamp +
        settings.timeBuffer;
    }

    emit AuctionBid(_auction.tokenId, msg.sender, msg.value, extended);

    if (extended) {
      emit AuctionExtended(_auction.tokenId, _auction.endTime);
    }
  }

  /**
   * @notice Pause the auction house.
   * @dev This function can only be called by the owner when the
   * contract is unpaused. While no new auctions can be started when paused,
   * anyone can settle an ongoing auction.
   */
  function pause() external override onlyOwner {
    _pause();
  }

  /**
   * @notice Unpause the auction house.
   * @dev This function can only be called by the owner when the
   * contract is paused. If required, this function will start a new auction.
   */
  function unpause() external override onlyOwner {
    _unpause();

    if (auction.startTime == 0 || auction.settled) {
      _createAuction();
    }
  }

  /**
   * @notice Create an auction.
   * @dev Store the auction details in the `auction` state variable and emit an AuctionCreated event.
   * If the mint reverts, the minter was updated without pausing this contract first. To remedy this,
   * catch the revert and pause this contract.
   */
  function _createAuction() internal {
    try algaerithmController.mintGenZero() returns (uint256 tokenId) {
      uint256 startTime = block.timestamp;
      uint256 endTime = startTime + settings.duration;

      auction = Auction({
        tokenId: tokenId,
        amount: 0,
        startTime: startTime,
        endTime: endTime,
        bidder: payable(0),
        settled: false
      });

      emit AuctionCreated(tokenId, startTime, endTime);
    } catch Error(string memory) {
      _pause();
    }
  }

  /**
   * @notice Set the auction reserve price.
   * @dev Only callable by the owner.
   */
  function setReservePrice(uint256 _reservePrice) external override onlyOwner {
    settings.reservePrice = _reservePrice;
    emit AuctionSettingsUpdated(
      _reservePrice,
      settings.duration,
      settings.minBidIncrementPercentage,
      settings.timeBuffer
    );
  }

  /**
   * @notice Set the auction duration.
   * @dev Only callable by the owner.
   */
  function setDuration(uint256 _duration) external override onlyOwner {
    settings.duration = _duration;
    emit AuctionSettingsUpdated(
      settings.reservePrice,
      _duration,
      settings.minBidIncrementPercentage,
      settings.timeBuffer
    );
  }

  /**
   * @notice Set the auction minimum bid increment percentage.
   * @dev Only callable by the owner.
   */
  function setMinBidIncrementPercentage(
    uint8 _minBidIncrementPercentage
  ) external override onlyOwner {
    settings.minBidIncrementPercentage = _minBidIncrementPercentage;

    emit AuctionSettingsUpdated(
      settings.reservePrice,
      settings.duration,
      _minBidIncrementPercentage,
      settings.timeBuffer
    );
  }

  /**
   * @notice Set the auction time buffer.
   * @dev Only callable by the owner.
   */
  function setTimeBuffer(uint256 _timeBuffer) external override onlyOwner {
    settings.timeBuffer = _timeBuffer;
    emit AuctionSettingsUpdated(
      settings.reservePrice,
      settings.duration,
      settings.minBidIncrementPercentage,
      _timeBuffer
    );
  }

  /**
   * @notice Settle the current auction.
   * @dev This function can only be called when the contract is paused.
   */
  function settleAuction() external override whenPaused nonReentrant {
    _settleAuction();
  }

  function withdraw(address payable to) public onlyOwner {
    to.transfer(address(this).balance);
  }

  /**
   * @notice Settle an auction, finalizing the bid and paying out to the owner.
   * @dev If there are no bids, the Token is burned.
   */
  function _settleAuction() internal {
    IAuctionHouse.Auction memory _auction = auction;

    require(_auction.startTime != 0, "Auction hasn't begun");
    require(!_auction.settled, 'Auction has already been settled');
    require(block.timestamp >= _auction.endTime, "Auction hasn't completed");

    auction.settled = true;

    if (_auction.bidder == address(0)) {
      algaerithm.burn(_auction.tokenId);
    } else {
      algaerithm.transferFrom(address(this), _auction.bidder, _auction.tokenId);
    }

    if (_auction.amount > 0) {
      _safeTransferETH(owner(), _auction.amount);
    }

    emit AuctionSettled(_auction.tokenId, _auction.bidder, _auction.amount);
  }

  /**
   * @notice Settle the current auction, mint a new Token, and put it up for auction.
   */
  function settleCurrentAndCreateNewAuction()
    external
    override
    nonReentrant
    whenNotPaused
  {
    _settleAuction();
    _createAuction();
  }
}

