pragma solidity ^0.8.17;

import '@openzeppelin/contracts/utils/math/SafeMath.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/token/ERC721/IERC721.sol';
import '@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/security/Pausable.sol';
import './interfaces/IRentalProtocol.sol';
import './interfaces/IRPERC721.sol';
import './interfaces/IRPERC1155.sol';

abstract contract RecoreBase is IRentalProtocol, Ownable, Pausable {
  /**
   * @dev Libaries
   */
  using SafeMath for uint256;

  /**
   * Storage
   */
  mapping(address => address) public stNFTs;
  mapping(address => address) public mNFTs;

  /**
   * @dev The struct that represents a lending.
   */
  uint256 internal nextLendingId = 1;
  mapping(uint256 => Lending) internal lendings;
  mapping(uint256 => uint256) internal hashToLendingId;

  /**
   * @dev Fee apply to every transaction
   *
   * - `fee` 100 means 1%
   */
  uint256 public feeRate = 100;

  function version() external pure virtual returns (uint8) {
    return 1;
  }

  /**
   * @dev Setups
   */
  function setFee(uint256 _feeRate) external onlyOwner {
    require(_feeRate > 0, 'invalid fee');

    feeRate = _feeRate;
  }

  /**
   * @dev Utilities
   */
  function calculateRentFee(uint256 _price) public view returns (uint256) {
    return _price.mul(feeRate).div(10000);
  }

  function getExpiresAt(
    uint32 rentedAt,
    uint32 duration
  ) public pure returns (uint32) {
    return rentedAt + (1 days * duration);
  }

  function isExpired(
    uint32 rentedAt,
    uint32 duration
  ) public view returns (bool) {
    return getExpiresAt(rentedAt, duration) < block.timestamp;
  }

  /**
   * @dev Modifiers
   */
  modifier limitDuration(uint8 duration) {
    require(duration > 0, 'RecoreBase: duration should be greater than 0');
    require(
      duration <= 180,
      'RecoreBase: duration must be less than or equal to 180 days'
    );

    _;
  }

  function _isERC721(address _nft) internal view returns (bool) {
    return IERC165(_nft).supportsInterface(type(IERC721).interfaceId);
  }

  function _isERC1155(address _nft) internal view returns (bool) {
    return IERC165(_nft).supportsInterface(type(IERC1155).interfaceId);
  }

  function _isDangerouslyERC20(address _token) internal view returns (bool) {
    return
      IERC20(_token).totalSupply() > 0 &&
      IERC20(_token).balanceOf(address(this)) >= 0;
  }
}

