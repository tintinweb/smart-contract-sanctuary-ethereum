import '@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol';
import '@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '../access/ContractOperators.sol';
import '../Recore721.sol';

abstract contract IRPERC721 is
  ERC721Enumerable,
  ERC721Holder,
  ContractOperators
{
  address public parent;
  address public rental;

  constructor(
    string memory name_,
    string memory symbol_,
    address _parent,
    address _rental
  ) ERC721(name_, symbol_) {
    parent = _parent;
    rental = _rental;
  }

  /**
   * @dev Virtual Mint
   * Mint or send staked NFT to the user
   */
  function mint(
    address to,
    uint256 tokenId
  ) external onlyContractOperator returns (uint256) {
    _safeMint(to, tokenId);

    return tokenId;
  }

  /**
   * @dev after
   */
  function _afterTokenTransfer(
    address from,
    address to,
    uint256 firstTokenId,
    uint256 batchSize
  ) internal virtual override {
    super._afterTokenTransfer(from, to, firstTokenId, batchSize);

    try Recore721(rental).sync(parent, firstTokenId) {} catch {}
  }

  /**
   * @dev See {IERC721-isApprovedForAll}.
   */
  function isApprovedForAll(
    address owner,
    address operator
  ) public view virtual override(ERC721, IERC721) returns (bool) {
    require(owner != address(0), 'IRPERC721: owner is zero address');
    require(operator != address(0), 'IRPERC721: operator is zero address');

    return
      owner == address(this) ||
      isContractOperator(operator) ||
      super.isApprovedForAll(owner, operator);
  }

  /**
   * @dev See {IERC165-supportsInterface}.
   */
  function supportsInterface(
    bytes4 interfaceId
  ) public view virtual override returns (bool) {
    return
      interfaceId == type(IRPERC721).interfaceId ||
      super.supportsInterface(interfaceId);
  }
}

// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.17;

import '@openzeppelin/contracts/utils/math/SafeMath.sol';
import '@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol';
import '@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol';
import './interfaces/IRentalProtocol.sol';
import './RecoreBase.sol';
import './RPERC721.sol';

contract Recore721 is RecoreBase, ERC721Holder {
  /**
   * @dev Libaries
   */
  using SafeMath for uint256;

  /**
   * @dev Events
   */
  event WrappedTokens(
    address indexed nftAddress,
    address stNftAddress,
    address mNftAddress
  );

  /**
   * @dev Setups
   */
  function setWrappedTokens(address nftAddress) public onlyOwner {
    require(nftAddress != address(0), 'Recore: invalid nft');
    require(
      stNFTs[nftAddress] == address(0) && mNFTs[nftAddress] == address(0),
      'Recore: already set'
    );
    require(_isERC721(nftAddress), 'Recore: contract must be erc721');

    string memory name;
    string memory symbol;
    if (
      IERC721(nftAddress).supportsInterface(type(IERC721Metadata).interfaceId)
    ) {
      name = IERC721Metadata(nftAddress).name();
      symbol = IERC721Metadata(nftAddress).symbol();
    } else {
      name = 'NFT';
      symbol = 'NFT';
    }

    stNFTs[nftAddress] = address(
      new RPERC721(
        string(abi.encodePacked('Staked ', name)),
        string(abi.encodePacked('st', symbol)),
        nftAddress
      )
    );
    mNFTs[nftAddress] = address(
      new RPERC721(
        string(abi.encodePacked('Mirror ', name)),
        string(abi.encodePacked('m', symbol)),
        nftAddress
      )
    );

    emit WrappedTokens({
      nftAddress: nftAddress,
      stNftAddress: stNFTs[nftAddress],
      mNftAddress: mNFTs[nftAddress]
    });
  }

  function getWrappedTokens(
    address nftAddress
  ) public view returns (address, address) {
    return (stNFTs[nftAddress], mNFTs[nftAddress]);
  }

  function isReady(address nftAddress) public view returns (bool) {
    return stNFTs[nftAddress] != address(0) && mNFTs[nftAddress] != address(0);
  }

  /**
   * @dev Register nft to be lent with staking
   * @param nftAddress NFT address (ERC721)
   * @param tokenId NFT token id
   * @param amount NFT amount (optional)
   * @param maxDuration Duration in days (1-180)
   * @param paymentTokenAddress Payment token address compatible to ERC20
   * @param dailyPrice Price to rent
   */
  function lend(
    address nftAddress,
    uint256 tokenId,
    uint256 amount,
    uint8 maxDuration,
    IERC20 paymentTokenAddress,
    uint256 dailyPrice
  )
    external
    override
    whenNotPaused
    onlyReady(nftAddress)
    limitDuration(maxDuration)
    returns (uint256)
  {
    require(nftAddress != address(this), 'Recore: stNFT can not be lent');
    require(
      _isDangerouslyERC20(address(paymentTokenAddress)),
      'Recore: payment token should have total supply to check ERC20 compatibility'
    );
    require(dailyPrice > 0, 'Recore: amount should be greater than 0');

    address lender = _msgSender();

    IERC721(nftAddress).safeTransferFrom(lender, address(this), tokenId);

    uint256 lendingId = _createLending({
      lender: lender,
      nftAddress: nftAddress,
      tokenId: tokenId,
      maxDuration: maxDuration,
      paymentTokenAddress: paymentTokenAddress,
      dailyPrice: dailyPrice
    });

    _virtualMint({
      nftAddress: stNFTs[nftAddress],
      to: lender,
      tokenId: tokenId
    });

    emit Lent({
      lendingId: lendingId,
      nftAddress: nftAddress,
      lender: lender,
      tokenId: tokenId,
      amount: amount,
      maxDuration: maxDuration,
      paymentTokenAddress: paymentTokenAddress,
      dailyPrice: dailyPrice
    });

    return lendingId;
  }

  function _createLending(
    address lender,
    address nftAddress,
    uint256 tokenId,
    uint8 maxDuration,
    IERC20 paymentTokenAddress,
    uint256 dailyPrice
  ) private returns (uint256) {
    _validateOwnership({
      owner: address(this),
      nftAddress: nftAddress,
      tokenId: tokenId
    });

    uint256 lendingId = nextLendingId;

    lendings[lendingId] = Lending({
      lender: lender,
      renter: address(0),
      nftAddress: nftAddress,
      tokenId: tokenId,
      amount: 1,
      maxDuration: maxDuration,
      rentedAt: 0,
      rentDuration: 0,
      paymentTokenAddress: paymentTokenAddress,
      dailyPrice: dailyPrice
    });
    hashToLendingId[_getTokenHash(nftAddress, tokenId)] = lendingId;

    nextLendingId = nextLendingId.add(1);

    return lendingId;
  }

  /**
   * @dev Update lending conditions
   * @param lendingId Lending ID
   * @param maxDuration Duration in days (1-180)
   * @param dailyPrice Price to rent
   */
  function updateLending(
    uint256 lendingId,
    uint8 maxDuration,
    uint256 dailyPrice
  ) external whenNotPaused limitDuration(maxDuration) {
    Lending storage lending = lendings[lendingId];
    require(lending.renter == address(0), 'Recore: already rented');

    _validateLender(_msgSender(), lendingId);

    lending.maxDuration = maxDuration;
    lending.dailyPrice = dailyPrice;

    emit Rented({
      lendingId: lendingId,
      renter: _msgSender(),
      nftAddress: lending.nftAddress,
      tokenId: lending.tokenId,
      amount: lending.amount,
      duration: lending.rentDuration,
      rentedAt: lending.rentedAt,
      expiresAt: getExpiresAt(lending.rentedAt, lending.rentDuration)
    });
  }

  /**
   * @dev Rent a nft
   * @param lendingId Lending id
   * @param duration Duration to rent
   */
  function rent(
    uint256 lendingId,
    uint8 duration
  ) external override whenNotPaused returns (uint256) {
    Lending storage lending = lendings[lendingId];
    address nftAddress = lending.nftAddress;
    address lender = lenderOf(nftAddress, lending.tokenId);

    require(lender != address(0), 'Recore: lending not found');
    require(
      IERC721(stNFTs[nftAddress]).ownerOf(lending.tokenId) != _msgSender(),
      'Recore: lender can not rent'
    );
    require(lending.renter == address(0), 'Recore: already rented');
    require(lending.maxDuration >= duration, 'Recore: invalid duration');

    _payRentalCost(lending, duration);

    lending.renter = _msgSender();
    lending.rentedAt = uint32(block.timestamp);
    lending.rentDuration = duration;

    emit Rented({
      lendingId: lendingId,
      renter: _msgSender(),
      nftAddress: nftAddress,
      tokenId: lending.tokenId,
      amount: lending.amount,
      duration: duration,
      rentedAt: lending.rentedAt,
      expiresAt: getExpiresAt(lending.rentedAt, duration)
    });

    _virtualMint({
      nftAddress: mNFTs[nftAddress],
      to: _msgSender(),
      tokenId: lending.tokenId
    });

    return lending.tokenId;
  }

  /**
   * @dev Pay lending cost with fee
   * @param lending Lending
   * @param duration Rent duration as days
   */
  function _payRentalCost(Lending memory lending, uint8 duration) private {
    uint256 cost = lending.dailyPrice.mul(duration);

    uint256 rentFee = calculateRentFee(cost);
    uint256 totalCost = cost.add(rentFee);

    require(
      lending.paymentTokenAddress.balanceOf(_msgSender()) >= totalCost,
      'Recore: not enough balance'
    );
    require(
      lending.paymentTokenAddress.allowance(_msgSender(), address(this)) >=
        totalCost,
      'Recore: not enough allowance'
    );

    address lender = IRPERC721(stNFTs[lending.nftAddress]).ownerOf(
      lending.tokenId
    );
    lending.paymentTokenAddress.transferFrom(_msgSender(), lender, cost);

    if (rentFee > 0) {
      lending.paymentTokenAddress.transferFrom(
        _msgSender(),
        address(this),
        rentFee
      );
    }
  }

  /**
   * @dev Extend lending
   * @param lendingId Lending id
   * @param duration Lending duration
   */
  function extend(
    uint256 lendingId,
    uint8 duration
  ) external override whenNotPaused {
    Lending storage lending = lendings[lendingId];
    require(lending.rentedAt > 0, 'Recore: rent first');

    _validateRenter(_msgSender(), lendingId);

    uint8 nextDuration = lending.rentDuration + duration;
    require(
      nextDuration <= lending.maxDuration,
      'Recore: exceeding lending max duration'
    );

    _payRentalCost(lending, duration);

    lending.rentDuration = nextDuration;

    emit Rented({
      lendingId: lendingId,
      nftAddress: lending.nftAddress,
      renter: _msgSender(),
      tokenId: lending.tokenId,
      amount: lending.amount,
      duration: nextDuration,
      rentedAt: lending.rentedAt,
      expiresAt: getExpiresAt(lending.rentedAt, nextDuration)
    });
  }

  /**
   * @dev Cancel lending and return NFT unless rented
   * @param lendingId Lending id
   */
  function cancel(uint256 lendingId) external override whenNotPaused {
    Lending memory lending = lendings[lendingId];
    address nftAddress = lending.nftAddress;

    address stakedNFT = stNFTs[nftAddress];
    require(
      IERC721(stakedNFT).ownerOf(lending.tokenId) == _msgSender(),
      'Recore: stNFT balance is below amount'
    );
    require(lending.renter == address(0), 'Recore: already rented');

    IERC721(nftAddress).safeTransferFrom(
      address(this),
      _msgSender(),
      lending.tokenId
    );
    _virtualBurn({
      nftAddress: stakedNFT,
      from: _msgSender(),
      tokenId: lending.tokenId
    });

    delete lendings[lendingId];
    delete hashToLendingId[_getTokenHash(nftAddress, lending.tokenId)];

    emit Cancelled({
      lendingId: lendingId,
      nftAddress: nftAddress,
      returnedAt: uint32(block.timestamp)
    });
  }

  /**
   * @dev Renounce lending and return NFT
   */
  function renounce(uint256 lendingId) external whenNotPaused {
    Lending storage lending = lendings[lendingId];
    address nftAddress = lending.nftAddress;

    address renter = renterOf(nftAddress, lending.tokenId);
    require(renter == _msgSender(), 'Recore: not renter');
    require(lending.rentedAt > 0, 'Recore: not rented');

    _virtualBurn({
      nftAddress: mNFTs[nftAddress],
      from: _msgSender(),
      tokenId: lending.tokenId
    });

    lending.renter = address(0);
    lending.rentedAt = 0;
  }

  /**
   * @dev Recall NFT from finished lending
   * @param lendingId Lending id
   */
  function recall(uint256 lendingId) external override whenNotPaused {
    Lending memory lending = lendings[lendingId];
    address nftAddress = lending.nftAddress;

    address lender = lenderOf(nftAddress, lending.tokenId);
    require(lender == _msgSender(), 'Recore: lender is not sender');
    require(lending.renter != address(0), 'Recore: not rented');
    require(lending.rentedAt > 0, 'Recore: not rented');
    require(
      getExpiresAt(lending.rentedAt, lending.rentDuration) < block.timestamp,
      'Recore: not expired'
    );

    IERC721(nftAddress).safeTransferFrom(
      address(this),
      lender,
      lending.tokenId
    );

    _virtualBurn({
      nftAddress: stNFTs[nftAddress],
      from: _msgSender(),
      tokenId: lending.tokenId
    });

    if (IERC721(mNFTs[nftAddress]).ownerOf(lending.tokenId) == lending.renter) {
      _virtualBurn({
        nftAddress: mNFTs[nftAddress],
        from: _msgSender(),
        tokenId: lending.tokenId
      });
    }

    delete lendings[lendingId];
    delete hashToLendingId[_getTokenHash(nftAddress, lending.tokenId)];

    emit Returned({
      lendingId: lendingId,
      nftAddress: nftAddress,
      returnedAt: uint32(block.timestamp)
    });
  }

  /**
   * @dev Sync lender & renter. Recommended to call after mNFT, stNFT's transfer
   */
  function sync(address nftAddress, uint256 tokenId) external {
    uint256 lendingId = hashToLendingId[_getTokenHash(nftAddress, tokenId)];
    Lending storage lending = lendings[lendingId];

    if (lending.lender != address(0)) {
      address lender = lenderOf(nftAddress, tokenId);
      if (lender != lending.lender) {
        lending.lender = lenderOf(nftAddress, tokenId);
      }
    }

    if (lending.renter != address(0)) {
      address renter = renterOf(nftAddress, tokenId);
      if (renter != lending.renter) {
        lending.renter = renterOf(nftAddress, tokenId);

        emit Rented({
          lendingId: lendingId,
          renter: lending.renter,
          nftAddress: lending.nftAddress,
          tokenId: lending.tokenId,
          amount: lending.amount,
          duration: lending.rentDuration,
          rentedAt: lending.rentedAt,
          expiresAt: getExpiresAt(lending.rentedAt, lending.rentDuration)
        });
      }
    }
  }

  /**
   * @dev userOf returns user of nft's tokenId
   */
  function userOf(
    address nftAddress,
    uint256 tokenId
  ) external view returns (address) {
    address mNFT = mNFTs[nftAddress];
    if (!_isERC721(mNFT)) {
      revert('Recore: not supported');
    }

    address owner = IERC721(mNFT).ownerOf(tokenId);
    if (owner == address(this)) {
      return address(0);
    }

    return owner;
  }

  function lenderOf(
    address nftAddress,
    uint256 tokenId
  ) internal view returns (address) {
    return IRPERC721(stNFTs[nftAddress]).ownerOf(tokenId);
  }

  function renterOf(
    address nftAddress,
    uint256 tokenId
  ) internal view returns (address) {
    return IRPERC721(mNFTs[nftAddress]).ownerOf(tokenId);
  }

  function getLending(
    uint256 lendingId
  ) external view returns (Lending memory) {
    return lendings[lendingId];
  }

  function getLendingByToken(
    address nftAddress,
    uint256 tokenId
  ) external view returns (Lending memory) {
    return lendings[hashToLendingId[_getTokenHash(nftAddress, tokenId)]];
  }

  function _getTokenHash(
    address nftAddress,
    uint256 tokenId
  ) private pure returns (uint256) {
    return uint256(keccak256(abi.encodePacked(nftAddress, tokenId)));
  }

  function _virtualMint(
    address nftAddress,
    address to,
    uint256 tokenId
  ) internal {
    IRPERC721 nft = IRPERC721(nftAddress);

    if (tokenId < nft.totalSupply()) {
      nft.safeTransferFrom(address(this), to, tokenId);
    } else {
      nft.mint(to, tokenId);
    }
  }

  function _virtualBurn(
    address nftAddress,
    address from,
    uint256 tokenId
  ) internal {
    IRPERC721(nftAddress).safeTransferFrom(from, address(this), tokenId);
  }

  function _validateLender(address owner, uint256 lendingId) private view {
    Lending memory lending = lendings[lendingId];

    _validateOwnership({
      owner: owner,
      nftAddress: stNFTs[lending.nftAddress],
      tokenId: lending.tokenId
    });
  }

  function _validateRenter(address owner, uint256 lendingId) private view {
    Lending memory lending = lendings[lendingId];

    _validateOwnership({
      owner: owner,
      nftAddress: mNFTs[lending.nftAddress],
      tokenId: lending.tokenId
    });
  }

  function _validateOwnership(
    address owner,
    address nftAddress,
    uint256 tokenId
  ) internal view {
    require(
      IERC721(nftAddress).ownerOf(tokenId) == owner,
      'Recore: invalid ownership'
    );
  }

  /**
   * @dev Modifiers
   */
  modifier onlyReady(address nftAddress) {
    require(isReady(nftAddress), 'Recore: not ready');

    _;
  }
}

