import '@openzeppelin/contracts/access/Ownable.sol';

contract ContractOperators is Ownable {
  mapping(address => bool) internal _contractOperators;

  event ContractOperator(address indexed operator, bool enabled);

  function setContractOperator(
    address _operator,
    bool _enabled
  ) public onlyOwner {
    _contractOperators[_operator] = _enabled;
    emit ContractOperator(_operator, _enabled);
  }

  function isContractOperator(address _operator) public view returns (bool) {
    return _contractOperators[_operator];
  }

  modifier onlyContractOperator() {
    require(
      _contractOperators[msg.sender],
      'ContractOperators: caller is not a contract operator'
    );

    _;
  }
}

