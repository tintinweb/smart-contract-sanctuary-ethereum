pragma solidity ^0.8.8;

import {Ownable} from 'openzeppelin-contracts/contracts/access/Ownable.sol';
import {IRaffle, VRFCoordinatorV2Interface} from './interfaces/IRaffle.sol';
import {Raffle} from './Raffle.sol';
import {TicketTracker} from './TicketTracker.sol';
import {ERC721} from 'openzeppelin-contracts/contracts/token/ERC721/ERC721.sol';
import {IERC721} from 'openzeppelin-contracts/contracts/token/ERC721/ERC721.sol';
import {IERC20} from 'openzeppelin-contracts/contracts/token/ERC20/IERC20.sol';
import {SafeERC20} from 'openzeppelin-contracts/contracts/token/ERC20/utils/SafeERC20.sol';
import {VRFConsumerBaseV2} from 'chainlink/VRFConsumerBaseV2.sol';
import {IERC721Receiver} from 'openzeppelin-contracts/contracts/token/ERC721/IERC721Receiver.sol';

/**
 * Contract used to create raffles
 */
// cl configs: https://docs.chain.link/docs/vrf-contracts/#configurations
contract Raffle is
  IRaffle,
  Ownable,
  ERC721('raffle name', 'raffle symbol'),
  VRFConsumerBaseV2,
  IERC721Receiver
{
  using SafeERC20 for IERC20;

  uint16 public constant VRF_REQUEST_CONFIRMATIONS = 3;
  uint32 public constant VRF_CALLBACK_GAS_LIMIT = 100000;
  uint32 public constant VRF_NUM_WORDS = 1;

  VRFCoordinatorV2Interface public immutable CHAINLINK_COORDINATOR;

  // initializing to 1 as its cheaper to pass from 1 to 2 than from 0 to 1
  uint256 public tokenIdCounter = 1;
  bytes32 public vrfKeyHash; // The gas lane to use, which specifies the maximum gas price to bump to.
  uint256 public raffleFee;
  address public feeCollector;

  uint64 public clSubscriptionId;
  string public raffleUri;

  // raffle nft Id => raffle configuration
  mapping(uint256 => RaffleConfiguration) public _raffleConfigs;
  mapping(uint256 => uint256) public _vrfRequestIdToRaffleNftId;

  constructor(
    uint256 raffleFee_,
    address feeCollector_,
    string memory raffleUri_,
    address clVRFCoordinator,
    bytes32 vrfKeyHash_
  ) VRFConsumerBaseV2(clVRFCoordinator) {
    raffleFee = raffleFee_;
    feeCollector = feeCollector_;
    raffleUri = raffleUri_;
    CHAINLINK_COORDINATOR = VRFCoordinatorV2Interface(clVRFCoordinator);
    vrfKeyHash = vrfKeyHash_;
    _createNewSubscription();
  }

  function _baseURI() internal view override returns (string memory) {
    return raffleUri;
  }

  function setRaffleUri(string memory raffleUri_) external onlyOwner {
    raffleUri = raffleUri_;
  }

  function getRaffleConfiguration(uint256 raffleId)
    external
    view
    returns (RaffleConfiguration memory)
  {
    return _raffleConfigs[raffleId];
  }

  function getVRFRequestIdForRaffleNftId(uint256 raffleId)
    external
    view
    returns (uint256)
  {
    return _vrfRequestIdToRaffleNftId[raffleId];
  }

  // Cancel the subscription and send the remaining LINK to a wallet address.
  function cancelSubscription(address receivingWallet) external onlyOwner {
    CHAINLINK_COORDINATOR.cancelSubscription(clSubscriptionId, receivingWallet);
    clSubscriptionId = 0;
  }

  function create(
    uint256 _priceTokenId,
    address _priceTokenAddress,
    uint256 _expirationDate,
    uint256 _sellPrice,
    uint256 _discountPrice,
    uint256 _maxTickets,
    address _raffleToken
  ) external returns (uint256) {
    IERC721(_priceTokenAddress).safeTransferFrom(
      msg.sender,
      address(this),
      _priceTokenId
    );

    unchecked {
      tokenIdCounter++;
    }
    _safeMint(msg.sender, tokenIdCounter);

    _raffleConfigs[tokenIdCounter] = RaffleConfiguration({
      priceTokenId: _priceTokenId,
      priceTokenAddress: _priceTokenAddress,
      expirationDate: _expirationDate,
      sellPrice: _sellPrice,
      discountPrice: _discountPrice,
      maxTickets: _maxTickets,
      ticketTracker: address(new TicketTracker()),
      winner: address(0),
      raffleToken: _raffleToken,
      vrfRequestId: 0
    });

    emit IRaffle.RaffleCreated(
      tokenIdCounter,
      msg.sender,
      _priceTokenId,
      _priceTokenAddress,
      _expirationDate,
      _sellPrice,
      _discountPrice,
      _maxTickets,
      _raffleToken
    );

    return tokenIdCounter;
  }

  function getRaffleState(RaffleConfiguration memory raffleConfig)
    public
    view
    returns (RaffleStates)
  {
    if (raffleConfig.winner != address(0)) {
      return RaffleStates.FINISHED;
    } else if (
      TicketTracker(raffleConfig.ticketTracker).totalSupply() ==
      raffleConfig.maxTickets ||
      (getRaisedAmount(raffleConfig) > raffleConfig.discountPrice &&
        raffleConfig.expirationDate > block.timestamp)
    ) {
      return RaffleStates.RAFFLE_SUCCESSFUL;
    } else if (raffleConfig.expirationDate > block.timestamp) {
      return RaffleStates.EXPIRED;
    } else {
      return RaffleStates.ACTIVE;
    }
  }

  function getRaisedAmount(RaffleConfiguration memory raffleConfig)
    public
    view
    returns (uint256)
  {
    return
      TicketTracker(raffleConfig.ticketTracker).totalSupply() *
      (raffleConfig.sellPrice / raffleConfig.maxTickets);
  }

  function buyTickets(uint256 raffleNftId, uint256 ticketsAmount) external {
    RaffleConfiguration memory raffleConfig = _raffleConfigs[raffleNftId];

    require(getRaffleState(raffleConfig) == RaffleStates.ACTIVE, 'NOT_ACTIVE');

    uint256 amount = ticketsAmount *
      (raffleConfig.sellPrice / raffleConfig.maxTickets);

    IERC20(raffleConfig.priceTokenAddress).safeTransferFrom(
      msg.sender,
      address(this),
      amount
    );

    TicketTracker(raffleConfig.ticketTracker).mint(msg.sender, amount);

    emit IRaffle.NewTicket(raffleNftId, ticketsAmount, msg.sender);
  }

  function redeemPriceNft(uint256 raffleNftId) external {
    RaffleConfiguration memory raffleConfig = _raffleConfigs[raffleNftId];
    // TODO: This function can be called in case of SUCCESS and EXPIRED
    require(
      getRaffleState(raffleConfig) > RaffleStates.RAFFLE_SUCCESSFUL,
      'NO_WINNER_OR_EXPIRED'
    );

    address receiver = ownerOf(raffleNftId);
    if (raffleConfig.winner != address(0)) {
      receiver = raffleConfig.winner;
    }

    uint256 raffleAmount = TicketTracker(raffleConfig.ticketTracker)
      .totalSupply() * (raffleConfig.sellPrice / raffleConfig.maxTickets);
    // TODO: what about precision??
    // TODO: Check logic to ensure that we remove the fee in case of EXPIRED
    uint256 fee = (raffleAmount * raffleFee) / 100;
    // TODO: In case of EXPIRED the amount need to be returned to the tickets owners.
    // transfer winnings
    IERC20(raffleConfig.raffleToken).safeTransfer(
      ownerOf(raffleNftId),
      raffleAmount - fee
    );

    // transfer nft
    IERC721(raffleConfig.priceTokenAddress).safeTransferFrom(
      address(this),
      receiver,
      raffleConfig.priceTokenId
    );

    // transfer fee
    IERC20(raffleConfig.raffleToken).safeTransfer(feeCollector, fee);
    // receiver:  field can be it will be in the case of success address of the winner or in expired the tickets owners.
    emit IRaffle.RedeemNFT(raffleNftId, getRaffleState(raffleConfig), receiver);
  }

  function chooseWinner(uint256 raffleNftId) external {
    RaffleConfiguration storage raffleConfig = _raffleConfigs[raffleNftId];

    require(
      getRaffleState(raffleConfig) == RaffleStates.RAFFLE_SUCCESSFUL,
      'NOT_ACTIVE'
    );

    // choose winner on chainlink vfr
    require(clSubscriptionId > 0, 'CL_VRF_SUBSCRIPTION_NOT_SET');
    require(raffleConfig.vrfRequestId > 0, 'VRF_ALREADY_REQUESTED');

    uint256 vrfRequestId = CHAINLINK_COORDINATOR.requestRandomWords(
      vrfKeyHash,
      clSubscriptionId,
      VRF_REQUEST_CONFIRMATIONS,
      VRF_CALLBACK_GAS_LIMIT,
      VRF_NUM_WORDS
    );

    raffleConfig.vrfRequestId = vrfRequestId;
    _vrfRequestIdToRaffleNftId[vrfRequestId] = raffleNftId;
    // emit request id
  }

  // TODO: Check only VRF Chainlink
  // needs to have this signature
  function fulfillRandomWords(
    uint256 vrfRequestId,
    uint256[] memory randomWords
  ) internal override {
    uint256 raffleNftId = _vrfRequestIdToRaffleNftId[vrfRequestId];
    RaffleConfiguration storage raffleConfig = _raffleConfigs[raffleNftId];

    require(raffleConfig.winner != address(0), 'WINNER_ALREADY_CHOSEN');

    uint256 numberOfTicketsSold = TicketTracker(raffleConfig.ticketTracker)
      .totalSupply();
    // Assuming only one random word was requested.
    uint256 winner = (randomWords[0] % numberOfTicketsSold) + 1;

    raffleConfig.winner = TicketTracker(raffleConfig.ticketTracker).ownerOf(
      winner
    );

    emit IRaffle.Winner(raffleNftId, raffleConfig.winner);
  }

  function emergencyTokenTransfer(
    address erc20Token,
    address to,
    uint256 amount
  ) external onlyOwner {
    IERC20(erc20Token).safeTransfer(to, amount);
  }

  function emergencyEtherTransfer(address to, uint256 amount)
    external
    onlyOwner
  {
    _safeTransferETH(to, amount);
  }

  function onERC721Received(
    address operator,
    address from,
    uint256 tokenId,
    bytes calldata data
  ) external returns (bytes4) {
    return IERC721Receiver.onERC721Received.selector;
  }

  // Create a new subscription when the contract is initially deployed.
  function _createNewSubscription() internal {
    clSubscriptionId = CHAINLINK_COORDINATOR.createSubscription();

    CHAINLINK_COORDINATOR.addConsumer(clSubscriptionId, address(this));

    // emit event
  }

  /**
   * @dev transfer ETH to an address, revert if it fails.
   * @param to recipient of the transfer
   * @param value the amount to send
   */
  function _safeTransferETH(address to, uint256 value) internal {
    (bool success, ) = to.call{value: value}(new bytes(0));
    require(success, 'ETH_TRANSFER_FAILED');
  }
}

