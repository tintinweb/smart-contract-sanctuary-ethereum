  }

  function setBlackHoleAddress(uint256 _share) external onlyRole(DEFAULT_ADMIN_ROLE) {
    PonzuStorage storage ps = LibPonzu.DS();
    ps.blackHoleShare = _share;
  }

  function setBlackHoleShare(uint256 _share) external onlyRole(DEFAULT_ADMIN_ROLE) {
    PonzuStorage storage ps = LibPonzu.DS();
    ps.blackHoleShare = _share;
  }

  function deposit(uint256 amount) external {
    PonzuStorage storage ps = LibPonzu.DS();
    ps.deposit(amount);
  }

  function withdraw(uint256 amount) external {
    PonzuStorage storage ps = LibPonzu.DS();
    ps.withdraw(amount);
  }

  // ==================== Views ==================== //

  function prizePool() external view returns (uint256) {
    return LibPonzu.currentPrizePool();
  }

  function ponzuPool() external view returns (uint256) {
    PonzuStorage storage ps = LibPonzu.DS();
    return ps.sumTotalValidDeposits();
  }

  function currentRoundInfo()
    external
    view
    returns (
      uint256,
      uint256,
      uint256
    )
  {
    PonzuStorage storage ps = LibPonzu.DS();
    return (ps.currentRoundStartTime(), ps.currentRoundDeadline(), ps.currentRoundEndTime());
  }

  function totalParticipants() external view returns (uint256) {
    PonzuStorage storage ps = LibPonzu.DS();
    return ps.participantsList.length;
  }

  function getParticipant(address _account)
    external
    view
    returns (
      uint256,
      uint256,
      uint256
    )
  {
    PonzuStorage storage ps = LibPonzu.DS();
    Participant memory participant = ps.participants[_account];

    return (
      participant.oldDepositAmount,
      participant.newDepositAmount,
      participant.newDepositRound
    );
  }
}

