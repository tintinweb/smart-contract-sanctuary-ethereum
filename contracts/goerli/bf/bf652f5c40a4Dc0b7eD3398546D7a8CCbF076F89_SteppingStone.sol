// 1st will be burned after 1 usage
// 2nd will be burned after 30 days

// Core Functions:
// --------------------
//  1. Minting Function
//  2. Burning Function
//  3. Automatic burn with chainlink keepers

// What can currently be done?
// fetches equivalent amount of usd in eth, ex: $50 ~0.032
// mints an nft based off this price

// What would we like to do?
// burn this nft after x amount of time

// ideas
// perform upkeep is not getting any value from perform data not sure why

// tokenId => Arr[tokenId] = block.timestamp
// Errors
error IncorrectAmountOfEthSent();
error UpKeepNotNeeded(uint256 timeStamp);
error TransferFailed();
error NothingToWithdraw();
error NotOwner();

contract SteppingStone is ERC721URIStorage, Ownable, KeeperCompatibleInterface {
    enum Type {
        ONE_TIME,
        MONTH
    }

    // Type Declarations
    using PriceConverter for uint256;

    // State Variables
    using Counters for Counters.Counter;
    Counters.Counter private _totalSupply;

    uint256 internal mintFee;
    uint256 private s_tokenCounter;

    address private immutable i_owner;
    mapping(uint256 => Type) private s_tokenIdToNft;
    string[] internal s_typeTokenUris;
    uint256 public constant MINIMUM_USD = 50 * 10**18;

    // TESTING
    struct Target {
        uint256 expires;
        uint256 timeStamp;
        uint256 tokenId;
    }

    mapping(uint256 => Target) internal s_targets;

    // TESTING

    // Price Feeds
    // AggregatorV3Interface internal priceFeed;
    AggregatorV3Interface private s_priceFeed;

    // Upkeep Variables
    uint public immutable interval;
    uint public lastTimeStamp;

    // ///////////////////
    ////// Events ///////
    //////////////////////

    event NftMinted(address minter, address nftContract, uint256 tokenId);
    event NftBurned(address burner, uint256 tokenId);
    event PerformUpKeepCompleted();

    constructor(
        address priceFeed,
        string[2] memory typeTokenUris,
        uint _interval
    ) ERC721("SteppingStoneNft", "SSN") {
        interval = _interval;
        lastTimeStamp = block.timestamp;
        s_typeTokenUris = typeTokenUris;
        i_owner = owner();
        s_tokenCounter = 0;

        s_priceFeed = AggregatorV3Interface(priceFeed);
    }

