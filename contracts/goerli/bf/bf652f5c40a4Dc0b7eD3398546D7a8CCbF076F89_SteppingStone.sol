    //    * @notice Gets a list of tokens that are expired
    //    * @return list of tokens that are expired
    //    */

    // function getExpiredTokens() public view returns (uint256[] memory) {
    //     uint256 amountOfTokens = _totalSupply.current();

    //     uint256[] memory tokensToUpdate = new uint256[](amountOfTokens);
    //     Target memory target;
    //     uint256 count = 0;

    //     for (uint256 i = 0; i < amountOfTokens; i++) {
    //         // target = s_targets[tokensToUpdate[i]];
    //         target = s_targets[i];

    //         console.log("target token id: ", target.tokenId);
    //         if ((target.timeStamp - lastTimeStamp) > interval) {
    //             tokensToUpdate[count] = i;

    //             count++;
    //         }
    //     }

    //     // console.log(tokensToUpdate.length);
    //     console.log("tokenToUpdate[0]: ", tokensToUpdate[0]);
    //     console.log("tokenToUpdate[1]: ", tokensToUpdate[1]);

    //     return tokensToUpdate;
    // }

    // for staging
    function STAGING_handleExpiredTokens() external {
        // currently x amount of nfts
        uint256 amountOfTokens = s_tokenCounter;

        Target memory target;

        if (amountOfTokens < 1) {
            return;
        }

        for (uint256 i = 0; i < amountOfTokens; i++) {
            // target = s_targets[tokensToUpdate[i]];
            target = s_targets[i];

            // if time has expired, should be for 1 month in seconds.
            // currently just getting creation of contract

            if ((target.expires - lastTimeStamp) > interval) {
                redeemNft(target.tokenId);
            }
        }
    }

    // for local
    function LOCAL_handleExpiredTokens() public {
        // currently x amount of nfts
        uint256 amountOfTokens = s_tokenCounter;

        Target memory target;

        if (amountOfTokens < 1) {
            return;
        }

        for (uint256 i = 0; i < amountOfTokens; i++) {
            // target = s_targets[tokensToUpdate[i]];
            target = s_targets[i];

            // if time has expired, should be for 1 month in seconds.
            // currently just getting creation of contract
            console.log("target.expires", target.expires);
            console.log("target.timeStamp", target.timeStamp);
            console.log("lastTimeStamp", lastTimeStamp);
            if ((target.expires - lastTimeStamp) > interval) {
                redeemNft(target.tokenId);
            }
        }
    }

    function checkUpkeep(
        bytes memory /* checkData */
    )
        public
        override
        returns (bool upkeepNeeded, bytes memory performData)
    // uint256 tokenId
    {
        // console.log("inside checkUpkeep!!!");
        // uint256[] memory tokensToUpdate = getExpiredTokens();
        // console.log(tokensToUpdate.length);
        // upkeepNeeded = tokensToUpdate.length > 0;
        // performData = abi.encode(tokensToUpdate);

        // return (upkeepNeeded, performData);

        upkeepNeeded = (block.timestamp - lastTimeStamp) > interval;
        // We don't use the checkData in this example. The checkData is defined when the Upkeep was registered.
    }

    function performUpkeep(bytes calldata performData) external override {
        //We highly recommend revalidating the upkeep in the performUpkeep function
        // (bool upKeepNeeded, , ) = checkUpkeep("");
        (bool upKeepNeeded, ) = checkUpkeep("");

        console.log("inside performUpkeep!!!");
        if (!upKeepNeeded) {
            revert UpKeepNotNeeded(block.timestamp);
        }

        console.log("upKeepNeeded: ", upKeepNeeded);
        // uint256[] memory tokensToUpdate = abi.decode(performData, (uint256[]));
        // console.log(tokensToUpdate[0]);

        LOCAL_handleExpiredTokens();

        emit PerformUpKeepCompleted();
    }

    // may need to update this to be 1 month from mint, block.timestamp +
    //    /**
    //    * @notice Mints a one time use nft that will burn after x amount of seconds or until redeemed
    //    * @emits NftMinted Event
    //    */

    function mintOneTimeUseNft() public payable returns (uint256) {
        uint256 mintFeeForOne = getMintFeeForOneTimeUse();
        if (msg.value != mintFeeForOne) {
            revert IncorrectAmountOfEthSent();
        }

        uint256 newTokenId = s_tokenCounter;

        _totalSupply.increment();
        s_tokenCounter = s_tokenCounter + 1;

        _safeMint(msg.sender, newTokenId);
        _setTokenURI(newTokenId, s_typeTokenUris[0]);

        s_targets[newTokenId] = Target({
            // expires: (block.timestamp + 2629746),
            expires: block.timestamp,
            timeStamp: block.timestamp,
            tokenId: newTokenId
        });

        emit NftMinted(msg.sender, address(this), newTokenId);

        // return msg.value;
    }

    //    /**
    //    * @notice Burns nft that has either been redeemed or expired
    //    * @emits NftBurned Event
    //    */
    function redeemNft(uint256 tokenId) public onlyOwner {
        _burn(tokenId);
        _totalSupply.decrement();
        delete (s_targets[tokenId]);

        emit NftBurned(msg.sender, tokenId);
    }

    function withdraw() public onlyOwner {
        if (msg.sender != i_owner) {
            revert NotOwner();
        }

        uint256 amount = address(this).balance;

        if (amount < 0) {
            revert NothingToWithdraw();
        }

        (bool success, ) = payable(msg.sender).call{value: amount}("");

        if (!success) {
            revert TransferFailed();
        }
    }

    // ///////////////////
    // Getter Functions //
    //////////////////////

    function getPriceFeed() public view returns (AggregatorV3Interface) {
        return s_priceFeed;
    }

    function getNftTokenUri(uint256 index) public view returns (string memory) {
        return s_typeTokenUris[index];
    }

    function getCurrentTotalSupply() public view returns (uint256) {
        return _totalSupply.current();
    }

    function getBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function getTokenData(uint256 tokenId) public view returns (Target memory) {
        return s_targets[tokenId];
    }

    function getMintFeeForOneTimeUse() public view returns (uint256) {
        uint256 price = PriceConverter.getConversionRate(
            MINIMUM_USD,
            s_priceFeed
        );

        return price;
    }

    function getTimestampOfWhenCreated(uint256 tokenId)
        public
        view
        returns (uint256)
    {
        return s_targets[tokenId].timeStamp;
    }
}

