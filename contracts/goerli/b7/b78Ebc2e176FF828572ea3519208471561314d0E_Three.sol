pragma solidity >0.8.0;

import {Ownable} from "openzeppelin-contracts/access/Ownable.sol";
import {ReentrancyGuard} from "openzeppelin-contracts/security/ReentrancyGuard.sol";

import {StarNFTV4} from "./contracts/StarNFT.sol";

contract Three is Ownable, ReentrancyGuard {
    event NewStart(
        address indexed caller,
        string indexed symbol,
        address indexed three
    );
    mapping(string => address) private _starts;

    function isOwner() public view returns (bool) {
        return owner() == msg.sender;
    }

    function newThree(
        string memory symbol
    ) public nonReentrant onlyOwner returns (address star) {
        bytes memory bytecode = type(StarNFTV4).creationCode;
        bytes32 salt = keccak256(abi.encodePacked(symbol));
        assembly {
            star := create2(0, add(bytecode, 32), mload(bytecode), salt)
        }
        emit NewStart(msg.sender, symbol, star);
        StarNFTV4(star).addMinter(msg.sender);
        StarNFTV4(star).setSymbol(symbol);

        StarNFTV4(star).transferOwnership(msg.sender);

        _starts[symbol] = star;

        return star;
    }
}

