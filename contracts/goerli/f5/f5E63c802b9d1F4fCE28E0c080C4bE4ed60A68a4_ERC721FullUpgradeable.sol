abstract contract PriceOracleUpgradeable is Initializable, IPriceOracleUpgradeable {
    using FixedPointMathLib for uint256;

    IAggregatorV3 internal _forexPriceFeed;
    mapping(address => IAggregatorV3) internal _priceFeeds;
    mapping(address => uint256) internal _tokenPrices;

    function getUsdPrice(address token) external view returns (uint256) {
        return _getUsdPrice(token);
    }

    function _getTokenAmountDown(address token, uint256 fiatAmount) internal view returns (uint256) {
        if (_forexPriceFeed == IAggregatorV3(address(0))) return fiatAmount.mulDivDown(1 ether, _getUsdPrice(token));
        return ((fiatAmount * 1 ether) / _getFiatPrice()).mulDivDown(1 ether, _getUsdPrice(token));
    }

    function _getTokenAmountUp(address token, uint256 fiatAmount) internal view returns (uint256) {
        if (_forexPriceFeed == IAggregatorV3(address(0))) return fiatAmount.mulDivUp(1 ether, _getUsdPrice(token));
        return ((fiatAmount * 1 ether) / _getFiatPrice()).mulDivUp(1 ether, _getUsdPrice(token));
    }

    function _getFiatPrice() private view returns (uint256) {
        (, int256 price, , , ) = _forexPriceFeed.latestRoundData();
        return uint256(price * 1e10);
    }

    function _getUsdPrice(address token) private view returns (uint256) {
        IAggregatorV3 priceFeed = _priceFeeds[token];
        if (priceFeed != IAggregatorV3(address(0))) {
            (, int256 price, , , ) = _priceFeeds[token].latestRoundData();
            return uint256(price * 1e10);
        }
        return _tokenPrices[token];
    }

    function _setPriceFeeds(address token_, address priceFeed_) internal {
        _priceFeeds[token_] = IAggregatorV3(priceFeed_);
        emit SetPriceFeeds(token_, priceFeed_);
    }

    function _setForexPriceFeed(address forexPriceFeed_) internal {
        _forexPriceFeed = IAggregatorV3(forexPriceFeed_);
        emit SetForexPriceFeed(address(forexPriceFeed_));
    }

    function _setTokenPrice(address token_, uint256 tokenPrice_) internal {
        _tokenPrices[token_] = tokenPrice_;
        emit SetTokenPrice(token_, tokenPrice_);
    }

    uint256[47] private __gap;
}

