pragma solidity ^0.8.17;

import "@chainlink/contracts/src/v0.8/AutomationCompatible.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol";
import "./interfaces/IWETH.sol";
import "hardhat/console.sol";

contract DcaBot is AutomationCompatibleInterface {
    /* fund bot with eth
    bot transacts with uniswap eth/usdc
    */
    event TradePairChanged(address indexed tokenB, address indexed aggregatorAddress);
    event TokensBought(uint256 indexed amount, address indexed token, address indexed forToken);
    event TokensSold(uint256 indexed amount, address indexed token, address indexed forToken);
    event StateChanged(states indexed newState);
    event DCAAmountChanged(uint256 indexed newAmount);

    error DcaBot__InsufficientBalance();
    error DcaBot__UpkeepNotNeeded();
    error DcaBot__Unauthorized();
    error DcaBot__TransactionFailed();

    enum states {
        Buy,
        Sell,
        Disabled
    }

    struct Pair {
        address weth;
        address tokenB;
    }

    Pair private s_pair;
    states private s_state;
    uint256 private s_lastTimeStamp;
    uint256 private s_dcaAmount;
    uint256 private s_interval;
    AggregatorV3Interface private s_aggregatorV3;

    address private immutable i_owner;
    ISwapRouter private immutable i_swapRouter;
    IWETH private immutable i_wethContract;

    uint24 public constant POOLFEE = 3000; // 0.3%
    uint256 private constant DECIMALS = 18;

    receive() external payable {}

    fallback() external payable {}

    constructor(uint256 _interval, ISwapRouter _swapRouter, address _wethAddress) {
        i_owner = msg.sender;
        i_swapRouter = _swapRouter;
        i_wethContract = IWETH(_wethAddress);

        s_interval = _interval;
        s_pair = Pair(_wethAddress, _wethAddress);
        s_state = states.Disabled;
        s_lastTimeStamp = block.timestamp;
    }

    modifier onlyOwner() {
        if (msg.sender != i_owner) {
            revert DcaBot__Unauthorized();
        }
        _;
    }

    function checkUpkeep(
        bytes memory /* checkData */
    ) public view override returns (bool upkeepNeeded, bytes memory) {
        // if time has passed, contract has weth balance, function is activated

        bool balance;
        bool time = (block.timestamp - s_lastTimeStamp) > s_interval;
        bool open = s_state != states.Disabled;

        if (s_state == states.Buy) {
            balance = i_wethContract.balanceOf(address(this)) >= s_dcaAmount;
        } else if (s_state == states.Sell) {
            balance = ERC20(s_pair.tokenB).balanceOf(address(this)) >= s_dcaAmount;
        }

        upkeepNeeded = balance && time && open;
    }

    function performUpkeep(bytes calldata) external override {
        (bool upkeepNeeded, ) = checkUpkeep("");
        if (!upkeepNeeded) {
            revert DcaBot__UpkeepNotNeeded();
        }

        // console.log("jeff0");
        (, int256 price, , , ) = s_aggregatorV3.latestRoundData(); // dai per eth
        uint256 minimum;
        // console.log("jeff1");

        if (s_state == states.Buy) {
            minimum = ((s_dcaAmount * 10 ** DECIMALS) / uint256(price));
            minimum -= (minimum * (POOLFEE + 10 ** 4)) / 10 ** 6;

            ISwapRouter.ExactInputSingleParams memory params = ISwapRouter.ExactInputSingleParams({
                tokenIn: s_pair.weth,
                tokenOut: s_pair.tokenB,
                fee: POOLFEE,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: s_dcaAmount,
                amountOutMinimum: minimum,
                sqrtPriceLimitX96: 0
            });

            TransferHelper.safeApprove(s_pair.weth, address(i_swapRouter), s_dcaAmount);
            uint256 outAmount = i_swapRouter.exactInputSingle(params);

            // console.log("jeff4");
            emit TokensBought(outAmount, s_pair.weth, s_pair.tokenB);
            // console.log("jeff3");
        } else if (s_state == states.Sell) {
            minimum = (s_dcaAmount * uint256(price)) / 10 ** DECIMALS;
            minimum -= (minimum * (POOLFEE + 10 ** 4)) / 10 ** 6;

            ISwapRouter.ExactInputSingleParams memory params = ISwapRouter.ExactInputSingleParams({
                tokenIn: s_pair.tokenB,
                tokenOut: s_pair.weth,
                fee: POOLFEE,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: s_dcaAmount,
                amountOutMinimum: minimum,
                sqrtPriceLimitX96: 0
            });

            TransferHelper.safeApprove(s_pair.tokenB, address(i_swapRouter), s_dcaAmount);
            uint256 outAmount = i_swapRouter.exactInputSingle(params);

            emit TokensSold(outAmount, s_pair.tokenB, s_pair.weth);
        }
    }

    function withdrawAll() external onlyOwner {
        (, uint256 tokenBalance) = getTokenBalances();
        if (tokenBalance > 0) {
            (, int256 price, , , ) = s_aggregatorV3.latestRoundData();
            uint256 minimum = (s_dcaAmount * uint256(price)) / 10 ** DECIMALS;
            minimum -= (minimum * (POOLFEE + 10 ** 4)) / 10 ** 6;

            ISwapRouter.ExactInputSingleParams memory params = ISwapRouter.ExactInputSingleParams({
                tokenIn: s_pair.tokenB,
                tokenOut: s_pair.weth,
                fee: POOLFEE,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: s_dcaAmount,
                amountOutMinimum: minimum,
                sqrtPriceLimitX96: 0
            });

            TransferHelper.safeApprove(s_pair.tokenB, address(i_swapRouter), s_dcaAmount);
            uint256 outAmount = i_swapRouter.exactInputSingle(params);

            emit TokensSold(outAmount, s_pair.tokenB, s_pair.weth);
        }

        (uint256 wethBalance, ) = getTokenBalances();
        // console.log(wethBalance);
        // console.log(i_wethContract.balanceOf(address(this)));

        if (wethBalance > 0) {
            i_wethContract.withdraw(wethBalance);
        } else {
            revert DcaBot__InsufficientBalance();
        }

        (bool success, ) = i_owner.call{value: address(this).balance}("");
        if (!success) {
            revert DcaBot__TransactionFailed();
        }
    }

    // starts bot with selected pair
    function enableStateAndPair(
        uint256 _state,
        address _tokenB,
        uint256 _dcaAmount,
        address _aggregatorV3Address
    ) external payable onlyOwner {
        s_state = states(_state);
        s_pair = Pair(s_pair.weth, _tokenB);
        s_dcaAmount = _dcaAmount;
        s_aggregatorV3 = AggregatorV3Interface(_aggregatorV3Address);

        if (msg.value > 0) {
            swapEthForWeth();
        }

        emit TradePairChanged(_tokenB, _aggregatorV3Address);
    }

    function swapEthForWeth() public {
        if (address(this).balance == 0) {
            revert DcaBot__InsufficientBalance();
        }

        i_wethContract.deposit{value: address(this).balance}();
    }

    function changeDcaAmount(uint256 _to) public onlyOwner {
        s_dcaAmount = _to;
        emit DCAAmountChanged(_to);
    }

    function changeState(uint256 _state, uint256 _dcaAmount) external onlyOwner {
        s_state = states(_state);
        if (_dcaAmount != 0) {
            s_dcaAmount = _dcaAmount;
        }

        emit StateChanged(s_state);
    }

    function changePair(
        address _token,
        uint256 _dcaAmount,
        address _aggregatorV3Address
    ) external onlyOwner {
        s_pair = Pair(s_pair.weth, _token);
        s_aggregatorV3 = AggregatorV3Interface(_aggregatorV3Address);
        changeDcaAmount(_dcaAmount);

        emit TradePairChanged(_token, _aggregatorV3Address);
    }

    function getTokenBalances() public view returns (uint256 balanceA, uint256 balanceB) {
        balanceA = i_wethContract.balanceOf(address(this));
        balanceB = ERC20(s_pair.tokenB).balanceOf(address(this));
    }

    function getPair() external view returns (address, address) {
        return (s_pair.weth, s_pair.tokenB);
    }

    function getState() external view returns (states) {
        return s_state;
    }

    function getInterval() external view returns (uint256) {
        return s_interval;
    }

    function getOwner() external view returns (address) {
        return i_owner;
    }

    function getDcaAmount() external view returns (uint256) {
        return s_dcaAmount;
    }

    function addEth() public payable onlyOwner {
        if (msg.sender != address(i_wethContract)) {
            swapEthForWeth();
        }
    }

    function getAggregatorAddress() external view returns (address) {
        return address(s_aggregatorV3);
    }
}

