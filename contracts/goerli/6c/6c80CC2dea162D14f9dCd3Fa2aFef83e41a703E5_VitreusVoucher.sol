// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "operator-filter-registry/src/upgradeable/DefaultOperatorFiltererUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/IERC721MetadataUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/IERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@uniswap/v2-core/contracts/interfaces/IERC20.sol";

// import "forge-std/console.sol";

contract VitreusVoucher is
  Initializable,
  ContextUpgradeable,
  ERC165Upgradeable,
  IERC721MetadataUpgradeable,
  DefaultOperatorFiltererUpgradeable,
  PausableUpgradeable,
  OwnableUpgradeable,
  ReentrancyGuardUpgradeable,
  UUPSUpgradeable
{
  using StringsUpgradeable for uint256;
  using StringsUpgradeable for uint48;
  using AddressUpgradeable for address;

  event Deposit(uint indexed id, uint round, uint amount);
  event PresaleRoundCreated(uint indexed round, PresaleRound presaleRound);
  event PresaleRoundChanged(uint indexed round, PresaleRound presaleRound);

  uint constant USDC_DECIMALS = 10 ** 6;
  // test USDC
  address constant USDC_ADDRESS = 0x07865c6E87B9F70255377e024ace6630C1Eaa37F;
  // test USDT
  // address constant USDC_ADDRESS = 0x5AB6F31B29Fc2021436B3Be57dE83Ead3286fdc7;
  // main USDC
  // address constant USDC_ADDRESS = 0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48;

  IUniswapV2Router02 _uniswapV2Router;
  address _usdcReceiver;
  IERC20 _usdc;
  address[] _path;

  struct PresaleRound {
    uint32 price;
    uint56 allocation; // max value $281,474,976 USDC
    uint48 minimumDeposit;
    uint48 maximumDeposit;
    uint48 totalDeposits;
    uint48 startingId;
    uint48 endingId;
    uint48 startTime;
    uint48 endTime;
  }

  PresaleRound[] _rounds;
  mapping(uint => mapping(uint => uint)) _deposits;
  mapping(uint256 => address) private _ownedTokens;
  mapping(address => uint256) private _tokenOwners;
  mapping(uint256 => address) private _tokenApprovals;
  mapping(address => mapping(address => bool)) private _operatorApprovals;
  uint256 _tokenIds;
  string _baseURI;

  function initialize(address uniswapV2RouterAddress_, address usdcReceiver_) public initializer {
    __Ownable_init();
    __Pausable_init();
    __UUPSUpgradeable_init();
    __Context_init();
    __ReentrancyGuard_init();
    __DefaultOperatorFilterer_init();
    _uniswapV2Router = IUniswapV2Router02(uniswapV2RouterAddress_);
    _usdcReceiver = usdcReceiver_;
    _path = new address[](2);
    _tokenIds = 1;
    (_path[0], _path[1]) = (_uniswapV2Router.WETH(), USDC_ADDRESS);
    _baseURI = "https://bafybeif33uwwlo6zdashrr4ljqxtvsd3b322jk2vu3tcni2bjbzlwliazm.ipfs.w3s.link/";
  }

  function name() external pure returns (string memory) {
    return "Vitreus Voucher";
  }

  function symbol() external pure returns (string memory) {
    return "ViVo";
  }

  function totalSupply() external view returns (uint256) {
    return _tokenIds - 1;
  }

  function createPresaleRound(
    uint32 price_,
    uint56 allocation_,
    uint48 depositMin_,
    uint48 depositMax_,
    uint48 startTime_,
    uint48 endTime_
  ) public onlyOwner {
    _rounds.push(
      PresaleRound(
        price_,
        allocation_,
        depositMin_,
        depositMax_,
        0,
        uint48(_tokenIds),
        0,
        startTime_,
        endTime_
      )
    );
    emit PresaleRoundCreated(_rounds.length - 1, _rounds[_rounds.length - 1]);
  }

  function getPresaleRound(uint roundId_) external view returns (PresaleRound memory) {
    return _rounds[roundId_];
  }

  function getPresaleRoundAmount() external view returns (uint) {
    return _rounds.length;
  }

  function getAllPresaleRounds() external view returns (PresaleRound[] memory) {
    return _rounds;
  }

  function getCurrentPresaleRound() external view returns (PresaleRound memory) {
    return _rounds[_currentPresaleRoundNumber()];
  }

  function changeUpcomingRound(uint roundId_, uint56 newAllocation_) external onlyOwner {
    require(_currentPresaleRoundNumber() < roundId_, "VitreusVoucher: changing not upcoming round");

    _rounds[roundId_].allocation = newAllocation_;

    emit PresaleRoundChanged(roundId_, _rounds[roundId_]);
  }

  receive() external payable whenNotPaused {
    if (balanceOf(msg.sender) > 0) {
      _deposit(_tokenOwners[msg.sender], _swapETHForUSDC(msg.value));
    } else {
      _mint(msg.sender, _nextTokenId(), msg.value);
    }
  }

  function mint() external payable whenNotPaused {
    _mint(msg.sender, _nextTokenId(), _swapETHForUSDC(msg.value));
  }

  function deposit() external payable whenNotPaused {
    _deposit(_tokenOwners[msg.sender], _swapETHForUSDC(msg.value));
  }

  function mintWithUSDC(address to_, uint96 value_) external whenNotPaused {
    require(
      _usdc.allowance(msg.sender, _usdcReceiver) >= value_,
      "VitreusVoucher: not enough USDC allowance"
    );

    _usdc.transferFrom(msg.sender, _usdcReceiver, value_);
    _mint(to_, _nextTokenId(), value_);
  }

  function depositUSDC(uint amount_) external whenNotPaused {
    require(
      _usdc.allowance(msg.sender, _usdcReceiver) >= amount_,
      "VitreusVoucher: not enough USDC allowance"
    );

    _usdc.transferFrom(msg.sender, _usdcReceiver, amount_);
    _deposit(_tokenOwners[msg.sender], amount_);
  }

  function forceMint(address to_, uint48 value_) external onlyOwner {
    uint nextToken = _nextTokenId();
    _deposit(nextToken, value_);
    _mint(to_, nextToken);
  }

  function tokenOf(address _owner) external view returns (uint256) {
    return _tokenOwners[_owner];
  }

  function statsOf(uint _tokenId) external view returns (uint[] memory) {
    uint[] memory roundStatistic = new uint[](_currentPresaleRoundNumber() + 1);

    for (uint i = 0; i <= _currentPresaleRoundNumber(); ) {
      roundStatistic[i] = _deposits[_tokenId][i];

      unchecked {
        ++i;
      }
    }

    return roundStatistic;
  }

  function setBaseURI(string calldata _newBaseUri) external onlyOwner {
    _baseURI = _newBaseUri;
  }

  function tokenURI(uint256 tokenId) public view override returns (string memory) {
    _requireMinted(tokenId);
    return string.concat(_baseURI, tokenId.toString());
  }

  function approve(
    address to,
    uint256 tokenId
  ) public virtual override onlyAllowedOperatorApproval(to) {
    address _owner = _ownerOf(tokenId);
    require(to != _owner, "VitreusVoucher: approval to current owner");

    require(
      _msgSender() == _owner || isApprovedForAll(_owner, _msgSender()),
      "VitreusVoucher: approve caller is not token owner or approved for all"
    );

    _approve(to, tokenId);
  }

  function getApproved(uint256 tokenId) public view virtual override returns (address) {
    _requireMinted(tokenId);
    return _tokenApprovals[tokenId];
  }

  function setApprovalForAll(
    address operator,
    bool approved
  ) public virtual override onlyAllowedOperatorApproval(operator) {
    _setApprovalForAll(_msgSender(), operator, approved);
  }

  function isApprovedForAll(
    address _owner,
    address operator
  ) public view virtual override returns (bool) {
    return _operatorApprovals[_owner][operator];
  }

  function transferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public virtual override onlyAllowedOperator(from) {
    _requireApprovedOrOwner(_msgSender(), tokenId);
    _transfer(from, to, tokenId);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public virtual override onlyAllowedOperator(from) {
    safeTransferFrom(from, to, tokenId, "");
  }

  function presaleRoundNumber() external view returns (uint) {
    return _currentPresaleRoundNumber();
  }

  // tokenId == 0 if check without NFT
  function availableDeposit(uint tokenId) external view returns (uint) {
    uint tokenDeposit = _currentPresaleRound().maximumDeposit -
      _deposits[tokenId][_currentPresaleRoundNumber()];
    uint roundDeposit = _currentPresaleRound().allocation *
      _currentPresaleRound().price -
      _currentPresaleRound().totalDeposits;

    return tokenDeposit > roundDeposit ? roundDeposit : tokenDeposit;
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId,
    bytes memory data
  ) public virtual override onlyAllowedOperator(from) {
    _requireApprovedOrOwner(_msgSender(), tokenId);
    _safeTransfer(from, to, tokenId, data);
  }

  function supportsInterface(
    bytes4 interfaceId
  ) public view virtual override(ERC165Upgradeable, IERC165Upgradeable) returns (bool) {
    return
      interfaceId == type(IERC721Upgradeable).interfaceId ||
      interfaceId == type(IERC721MetadataUpgradeable).interfaceId ||
      super.supportsInterface(interfaceId);
  }

  function balanceOf(address _owner) public view virtual override returns (uint256) {
    return _balanceOf(_owner);
  }

  function ownerOf(uint256 tokenId) public view virtual override returns (address) {
    _requireMinted(tokenId);
    return _ownerOf(tokenId);
  }

  function _swapETHForUSDC(uint256 amountIn) internal returns (uint256) {
    uint256[] memory amounts = _uniswapV2Router.swapExactETHForTokens{value: amountIn}(
      0, // accept any amount of USDC
      _path,
      _usdcReceiver,
      block.timestamp
    );
    return amounts[1];
  }

  function _requireZeroBalance(address to_) internal view {
    require(_balanceOf(to_) == 0, "VitreusVoucher: only one voucher per address");
  }

  function _requirePresaleRoundOpen(uint _depositValue) internal view {
    require(
      _currentPresaleRound().startTime <= block.timestamp,
      "VitreusVoucher: presale round not started"
    );
    require(
      _currentPresaleRound().endTime >= block.timestamp,
      "VitreusVoucher: presale round ended"
    );
    require(
      _currentPresaleRound().allocation * _currentPresaleRound().price >
        _currentPresaleRound().totalDeposits + _depositValue,
      "VitreusVoucher: presale round allocation reached"
    );
  }

  function _requireDepositAboveMin(uint tokenId) internal view {
    require(
      _deposits[tokenId][_currentPresaleRoundNumber()] >= _currentPresaleRound().minimumDeposit,
      "VitreusVoucher: deposit below minimum"
    );
  }

  function _requireDepositsBelowMax(uint tokenId) internal view {
    require(
      _deposits[tokenId][_currentPresaleRoundNumber()] <= _currentPresaleRound().maximumDeposit,
      "VitreusVoucher: deposits above maximum"
    );
  }

  function _requireApprovedOrOwner(address account_, uint tokenId_) internal view {
    require(
      _isApprovedOrOwner(account_, tokenId_),
      "VitreusVoucher: caller is not token owner or approved"
    );
  }

  function _requireMinted(uint256 tokenId) internal view virtual {
    require(_exists(tokenId), "VitreusVoucher: invalid token ID");
  }

  function _requireNotMinted(uint256 tokenId) internal view virtual {
    require(!_exists(tokenId), "VitreusVoucher: token already minted");
  }

  function _requireOwned(uint256 tokenId) internal view {
    require(_ownerOf(tokenId) != address(0), "VitreusVoucher: caller is not token owner");
  }

  function _mint(address to_, uint tokenId_, uint value_) internal nonReentrant {
    _requirePresaleRoundOpen(value_);
    _deposit(tokenId_, value_);
    _currentPresaleRound().endingId = uint48(tokenId_);
    _mint(to_, tokenId_);
  }

  function _currentPresaleRound() internal view returns (PresaleRound storage) {
    return _rounds[_currentPresaleRoundNumber()];
  }

  function _currentPresaleRoundNumber() internal view returns (uint) {
    uint roundsAmount = _rounds.length;

    for (uint i = 0; i < roundsAmount; ) {
      if (_rounds[i].endTime >= block.timestamp && _rounds[i].startTime <= block.timestamp) {
        return i;
      }

      unchecked {
        ++i;
      }
    }

    return roundsAmount > 0 ? roundsAmount - 1 : 0;
  }

  function _deposit(uint tokenId_, uint value_) internal {
    _requirePresaleRoundOpen(value_);
    _requireDepositsBelowMax(tokenId_);
    _deposits[tokenId_][_currentPresaleRoundNumber()] += value_;
    _requireDepositAboveMin(tokenId_);
    _currentPresaleRound().totalDeposits += uint48(value_);
    emit Deposit(tokenId_, _currentPresaleRoundNumber(), value_);
  }

  function _safeTransfer(
    address from,
    address to,
    uint256 tokenId,
    bytes memory data
  ) internal virtual {
    _transfer(from, to, tokenId);
    require(
      _checkOnVitreusVoucherReceived(from, to, tokenId, data),
      "VitreusVoucher: transfer to non ERC721Receiver implementer"
    );
  }

  function _ownerOf(uint256 tokenId) internal view virtual returns (address) {
    return _ownedTokens[tokenId];
  }

  function _exists(uint256 tokenId) internal view virtual returns (bool) {
    return _ownerOf(tokenId) != address(0);
  }

  function _isApprovedOrOwner(
    address spender,
    uint256 tokenId
  ) internal view virtual returns (bool) {
    address _owner = VitreusVoucher.ownerOf(tokenId);
    return (spender == _owner ||
      isApprovedForAll(_owner, spender) ||
      getApproved(tokenId) == spender);
  }

  function _safeMint(address to, uint256 tokenId) internal virtual {
    _safeMint(to, tokenId, "");
  }

  function _safeMint(address to, uint256 tokenId, bytes memory data) internal virtual {
    _mint(to, tokenId);
    require(
      _checkOnVitreusVoucherReceived(address(0), to, tokenId, data),
      "VitreusVoucher: transfer to non VitreusVoucherReceiver implementer"
    );
  }

  function _nextTokenId() internal returns (uint256) {
    return _tokenIds++;
  }

  function _mint(address to, uint256 tokenId) internal virtual {
    _requireZeroBalance(to);
    require(to != address(0), "VitreusVoucher: mint to the zero address");
    _requireNotMinted(tokenId);
    _ownedTokens[tokenId] = to;
    _tokenOwners[to] = tokenId;
    emit Transfer(address(0), to, tokenId);
  }

  function _transfer(address from, address to, uint256 tokenId) internal virtual {
    require(
      VitreusVoucher.ownerOf(tokenId) == from,
      "VitreusVoucher: transfer from incorrect owner"
    );
    require(to != address(0), "VitreusVoucher: transfer to the zero address");
    require(_tokenOwners[to] == 0, "VitreusVoucher: transfer to Voucher holder");

    delete _tokenApprovals[tokenId];
    _ownedTokens[tokenId] = to;
    delete _tokenOwners[from];
    _tokenOwners[to] = tokenId;

    emit Transfer(from, to, tokenId);
  }

  function _approve(address to, uint256 tokenId) internal virtual {
    _tokenApprovals[tokenId] = to;
    emit Approval(VitreusVoucher.ownerOf(tokenId), to, tokenId);
  }

  function _balanceOf(address _owner) internal view virtual returns (uint256) {
    return _tokenOwners[_owner] == 0 ? 0 : 1;
  }

  function _setApprovalForAll(address _owner, address operator, bool approved) internal virtual {
    require(_owner != operator, "VitreusVoucher: approve to caller");
    _operatorApprovals[_owner][operator] = approved;
    emit ApprovalForAll(_owner, operator, approved);
  }

  function _checkOnVitreusVoucherReceived(
    address from,
    address to,
    uint256 tokenId,
    bytes memory data
  ) private returns (bool) {
    if (to.isContract()) {
      try IERC721Receiver(to).onERC721Received(_msgSender(), from, tokenId, data) returns (
        bytes4 retval
      ) {
        return retval == IERC721Receiver.onERC721Received.selector;
      } catch (bytes memory reason) {
        if (reason.length == 0) {
          revert("VitreusVoucher: transfer to non IERC721 implementer");
        } else {
          /// @solidity memory-safe-assembly
          assembly {
            revert(add(32, reason), mload(reason))
          }
        }
      }
    } else {
      return true;
    }
  }

  function _authorizeUpgrade(address newImplementation) internal virtual override onlyOwner {}
}

