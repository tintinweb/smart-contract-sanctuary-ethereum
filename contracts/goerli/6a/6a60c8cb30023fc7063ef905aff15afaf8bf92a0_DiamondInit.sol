import {ReentrancyGuardUpgradeable} from "openzeppelin-contracts-diamond/contracts/security/ReentrancyGuardUpgradeable.sol";
import {IERC20PermitUpgradeable} from "openzeppelin-contracts-diamond/contracts/token/ERC20/extensions/draft-IERC20PermitUpgradeable.sol";  
import {IERC20Upgradeable} from "openzeppelin-contracts-diamond/contracts/token/ERC20/IERC20Upgradeable.sol";
import {ERC721AUpgradeable} from "ERC721A-Upgradeable/contracts/ERC721AUpgradeable.sol";
import {RequestGuildRole} from "./RequestGuildRole.sol";


// Example library to show a simple example of diamond storage
library MembershipsFacetStorage {

  bytes32 constant DIAMOND_STORAGE_POSITION = keccak256("decentragora.diamond.dAgora.memberships");

  struct TestState {
      address myAddress;
      uint256 myNum;
  }

  function diamondStorage() internal pure returns (TestState storage ds) {
      bytes32 position = DIAMOND_STORAGE_POSITION;
      assembly {
          ds.slot := position
      }
  }

}

contract MembershipsFacet is 
  // RequestGuildRole,
  OwnableUpgradeable,
  ERC721AUpgradeable,
  ReentrancyGuardUpgradeable
{
  enum Tier {
    ECCLESIA,
    DAGORAIAN,
    HOPLITE,
    PERCLESIA
  }

  struct Membership {
    uint8 tier;
    address member;
    uint256 gracePeriod;
    uint256 tokenId;
  }

  string public cid;
  bool public paused;
  address public immutable DAI = 0x6B175474E89094C44Da98b954EedeAC495271d0F;
  uint256 public constant GRACE_PERIOD = 1 days;

  address public dAgoraTreasury;
  uint96 public rewardedRole;
  uint256 public ecclesiaPrice;
  uint256 public dagorianPrice;
  uint256 public hoplitePrice;
  uint256 public periclesiaPrice;
  
  uint256 public baseMonthlyCost;
  uint256 public discountRate;

  mapping(uint256 => Membership) private tokenTier;
  mapping(uint256 => uint256) public expires;
  mapping(uint256 => address) public tokenIndexedToOwner;
  mapping(address => bool) public claimed;
  mapping(uint256 => address[]) internal _tokenDelegates;

  function initialize(
    string memory _name,
    string memory _symbol,
    string memory _cid,
    address _dAgoraTreasury,
    uint96 _rewardedRole,
    uint256 _ecclesiaPrice,
    uint256 _dagorianPrice,
    uint256 _hoplitePrice,
    uint256 _periclesiaPrice,
    uint256 _baseMonthlyCost,
    uint256 _discountRate
  ) public initializer {
    __Ownable_init();
    __ERC721A_init(_name, _symbol);
    __ReentrancyGuard_init();
    cid = _cid;
    dAgoraTreasury = _dAgoraTreasury;
    rewardedRole = _rewardedRole;
    ecclesiaPrice = _ecclesiaPrice;
    dagorianPrice = _dagorianPrice;
    hoplitePrice = _hoplitePrice;
    periclesiaPrice = _periclesiaPrice;
    baseMonthlyCost = _baseMonthlyCost;
    discountRate = _discountRate;
  }

// =================================================================================================================================
//                                                       MODIFIERS & GUARDS
// =================================================================================================================================
  modifier isNotPaused() {
    require(!paused, "dAgoraMemberships: Contract is paused");
    _;
  }

  modifier durationCheck(uint8 _duration) {
    _durationCheck(_duration);
    _;
  }

  /// @notice Checks if users address has already minted or not.
  modifier isNotMember() {
      _isNotMember();
      _;
  }

  /// @notice Checks that the msg.sender is either the tokenId owner or a delegate.
  /// @dev Modifier guard for delegate & owner functions
  /// @param _tokenId The id of the membership.
  modifier isDelegateOrOwner(uint256 _tokenId) {
      _isDelegateOrOwner(_tokenId);
      _;
  }

  modifier isExpiredSoon(uint256 tokenId) {
    _isExpiredSoon(tokenId);
    _;
  }

  modifier onlyController(uint256 _tokenId) { 
    _onlyController(_tokenId);
    _;
  }

// =================================================================================================================================
//                                                      MEMBERSHIP FUNCNTIONS
// =================================================================================================================================

  function mintMembersip(
    uint8 _durationInMonths,
    Membership calldata tier,
    uint256 _deadline,
    uint8 _v,
    bytes32 _r,
    bytes32 _s
  ) public 
    isNotPaused
    isNotMember()
    durationCheck(_durationInMonths) 
    nonReentrant
  {
    require(tier.tier != 0, "dAgoraMemberships: Tier must be greater than 0");
    require(tier.tier <= 4, "dAgoraMemberships: Tier must be less than 4");

    uint256 _tokenId = totalSupply() + 1;
    uint256 _duration = block.timestamp + _durationInMonths * 30 days;
    uint256 _price = _getPrice(_durationInMonths, tier.tier);

    IERC20PermitUpgradeable(DAI).permit(
      msg.sender,
      address(this),
      _price,
      _deadline,
      _v,
      _r,
      _s
    );

    IERC20Upgradeable(DAI).transferFrom(msg.sender, dAgoraTreasury, _price);
    tokenIndexedToOwner[_tokenId] = msg.sender;
    tokenTier[_tokenId] = tier;
    expires[_tokenId] = _duration + GRACE_PERIOD;
    claimed[msg.sender] = true;
    _safeMint(msg.sender, 1);

  }

  function renewMembership(
    uint256 _tokenId,
    uint8 _newDuration,
    uint256 _deadline,
    uint8 v,
    bytes32 r,
    bytes32 s
  ) public 
    isNotPaused
    isDelegateOrOwner(_tokenId)
    durationCheck(_newDuration)
    isExpiredSoon(_tokenId)
    nonReentrant
  {
    uint256 _duration = _newDuration * 30 days;
    uint256 _price = _getMonthlyCost(tokenTier[_tokenId].tier) * _newDuration;

    IERC20PermitUpgradeable(DAI).permit(
      msg.sender,
      address(this),
      _price,
      _deadline,
      v,
      r,
      s
    );

    IERC20Upgradeable(DAI).transferFrom(msg.sender, dAgoraTreasury, _price);

    expires[_tokenId] = expires[_tokenId] + (_duration + GRACE_PERIOD);

  }

  function upgradeMembership(
    uint256 _tokenId,
    Membership calldata newTier,
    uint256 _deadline,
    uint8 v,
    bytes32 r,
    bytes32 s
  ) public 
    isNotPaused
    onlyController(_tokenId)
    nonReentrant
  {
    uint8 oldTier = tokenTier[_tokenId].tier;
    require(newTier.tier > oldTier, "New tier is the same as current tier");
    require(newTier.tier < 4, "Token is already a precisian member");
    uint256 price;
    if (newTier.tier == 1 && oldTier == 0) {
      price = dagorianPrice;
    } else if (newTier.tier == 2 && oldTier == 0) {
      price = dagorianPrice + hoplitePrice;
    } else if (newTier.tier == 3 && oldTier == 0) {
      price = dagorianPrice + hoplitePrice + periclesiaPrice;
    } else if (newTier.tier == 2 && oldTier == 1) {
      price = hoplitePrice - dagorianPrice;
    } else if (newTier.tier == 3 && oldTier == 1) {
      price = (hoplitePrice - dagorianPrice) + periclesiaPrice;
    } else if (newTier.tier == 3 && oldTier == 2) {
      price = periclesiaPrice - (hoplitePrice + dagorianPrice);
    } else {
      require(false, "Invalid tier");
    }

    IERC20PermitUpgradeable(DAI).permit(
      msg.sender,
      address(this),
      price,
      _deadline,
      v,
      r,
      s
    );

    IERC20Upgradeable(DAI).transferFrom(msg.sender, dAgoraTreasury, price);

    tokenTier[_tokenId] = newTier;
    //emit MembershipUpgraded(_tokenId, oldTier, newTier);
  }

  function cancelMembership(
    uint256 _tokenId
  ) public
    isNotPaused
    onlyController(_tokenId)
    nonReentrant 
  {
    expires[_tokenId] = block.timestamp + GRACE_PERIOD;
