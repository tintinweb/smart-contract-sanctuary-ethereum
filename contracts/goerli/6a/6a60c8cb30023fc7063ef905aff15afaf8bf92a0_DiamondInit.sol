  function _getPrice(uint256 _durationInMonths, uint8 _tier) private view returns (uint256 _price) {
    uint256 price;
    uint256 _monthlyCost = _getMonthlyCost(_tier);
    if(_durationInMonths == 12) {
      if(_tier == 1) {
        price = (_monthlyCost + dagorianPrice) - discountRate;
      } else if(_tier == 2) {
        price = (_monthlyCost + hoplitePrice) - discountRate;
      } else if(_tier == 3) {
        price = (_monthlyCost + periclesiaPrice) - discountRate;
      }
    } else {
      if(_tier == 1) {
        price = (_monthlyCost + dagorianPrice);
      } else if(_tier == 2) {
        price = (_monthlyCost + hoplitePrice);
      } else if(_tier == 3) {
        price = (_monthlyCost + periclesiaPrice);
      }
    }
    return price;
  }

  function _getMonthlyCost(uint8 _tier) private view returns (uint256 _monthlyCost) {
    if (_tier == 1) {
      return baseMonthlyCost;
    } else if (_tier == 2) {
      return baseMonthlyCost;
