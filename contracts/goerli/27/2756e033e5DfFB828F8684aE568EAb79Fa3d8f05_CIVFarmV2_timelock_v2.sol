// File: contracts/CIVFarmV2_timelock_v2.sol


pragma solidity ^0.8.0;










/// @title  Civ FT Farm
/// @author Lorenz-Ren
/// @notice Yield farming dApp to reward locking LP and tokens, exchanged with ERC20 represent xTokens
/// @dev calculateYieldTotal is for the frontend data reporting. User can withdraw at any time.
///		 With new staking positions and withdraw the rewards are computed.
///      This contract must have minting rights to the represent tokens and have reward token deposited.

/// At any point in time, the amount of reward tokens (0NE)
/// entitled to a user but pending to be distributed is:
///
/// pending reward = (user.amount  * accTokenPerShare) - user.rewardDebt  <<<< Wallet Earn (WE)
///
/// Whenever an user deposits or withdraws LP tokens to the pool:
///   1. The pool's `accTokenPerShare` (and `lastRewardTime`) gets updated
///   2. User receives the pending reward sent to their address
///   3. User's `amount` gets updated
///   4. User's `rewardDebt` gets updated

contract CIVFarmV2_timelock_v2 is ICivFarmV2ld, Ownable, ReentrancyGuard, Pausable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    struct PoolInfo {
        // Info on each pool
        IERC20 lpToken; // Address of the rewardable contract: token for staking, LP for farming, or NFT for NFT staking
        uint256 allocPoint; // TP logic (pool weight): how many allocated Token Points (TP) are assigned to this pool
        uint256 lastRewardBlock; // Last block when reward distribution has been completed for this pool
        uint256 accTokensPerShare; // Reward tokens per share including decimals (times 1e18), starts at 0
        ICivRT representToken; // Represent tokens exchanged for the user's share in this pool, e.g. xSomething
        uint256 totalDepositAmount; // Tokens deposited in the pool, starts at 0
        uint256 timelock; // Timelock for this pool
    }

    ICivRT public representToken; // Represent token exchanged against the user's deposit, e.g. CIV deposited -> xCIV represent token
    IERC20 public rewardToken; // Reward tokens distributed by this contract
    uint256 public tokensPerBlock; // Reward tokens per block: Farming Rate (FR) logic
    uint256 public BONUS_MULTIPLIER = 1; // Bonus booster multiplier, could for example be modified by a DAO vote

    PoolInfo[] public poolInfo; // structure with info on each pool
    mapping(uint256 => mapping(address => UserInfo)) public userInfo; // Info of each user that enters the farm
    uint256 public totalAllocPoint; // Total TP allocation points summing across all pools, initially 0
    uint256 public startBlock; // Block number when farming starts
    uint256 public immutable MAX_UINT = 2**256 - 1; // Prevents overflows

    //events triggered are mostly in the interface, just AddPool is still different
    event AddPool(
        uint256 indexed pid,
        uint256 indexed allocPoint,
        IERC20 lpToken,
        IERC20 representToken
    );

    //pool 0 must be "staking the reward token" due to deposit function requirements
    constructor(
        IERC20 _rewardToken, //this token must be the reward token itself
        uint256 _allocPoint, //number of points assigned to staking the rewards token
        ICivRT _representToken,
        uint256 _timeLock
    ) {
        representToken = _representToken;
        rewardToken = _rewardToken;
        tokensPerBlock = 156963470; //Farming Rate (FR); 164890918 last for 2 years, minus the portion of the other contract
        startBlock = block.number; // Farming starts when this contract is deployed

        addPool(_allocPoint, _rewardToken, _representToken,_timeLock); //beware the potential confusion here
        // lptoken = reward token by design ONLY for pool 0
    }

    //update bonus multiplier for early farmers. Can only be called by the owner.
    function updateMultiplier(uint256 multiplierNumber) external onlyOwner {
        BONUS_MULTIPLIER = multiplierNumber;
    }

    //update Farming Rate, reward tokensdistributed per block. Can only be called by the owner.
    function updateRate(uint256 rateNumber) external onlyOwner {
        emit SetTokensPerBlock(tokensPerBlock, rateNumber);
        tokensPerBlock = rateNumber;
    }

    //should not be called except for bugs, as the total is calculated from time to time when pools are updated
    function setTotalAllocPoint(uint256 _totalAllocPoint) external onlyOwner {
        emit SetTotalAllocPoint(totalAllocPoint, _totalAllocPoint);
        totalAllocPoint = _totalAllocPoint;
    }

    //number of pools
    function poolLength() public view returns (uint256) {
        return poolInfo.length;
    }

    // Add a new lp to the pool. Can only be called by the owner.
    function addPool(
        uint256 _allocPoint,
        IERC20 _lpToken,
        ICivRT _representToken,
        uint256 _timeLock
    ) public onlyOwner {
        uint256 lastRewardBlock = block.number > startBlock
            ? block.number
            : startBlock;

        poolInfo.push(
            PoolInfo({
                lpToken: _lpToken, // Rewardable contract: token for staking, LP for farming, or NFT for NFT staking
                allocPoint: _allocPoint, // Allocated Token Points (TP) assigned to this pool
                lastRewardBlock: lastRewardBlock,
                accTokensPerShare: 0, //init = always 0
                representToken: _representToken,
                totalDepositAmount: 0,
                timelock: _timeLock
            })
        );

        totalAllocPoint += _allocPoint;

        emit AddPool(
            poolInfo.length - 1,
            _allocPoint,
            _lpToken,
            _representToken
        );
    }

    // Update the token allocation points for a pool, only owner
    function setPool(uint256 _pid, uint256 _allocPoint) external onlyOwner {
        require(poolInfo.length > _pid, "Pool does not exist");
        uint256 previousAllocPoint = poolInfo[_pid].allocPoint;
        emit SetPool(_pid, previousAllocPoint, _allocPoint);

        poolInfo[_pid].allocPoint = _allocPoint;
        if (_allocPoint != previousAllocPoint) {
            totalAllocPoint += _allocPoint - previousAllocPoint;
            updatePool(_pid);
        }
    }

    // Reward multiplier over the given _from to _to block considering possible MULTIPLIER for EARLY FARMERS/STAKERS
    function getMultiplier(uint256 _from, uint256 _to)
        public
        view
        returns (uint256)
    {
        return (_to - _from) * (BONUS_MULTIPLIER);
    }

    // Number of rewardable tokens provided by user on a given pool
    function getUserLP(uint256 _pid, address _user)
        public
        view
        returns (uint256)
    {
        UserInfo memory user = userInfo[_pid][_user];
        return user.amount;
    }

    // View function to see pending Tokens on frontend.
   function pendingTokens(uint256 _pid, address _user)
        public
        view
        returns (uint256)
    {
        PoolInfo memory pool = poolInfo[_pid];
        UserInfo memory user = userInfo[_pid][_user];
        uint256 accTokensPerShare = pool.accTokensPerShare;
        uint256 lpSupply = pool.totalDepositAmount;
        if (block.number > pool.lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(
                pool.lastRewardBlock,
                block.number
            );
            uint256 tokensReward = (multiplier *
                tokensPerBlock *
                pool.allocPoint *
                (10**18)) / totalAllocPoint;
            accTokensPerShare += (tokensReward * (10**18)) / lpSupply;
        }
        uint256 totalPending = ((user.amount * accTokensPerShare) / (10**18)) - user.rewardDebt;

        if( block.timestamp >= user.OGdeposit + pool.timelock && user.OGdeposit > 0) {
            if (user.claimedReward > 0)
                return totalPending-user.claimedReward;
            else
                return totalPending;
        }
        else if( user.claimedReward == 0) {
            uint256 actualReward = totalPending / 10;
            return actualReward;
        }  
        return 0;
    }

    // View function to see locked Tokens on frontend.
    function lockedTokens(uint256 _pid, address _user)
        external
        view
        returns (uint256)
    {
        PoolInfo memory pool = poolInfo[_pid];
        UserInfo memory user = userInfo[_pid][_user];
        uint256 accTokensPerShare = pool.accTokensPerShare;
        uint256 lpSupply = pool.totalDepositAmount;
        if (block.number > pool.lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(
                pool.lastRewardBlock,
                block.number
            );
            uint256 tokensReward = (multiplier *
                tokensPerBlock *
                pool.allocPoint *
                (10**18)) / totalAllocPoint;
            accTokensPerShare += (tokensReward * (10**18)) / lpSupply;
        }
        uint256 totalPending = ((user.amount * accTokensPerShare) / (10**18)) - user.rewardDebt;
        if( block.timestamp >= user.OGdeposit + pool.timelock && user.OGdeposit > 0) {
            return 0;
        }
        else if( user.claimedReward == 0 ) {
            
            return totalPending;
        }  
        return totalPending - user.claimedReward;
    }
    
    //substituted by pendingtokens
    /*
    function getClaimableAmount( uint256 _pid, address _user) internal returns(uint256) {
        PoolInfo memory pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];

        if( block.timestamp >= user.OGdeposit + pool.timelock && user.OGdeposit > 0) {
            user.OGdeposit = block.timestamp;
            uint256 lockedAmount = user.lockedReward;
            user.lockedReward = 0;
            return _pending + lockedAmount;
        }
        else if( user.claimedReward == false) {
            uint256 actualReward = _pending / 10;
            user.lockedReward += (_pending - actualReward);
            return actualReward;
        }    
        user.lockedReward += _pending;
        user.claimedReward = false;    
        return 0;
    }
    */

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) internal {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.totalDepositAmount;
        if (lpSupply == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        uint256 tokensReward = (multiplier *
            tokensPerBlock *
            pool.allocPoint *
            (10**18)) / totalAllocPoint;
        pool.accTokensPerShare += (tokensReward * (10**18)) / lpSupply;
        pool.lastRewardBlock = block.number;
    }

    // Deposit rewardable tokens, get represent xTokens and reward token allocation
    function deposit(uint256 _pid, uint256 _amount)
        public
        nonReentrant
        whenNotPaused
    {
        require(poolLength() > _pid, "Can't find pool");
        require(_amount <= uint256(MAX_UINT), "Wrong amount");

        UserInfo storage user = userInfo[_pid][_msgSender()];
        updatePool(_pid);
        PoolInfo storage pool = poolInfo[_pid]; //pool.totalDepositAmount is changed so it needs storage

        uint256 pending = ((user.amount * pool.accTokensPerShare) / (10**18)) -
            user.rewardDebt;
        user.rewardDebt =
            ((user.amount + _amount) * pool.accTokensPerShare) /
            (10**18);

        if (pending > 0) {
            require(
                rewardToken.balanceOf(address(this)) -
                    poolInfo[0].totalDepositAmount >=
                    pending,
                "No Tokens left for reward" //pool 0 has reward tokens staked, which we don't want to mix
            );
            uint256 availablePending = pendingTokens( _pid, _msgSender());
            if( availablePending > 0 ) {
                rewardToken.safeTransfer(_msgSender(), availablePending);
                emit YieldWithdraw(_msgSender(), _pid, availablePending);
            }
        }

        if (_amount > 0) {
            pool.totalDepositAmount += _amount;
            pool.lpToken.safeTransferFrom(_msgSender(), address(this), _amount);
            user.amount += _amount;
            if( user.OGdeposit == 0 ) {
                user.OGdeposit = block.timestamp;
            }
            pool.representToken.mint(_msgSender(), _amount); // decimals of rewardable and represent tokens should be be the same
        }

        emit Deposit(_msgSender(), _pid, _amount);
    }

    // Withdraw rewardable tokens, burn represent xTokens and claim reward tokens
    function withdraw(uint256 _pid, uint256 _amount)
        public
        nonReentrant
        whenNotPaused
    {
        require(poolLength() > _pid, "Can't find pool");
        require(_amount <= uint256(MAX_UINT), "Wrong amount");

        UserInfo storage user = userInfo[_pid][_msgSender()];
        updatePool(_pid);
        PoolInfo storage pool = poolInfo[_pid]; //pool.totalDepositAmount is changed so it needs storage

        require(user.amount >= _amount, "amount exceeds user's balance");
        uint256 pending = ((user.amount * pool.accTokensPerShare) / (10**18)) -
            user.rewardDebt;
        user.rewardDebt =
            ((user.amount - _amount) * pool.accTokensPerShare) /
            (10**18);

        if (pending > 0) {
            require(
                rewardToken.balanceOf(address(this)) -
                    poolInfo[0].totalDepositAmount >=
                    pending,
                "No Tokens left for reward" //pool 0 has reward tokens staked, which we don't want to mix
            );
            uint256 availablePending = pendingTokens( _pid, _msgSender());
            if( availablePending > 0 ) {
                rewardToken.safeTransfer(_msgSender(), availablePending);
                emit YieldWithdraw(_msgSender(), _pid, availablePending);
            }
        }
        if (_amount > 0) {
            pool.representToken.burnFrom(_msgSender(), _amount);
            user.amount -= _amount;
			if (user.amount == 0) {		
				//free up storage space and get gas refund
				delete userInfo[_pid][_msgSender()];
			}
            pool.totalDepositAmount -= _amount;
            pool.lpToken.safeTransfer(address(_msgSender()), _amount);
        }
        emit Withdraw(_msgSender(), _pid, _amount);
    }

    // Withdraw rewards only, leaving rewardable tokens in the Farm
    function withdrawYield(uint256 _pid) public whenNotPaused {
        require(poolLength() > _pid, "Can't find pool");
        UserInfo storage user = userInfo[_pid][_msgSender()];
        updatePool(_pid);
        PoolInfo memory pool = poolInfo[_pid];
        require(
            block.timestamp >= user.OGdeposit + pool.timelock || user.claimedReward == 0,
            "Timelock not expired and already claimed 10% reward"
        );
        uint256 toTransfer = ((user.amount * pool.accTokensPerShare) /
                (10**18)) - user.rewardDebt;
        user.rewardDebt = (user.amount * pool.accTokensPerShare) / (10**18);

        if( block.timestamp >= user.OGdeposit + pool.timelock && user.OGdeposit > 0) {
            if (user.claimedReward > 0)
                toTransfer = toTransfer - user.claimedReward;
            
        }
        else if( user.claimedReward == 0) {
            uint256 actualReward = toTransfer / 10;
            user.claimedReward = actualReward;
            toTransfer = actualReward;
        }
        else  
            toTransfer = 0;

        require(
            rewardToken.balanceOf(address(this)) -
                poolInfo[0].totalDepositAmount >=
                toTransfer,
            "No Tokens left for reward" //pool 0 has reward tokens staked, which we don't want to mix
        );
        require(toTransfer > 0, "Nothing to withdraw");
       
        rewardToken.safeTransfer(_msgSender(), toTransfer);
        emit YieldWithdraw(_msgSender(), _pid, toTransfer);
        
    }

    // Withdraw and waive any accrued rewards. EMERGENCY ONLY, as users lose their rewards by calling this
    function emergencyWithdraw(uint256 _pid) public whenNotPaused nonReentrant {
        require(poolLength() > _pid, "Can't find pool");

        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_msgSender()];

        uint256 claimAmount = user.amount;
        require(claimAmount > 0, "Not enough tokens in this pool to withdraw");

        pool.representToken.burnFrom(_msgSender(), claimAmount);
        pool.totalDepositAmount -= claimAmount;
        user.amount = 0;
        user.rewardDebt = 0;
        user.lockedReward = 0;
        user.OGdeposit = 0;
        pool.lpToken.safeTransfer(address(_msgSender()), claimAmount);

        emit EmergencyWithdraw(_msgSender(), _pid, claimAmount);
		
		//free up storage space and get gas refund
        delete userInfo[_pid][_msgSender()];
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    /* Just in case anyone sends tokens by accident to this contract */

    /// @notice Transfers ETH to the recipient address
    /// @dev Fails with `STE`
    /// @param to The destination of the transfer
    /// @param value The value to be transferred
    function safeTransferETH(address to, uint256 value) internal {
        (bool success, ) = to.call{value: value}(new bytes(0));
        require(success, "CivFarm");
    }

    function withdrawETH() external payable onlyOwner {
        safeTransferETH(_msgSender(), address(this).balance);
    }

    function withdrawERC20(IERC20 _tokenContract) external onlyOwner {
        _tokenContract.safeTransfer(
            _msgSender(),
            _tokenContract.balanceOf(address(this))
        );
    }

