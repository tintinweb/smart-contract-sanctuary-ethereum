import "openzeppelin/proxy/utils/Initializable.sol";
import { ISonaTrack } from "./interfaces/ISonaTrack.sol";
import { SonaMinter } from "./access/SonaMinter.sol";
import "erc721a-upgradeable/ERC721AUpgradeable.sol";

contract SonaTrack is SonaMinter, ISonaTrack {
	/*//////////////////////////////////////////////////////////////
                               CONSTANTS
    //////////////////////////////////////////////////////////////*/

	/*//////////////////////////////////////////////////////////////
                                MAPPINGS
    //////////////////////////////////////////////////////////////*/

	mapping(uint256 => ISonaTrack.Track) public tracks;

	/*//////////////////////////////////////////////////////////////
								Modifiers
	//////////////////////////////////////////////////////////////*/

	/// @dev Modifier that ensures the calling wallet has the admin role or is the specified owner
	modifier onlySonaAdminOrCreator(uint256 _tokenId) {
		if (!isSonaAdmin(msg.sender) && msg.sender != tracks[_tokenId].trackCreator) revert SonaTrack_Unauthorized();
		_;
	}

	/*//////////////////////////////////////////////////////////////
                               FUNCTIONS
    //////////////////////////////////////////////////////////////*/

	/// @dev Initializes the contract
	function initialize(string memory _name, string memory _symbol, string memory _baseURI, string memory _contractURI) public initializerERC721A {
		// Setup role for contract creator, otherwise subsequent checks will not work
		_setupRole(ADMIN_ROLE, msg.sender);

		// Prevent double initialization.
		if (_nextTokenId() != 0) revert SonaTrack_Unauthorized();

		// Initialize ERC721A
		__ERC721A_init(_name, _symbol);

		emit Initialized(msg.sender, _name, _symbol);
	}

	/// @dev Mints multiple new tracks
	/// @param _cids The CIDs of the tracks' metadata
	function mintTracks(string[] memory _cids) external {
		// Only risk of overflow is if we wind up with more than quattuor­vigint­illions of tokens
		// unchecked saves gas on iteration [++i] and calculating the index to modify [_totalMinted() + i]
		unchecked {
			for (uint256 i = 0; i < _cids.length; ++i) {
				// Save creator and pointer to IPFS metadata on-chain
				tracks[_totalMinted() + i] = Track({ trackCreator: msg.sender, ipfsCid: _cids[i], hasBeenAuctioned: false });

				emit TrackMinted(_totalMinted() + i, _cids[i]);
			}
		}

		// Mint new tokens equivalent to the number of CIDs passed
		_mint(msg.sender, _cids.length);
	}

	/// @dev Updates the IPFS CID for the metadata for a given track
	/// @param _tokenId The ID of the token that will be updated
	/// @param _cid The metadata's IPFS CID
	function updateIpfsCid(uint256 _tokenId, string calldata _cid) external onlySonaAdminOrCreator(_tokenId) {
		if (!_exists(_tokenId)) revert SonaTrack_TokenIdDoesNotExist();

		tracks[_tokenId].ipfsCid = _cid;

		emit TrackCidUpdated({ tokenId: _tokenId, cid: _cid });
	}

	/// @dev Updates the creator info on the token, only available to Sona admins
	/// @param _tokenId The ID of the token that will be updated
	/// @param _trackCreator The new address to set for the creator
	function updateCreator(uint256 _tokenId, address _trackCreator) external onlySonaAdminOrCreator(_tokenId) {
		if (!_exists(_tokenId)) revert SonaTrack_TokenIdDoesNotExist();

		tracks[_tokenId].trackCreator = _trackCreator;

		emit TrackCreatorUpdated({ tokenId: _tokenId, trackCreator: _trackCreator });
	}

	/// @dev Removes a track from the protocol, burning the NFT and striking the data from on-chain memory
	/// @param _tokenId The ID of the token that will be deleted
	function removeTrack(uint256 _tokenId) external onlySonaAdminOrCreator(_tokenId) {
		if (!_exists(_tokenId)) revert SonaTrack_TokenIdDoesNotExist();

		delete tracks[_tokenId];

		_burn(_tokenId);

		emit TrackRemoved({ tokenId: _tokenId });
	}

	/// @dev Get a track's metadata from IPFS
	/// @param tokenId The ID of the token to fetch
	function tokenURI(uint256 tokenId) public view override(ERC721AUpgradeable, ISonaTrack) returns (string memory) {
		// TODO Ensure we have a consistent way of looking for the JSON under a given CID
		// (Or we could store all of the IPFS path, but that would be more expensive)

		if (!_exists(tokenId)) revert SonaTrack_TokenIdDoesNotExist();

		return string(abi.encodePacked("ipfs://", tracks[tokenId].ipfsCid, "/data.json"));
	}

	/// @dev Force tracks to be pseudo-soulbound
	/// @param _from The address from where the Track will be transferred
	/// @param _to The address to which the Track will be transferred
	/// @param _tokenId The token ID of the track
	function transferFrom(address _from, address _to, uint256 _tokenId) public payable override onlySonaAdminOrCreator(_tokenId) {
		super.transferFrom(_from, _to, _tokenId);
	}

	/// @dev Force tracks to be pseudo-soulbound
	/// @param _from The address from where the Track will be transferred
	/// @param _to The address to which the Track will be transferred
	/// @param _tokenId The token ID of the track
	function safeTransferFrom(address _from, address _to, uint256 _tokenId) public payable override onlySonaAdminOrCreator(_tokenId) {
		super.safeTransferFrom(_from, _to, _tokenId);
	}

	/// @dev Returns the CID of the track
	/// @param _tokenId The ID of the token to fetch
	function getTrackCID(uint256 _tokenId) external view returns (string memory) {
		if (!_exists(_tokenId)) revert SonaTrack_TokenIdDoesNotExist();

		return tracks[_tokenId].ipfsCid;
	}

	/// @dev Returns the creator of a track
	/// @param _tokenId The ID of the token to fetch
	function getTrackCreator(uint256 _tokenId) external view returns (address) {
		if (!_exists(_tokenId)) revert SonaTrack_TokenIdDoesNotExist();

		return tracks[_tokenId].trackCreator;
	}

	/// @dev Function for setting a track as having been auctioned
	/// @param _tokenId The ID of the token to fetch
	function setTrackAsAuctioned(uint256 _tokenId) external onlySonaAdminOrCreator(_tokenId) {
		if (!_exists(_tokenId)) revert SonaTrack_TokenIdDoesNotExist();

		tracks[_tokenId].hasBeenAuctioned = true;
	}

	/// @dev Function for getting a full track object
	/// @param _tokenId The ID of the token to fetch
	function getTrack(uint256 _tokenId) external view returns (Track memory) {
		if (!_exists(_tokenId)) revert SonaTrack_TokenIdDoesNotExist();

		return tracks[_tokenId];
	}
}

