import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/draft-ERC20Permit.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interfaces/bridge/IBridge.sol";
import "./interfaces/bundle/IBundler.sol";
import "hardhat/console.sol";
import "../library/BytesAddress.sol";

contract SwapERC20 {
    using SafeERC20 for IERC20;
    using BytesAddress for bytes;

    ISwapRouter public swapRouter;
    IBridge public bridge;

    constructor(ISwapRouter router_, IBridge bridge_) {
        swapRouter = router_;
        bridge = bridge_;
    }

    function safeTransferWithApprove(
        uint256 amountIn,
        address routerAddress,
        IERC20 tokenIn
    ) internal {
        tokenIn.safeTransferFrom(msg.sender, address(this), amountIn);
        tokenIn.safeApprove(routerAddress, amountIn);
    }

    function checkAllowance(IERC20 token, address to) internal {
        if (IERC20(token).allowance(address(this), to) == 0) {
            IERC20(token).safeApprove(to, type(uint256).max);
        }
    }

    function transferAndApproveRouter(IERC20 token, uint256 amount) internal {
        token.safeTransferFrom(msg.sender, address(this), amount);
        checkAllowance(token, address(swapRouter));
    }

    function swapExactInputSingle(
        uint256 amountIn,
        uint256 amountOutMinimum,
        IERC20 tokenIn,
        IERC20 tokenOut
    ) private returns (uint256 amountOut) {
        transferAndApproveRouter(tokenIn, amountIn);
        ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
            .ExactInputSingleParams({
                tokenIn: address(tokenIn),
                tokenOut: address(tokenOut),
                fee: 3000,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: amountIn,
                amountOutMinimum: amountOutMinimum,
                sqrtPriceLimitX96: 0
            });

        amountOut = swapRouter.exactInputSingle(params);
    }

    function swapExactOutputSingle(
        uint256 amountOut,
        uint256 amountInMaximum,
        IERC20 tokenIn,
        IERC20 tokenOut,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) private returns (uint256 amountIn) {
        IERC20Permit(address(tokenIn)).permit(msg.sender, address(this), amountInMaximum, block.timestamp, v, r, s);
        transferAndApproveRouter(tokenIn, amountInMaximum);
        ISwapRouter.ExactOutputSingleParams memory params = ISwapRouter
            .ExactOutputSingleParams({
                tokenIn: address(tokenIn),
                tokenOut: address(tokenOut),
                fee: 3000,
                recipient: address(this),
                deadline: block.timestamp,
                amountOut: amountOut,
                amountInMaximum: amountInMaximum,
                sqrtPriceLimitX96: 0
            });

        amountIn = swapRouter.exactOutputSingle(params);
        if (amountIn < amountInMaximum) {
            // Reset approval on router
            tokenIn.safeApprove(address(swapRouter), 0);
            // Refund WETH to user
            tokenIn.safeTransfer(msg.sender, amountInMaximum - amountIn);
        }
    }

    function swapExactInputSingleThenBridge(
        uint256 amountIn,
        uint256 amountOutMinimum,
        IERC20 tokenIn,
        IERC20 tokenOut,
        string memory receiver,
        string memory network,
        bool isWrapped,
        IBundler.Bundle calldata bundle_
    ) external returns (uint256 amountOut) {
        amountOut = swapExactInputSingle(
            amountIn,
            amountOutMinimum,
            tokenIn,
            tokenOut
        );

        tokenOut.safeApprove(address(bridge), amountOut);
        bridge.depositERC20(
            address(tokenOut),
            amountOut,
            bundle_,
            network,
            receiver,
            isWrapped
        );
    }

    function swapExactOutputSingleThenBridge(
        uint256 amountOut,
        uint256 amountInMaximum,
        IERC20 tokenIn,
        IERC20 tokenOut,
        string memory receiver,
        string memory network,
        bool isWrapped,
        IBundler.Bundle calldata bundle_,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external returns (uint256 amountIn) {
        amountIn = swapExactOutputSingle(
            amountOut,
            amountInMaximum,
            tokenIn,
            tokenOut,
            v,
            r,
            s
        );
        tokenOut.safeApprove(address(bridge), amountOut);
        bridge.depositERC20(
            address(tokenOut),
            amountOut,
            bundle_,
            network,
            receiver,
            isWrapped
        );
    }

    function swapExactOutputMultihop(
        uint256 amountOut,
        uint256 amountInMaximum,
        bytes calldata path
    ) private returns (uint256 amountIn) {
        (address tokenIn, ) = path.getInOutTokens();
        transferAndApproveRouter(IERC20(tokenIn), amountInMaximum);
        ISwapRouter.ExactOutputParams memory params = ISwapRouter
            .ExactOutputParams({
                path: path,
                recipient: address(this),
                deadline: block.timestamp,
                amountOut: amountOut,
                amountInMaximum: amountInMaximum
            });

        amountIn = swapRouter.exactOutput(params);
        if (amountIn < amountInMaximum) {
            // Reset approval on router
            IERC20(tokenIn).safeApprove(address(swapRouter), 0);

            // Refund WETH to user
            IERC20(tokenIn).safeTransfer(
                msg.sender,
                amountInMaximum - amountIn
            );
        }
    }

    function swapExactOutputMultiHopThenBridge(
        uint256 amountOut,
        uint256 amountInMaximum,
        bytes calldata path,
        string memory receiver,
        string memory network,
        bool isWrapped,
        IBundler.Bundle calldata bundle_
    ) external returns (uint256 amountIn) {
        amountIn = swapExactOutputMultihop(amountOut, amountInMaximum, path);
        (, address tokenOut) = path.getInOutTokens();

        IERC20(tokenOut).safeApprove(address(bridge), amountOut);
        bridge.depositERC20(
            address(tokenOut),
            amountOut,
            bundle_,
            network,
            receiver,
            isWrapped
        );
    }
}

//SPDX-License-Identifier: Unlicense
pragma solidity ^0.8.9;

library BytesAddress {
    function getInOutTokens(bytes calldata path) internal pure returns(address,address) {
        require(path.length > 42, "BytesAddress: invalid path length");
        return (_toAddress(path, path.length-20), _toAddress(path, 0));
    }

    function _toAddress(bytes memory _bytes, uint256 _start) internal pure returns (address parsedAddress) {
        assembly {
            parsedAddress := div(mload(add(add(_bytes, 0x20), _start)), 0x1000000000000000000000000)
        }
    }
}

