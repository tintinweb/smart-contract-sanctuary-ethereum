pragma solidity ^0.8.9;

import "./BaseClaimer.sol";
import "./interfaces/IOptimismTokenBridge.sol";
import "./interfaces/IOptimismL1Messenger.sol";

import "@openzeppelin/contracts/access/Ownable.sol";

import "hardhat/console.sol";

contract L1Claimer is BaseClaimer, Ownable {

    /// @dev Address of the bridge contract on L1
    IOptimismL1Messenger public messengerBridge;
    IOptimismTokenBridge public tokenBridge;

    /// @dev Address of the L2 claimer contract.
    address public l2Claimer;

    /// @dev Address of the token on L2 (for bridging balance).
    address public l2Token;

    /// @dev Indicates if the contract has been bridged.
    bool public isBridged = false;

    /// @dev Time until airdrop is claimable on this contract.
    uint256 public claimeableUntil;

    constructor(address pToken, uint256 pClaimeableUntil, address pTokenBridge, address pMessengerBridge, address pL2Token, bytes32 pMerkleRoot) BaseClaimer(pToken) {
        require(pClaimeableUntil > block.timestamp, "claimableUntil cannot be in the past");
        tokenBridge = IOptimismTokenBridge(pTokenBridge);
        l2Token = pL2Token;
        messengerBridge = IOptimismL1Messenger(pMessengerBridge);
        merkleRoot = pMerkleRoot;
        claimeableUntil = pClaimeableUntil;
    }

    /***
     * @dev Used to claim airdrop
     */
    function claim(address pRecipient, uint256 pAmount, bytes32[] memory pProof) public {
        require(!isBridged, "already bridged");
        require(block.timestamp <= claimeableUntil, "claim expired");
        super._claim(pRecipient, pAmount, pProof);
    }

    /***
     * @dev move funds and merkle to L2
     */
    function moveToL2() public  {
        require(l2Claimer != address(0), "L2 claimer not set");
        require(!isBridged, "already bridged");
        require(block.timestamp > claimeableUntil, "claim still open");
    
        uint256 curBalance = BaseClaimer.token.balanceOf(address(this));
        // Approve tokens for bridge
        require(token.approve(address(tokenBridge), curBalance), "failed to approve ERC20");
        // Transfer the tokens to the L2 claimer
        tokenBridge.depositERC20To(address(token), l2Token, l2Claimer, curBalance, 1_000_000, "0x");

        bytes memory message = abi.encodeWithSignature("receiveClaim(bytes32, uint256)", merkleRoot);
        messengerBridge.sendMessage(l2Claimer, message, 1_000_000);
    }

    function setL2Claimer(address pL2Claimer) public onlyOwner {
        l2Claimer = pL2Claimer;
    }

}

// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "hardhat/console.sol";

contract BaseClaimer {

    event Claimed(address indexed claimer, uint256 amount);

    /// @dev stores the current merkle root.
    bytes32 public merkleRoot;

    /// @dev The claimable ERC20 token
    IERC20 token;
    constructor(address pToken) {
        token = IERC20(pToken);
    }

    /// @dev Claim some balance for the recipient. Any account can call this, but recipient must be on the merkle tree.
    function _claim(address pRecipient, uint256 pAmount, bytes32[] memory pProof) internal {
        require(merkleRoot != 0, "no merkle root");
        require(
                MerkleProof.verify(
                    pProof,
                    merkleRoot,
                    keccak256(abi.encodePacked(pRecipient,pAmount))
                ),
                "not whitelisted"
            );

        require(token.balanceOf(address(this)) >= pAmount, "not enough balance to claim");

        // Transfer the tokens
        require(token.transfer(pRecipient, pAmount), "transfer failed");

        // Remove the claimer from the tree using the proof
        removeClaimerFromRoot(pProof);

        emit Claimed(pRecipient, pAmount);
    }

    /// @dev Removes the claimer from the merkle tree by reprocessing the root assuming the leaf is address 0 and amount 0
    function removeClaimerFromRoot(bytes32[] memory pProof) internal {
        bytes32 newRoot = MerkleProof.processProof(pProof, keccak256(abi.encodePacked(address(0), uint256(0))));
        merkleRoot = newRoot;
    }
}

