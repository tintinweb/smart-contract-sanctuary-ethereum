pragma solidity ^0.8.x;

import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./AggregatorV3Interface.sol";

interface IIERC20 is IERC20{
    function burn(uint256 amount) external;
    function decimals() external view returns (uint8);
}

contract PresaleToken is Ownable, ReentrancyGuard {
    using SafeERC20 for IIERC20;
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    IERC20 public USDTInterface;
    AggregatorV3Interface public aggregatorV3Interface;
      
    IIERC20 public token;
    address payable public collectedFunds;
    bool public saleActive;
    bool public claimStatus;
    uint256 public startTime;
    uint256 public endTime;
    uint256 public totalTokensSold;
    uint256 public tokenAmountSentPerTier;
    mapping(address => uint256) public investors;
    mapping(uint256 => uint256) public tierPrices;
    mapping(uint256 => uint256) public tierSupply;
    mapping(address => bool) public blacklist;
    uint8 public currentTier;
    uint8 public endTier;
    uint256 public totalSupply;
    uint256 public totalFunds;

    event SetToken(address indexed account);
    event AddToken(address indexed account, uint256 amount);
    event NewInvestment(address investor, uint256 value);
    event TokensSent(address receiver, uint256 amount);
    event TokensBurned(uint256 amount);
    event TierChanged(uint256 newTier);
    event TierPricesChanged(uint256 tier, uint256 value);
    event TierSupplyChanged(uint256 tier, uint256 value);
    event SaleStarted();
    event SaleEnded();
    event ClaimStarted();

    modifier notBlacklisted() {
        require(!blacklist[msg.sender], "You are in blacklist");
        _;
    }

    /**
     * @dev Creates the contract
     * @param _saleToken start      - Address of CODO token
     * @param _aggregatorInterface  - Address of Chainlink ETH/USD price feed
     * @param _USDTInterface        - Address of USDT token
     */
    constructor(
        address _saleToken,
        address _aggregatorInterface,
        address _USDTInterface
    ) {
        require(_saleToken != address(0), "Zero Sale Token Address");
        require(_aggregatorInterface != address(0), "Zero Aggregator Address");
        require(_USDTInterface != address(0), "Zero USDT Address");

        token = IIERC20(_saleToken);
        // aggregatorV3Interface = AggregatorV3Interface(_aggregatorInterface);
        // USDTInterface = IERC20(_USDTInterface);
        aggregatorV3Interface = AggregatorV3Interface(address(0xD4a33860578De61DBAbDc8BFdb98FD742fA7028e));
        USDTInterface = IERC20(address(0xb0f7554a44cC178e935Ea10c79e7c042D1840044));
        // collectedFunds = payable(owner());
        collectedFunds = payable(address(0xf946cE2Fe9f127F83835dB47526b885775727628));
        saleActive = false;
        claimStatus = false;
        currentTier = 1;
        startTime = 0;
        endTime = 0;
        totalTokensSold = 0;

        // Set up tier prices and supply
        tierPrices[1] = 12e3;
        tierPrices[2] = 14e3;
        tierPrices[3] = 16e3;
        tierPrices[4] = 18e3;
        tierPrices[5] = 20e3;
        tierPrices[6] = 22e3;
        tierPrices[7] = 24e3;
        tierPrices[8] = 24e3;

        tierSupply[1] = 25_000_000 * 1e18;
        tierSupply[2] = 50_000_000 * 1e18;
        tierSupply[3] = 100_000_000 * 1e18;
        tierSupply[4] = 150_000_000 * 1e18;
        tierSupply[5] = 225_000_000 * 1e18;
        tierSupply[6] = 300_000_000 * 1e18;
        tierSupply[7] = 400_000_000 * 1e18;
        tierSupply[8] = 500_000_000 * 1e18;

        endTier = 8;
        totalSupply = 0;
        totalFunds = 0;

        emit SetToken(_saleToken);
    }

    /**
    * @dev To add amount of token to sell
    * @param amount - amount of token
    */
    function addToken(uint256 amount) external onlyOwner {
        require(address(token) != address(0), "Zero Token Address");
        require(amount > 0, "Invalid Amount");

        uint256 oldBalance = token.balanceOf(address(this));
        token.safeTransferFrom(msg.sender, address(this), amount);

        uint256 newBalance = token.balanceOf(address(this));
        uint256 diff = newBalance - oldBalance;

        totalSupply += diff;

        emit AddToken(msg.sender, diff);
    }

    /**
     * @dev To add users to blacklist
     * @param _users - Array of addresses to add in blacklist
     */
    function addToBlacklist(address[] calldata _users) external onlyOwner {
        uint256 usersAmount = _users.length;
        uint256 i = 0;
        while(i<usersAmount) blacklist[_users[i++]] = true;
    }

    /**
     * @dev To remove users from blacklist
     * @param _users - Array of addresses to remove from blacklist
     */
    function removeFromBlacklist(address[] calldata _users) external onlyOwner {
        uint256 usersAmount = _users.length;
        uint256 i = 0;
        while(i<usersAmount) blacklist[_users[i++]] = false;
    }

    /**
    * @dev To set start and end time to sale token
    * @param _startTime - New sales start time
    * @param _endTime   - New sales end time
    */
    function startSale(uint256 _startTime, uint256 _endTime) external onlyOwner {
        require(!saleActive, "Sale Already Started");
        // require(_startTime >= block.timestamp, "Invalid Value");
        require(_endTime > _startTime, "Invalid Value");

        startTime = _startTime;
        endTime = _endTime;
        saleActive = true;

        emit SaleStarted();
    }

    /**
    * @dev To end sale
    */
    function endSale() external onlyOwner {
        require(saleActive, "No Sale Started");

        saleActive = false;

        emit SaleEnded();
    }

    function setClaim(bool _claimStatus) external onlyOwner returns (bool) {
        require(!saleActive, "No Sale Ended");
        require(token.balanceOf(address(this)) >= totalTokensSold * 1e18, "Not Enough Balance");

        claimStatus = _claimStatus;

        return true;
    }

    /**
    * @dev To claim tokens after claiming starts
    */
    function claim() public nonReentrant {
        require(claimStatus, "No Claim Enabled");
        uint256 tokensToClaim = investors[msg.sender];
        require(tokensToClaim > 0, "No Claimalble Token");
        require(token.balanceOf(address(this)) >= tokensToClaim, "Token Balance Not Sufficient");

        require(token.transfer(msg.sender, tokensToClaim));
        delete investors[msg.sender];
        
        emit TokensSent(msg.sender, tokensToClaim);
    }

    /**
    * @dev To buy into a presale using ETH
    * @param _amount - Amount of tokens to buy
    */
    function buyWithEth(uint256 _amount) external payable notBlacklisted nonReentrant{
        // require(msg.value >= tierPrices[currentTier]);
        require(saleActive);
        require(block.timestamp >= startTime, "Too Soon");
        require(block.timestamp <= endTime, "Too Late");

        uint256 ethAmount = ethBuyHelper(_amount);

        console.log("input token amount: ", _amount);
        console.log("input eth amount: ", msg.value);
        console.log("calculated eth amount:", ethAmount);

        require(msg.value >= ethAmount, "Less payment");
        (bool success1,) = payable(collectedFunds).call{value : ethAmount}("");
        require(success1, "ETH transer failed");

        uint256 excess = msg.value - ethAmount;
        if(excess > 0) {
            (bool success,) = payable(msg.sender).call{value : excess}("");
            require(success, "ETH refund failed");
        }

        totalTokensSold += _amount;
        investors[msg.sender] += _amount * 1e18;

        uint8 tierAfterPurchase = _getTierByTotalSoldAmount();
        console.log("tierAfterPurchase: ", tierAfterPurchase);
        if(tierAfterPurchase > currentTier) {
            currentTier = tierAfterPurchase;
            emit TierChanged(currentTier);
        }

        emit NewInvestment(msg.sender, msg.value);
    }

    /**
    * @dev To buy into a presale using USDT
    * @param _amount - Amount of tokens to buy
    */
    function buyWithUSDT(uint256 _amount) external notBlacklisted nonReentrant{
        require(saleActive);
        require(block.timestamp >= startTime, "Too Soon");
        require(block.timestamp <= endTime, "Too Late");

        uint256 usdtPrice = usdtBuyHelper(_amount);
        require(usdtPrice <= USDTInterface.allowance(msg.sender, address(this)), "Not enough allowance");

        USDTInterface.safeTransferFrom(msg.sender, collectedFunds, usdtPrice);
        totalTokensSold += _amount;
        investors[msg.sender] += _amount * 1e18;

        uint8 tierAfterPurchase = _getTierByTotalSoldAmount();
        if(tierAfterPurchase > currentTier) {
            currentTier = tierAfterPurchase;
            emit TierChanged(currentTier);
        }
    }

    /**
    * @dev To calculate Eth price for given amount
    * @param _amount - Amount of tokens to calculate price
    * @notice Return result in e18 format
    */
    function ethBuyHelper(uint256 _amount) public view returns (uint256 ethAmount) {
        ethAmount = calculatePrice(_amount) * 1e18 / getLatestPrice();
        return ethAmount;
    }

    /**
     * @dev Calculate USDT price for given amount
     * @param _amount - Amount of tokens to calculate price
     * @notice Return result in 1e6 format
     */
    function usdtBuyHelper(uint256 _amount) public view returns (uint256 usdtPrice) {
        // usdtPrice = calculatePrice(_amount) / 1e12;
        usdtPrice = calculatePrice(_amount);
    }

    /**
    * @dev To calculate the price in USD for given amount of tokens
    * @param _amount - Amount of tokens to calculate price
    * @notice Return result in e18 format
    */
    function calculatePrice(uint256 _amount) public view returns (uint256) {
        require(_amount + totalTokensSold <= tierSupply[endTier], "Insufficient token amount");
        return _calculateInternalCost(_amount, currentTier, totalTokensSold);
    }

    /**
    * @dev To calculate cost for specified conditions
    * @param _amount            - Amount of tokens to calculate price
    * @param _currentTier       - Starting tier to calculate price
    * @param _totalTokensSold   - Starting total tokens sold amount to calculate price
    */
    function _calculateInternalCost(uint256 _amount, uint256 _currentTier, uint256 _totalTokensSold) internal view returns (uint256 cost) {
        uint256 currentPrice = tierPrices[_currentTier];
        uint256 currentAmount = tierSupply[_currentTier];
        if(_totalTokensSold + _amount <= currentAmount) {
            cost = _amount * currentPrice;
        } else {
            uint256 currentTierAmount = currentAmount - _totalTokensSold;
            uint256 nextTierAmount = _amount - currentTierAmount;
            cost = currentTierAmount * currentPrice + _calculateInternalCost(nextTierAmount, _currentTier + 1, currentAmount);
        }
        return cost;
    }

    /**
     * @dev To get latest ETH/USD price
     * @notice Return result in 1e6 format
     */
    function getLatestPrice() public view returns (uint256) {
        (, int price, , ,) = aggregatorV3Interface.latestRoundData();
        return uint256(price / 1e2);
    }

    function burnUnsoldTokens() external onlyOwner {
        require(!saleActive);

        uint remainsAmount = token.balanceOf(address(this));
        require(remainsAmount > 0, "Token Sale: No Burnable Token");

        token.burn(remainsAmount);

        emit TokensBurned(remainsAmount);
    }

    function setCollectedFunds(address payable _collectedFunds) external onlyOwner {
        require(_collectedFunds != address(0));
        collectedFunds = _collectedFunds;
    }

    /**
     * @dev Returns price for current step
     */
    function getCurrentPrice() external view returns (uint256) {
        return tierPrices[currentTier];
    }

    function getTierPrice(uint8 tier) public view returns (uint256) {
        return tierPrices[tier];
    }

    function setTierPrice(uint8 tier, uint256 value) external onlyOwner {
        require(tier > currentTier, "Tier must be greater than current tier");
        require(tier <= endTier, "Tier must be less than max tier");
        require(value > 0, "Invalid Price");

        tierPrices[tier] = value;

        emit TierPricesChanged(tier, value);
    }

    function getTierSupply(uint8 tier) public view returns (uint256) {
        return tierSupply[tier];
    }

    function setTierSupply(uint8 tier, uint256 _amount) external onlyOwner {
        require(tier > currentTier, "Tier must be greater than current tier");
        require(tier <= endTier, "Tier must be less than max tier");
        require(_amount > 0, "Invalid Price");

        uint256 diff = _amount - tierSupply[tier];
        tierSupply[tier] = _amount;
        
        uint8 tierIndex = endTier;
        while (tierIndex > tier) {
            tierSupply[tierIndex] += diff;
            tierIndex -= 1;
        }

        emit TierSupplyChanged(tier, _amount);
    }

    // function getRemaindTokenAmount() public view returns (uint256) {
    //     uint256 decimal = token.decimals();
    //     uint256 one = 10 ** decimal;
    //     return one * tierSupply[currentTier] - tokenAmountSentPerTier;
    // }

    function getEndTier() public view returns (uint8) {
        return endTier;
    }

    function getCurrentTier() public view returns (uint8) {
        return currentTier;
    }

    function setTier(uint8 newTier) external onlyOwner {
        require(newTier > currentTier, "Tier must be greater than current tier");
        require(newTier <= endTier, "Tier must be less than end tier");

        currentTier = newTier;

        emit TierChanged(currentTier);
    }

    /**
     * @dev Returns amount of tokens sold on current step
     */
    function getSoldOnCurrentStage() external view returns (uint256 soldOnCurrentStage) {
        soldOnCurrentStage = totalTokensSold - ((currentTier == 0)? 0 : tierSupply[currentTier-1]);
    }

    function getTotalSoldAmount() public view returns (uint256) {
        return totalTokensSold;
    }

    /**
     * @dev Returns total price of sold tokens
     */
    function totalSoldPrice() external view returns (uint256) {
        return _calculateInternalCost(totalTokensSold, 0 ,0);
    }

    /**
     * @dev Returns presale last stage token amount limit
     */
    function getTotalPresaleAmount() external view returns (uint256) {
        return tierSupply[endTier];
    }

    /**
     * @dev Returns total supply amount
     */
    function getTotalSupply() public view returns (uint256) {
        return totalSupply;
    }

    function getUserBalance(address _account) public view returns (uint256) {
        return investors[_account];
    }

    

    // function getTotalFunds() public view returns (uint256) {
    //     return totalFunds;
    // }

    function _getTierByTotalSoldAmount() internal view returns (uint8) {
        uint8 tierIndex = endTier;
        while(tierIndex > 0) {
            if(tierSupply[tierIndex - 1] < totalTokensSold) break;
            tierIndex -= 1;
        }
        return tierIndex;
    }
}

