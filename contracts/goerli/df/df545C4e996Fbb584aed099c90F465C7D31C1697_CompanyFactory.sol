pragma solidity ^0.8.0;


import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
    import "@openzeppelin/contracts/proxy/Clones.sol";

import "./../tokens/Token.sol";

import "./../interfaces/ICompanyFactory.sol";
import "./../interfaces/ICompany.sol";
import "./../interfaces/IStaking.sol";
import "./../interfaces/IKYC.sol";

import "./Company.sol";



/// @title Company factory
contract CompanyFactory is OwnableUpgradeable, ICompanyFactory {
    address public override kyc;
    address public override staking;
    address public override companyImplementation;
    address public override want;

    address public swapFactory;
    address public swapRouter;

    uint public override companyCreatorRequiredStatus;

    modifier onlyCompanyCreator(address sender) {
        require(IKYC(kyc).checkStatus(sender, companyCreatorRequiredStatus));

        _;
    }

    function initialize(
        address _kyc,
        address _staking,
        address _owner,
        address _companyImplementation,
        address _want,
        uint _companyCreatorRequiredStatus,
        address _swapFactory,
        address _swapRouter
    ) external initializer {
        kyc = _kyc;
        emit UpdateKYC(kyc);

        staking = _staking;
        emit UpdateStaking(staking);

        companyImplementation = _companyImplementation;
        emit UpdateCompanyImplementation(companyImplementation);

        companyCreatorRequiredStatus = _companyCreatorRequiredStatus;
        emit UpdateCompanyCreatorRequiredStatus(companyCreatorRequiredStatus);

        want = _want;
        swapFactory = _swapFactory;
        swapRouter = _swapRouter;

        __Ownable_init();
        transferOwnership(_owner);
    }

    /// @notice Update user's required status for creating company
    /// Can be called only by `owner`
    /// Doesn't affect already created companies
    /// @param _companyCreatorRequiredStatus New value for required status
    function updateCompanyCreatorRequiredStatus(
        uint _companyCreatorRequiredStatus
    ) external override onlyOwner {
        companyCreatorRequiredStatus = _companyCreatorRequiredStatus;

        emit UpdateCompanyCreatorRequiredStatus(companyCreatorRequiredStatus);
    }

    /// @notice Update company implementation
    /// Can be called only by `owner`
    /// Doesn't affect already created companies
    /// @param _companyImplementation New value for company implementation
    function updateCompanyImplementation(
        address _companyImplementation
    ) external override onlyOwner {
        companyImplementation = _companyImplementation;

        emit UpdateCompanyImplementation(companyImplementation);
    }

    /// @notice Update KYC contract address
    /// Can be called only by owner
    /// Doesn't affect already created companies
    /// @param _kyc New KYC address
    function updateKYC(
        address _kyc
    ) external override onlyOwner {
        kyc = _kyc;

        emit UpdateKYC(kyc);
    }

    /// @notice Update staking contract address
    /// Can be called only by `owner`
    /// Doesn't affect already created companies
    /// @param _staking New staking address
    function updateStaking(
        address _staking
    ) external override onlyOwner {
        staking = _staking;

        emit UpdateStaking(staking);
    }

    /// @notice Create new company
    /// Can be called only by address with `companyCreatorRequiredStatus` enabled
    /// @dev Token decimals are always 18
    /// Company initialized as upgradable proxy
    /// @param name Token name.
    /// @param symbol Token symbol.
    /// @param totalSupply Token total supply. Minted to the company, cant be extended.
    /// @param listingPrice Token listing price, in BPS.
    /// @param liquidityShare How much contributions to spend on supply liquidity, in BPS.
    function createCompany(
        string memory name,
        string memory symbol,
        uint256 totalSupply,
        uint256 listingPrice,
        uint256 liquidityShare
    )
        external
        override
        onlyCompanyCreator(msg.sender)
    {
        address company = Clones.clone(companyImplementation);

        Token token = new Token(name, symbol, 0, company);

        ICompany(company).initialize(
            msg.sender,
            address(token),
            want,
            address(this),
            staking,
            totalSupply,
            listingPrice,
            liquidityShare,
            swapFactory,
            swapRouter
        );

        IStaking(staking).registerCompany(company);

        emit CompanyCreated(msg.sender, address(token), company);
    }

    /// @notice Create new company by factory owner.
    /// Can be called only by `owner`
    /// Company initialized as upgradable proxy
    /// @param name Token name
    /// @param _owner Company owner
    /// @param name Token name.
    /// @param symbol Token symbol.
    /// @param totalSupply Token total supply. Minted to the company, cant be extended.
    /// @param listingPrice Token listing price, in BPS.
    /// @param liquidityShare How much contributions to spend on supply liquidity, in BPS.
    function createCompanyByOwner(
        address _owner,
        string memory name,
        string memory symbol,
        uint256 totalSupply,
        uint256 listingPrice,
        uint256 liquidityShare
    ) external override onlyOwner {
        address company = Clones.clone(companyImplementation);

        Token token = new Token(name, symbol, 0, company);

        ICompany(company).initialize(
            _owner,
            address(token),
            want,
            address(this),
            staking,
            totalSupply,
            listingPrice,
            liquidityShare,
            swapFactory,
            swapRouter
        );

        IStaking(staking).registerCompany(company);

        emit CompanyCreated(msg.sender, address(token), company);
    }
}

