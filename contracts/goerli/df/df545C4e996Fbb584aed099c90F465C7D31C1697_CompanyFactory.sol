pragma solidity ^0.8.0;


interface ICompany {
    enum RoundStatus{ Empty, Initialized, Active, Succeeded, Failed }

    struct Contribution {
        bool subscribed;
        uint contributed;
        uint lastContribution;
        uint checkpoint;
        bool withdrawn;
        bool claimedExceeding;
    }

    struct VestingCheckpoint {
        uint timestamp;
        uint share;
    }

    struct Round {
        uint activation;
        uint price;
        uint startTimestamp;
        uint endTimestamp;
        uint minCap;
        uint maxCap;
        bool isFinal;
        bool ownerShareDistributed;
        uint totalContribution;
        VestingCheckpoint[] vesting;
        bytes32 whitelist;
    }

    function initialize(
        address _owner,
        address _token,
        address _want,
        address _factory,
        address _staking,
        uint _totalSupply,
        uint _listingPrice,
        uint _liquidityShare,
        address _swapFactory,
        address _swapRouter
    ) external;

    function subscribe() external;
    function unsubscribe(uint roundId) external;

    function contribute(uint amount, bytes32[] memory merkleProof) external;
    function refund(uint amount) external;
    function withdraw(uint roundId) external;
    function claimExceeding(uint roundId) external;
    function claim(uint roundId) external;
    function claimOwnerShare(uint roundId, address receiver) external;

    function getExceeding(address user, uint roundId) external view returns (uint amount);
    function getContributionWithoutExceeding(
        uint roundId,
        address user
    ) external view returns (uint256 amount);

    function isWhitelisted(
        address user,
        uint roundId,
        bytes32[] memory merkleProof
    ) external view returns (bool status);

    function startRound(
        uint price,
        uint startTimestamp,
        uint endTimestamp,
        uint minCap,
        uint maxCap,
        bool isFinal,
        VestingCheckpoint[] memory vesting,
        bytes32 whitelist
    ) external;


    event RoundStarted(
        uint roundId,
        uint price,
        uint startTimestamp,
        uint endTimestamp,
        uint minCap,
        uint maxCap,
        bool isFinal
    );

    event RoundsFinalize(
        uint stage,
        uint finished
    );

    event RoundContributed(uint roundId, address user, uint amount);
    event RoundRefunded(uint roundId, address user, uint amount);
    event RoundWithdrawn(uint roundId, address user, uint amount);
    event RoundClaimedExceeding(uint roundId, address user, uint amount);
    event RoundOwnerShareClaimed(uint roundId);
}

