    function getExceeding(
        address user,
        uint roundId
    )
        public
        view
        override
        onlyRoundContributor(user, roundId)
    returns (uint amount) {
        Round storage round = rounds[roundId];

        // Total round contribution is less than `maxCap`
        if (round.totalContribution <= round.maxCap) {
            return 0;
        }

        uint contributed = contributions[user][roundId].contributed;
        uint exceeding = contributed * round.maxCap / round.totalContribution;

        return contributed - exceeding;
    }

    /// @notice Get user's contribution without exceeding part
    /// @param roundId Round ID
    /// @param user User address
    /// @return amount Normalized contribution amount
    function getContributionWithoutExceeding(
        uint roundId,
        address user
    )
        public
        view
        override
