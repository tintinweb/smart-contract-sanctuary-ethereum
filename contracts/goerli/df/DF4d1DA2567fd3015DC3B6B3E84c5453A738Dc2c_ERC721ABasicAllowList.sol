        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _updateAccount(_index, _newAccount);
    }

    function updateDistribution(
        address[] memory wallets,
        uint256[] memory percents
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _updateDistribution(wallets, percents);
    }

    function setOpen(bool _open) external onlyRole(DEFAULT_ADMIN_ROLE) {
        open = _open;
    }

