pragma solidity 0.8.17;

import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

/**
 * @title Distribution
 * @dev This contract allows to distribute payments among a group of accounts.
 */

abstract contract Distribution {
    address[] public wallets;
    uint256[] public percents;
    uint256 private immutable bps = 10**4;

    event DistributionSuccess();
    event DistributionUpdate();
    event UpdateAccount(address account);

    function percent(uint256 index) public view returns (uint256) {
        return percents[index];
    }

    function wallet(uint256 index) public view returns (address) {
        return wallets[index];
    }

    function walletCount() public view returns (uint256) {
        return wallets.length;
    }

    function getAmount(uint256 balance, uint256 index)
        public
        view
        returns (uint256)
    {
        return (balance * percents[index]) / bps;
    }

    function _updateAccount(uint256 index, address newAccount) internal {
        require(newAccount != address(0), "Address cannot be zero");

        wallets[index] = newAccount;
        emit UpdateAccount(newAccount);
    }

    function _updateDistribution(
        address[] memory _wallets,
        uint256[] memory _percents
    ) internal {
        wallets = new address[](0);
        percents = new uint256[](0);

        _setDistribution(_wallets, _percents);
        emit DistributionUpdate();
    }

    function distribute(uint256 balance) public payable {
        uint256 count = walletCount();

        for (uint256 i = 0; i < count; i++) {
            uint256 amount = getAmount(balance, i);
            Address.sendValue(payable(wallet(i)), amount);
        }

        emit DistributionSuccess();
    }

    function _setDistribution(
        address[] memory _wallets,
        uint256[] memory _percents
    ) internal noEmptyValues(_wallets, _percents) {
        for (uint256 i = 0; i < _wallets.length; i++) {
            wallets.push(_wallets[i]);
            percents.push(_percents[i]);
        }
    }

    modifier noEmptyValues(
        address[] memory _wallets,
        uint256[] memory _percents
    ) {
        uint256 totalPercents;
        require(_wallets.length > 0, "Distribution: wallets are required");
        require(
            _wallets.length == _percents.length,
            "Distribution: wallets and percents length mismatch"
        );

        for (uint256 i = 0; i < _wallets.length; i++) {
            require(
                _wallets[i] != address(0),
                "Distribution: account address cannot be zero"
            );
            require(
                _percents[i] > 0,
                "Distribution: percent value cannot be zero"
            );
            totalPercents += _percents[i];
        }

        require(totalPercents == bps, "Distribution: percents are not 100");
        _;
    }
}

