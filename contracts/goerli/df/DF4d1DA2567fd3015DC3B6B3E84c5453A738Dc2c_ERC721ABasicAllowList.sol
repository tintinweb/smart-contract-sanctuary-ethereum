    }

    function airdropBatch(address[] memory receivers, uint256[] memory amounts)
        public
        payable
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(receivers.length > 0, "Receivers cannot be empty");
        require(
            receivers.length == amounts.length,
            "Receivers and amounts length mismatch"
        );

        uint256 totalAmounts;

        for (uint256 i = 0; i < amounts.length; i++) {
            require(
                receivers[i] != address(0),
                "Receivers address cannot be empty"
            );
            require(amounts[i] != 0, "Amounts cannot be empty");
            require(amounts[i] <= mintMax, "Minting limit reached");
            totalAmounts += amounts[i];
        }

        require(
            maxSupply >= totalAmounts + _totalMinted(),
            "Supply limit reached"
        );

        for (uint256 i = 0; i < receivers.length; i++) {
            _buy(receivers[i], amounts[i]);
        }
    }

    function mint(
        address to,
        uint256 quantity,
        bytes calldata signature
    ) external payable nonReentrant {
        require(open, "Contract is closed");
        require(msg.value >= quantity * mintPrice, "Insufficient ETH amount");
        if (whitelisted) {
            require(
                Signature.verify(signerAddress, signature, 1),
                "Address is not whitelisted"
            );
        }
        _buy(to, quantity);
        payPlatformRoyalty(mintPrice * quantity);
    }

    function _buy(address to, uint256 quantity)
        internal
        noZeroAddress(to)
        noZeroValue(quantity)
    {
        require(mintMax >= quantity, "Mint max limit reached");
        require(
            holdMax >= quantity + _numberMinted(to),
            "Hold max limit reached"
        );
        require(
            maxSupply >= quantity + _totalMinted(),
            "Max supply limit reached"
        );
        _safeMint(to, quantity);
