        external
        noZeroAddress(newSigner)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(newSigner != signerAddress, "Address is the actual signer");
        signerAddress = newSigner;
    }

    function setWhitelisted(bool _whitelisted)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        whitelisted = _whitelisted;
    }

    function setMintPrice(uint256 newMintPrice)
        external
        noZeroValue(newMintPrice)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        mintPrice = newMintPrice;
    }

