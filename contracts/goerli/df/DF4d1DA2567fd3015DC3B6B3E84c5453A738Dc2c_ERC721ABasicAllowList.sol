pragma solidity 0.8.17;

import "@openzeppelin/contracts/utils/Address.sol";

/**
 * @title Platform Royalties
 * @dev Modifiers that will help simplify the contract code.
 */

abstract contract PlatformRoyalties {
    /**
     * @dev Platform royalty data.
     */
    address private immutable _receiver = 0xA8eA98c439898d363b1C4457F948Ec9f82354337;
    uint256 private immutable _fraction = 200;

    /**
     * @dev The _maxbps with which to interpret the fee set as a fraction of the sale price.
     * Defaults to 10000 so fees are expressed in basis points.
     */
    uint256 private immutable _maxbps = 10000;

    /**
     * @dev Returns the platform royalty info.
     */
    function royaltyInfo() public view virtual returns (address, uint256) {
        return (_receiver, _fraction);
    }

    /**
     * @dev Sets the platform royalty payment.
     *
     * Requirements:
     *
     * - `_salePrice` cannot be the zero.
     */
    function payPlatformRoyalty(uint256 _salePrice) public payable {
