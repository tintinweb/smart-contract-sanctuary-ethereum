import "../custom/Modifiers.sol";

/**
 * @title Splitter
 * @dev This contract allows to split Ether payments among a group of accounts.
 */

contract Splitter is Modifiers, AccessControl, ReentrancyGuard {
    address[] public payees;
    uint256[] public shares;
    uint256 private immutable bps = 10**4;

    bytes32 public constant DISTRIBUTOR_ROLE = keccak256("DISTRIBUTOR_ROLE");

    event DistributionSuccess();
    event PaymentReceived(address, uint256);
    event DistributionUpdate();
    event UpdatePayee(address account);

    constructor(address[] memory _payees, uint256[] memory _shares) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(DISTRIBUTOR_ROLE, msg.sender);

        _setDistribution(_payees, _shares);
    }

    receive() external payable {
        emit PaymentReceived(msg.sender, msg.value);
    }

    function share(uint256 index) public view returns (uint256) {
        return shares[index];
    }

    function payee(uint256 index) public view returns (address) {
        return payees[index];
    }

    function payeeCount() public view returns (uint256) {
        return payees.length;
    }

    function getAmount(uint256 balance, uint256 index)
        public
        view
        returns (uint256)
    {
        return (balance * shares[index]) / bps;
    }

    function updatePayee(uint256 index, address newPayee)
        external
        noZeroAddress(newPayee)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        payees[index] = newPayee;
        emit UpdatePayee(newPayee);
    }

    function updateDistribution(
        address[] memory _payees,
        uint256[] memory _shares
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        payees = new address[](0);
        shares = new uint256[](0);

        _setDistribution(_payees, _shares);

        emit DistributionUpdate();
    }

    function distribute()
        external
        payable
        nonReentrant
        onlyRole(DISTRIBUTOR_ROLE)
    {
        require(address(this).balance > 0, "Splitter: Insuficient balance");

        uint256 count = payeeCount();
        uint256 balance = address(this).balance;

        for (uint256 i = 0; i < count; i++) {
            uint256 amount = getAmount(balance, i);
            Address.sendValue(payable(payee(i)), amount);
        }

        emit DistributionSuccess();
    }

    function _setDistribution(
        address[] memory _payees,
        uint256[] memory _shares
    ) internal noEmptyValues(_payees, _shares) {
        for (uint256 i = 0; i < _payees.length; i++) {
            _addPayee(_payees[i], _shares[i]);
        }
    }

    function _addPayee(address _payee, uint256 _share) internal {
        payees.push(_payee);
        shares.push(_share);
    }

    modifier noEmptyValues(address[] memory _payees, uint256[] memory _shares) {
        uint256 totalShares;
        require(_payees.length > 0, "Splitter: payees are required");
        require(
            _payees.length == _shares.length,
            "Splitter: payees and shares length mismatch"
        );

        for (uint256 i = 0; i < _payees.length; i++) {
            require(
                _payees[i] != address(0),
                "Splitter: Payee address cannot be zero"
            );
            require(_shares[i] > 0, "Splitter: Share value cannot be zero");
            totalShares += _shares[i];
        }

        require(totalShares == bps, "Splitter: Shares are not 100 percent");
        _;
    }
}

