import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "../library/Signature.sol";
import "../custom/Modifiers.sol";
import "../custom/Distribution.sol";
import "../custom/PlatformRoyalties.sol";

contract ERC721FullCrossMint is
    ERC721,
    ERC721URIStorage,
    Pausable,
    AccessControl,
    Modifiers,
    ReentrancyGuard,
    ERC721Burnable,
    PlatformRoyalties,
    Distribution
{
    using Strings for uint256;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdCounter;

    string public baseURI;
    uint256 public mintPrice;
    uint256 public maxSupply;
    address public signerAddress;
    bool public whitelisted;
    address public gateway;
    bool public open;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    event AirdropSuccess();
    event PaymentReceived(address, uint256);
    event WithdrawalSuccess(uint256 amount);

    struct SettingsStruct {
        string name;
        string symbol;
        uint256 maxSupply;
        uint256 mintPrice;
        string baseURI;
        bool open;
        bool paused;
        uint256 current;
        address signerAddress;
        bool whitelisted;
        address gateway;
        address[] wallets;
        uint256[] percents;
    }

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _uri,
        uint256 _mintPrice,
        uint256 _maxSupply,
        address _signer,
        address _gateway,
        address[] memory wallets,
        uint256[] memory percents
    ) ERC721(_name, _symbol) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);

        baseURI = _uri;
        mintPrice = _mintPrice;
        maxSupply = _maxSupply;
        signerAddress = _signer;
        gateway = _gateway;
        whitelisted = true;
        open = false;

        _tokenIdCounter.increment();
        _setDistribution(wallets, percents);
    }

    receive() external payable {
        emit PaymentReceived(msg.sender, msg.value);
    }

    function getSettings() external view returns (SettingsStruct memory) {
        SettingsStruct memory settings = SettingsStruct({
            name: name(),
            symbol: symbol(),
            maxSupply: maxSupply,
            mintPrice: mintPrice,
            baseURI: baseURI,
            open: open,
            paused: paused(),
            current: _tokenIdCounter.current(),
            signerAddress: signerAddress,
            whitelisted: whitelisted,
            gateway: gateway,
            wallets: wallets,
            percents: percents
        });
        return settings;
    }

    function updateAccount(uint256 _index, address _newAccount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _updateAccount(_index, _newAccount);
    }

    function updateDistribution(
        address[] memory wallets,
        uint256[] memory percents
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _updateDistribution(wallets, percents);
    }

    function setOpen(bool _open) external onlyRole(DEFAULT_ADMIN_ROLE) {
        open = _open;
    }

    function pause() external onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function current() external view returns (uint256) {
        return _tokenIdCounter.current();
    }

    function setGateway(address _gateway)
        external
        noZeroAddress(_gateway)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        gateway = _gateway;
    }

    function setSigner(address newSigner)
        external
        noZeroAddress(newSigner)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(newSigner != signerAddress, "Address is the actual signer");
        signerAddress = newSigner;
    }

    function setWhitelisted(bool _whitelisted)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        whitelisted = _whitelisted;
    }

    function setBaseURI(string calldata newBaseURI)
        external
        noEmptyURI(newBaseURI)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        baseURI = newBaseURI;
    }

    function setTokenURI(uint256 tokenId, string calldata _tokenURI)
        external
        noEmptyURI(_tokenURI)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _setTokenURI(tokenId, _tokenURI);
    }

    function setMintPrice(uint256 newPrice)
        external
        noZeroValue(newPrice)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        mintPrice = newPrice;
    }

    function setMaxSupply(uint256 newMaxSupply)
        external
        noZeroValue(newMaxSupply)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(
            newMaxSupply >= _tokenIdCounter.current(),
            "ERC721: value must be greater than total minted"
        );
        maxSupply = newMaxSupply;
    }

    function airdrop(address to)
        external
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _buy(to);
    }

    function airdropBatch(address[] calldata receivers)
        external
        payable
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(receivers.length > 0, "Receivers cannot be empty");
        require(
            maxSupply >= receivers.length + _tokenIdCounter.current(),
            "Supply limit reached"
        );

        for (uint256 i = 0; i < receivers.length; i++) {
            require(
                receivers[i] != address(0),
                "Receivers address cannot be empty"
            );
        }

        for (uint256 i = 0; i < receivers.length; i++) {
            _buy(receivers[i]);
        }

        emit AirdropSuccess();
    }

    function mint(bytes calldata signature) external payable nonReentrant {
        require(open, "Contract is closed");
        require(msg.value >= mintPrice, "ERC721: mint from insuficient funds");
        if (whitelisted) {
            require(
                Signature.verify(signerAddress, signature, 1),
                "Address is not whitelisted"
            );
        }
        _buy(msg.sender);
        payPlatformRoyalty(mintPrice);
    }

    function crossMint(address to) external payable nonReentrant {
        require(open, "Contract is closed");
        require(msg.sender == gateway, "Mint from invalid gateway");
        _buy(to);
        payPlatformRoyalty(mintPrice);
    }

    function _buy(address to) internal {
        uint256 minted = _tokenIdCounter.current();
        require(maxSupply >= minted, "ERC721: mint for supply limit");
        _safeMint(to, minted);
        _tokenIdCounter.increment();
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function withdrawDistribution(uint256 amount)
        external
        payable
        nonReentrant
        noZeroValue(amount)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        distribute(amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function _burn(uint256 tokenId)
        internal
        override(ERC721, ERC721URIStorage)
    {
        super._burn(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

