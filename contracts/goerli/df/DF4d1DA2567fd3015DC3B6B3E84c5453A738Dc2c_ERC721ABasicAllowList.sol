import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";

import "../custom/Modifiers.sol";

/**
 * @title ERC20BasicUpgradeableV2
 * @dev V2 contract for testing purpose only!
 */
contract ERC20BasicUpgradeableV2 is
    Initializable,
    Modifiers,
    ERC20Upgradeable,
    ERC20BurnableUpgradeable,
    PausableUpgradeable,
    AccessControlUpgradeable,
    ReentrancyGuardUpgradeable,
    UUPSUpgradeable
{
    uint8 private _decimals;
    bool public open;
    address public withdrawalAddress;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    event PaymentReceived(address, uint256);
    event WithdrawalSuccess(uint256 amount);

    struct SettingsStruct {
        string name;
        string symbol;
        uint256 totalSupply;
        uint8 decimals;
        bool open;
        bool paused;
        address withdrawalAddress;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        string memory name,
        string memory symbol,
        uint256 initialSupply,
        address _withdrawalAddress
    ) public initializer {
        __ERC20_init(name, symbol);
        __ERC20Burnable_init();
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);

        _decimals = 18;

        _mint(_msgSender(), initialSupply);

        open = false;
        withdrawalAddress = _withdrawalAddress != address(0)
            ? _withdrawalAddress
            : _msgSender();
    }

    receive() external payable {
        emit PaymentReceived(msg.sender, msg.value);
    }

    function getSettings() external view returns (SettingsStruct memory) {
        SettingsStruct memory settings = SettingsStruct({
            name: name(),
            symbol: symbol(),
            totalSupply: totalSupply(),
            decimals: decimals(),
            open: open,
            paused: paused(),
            withdrawalAddress: withdrawalAddress
        });
        return settings;
    }

    function setWithdrawalAddress(address _newAddress)
        external
        noZeroAddress(_newAddress)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        withdrawalAddress = _newAddress;
    }

    function setOpen(bool _open) external onlyRole(DEFAULT_ADMIN_ROLE) {
        open = _open;
    }

    function setDecimals(uint8 newDecimals)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        noZeroValue(newDecimals)
    {
        _decimals = newDecimals;
    }

    function decimals() public view virtual override returns (uint8) {
        return _decimals;
    }

    function pause() external onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function mint(address to, uint256 amount)
        external
        nonReentrant
        noZeroValue(amount)
    {
        require(open, "Contract is closed");
        _mint(to, amount);
    }

    function burn(uint256 amount) public virtual override noZeroValue(amount) {
        _burn(_msgSender(), amount);
    }

    function withdraw(uint256 amount)
        external
        payable
        nonReentrant
        noZeroValue(amount)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        AddressUpgradeable.sendValue(payable(withdrawalAddress), amount);
        emit WithdrawalSuccess(amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, amount);
    }

    function transferInternal(
        address from,
        address to,
        uint256 amount
    ) public nonReentrant noZeroValue(amount) {
        _transfer(from, to, amount);
    }

    function approveInternal(
        address owner,
        address spender,
        uint256 amount
    ) public noZeroValue(amount) {
        _approve(owner, spender, amount);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(UPGRADER_ROLE)
    {}

    // Added this function to V2
    function mintFive(address to) external nonReentrant {
        require(open, "Contract is closed");
        _mint(to, 5);
    }
}

