contract RentableLandNFT is ERC4907, Ownable, VRFConsumerBaseV2 {
    
    using Counters for Counters.Counter;
    using SafeMath for uint;

    //Common, rare, legend token id counters
    Counters.Counter public _comTokenIdCounter;
    Counters.Counter public _rareTokenIdCounter;
    Counters.Counter public _legendTokenIdCounter;

    //usdt, zomfi token addresses

    IERC20 usdt = IERC20(0x509Ee0d083DdF8AC028f2a56731412edD63223B9);
    IERC20 zomfi = IERC20(0x509Ee0d083DdF8AC028f2a56731412edD63223B9);

    uint256 public baseAmount = 20000000000000;
    uint256 public limit = 1000;

    uint256[] private discount_rates = [20, 10, 5, 0];
    uint256 private wl_period = 86400;

    //chainlink configurations

    VRFCoordinatorV2Interface COORDINATOR;

    // Subscription ID.
    uint64 public s_subscriptionId;
    
    // Coordinator value 
    address public vrfCoordinator;
    
    // The gas lane to use, which specifies the maximum gas price to bump to.
    bytes32 keyhash;

    // Depends on the number of requested values that you want sent to the
    // fulfillRandomWords() function. Storing each word costs about 20,000 gas,
    // so 100,000 is a safe default for this example contract. Test and adjust
    // this limit based on the network that you select, the size of the request,
    // and the processing of the callback request in the fulfillRandomWords()
    // function.
    
    uint32 callbackGasLimit = 100000;

    // Confirmation value

    uint16 requestConfirmations = 3;

    // Number of values should be retrieved in one request

    uint32 numWords = 1;

    uint256 public s_randomValue;
    uint256 public s_requestId;

    struct allocatedNFT {
        bool isAllocated;
        uint256 common;
        uint256 rare;
        uint256 legend;
    }

    //nftPrice structure

    struct nftPrice {
        uint256 common_eth;
        uint256 rare_eth;
        uint256 legend_eth;
        uint256 common_usdt;
        uint256 rare_usdt;
        uint256 legend_usdt;
        uint256 common_zomfi;
        uint256 rare_zomfi;
        uint256 legend_zomfi;
    }

    struct mintedNFT {
        uint256 common;
        uint256 rare;
        uint256 legend;
    }

    struct RentableItem {
        bool rentable;
        uint256 amountPerMinute;
    }

    mapping(uint256 => mapping(address => allocatedNFT)) public _AllocatedNFT;
    
    mapping(address => mintedNFT) private _mintedNFT;

    mapping(address => uint256) public remainlimit;

    mapping(address => bool) private isManager;

    mapping(string => bool) public isAlreadyListed;

    mapping(uint256 => RentableItem) public rentables;

    uint256 public totCommon;
    uint256 public totLegend;
    uint256 public totRare;
    uint256 public totMythical;

    uint256 public minCommon;
    uint256 public minLegend;
    uint256 public minRare;

    nftPrice prices;

    // struct NFTforSale{
    //     uint256 tokenid;
    //     address owner;
    //     uint256 amount;
    //     bool isForSale;
    // }

    // mapping(uint256 => NFTforSale) private _nftforSale;

    event MINTNFT(address owner, uint256 token_id, uint256 reaminNFT);
    constructor(uint64 _s_subscriptionId, address _vrfCoordinator, bytes32 _keyhash) VRFConsumerBaseV2(_vrfCoordinator) ERC4907("Zomfi Land", "ZOMFIL") {
        totCommon = 6000;
        totRare = 3000;
        totLegend = 990;
        totMythical = 10;
        prices.common_eth = 1 ether;
        prices.rare_eth = 3 ether;
        prices.legend_eth = 5 ether;
        prices.common_usdt = 2500000000000000000000;
        prices.rare_usdt = 7500000000000000000000;
        prices.legend_usdt = 12500000000000000000000;
        prices.common_zomfi = 2500000000000000000000;
        prices.rare_zomfi = 7500000000000000000000;
        prices.legend_zomfi = 12500000000000000000000;
        COORDINATOR =  VRFCoordinatorV2Interface(_vrfCoordinator);
        s_subscriptionId = _s_subscriptionId;
        keyhash = _keyhash;
    }

    function rent(uint256 _tokenId, uint64 _expires) public payable virtual {
        uint256 dueAmount = rentables[_tokenId].amountPerMinute * _expires;
        require(msg.value == dueAmount, "Uncorrect amount");
        require(userOf(_tokenId) == address(0), "Already rented");
        require(rentables[_tokenId].rentable, "Renting disabled for the NFT");
        payable(ownerOf(_tokenId)).transfer(dueAmount);
        UserInfo storage info = _users[_tokenId];
        info.user = msg.sender;
        info.expires = block.timestamp + (_expires * 60);
        emit UpdateUser(_tokenId, msg.sender, _expires);
    }

    function setRentFee(uint256 _tokenId, uint256 _amountPerMinute) public {
        require(_isApprovedOrOwner(_msgSender(), _tokenId), "Caller is not token owner nor approved");
        rentables[_tokenId].amountPerMinute = _amountPerMinute;
    }

    function setRentable(uint256 _tokenId, bool _rentable) public {
        require(_isApprovedOrOwner(_msgSender(), _tokenId), "Caller is not token owner nor approved");
        rentables[_tokenId].rentable = _rentable;
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://ipfs.io/ipfs/";
    }

    // function sellNFT(uint256 _amount, uint256 _tokenid) public {
    //     require(msg.sender == ownerOf(_tokenid), "You are not the owner");
    //     require(_nftforSale[_tokenid].isForSale == false, "Already Listed For Sale");
    //     require(getApproved(_tokenid) == address(this), "ERC721: Not yet Approved");

    //     _nftforSale[_tokenid].isForSale = true;
    //     _nftforSale[_tokenid].tokenid = _tokenid;
    //     _nftforSale[_tokenid].amount = _amount;
    //     _nftforSale[_tokenid].owner = msg.sender;

    //     transferFrom(msg.sender, address(this), _tokenid);

    //     emit SellNFT(msg.sender, _amount, _tokenid);
    // }

    // function buyNFT(uint256 _tokenid) public {
    //     require(_nftforSale[_tokenid].amount <= token.balanceOf(msg.sender), "ERC20: There is no Balance!");
    //     uint256 approvedamount = token.allowance(msg.sender, address(this));
    //     require(_nftforSale[_tokenid].amount == approvedamount, "ERC20: Not yet Approved");
    //     require(_nftforSale[_tokenid].isForSale == true, "Not Listed For Sale");

    //     (bool suc) = token.transferFrom(msg.sender, address(this), _nftforSale[_tokenid].amount);

    //     if(suc){
    //         _transfer(address(this), msg.sender, _tokenid);

    //         (bool suc2) = token.transfer(_nftforSale[_tokenid].owner, _nftforSale[_tokenid].amount);

    //         if(suc2){
    //             _nftforSale[_tokenid].isForSale = false;
    //             _nftforSale[_tokenid].tokenid = _tokenid;
    //             _nftforSale[_tokenid].owner = msg.sender;
    //             _nftforSale[_tokenid].amount = 0;

    //             emit BuyedNFT(msg.sender, _nftforSale[_tokenid].amount, _tokenid);
    //         }
    //     }
    // }

    function mint(
        uint256 mint_type, 
        uint256 pay_type, 
        uint256 com, 
        uint256 rare, 
        uint256 legend
    ) external payable {
        //token type is 0: common, 1: rare, 2: legend
        //if random number is equal to certain number x, mythical nft can be minted.
        //pay_type is 0: ether, 1: usdt, 2: zomfi
        //mint type is 0: vcs_dao, 2: guilds, 3: whitelist, 4: public sale
        bool suc = false;
        uint256 totalPrice;
        require(totCommon>=_comTokenIdCounter.current()+com+1, "No remaining common nfts");
        require(totRare>=_rareTokenIdCounter.current()+rare+1, "No remaining rare nfts");
        require(totLegend>=_legendTokenIdCounter.current()+legend+1,"No remaining legend nfts");
        // check allocated nft
        if(mint_type < 3) {
            require(((_AllocatedNFT[mint_type][msg.sender].common >= com) && (_AllocatedNFT[mint_type][msg.sender].rare >= rare) && (_AllocatedNFT[mint_type][msg.sender].legend >= legend)), "Invalid values");
        }        
        if(pay_type == 0) {
            totalPrice = (prices.common_eth * com + prices.rare_eth * rare + prices.legend_eth * legend) * (100-discount_rates[mint_type])/100;
            require(msg.value > totalPrice, "Low funds");
        } else if (pay_type == 1) {
            totalPrice = (prices.common_usdt * com + prices.rare_usdt * rare + prices.legend_usdt * legend) * (100-discount_rates[mint_type])/100;
            uint256 approved_amount = usdt.allowance(msg.sender, address(this));
            require(approved_amount >= totalPrice, "Low funds");
            suc = usdt.transferFrom(msg.sender, address(this), totalPrice);
        } else if (pay_type == 2) {
            totalPrice = (prices.common_usdt * com + prices.rare_usdt * rare + prices.legend_usdt * legend) * (100-discount_rates[mint_type])/100;
            uint256 approved_amount = usdt.allowance(msg.sender, address(this));
            require(approved_amount >= totalPrice, "Low funds");
            suc = usdt.transferFrom(msg.sender, address(this), totalPrice);
        }
        if(suc) {
            if( com > 0) {
                for(uint256 i=0; i<com; i++) {
                    uint256 tokenId = _comTokenIdCounter.current();
                    _comTokenIdCounter.increment();
                    _safeMint(msg.sender, tokenId);
                }
            } else if ( rare > 0) {
                for(uint256 i=0; i<rare; i++) {
                    uint256 tokenId = totCommon+_rareTokenIdCounter.current();
                    _rareTokenIdCounter.increment();
                    _safeMint(msg.sender, tokenId);
                }
            } else if ( legend > 0) {
                for(uint256 i=0; i<legend; i++) {
                    uint256 tokenId = totCommon + totRare + _legendTokenIdCounter.current();
                    _legendTokenIdCounter.increment();
                    _safeMint(msg.sender, tokenId);
                }
            }
        }
    }

    function _burn(uint256 tokenId) internal override(ERC721) {
        require(msg.sender == ownerOf(tokenId), "You are not the Owner");
        super._burn(tokenId);
    }

    // Assumes the subscription is funded sufficiently

    function requestRandomWords() internal {
        s_requestId = COORDINATOR.requestRandomWords(keyhash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
    }

    // Get random value

    function fulfillRandomWords(uint256, uint256[] memory randomWords) internal override {
        s_randomValue = (randomWords[0] % 10000) +1;
    }

    // function getTheListedNFTs(uint256 tokenid) public view returns (address, uint256, uint256, string memory){
    //     require(_nftforSale[tokenid].isForSale == true, "Not for Sale");
    //     return (_nftforSale[tokenid].owner, tokenid, _nftforSale[tokenid].amount, tokenURI(tokenid));
    // }

    function getTotalNFTs() public view returns (uint256){
        return (_comTokenIdCounter.current() + _rareTokenIdCounter.current() + _legendTokenIdCounter.current());
    }

    function getMyNFTCount() public view returns(uint256, uint256 , uint256){
        return (_mintedNFT[msg.sender].common, _mintedNFT[msg.sender].rare, _mintedNFT[msg.sender].legend);
    }

    // function isNFTisForSale(uint256 tokenId) public view returns(bool){
    //     return _nftforSale[tokenId].isForSale;
    // }

    /// Get amounts of each allocated nfts
    /// @param allocate_type value of allocate type
    /// @return amount values
    function getAllocatedAmount(uint256 allocate_type) public view returns(uint256, uint256, uint256) {
        require(allocate_type<3, "Invaild allocate type");
        return(_AllocatedNFT[allocate_type][msg.sender].common, _AllocatedNFT[allocate_type][msg.sender].rare, _AllocatedNFT[allocate_type][msg.sender].legend);
    }

    /// Get discount rates
    function getDiscountRate(uint256 allocate_type) public view returns (uint256) {
        require(allocate_type<3, "Invalid allocate types");
        return discount_rates[allocate_type];
    }

    /// Check whether inputed address is manager or not.
    /// @param _addr address that should be checked.
    /// @dev retrieves the result of checking address.
    /// @return bool the result of checking address.

    function isManagerOfContract(address _addr) public view returns(bool){
        return (isManager[_addr]);
    }

    /// Add wallet address to DAO, Guild, White list.
    /// @param allocate_type value that represent one of the allocate types.
    /// @param _addr wallet address should be added to allocate list.
    /// @param _com amount of common nfts that can be allocated.
    /// @param _rare amount of rare nfts that can be allocated.
    /// @param _legend amount of legend nfts that can be allocated.
    /// @dev stores the address, amount of com, rare, legend nfts that can be allocated.

    function whiteListNFTByManager(uint256 allocate_type, address _addr, uint256 _com, uint256 _rare, uint256 _legend) public {
        require(isManager[msg.sender] == true, "You are not the Manager!");
        require(allocate_type<3, "Invalid allocate_type");
        _AllocatedNFT[allocate_type][_addr].common = _com;
        _AllocatedNFT[allocate_type][_addr].rare = _rare;
        _AllocatedNFT[allocate_type][_addr].legend = _legend;
        _AllocatedNFT[allocate_type][_addr].isAllocated = true;
    }

    /// Add address, com, rare, legend amount to DAOs, Guilds, Whitelists
    /// @param allocate_type value that represent DAOs, Guilds, Whitelist
    /// @param _addr address arrays that should be added.
    /// @param _com com amount arrays that should be added.
    /// @param _rare rare amount arrays that should be added.
    /// @dev Add address, com, rare, legend amount to lists.

    function BulkwhiteListNFTByManager(uint256 allocate_type, address[] calldata _addr, uint256[] calldata _com, uint256[] calldata _rare, uint256[] calldata _legend) public {
        require(isManager[msg.sender] == true, "You are not the Manager!");
        require(allocate_type < 3, "Invalid allocate_type");
        for(uint256 j = 0; j <_addr.length;j++) {
            _AllocatedNFT[allocate_type][_addr[j]].common = _com[j];
            _AllocatedNFT[allocate_type][_addr[j]].rare = _rare[j];
            _AllocatedNFT[allocate_type][_addr[j]].legend = _legend[j];
            _AllocatedNFT[allocate_type][_addr[j]].isAllocated = true;
        }
    }
    //---------------------------------------------Owner Functions----------------------------------------

    // function withdrawAmount(address _adr) public onlyOwner {
    //     token.transfer(_adr, token.balanceOf(address(this)));
    // }

    /// Add address to manager list.
    /// @param _addr address should be added to manager list.
    /// @dev adds address to manager list

    function addManager(address _addr) public onlyOwner {
        isManager[_addr] = true;
    }

    /// Revoke address from manager list.
    /// @param _addr address should be revoked from manager list.
    /// @dev revoke address from manager list.
    
    function revokeManager(address _addr) public onlyOwner{
        isManager[_addr] = false;
    }
}

