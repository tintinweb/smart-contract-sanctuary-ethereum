// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";

contract StakingLottery is
    ERC1155Holder,
    ERC721Holder,
    AccessControl,
    Ownable,
    Pausable,
    ReentrancyGuard,
    VRFConsumerBaseV2
{
    /// @notice byte32 for ADMIN_ROLE
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    using SafeERC20 for IERC20;

    /// @notice interFace for staking erc20 token
    IERC20 internal token;

    /// @notice byte4 for ERC-1155
    bytes4 public constant ERC1155interfaceID = 0xd9b67a26;

    /// @notice byte4 for ERC-721
    bytes4 public constant ERC721interfaceID = 0x80ac58cd;

    /// @notice mapping for each lottery to every user staking status
    mapping(uint256 => mapping(address => UserStatus)) public stakingStatus;

    /// @notice mapping for each user to number of token they staked in this contract
    mapping(address => uint256) public stakedAmount;

    /// @notice struct for user status
    struct UserStatus {
        address user;
        uint256 balance;
        uint256 ticketNum;
    }

    /// @notice struct for ticket distribution struct
    struct TicketDistributionStruct {
        address playerAddress;
        uint256 startIndex; // inclusive
        uint256 endIndex; // inclusive
    }

    /// @notice struct for a lottery
    struct Lottery {
        address nft;
        uint256 nftID;
        address walletHavingNFT;
        uint256 minDeposit;
        uint256 openTime;
        uint256 closeTime;
        address[] participants;
        uint256 maxLoops;
        uint256 loopCount;
        uint256 totalTickets;
        address winner;
    }

    mapping(uint256 => uint256) public wintickets;

    /// @notice each lottery map to their ticket distribution struct
    mapping(uint256 => TicketDistributionStruct[]) public ticketDistribution;

    /// @notice Lottery type array to record all lotteries
    Lottery[] public lotteries;

    /// @notice stage for lottery
    enum Status {
        OPEN,
        WAIT_TO_CLAIM,
        CLAIMED
    }

    /// @notice event for create a lottery
    event LotteryCreated(
        uint256 id,
        address nft,
        uint256 nftID,
        address walletHavingNFT,
        uint256 minDeposit,
        uint256 openTime,
        uint256 closeTime
    );

    /// @notice interface for chainlink VRFCoordinator
    VRFCoordinatorV2Interface COORDINATOR;

    /// @notice interface for LINK token
    LinkTokenInterface LINKTOKEN;

    /// @notice struct for request
    struct RequestStatus {
        bool exist;
        bool fulfilled; // whether the request has been successfully fulfilled
        uint256[] randomWords;
    }

    /// @notice id for subscripttion in VRFCoordinator
    uint64 public subscriptionId;

    /// @notice keyHash for calculate fee
    mapping(uint256 => RequestStatus) public s_requests; /* requestId --> requestStatus */

    /// @notice array request ids
    uint256[] public requestIds;

    /// @notice id for latest request
    uint256 public lastRequestId;

    /// @notice keyHash for calculate fee
    mapping(uint256 => uint256) private lotteryRandomnessRequest;

    /// @notice keyHash for calculate fee
    bytes32 keyHash = 0x79d3d8832d904592c0bf9818b621522c988bb8b0c05cdc3b15aea1b6e8db0c15;

    // Depends on the number of requested values that you want sent to the
    // fulfillRandomWords() function. Test and adjust
    // this limit based on the network that you select, the size of the request,
    // and the processing of the callback request in the fulfillRandomWords()
    // function.
    /// @notice gas limit for VRFCoordinator call fulfillRandomWords() in this contract
    uint32 callbackGasLimit = 200000;

    // The default is 3, but you can set this higher.
    /// @notice number for confirmation wait random number confirmed
    uint16 requestConfirmations = 3;

    // For this example, retrieve 1 random values in one request.
    // Cannot exceed VRFCoordinatorV2.MAX_NUM_WORDS.
    /// @notice how many random number from once request
    uint32 numWords = 1;

    /// @notice event for random number request sent to VRFCoordinator
    event RequestSent(uint256 requestId, uint32 numWords);

    /// @notice event for random number fulfilled by VRFCoordinator
    event RequestFulfilled(uint256 requestId, uint256[] randomWords);

    /// @notice event for stake in a lottery
    event Stake(uint256 id, address indexed user, uint256 amount);

    /// @notice event for draw a winner in a lottery
    event Draw(uint256 id);

    /// @notice event for withdraw staked token
    event Withdraw(uint256 id, address indexed user, uint256 amount);

    /**
     * @notice Construct a staking ERC20 token lottery contract
     * @param tokenAddress Address of erc20 token for staking
     */
    constructor(
        address tokenAddress,
        address _coordinatorAddress,
        uint64 _subscriptionId
    ) VRFConsumerBaseV2(_coordinatorAddress) {
        token = IERC20(tokenAddress);
        COORDINATOR = VRFCoordinatorV2Interface(_coordinatorAddress);
        subscriptionId = _subscriptionId;
        _setupRole(ADMIN_ROLE, _msgSender());
    }

    /**
     * @notice create a lottery
     * @param nft a nft address of lottery reward (ERC721 or ERC1155)
     * @param nftID an id of reward nft
     * @param walletHavingNFT an address having this nft
     * @param minDeposit a minimun token number need to be staked in lottery to attend lottery
     * @param openTime a number for lottery open time
     * @param closeTime a number for lottery close time
     */
    function create(
        address nft,
        uint256 nftID,
        address walletHavingNFT,
        uint256 minDeposit,
        uint256 openTime,
        uint256 closeTime
    ) external onlyRole(ADMIN_ROLE) whenNotPaused {
        require(closeTime > openTime, "closeTime should after openTime");
        require(
            ERC165Checker.supportsInterface(nft, ERC721interfaceID) ||
                ERC165Checker.supportsInterface(nft, ERC1155interfaceID),
            "nft should support ERC721 or ERC1155"
        );
        address[] memory participants = new address[](0);
        Lottery memory lottery = Lottery(
            nft,
            nftID,
            walletHavingNFT,
            minDeposit,
            openTime,
            closeTime,
            participants,
            10,
            0,
            0,
            address(this)
        );
        lotteries.push(lottery);
        uint256 id = lotteries.length - 1;

        emit LotteryCreated(
            id,
            nft,
            nftID,
            walletHavingNFT,
            minDeposit,
            openTime,
            closeTime
        );
    }

    /**
     * @notice stake token in a lottery to get ticket to win lottery
     * @param id an id for which lottery
     * @param amount a number for how many tokens will stake
     */
    function stake(uint256 id, uint256 amount) external whenNotPaused nonReentrant {
        require(
            block.timestamp >= lotteries[id].openTime && block.timestamp <= lotteries[id].closeTime,
            "lottery not opening"
        );
        uint256 oldTicketNum = stakingStatus[id][_msgSender()].ticketNum;
        stakingStatus[id][_msgSender()].balance += amount;
        stakingStatus[id][_msgSender()].user = _msgSender();
        stakedAmount[_msgSender()] += amount;
        require(
            stakingStatus[id][_msgSender()].balance >= lotteries[id].minDeposit,
            "need over minDeposit"
        );
        if (stakingStatus[id][_msgSender()].ticketNum == 0) {
            lotteries[id].participants.push(_msgSender());
        }

        stakingStatus[id][_msgSender()].ticketNum =
            stakingStatus[id][_msgSender()].balance /
            lotteries[id].minDeposit;
        lotteries[id].totalTickets += stakingStatus[id][_msgSender()].ticketNum - oldTicketNum;
        token.safeTransferFrom(_msgSender(), address(this), amount);
        emit Stake(id, _msgSender(), amount);
    }

    /**
     * @notice return staking balance of msg.sender
     * @param id an id for which lottery
     */
    function myStake(uint256 id) external view returns (uint256) {
        return stakingStatus[id][_msgSender()].balance;
    }

    /**
     * @notice return all staking balance of participants in a lottery
     * @param id an id for which lottery
     */
    function allStake(uint256 id) external view returns (UserStatus[] memory) {
        address[] memory participants = lotteries[id].participants;
        UserStatus[] memory allStatus = new UserStatus[](participants.length);
        for (uint256 i = 0; i < participants.length; i++) {
            UserStatus memory status = stakingStatus[id][participants[i]];
            allStatus[i] = status;
        }
        return allStatus;
    }

    /**
     * @notice change minDeposit number of a lottery
     * @param id an id for which lottery
     * @param minDeposit a new number for minDeposit
     */
    function changeMinDeposit(uint256 id, uint256 minDeposit) external onlyRole(ADMIN_ROLE) {
        lotteries[id].minDeposit = minDeposit;
    }

    /**
     * @notice withdraw staked toen after lottery close
     * @param id an id for which lottery
     * @param amount a number for how many token will be withdrawed
     */
    function withdraw(uint256 id, uint256 amount) external whenNotPaused nonReentrant {
        require(block.timestamp > lotteries[id].closeTime, "wait lottery closed");
        require(stakingStatus[id][_msgSender()].balance >= amount, "no enough amount to withdraw");
        stakedAmount[_msgSender()] -= amount;
        stakingStatus[id][_msgSender()].balance -= amount;
        token.safeTransfer(_msgSender(), amount);
        emit Withdraw(id, _msgSender(), amount);
    }

    /**
     * @notice winner withdraw reward from lottery
     * @param id an id for which lottery
     * @return a bool for success or not
     */
    function winnerWithdraw(uint256 id) external whenNotPaused nonReentrant returns (bool) {
        Lottery memory lottery = lotteries[id];
        require(_msgSender() == lottery.winner, "you are not winner");
        if (ERC165Checker.supportsInterface(lottery.nft, ERC721interfaceID)) {
            IERC721(lottery.nft).safeTransferFrom(address(this), _msgSender(), lottery.nftID);
        } else if (ERC165Checker.supportsInterface(lottery.nft, ERC1155interfaceID)) {
            IERC1155(lottery.nft).safeTransferFrom(
                address(this),
                _msgSender(),
                lottery.nftID,
                1,
                bytes("0")
            );
        } else {
            revert("not support type");
        }

        return true;
    }

    /**
     * @notice transfer staked token to next lottery
     * @param id an id for transfer stake from which lottery
     * @param targetId an id for which lottery should transfer to
     * @return a bool for success or not
     */
    function transferAllStake(uint256 id, uint256 targetId) external whenNotPaused onlyRole(ADMIN_ROLE) returns (bool) {
        require(
            block.timestamp > lotteries[id].closeTime,
            "old lottery is still opening"
        );
        require(
            block.timestamp >= lotteries[targetId].openTime &&
                block.timestamp <= lotteries[targetId].closeTime,
            "new lottery not opening"
        );
        address[] memory participants = lotteries[id].participants;
        for (uint256 i = 0; i < participants.length; i++) {
            address user = participants[i];
            stakingStatus[targetId][user].balance += stakingStatus[id][user].balance;
            stakingStatus[targetId][user].user = user;

            if (stakingStatus[targetId][user].ticketNum == 0) {
                lotteries[targetId].participants.push(user);
            }
            stakingStatus[targetId][user].ticketNum +=
                stakingStatus[targetId][user].balance /
                lotteries[targetId].minDeposit;
            lotteries[targetId].totalTickets += stakingStatus[targetId][user].balance / lotteries[targetId].minDeposit;
        }
        
        return true;
    }

    /**
     * @notice randomly pick a winner of a lottery
     * @param id an id for which lottery
     * @return an address of winner
     */
    function draw(uint256 id) external whenNotPaused returns (address) {
        require(block.timestamp > lotteries[id].closeTime, "wait lottery closed");
        require(lotteries[id].winner == address(this), "can not pick a winner again");
        _playerTicketDistribution(id);
        requestRandomWords(id);
        emit Draw(id);

        return lotteries[id].winner;
    }

    /**
     * @notice send request for random number to VRFCoordinator
     * @param _lotteryId an id for which lottery
     * @return requestId
     */
    function requestRandomWords(uint256 _lotteryId) public returns (uint256 requestId) {
        requestId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        lotteryRandomnessRequest[requestId] = _lotteryId;
        s_requests[requestId] = RequestStatus({
            exist: true,
            fulfilled: false,
            randomWords: new uint256[](0)
        });
        requestIds.push(requestId);
        lastRequestId = requestId;
        emit RequestSent(requestId, 1);
        return requestId;
    }

    /**
     * @notice callback function to let VRFCoordinator call after VRFCoordinator fulfilled random number
     * @param _requestId an id for request
     * @param _randomWords a randomnumber array from VRFCoordinator
     */
    function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords)
        internal
        override
    {
        uint256 _lotteryId = lotteryRandomnessRequest[_requestId];
        wintickets[_lotteryId] = _randomWords[0] % lotteries[_lotteryId].totalTickets;
        require(s_requests[_requestId].exist, "request not found");
        s_requests[_requestId].fulfilled = true;
        s_requests[_requestId].randomWords = _randomWords;
        emit RequestFulfilled(_requestId, _randomWords);
    }

    /**
     * @notice get request status by `_requestId`
     * @param _requestId an id for request
     * @return fulfilled and randomWords of request
     */
    function getRequestStatus(uint256 _requestId)
        external
        view
        returns (bool fulfilled, uint256[] memory randomWords)
    {
        require(s_requests[_requestId].exist, "request not found");
        RequestStatus memory request = s_requests[_requestId];
        return (request.fulfilled, request.randomWords);
    }

    /**
     * @notice disbute tickets to prepare doing propotional lottery
     * @param id an id for which lottery
     */
    function _playerTicketDistribution(uint256 id) private {
        address[] memory _players = lotteries[id].participants;
        uint256 _ticketIndex = 0;
        for (uint256 i = 0; i < _players.length; i++) {
            address _playerAddress = _players[i];
            uint256 _numTickets = stakingStatus[id][_playerAddress].ticketNum;

            TicketDistributionStruct memory newDistribution = TicketDistributionStruct({
                playerAddress: _playerAddress,
                startIndex: _ticketIndex,
                endIndex: _ticketIndex + _numTickets - 1 // sub 1 to account for array indices starting from 0
            });
            ticketDistribution[id].push(newDistribution);

            stakingStatus[id][_playerAddress].ticketNum = 0; // reset player's tickets to 0 after they've been counted
            _ticketIndex = _ticketIndex + _numTickets;
        }
    }

    /**
     * @notice find address of winner by ticket index
     * @param id an id for which lottery
     * @return an address of winner
     */
    function findWinnerAddress(uint256 id) external returns (address) {
        require(lotteries[id].winner == address(this), "can not pick a winner again");
        // trivial case, no search required
        uint256 _numPlayers = lotteries[id].participants.length;
        if (_numPlayers == 1) {
            lotteries[id].winner = ticketDistribution[id][0].playerAddress;
            return ticketDistribution[id][0].playerAddress;
        } else {
            // do binary search on ticketDistribution array to find winner
            uint256 _winningPlayerIndex = _binarySearch(id, 0, _numPlayers - 1, wintickets[id]);
            if (_winningPlayerIndex >= _numPlayers) {
                // sanity check
                revert("invalid winner index");
            }
            lotteries[id].winner = ticketDistribution[id][_winningPlayerIndex].playerAddress;

            return ticketDistribution[id][_winningPlayerIndex].playerAddress;
        }
    }

    /**
     * @notice binary search to find win ticket index
     * @param id an id for which lottery
     * @param leftIndex_ left bound index for binary search
     * @param rightIndex_ left bound index for binary search
     * @param ticketIndexToFind_ index of win ticket
     * @return an index of ticketDistributionStruct
     */
    function _binarySearch(
        uint256 id,
        uint256 leftIndex_,
        uint256 rightIndex_,
        uint256 ticketIndexToFind_
    ) private returns (uint256) {
        uint256 _searchIndex = (rightIndex_ - leftIndex_) / (2) + (leftIndex_);
        uint256 _loopCount = lotteries[id].loopCount;
        // counter
        lotteries[id].loopCount = _loopCount + 1;
        if (_loopCount + 1 > lotteries[id].maxLoops) {
            // emergency stop in case infinite loop due to unforeseen bug
            return lotteries[id].participants.length + 1;
        }

        if (
            ticketDistribution[id][_searchIndex].startIndex <= ticketIndexToFind_ &&
            ticketDistribution[id][_searchIndex].endIndex >= ticketIndexToFind_
        ) {
            return _searchIndex;
        } else if (ticketDistribution[id][_searchIndex].startIndex > ticketIndexToFind_) {
            // go to left subarray
            rightIndex_ = _searchIndex - 1;

            return _binarySearch(id, leftIndex_, rightIndex_, ticketIndexToFind_);
        } else if (ticketDistribution[id][_searchIndex].endIndex < ticketIndexToFind_) {
            // go to right subarray
            leftIndex_ = _searchIndex + 1;
            return _binarySearch(id, leftIndex_, rightIndex_, ticketIndexToFind_);
        }

        // if nothing found (bug), return an impossible player index
        return lotteries[id].participants.length + 1;
    }

    /**
     * @notice return what stage of a lottery is now
     * @param id an id for which lottery
     * @return Status for a lottery
     */
    function getLotteryStatus(uint256 id) external view returns (Status) {
        Lottery memory lottery = lotteries[id];
        if (lottery.winner == address(this) && block.timestamp > lottery.closeTime) {
            return Status.WAIT_TO_CLAIM;
        } else if (lottery.winner != address(this)) {
            return Status.CLAIMED;
        } else {
            return Status.OPEN;
        }
    }

    /**
     * @notice return number of how many lotteries
     * @return a number of how many lotteries
     */
    function getLotteryLength() external view returns (uint256) {
        return lotteries.length;
    }

    /**
     * @notice return participants of a lottery
     * @param id an id for which lottery
     * @return an array of participants in a lottery
     */
    function getLotteryParticipants(uint256 id) external view returns (address[] memory) {
        return lotteries[id].participants;
    }

    /**
     * @notice give admin role to `user`
     * @param user An address of an user
     */
    function addAdminUser(address user) external onlyOwner {
        _setupRole(ADMIN_ROLE, user);
    }

    /**
     * @notice remove admin role of `user`
     * @param user An address of an user
     */
    function removeAdminUser(address user) external onlyOwner {
        _revokeRole(ADMIN_ROLE, user);
    }

    /**
     * @notice pause some main function of this contract in imergency condiction
     */
    function pause() external onlyRole(ADMIN_ROLE) {
        _pause();
    }

    /**
     * @notice unpause some main function of this contract in imergency condiction
     */
    function unpause() external onlyRole(ADMIN_ROLE) {
        _unpause();
    }

    /**
     * @notice transfer ownerShip of this contract to `newOwner`
     * @param newOwner An address of an user
     */
    function transferOwnership(address newOwner) public override onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        _transferOwnership(newOwner);
        _setupRole(ADMIN_ROLE, newOwner);
        _revokeRole(ADMIN_ROLE, msg.sender);
    }

    /**
     * @notice check contract support any EIP
     * @param interfaceId A `interfaceId` bytes4 data present EIP
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(AccessControl, ERC1155Receiver)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
