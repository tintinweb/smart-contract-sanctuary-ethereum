pragma solidity 0.8.7;

import {FxBaseRootTunnel} from "./FxBaseRootTunnel.sol";
import "./ICustomFxERC721.sol";
import "./ERC721.sol";

contract FxERC721CustomRootTunnel is FxBaseRootTunnel {
    bytes32 public constant WITHDRAW = keccak256("WITHDRAW");
    bytes32 public constant WITHDRAW_BATCH = keccak256("WITHDRAW_BATCH");
    bytes32 public constant DEPOSIT = keccak256("DEPOSIT");
    bytes32 public constant DEPOSIT_BATCH = keccak256("DEPOSIT_BATCH");
    address public childToken;
    address public rootToken;

    event TokenMappedERC721(address indexed rootToken, address indexed childToken);
    event Withdraw(
        string orderId,
        address owner,
        address receiver,
        uint256 id
    );
    event WithdrawBatch(
        string orderId,
        address owner,
        address receiver,
        uint256[] ids
    );
    
    event Deposit(
        string orderId,
        address receiver,
        uint256 id
    );
    
    event DepositBatch(
        string orderId,
        address receiver,
        uint256[] ids
    );

    constructor(
        address _checkpointManager,
        address _fxRoot,
        address _childTunnel,
        address _rootToken,
        address _childToken
    ) FxBaseRootTunnel(_checkpointManager, _fxRoot) {
        // compute child token template code hash
        _setFxChildTunnel(_childTunnel);
        rootToken = _rootToken;
        childToken = _childToken;
    }

    function withdraw(
        string memory orderId,
        uint256 tokenId
    ) external {
        _withdraw(orderId, tokenId, msg.sender, '');
    }
    
    function withdrawTo(
        string memory orderId,
        uint256 tokenId,
        address to
    ) external {
        _withdraw(orderId, tokenId, to, '');
    }
    
    function withdrawToWithData(
        string memory orderId,
        uint256 tokenId,
        address to,
        bytes memory data
    ) external {
        _withdraw(orderId, tokenId, to, data);
    }

    function withdrawBatch(
        string memory orderId,
        uint256[] memory tokenIds
    ) external {
        _withdrawBatch(orderId, tokenIds, msg.sender, '');
    }
    
    function withdrawBatchTo(
        string memory orderId,
        uint256[] memory tokenIds,
        address to
    ) external {
        _withdrawBatch(orderId, tokenIds, to, '');
    }
    
    function withdrawBatchToWithData(
        string memory orderId,
        uint256[] memory tokenIds,
        address to,
        bytes calldata data
    ) external {
        _withdrawBatch(orderId, tokenIds, to, data);
    }
    
    // internal functions
    function _processMessageFromChild(bytes memory data) internal override {
        (bytes32 syncType, bytes memory syncData) = abi.decode(data, (bytes32, bytes));

        if (syncType == DEPOSIT) {
            _syncDeposit(syncData);
        } else if (syncType == DEPOSIT_BATCH) {
            _syncDepositBatch(syncData);
        } else {
            revert("RootTunnel: Invalid sync type");
        }
    }
    
    function _syncDeposit(
        bytes memory syncData
    ) internal {
        (string memory orderId, address tokenAddress, address to, uint256 tokenId, bytes memory data) = abi.decode(
            syncData,
            (string, address, address, uint256, bytes)
        );
        
        require(childToken == tokenAddress, string(abi.encodePacked("RootTunnel: Invalid token address ", tokenAddress)));

        ICustomFxERC721(rootToken).mint(
            to,
            tokenId,
            data
        );
        
        emit Deposit(orderId, to, tokenId);
    }
    
    function _syncDepositBatch(
        bytes memory syncData
    ) internal {
        (string memory orderId, address tokenAddress, address to, uint256[] memory tokenIds, bytes memory data) = abi.decode(
            syncData,
            (string, address, address, uint256[], bytes)
        );
        
        require(childToken == tokenAddress, string(abi.encodePacked("RootTunnel: Invalid token address ", tokenAddress)));
        
        for (uint i=0; i<tokenIds.length; i++) {
            ICustomFxERC721(rootToken).mint(
                to,
                tokenIds[i],
                data
            );
        }
        
        emit DepositBatch(orderId, to, tokenIds);
    }
    
    function _withdraw(
        string memory orderId,
        uint256 tokenId,
        address to,
        bytes memory data
    ) internal {
        require(msg.sender == ICustomFxERC721(rootToken).ownerOf(tokenId), string(abi.encodePacked("RootTunnel: Invalid token owner ", tokenId)));
        
        // withdraw token
        ICustomFxERC721(rootToken).burn(tokenId);

        // send message to root regarding token burn
        bytes memory message = abi.encode(WITHDRAW, abi.encode(orderId, rootToken, to, tokenId, data));
        _sendMessageToChild(message);
        emit Withdraw(orderId, msg.sender, to, tokenId);
    }
    
    function _withdrawBatch(
        string memory orderId,
        uint256[] memory tokenIds,
        address to,
        bytes memory data
    ) internal {
        require(tokenIds.length <= 10, "RootTunnel: Maximum batch size is 10");
        
        // withdraw token
        for (uint i=0; i<tokenIds.length; i++) {
            require(msg.sender == ICustomFxERC721(rootToken).ownerOf(tokenIds[i]), string(abi.encodePacked("RootTunnel: Invalid token owner ", tokenIds[i])));
            ICustomFxERC721(rootToken).burn(tokenIds[i]);
        }

        // send message to root regarding token burn
        bytes memory message = abi.encode(WITHDRAW_BATCH, abi.encode(orderId, rootToken, to, tokenIds, data));
        _sendMessageToChild(message);
        emit WithdrawBatch(orderId, msg.sender, to, tokenIds);
    }
}

