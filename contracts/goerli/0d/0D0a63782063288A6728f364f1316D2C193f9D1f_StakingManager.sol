pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/governance/TimelockControllerUpgradeable.sol";

contract Timelock is TimelockControllerUpgradeable {
  constructor() {
    _disableInitializers();
  }

  function initialize(uint256 minDelay, address[] memory proposers, address[] memory executors, address admin) external virtual initializer {
    __Timelock_init(minDelay, proposers, executors, admin);
  }

  function __Timelock_init(uint256 minDelay, address[] memory proposers, address[] memory executors, address admin) internal onlyInitializing {
    __TimelockController_init(minDelay, proposers, executors, admin);
    __Timelock_init_unchained();
  }

  function __Timelock_init_unchained() internal onlyInitializing {}
}