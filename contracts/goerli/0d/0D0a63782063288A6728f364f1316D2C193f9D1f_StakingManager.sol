  }

  modifier ensureContract(address addr) {
    require(AddressUpgradeable.isContract(addr), "StakingManager: invalid contract address.");
    _;
  }

  modifier ensureValue(uint256 amount) {
    require(amount > 0, "StakingManager: invalid value.");
    _;
  }

  modifier ensureMinInitialAmount() {
    require(msg.value >= _initialStakeAmountRequired, "StakingManager: Initial staking amount under threshold.");
    _;
  }

  // --------------------------------------------------------------------------

  function getInitialAmount() external view override returns (uint256) {
    return _initialStakeAmountRequired;
  }

  function setInitialAmount(uint256 amount) external override onlyOwner ensureValue(amount) {
    _initialStakeAmountRequired = amount;
  }

  // --------------------------------------------------------------------------

  function isDaoLogicExists(address addr) external view override returns (bool) {
    return _isDaoLogicExists(addr);
  }

  function addDaoLogic(address addr) external override onlyOwner ensureContract(addr) {
    require(!_isDaoLogicExists(addr), "StakingManager: DAO logic has already added.");

    _mapDaoLogic[addr] = STATUS_ACTIVE;
    _daoLogic = addr;

    emit DaoLogicAdded(addr);
  }

  function disableDaoLogic(address addr) external override onlyOwner {
    require(_isDaoLogicExists(addr), "StakingManager: DAO logic contract does not exists.");
    require(addr != _daoLogic, "StakingManager: Could not disable an active DAO logic.");
    _mapDaoLogic[addr] = STATUS_INACTIVE;
    emit DaoLogicDisabled(addr);
  }

  function isDaoExists(address addr) external view override returns (bool) {
    return _isDaoExists(addr);
  }

  /**
   * The parameters of this function might be updated based on
   * changing parameters of initialize function or
   * the way to implement the initialize function in StakingDAO contract.
   */
  function createStakingDAO() external payable override nonReentrant ensureMinInitialAmount {
    require(_daoLogic != address(0), "StakingManager: No DAO Logic contract found.");
    require(_isDaoLogicActive(_daoLogic), "StakingManager: No active DAO Logic found.");

    // Process - START
    // 1. Create DAO Admin which support upgradeable functions
    ProxyAdmin daoProxyAdmin = new ProxyAdmin();

    // 2. Transfer ownership of DAO admin proxy to owner of StakeManager.
    daoProxyAdmin.transferOwnership(owner());
    /**
     * Notes:
     * Do NOT transfer ownership of DAO admin proxy to timelock
     * because we do not support upgrade through timelock at the moment.
     * daoProxyAdmin.transferOwnership(address(_timelock));
     */

    // 3. Create data for initialize function of StakingDAO
    bytes memory initializeData = abi.encodeWithSignature(
      "initialize(address,address,address,address,address,address)",
      _ethDepositContract,
      _ssvNetwork,
      _ssvToken,
      address(this),
      _msgSender(),
      payable(_msgSender())
    );

    // 4. Create DAO Proxy
    TransparentUpgradeableProxy daoProxy = (new TransparentUpgradeableProxy){value: msg.value}(_daoLogic, address(daoProxyAdmin), initializeData);

    // 5. Transfer DAO ownership to owner of StakeManager.
    OwnableUpgradeable daoProxyOwner = OwnableUpgradeable(address(daoProxy));
    daoProxyOwner.transferOwnership(owner());
    /**
     * Notes:
     * Do NOT transfer ownership of DAO admin proxy to timelock
     * because we do not support upgrade through timelock at the moment.
     * daoProxyOwner.transferOwnership(address(_timelock));
     */

    // 6. Set PROPOSER_ROLE and CANCELLER_ROLE of timelock for DAO
    _timelock.grantRole(_timelock.PROPOSER_ROLE(), address(daoProxy));
    _timelock.grantRole(_timelock.CANCELLER_ROLE(), address(daoProxy));
    _timelock.grantRole(_timelock.EXECUTOR_ROLE(), address(daoProxy));
    // Process -END

    _mapDaoProxy[address(daoProxy)] = STATUS_ACTIVE;

    emit DaoCreated(address(daoProxy), address(daoProxyAdmin), msg.value);
  }

  // --------------------------------------------------------------------------

  function _isDaoLogicExists(address addr) internal view returns (bool) {
    return _mapDaoLogic[addr] > STATUS_NOT_EXISTS;
  }

  function _isDaoLogicActive(address addr) internal view returns (bool) {
    return _isDaoLogicExists(addr) && _mapDaoLogic[addr] == STATUS_ACTIVE;
  }

  function _isDaoExists(address addr) internal view returns (bool) {
    return _mapDaoProxy[addr] > STATUS_NOT_EXISTS;
  }

  // --------------------------------------------------------------------------
  // GAP for upgrading later
  // --------------------------------------------------------------------------
  /**
   * @dev This empty reserved space is put in place to allow future versions to add new
   * variables without shifting down storage in the inheritance chain.
   * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
   * See https://docs.openzeppelin.com/upgrades-plugins/1.x/writing-upgradeable#storage-gaps
   */
  uint256[100] private __gap; // reserve 50 slot for adding new variable later. 1 slot = 32 bytes
}

