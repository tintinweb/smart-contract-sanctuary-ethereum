pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "../libs/Permission.sol";
import "./interfaces/IAppConf.sol";

contract AppConf is IAppConf, Ownable {

    address burnAddr;
    address coolAddr;

    mapping(address => uint8) blacklistMap;
    mapping(address => uint8) whitelistMap;

    // nfttoken -> status, 1=enabled
    mapping(address => uint8) stakingNftTokenMap; // for staking
    address nftFactoryAddr; //

    address rewardNftToken; // for reward

    // gen -> reward amount
    mapping(uint8 => uint256) rewardAmountMap;

    uint8 rewardNftTokenGen = 2; // nft token gen
    uint8 rewardType = Model.REWARD_TYPE_FIXED; // 1=fixed, 2=cycled

    // gen -> seconds, reward period, 0 is not reward
    mapping(uint8 => uint256) rewardPeriodMap;

    // nftype => status, which nft type can staking
    mapping(uint8 => uint8) stakingNftTypeMap;

    uint256 constant SECONDS_IN_DAY = 86400;

    mapping(address => uint8) public farmAddrMap;

    Model.FarmAddr farmAddr;

    mapping(address => uint8) nftTokenToTypeMap;
    mapping(uint8 => address) nftTypeToTokenMap;

    constructor() {
        burnAddr = 0x000000000000000000000000000000000000dEaD;
        coolAddr = _msgSender();

        rewardPeriodMap[1] = 3 * SECONDS_IN_DAY; // 3 days
        rewardPeriodMap[2] = 6 * SECONDS_IN_DAY; // 6 days

        rewardAmountMap[1] = 1;
        rewardAmountMap[2] = 1;
    }

    function validBlacklist(address addr) public view override returns(bool) {
        return blacklistMap[addr] == 1;
    }

    function validWhitelist(address addr) public view override returns(bool) {
        return whitelistMap[addr] == 1;
    }

    function setBlacklist(address[] calldata addrs, uint8 status) public onlyOwner {
        for (uint256 i = 0; i < addrs.length; i++) {
            blacklistMap[addrs[i]] = status;
        }
    }

    function setWhitelist(address[] calldata addrs, uint8 status) public onlyOwner {
        for (uint256 i = 0; i < addrs.length; i++) {
            whitelistMap[addrs[i]] = status;
        }
    }

    function setBurnAddr(address _burnAddr) public onlyOwner {
        burnAddr = _burnAddr;
    }

    function getBurnAddr() public view override returns(address) {
        return burnAddr;
    }

    function setCoolAddr(address _coolAddr) public onlyOwner {
        coolAddr = _coolAddr;
    }

    function getCoolAddr() public view override returns (address) {
        return coolAddr;
    }

    function setStakingNftToken(address nftToken, uint8 status) public onlyOwner {
        stakingNftTokenMap[nftToken] = status;
    }

    function validStakingNftToken(address nftToken) public view override returns(bool) {
        return stakingNftTokenMap[nftToken] == 1;
    }

    function setNftFactoryAddr(address _nftFactoryAddr) public onlyOwner {
        nftFactoryAddr = _nftFactoryAddr;
    }

    function getNftFactoryAddr() public view override returns(address) {
        return nftFactoryAddr;
    }

    function setRewardNftToken(address _rewardNftToken) public onlyOwner {
        rewardNftToken = _rewardNftToken;
    }

    function getRewardNftToken() public view override returns(address) {
        return rewardNftToken;
    }

    function setRewardNftTokenGen(uint8 _rewardNftTokenGen) public onlyOwner {
        rewardNftTokenGen = _rewardNftTokenGen;
    }

    function getRewardNftTokenGen() public view returns(uint8) {
        return rewardNftTokenGen;
    }

    function getRewardPeriod(uint8 gen) public view override returns(uint256) {
        return rewardPeriodMap[gen];
    }

    function setRewardPeriod(uint8 gen, uint256 period) public onlyOwner {
        rewardPeriodMap[gen] = period;
    }

    function validStakingNftType(uint8 nftType) public view override returns(bool) {
        return stakingNftTypeMap[nftType] == 1;
    }

    function setStakingNftType(uint8 nftType, uint8 status) public onlyOwner {
        stakingNftTypeMap[nftType] = status;
    }

    function setRewardAmount(uint8 gen, uint256 rewardAmount) public onlyOwner {
        rewardAmountMap[gen] = rewardAmount;
    }

    function getRewardAmount(uint8 gen) public view override returns(uint256) {
        return rewardAmountMap[gen];
    }

    function setRewardType(uint8 _rewardType) public onlyOwner {
        rewardType = _rewardType;
    }

    function getRewardType() public view returns(uint8) {
        return rewardType;
    }

    function setFarmAddr(Model.FarmAddr calldata _farmAddr) public onlyOwner {
        farmAddr = _farmAddr;
    }

    function getFarmAddr() public view returns(Model.FarmAddr memory) {
        return farmAddr;
    }

    function setNftTokenType(address nftToken, uint8 nftType) public onlyOwner {
        nftTokenToTypeMap[nftToken] = nftType;
    }

    function getNftTokenType(address nftToken) external view override returns(uint8) {
        return nftTokenToTypeMap[nftToken];
    }

    function setNftTypeToken(uint8 nftType, address nftToken) public onlyOwner {
        nftTypeToTokenMap[nftType] = nftToken;
    }

    function getNftTypeToken(uint8 nftType) external view override returns(address) {
        return nftTypeToTokenMap[nftType];
    }

    function setFarmAddr(address[] calldata farmAddrs, uint8 status) public onlyOwner {
        for (uint256 index = 0; index < farmAddrs.length; index++) {
            farmAddrMap[farmAddrs[index]] = status;
        }
    }

    function validFarm(address addr) public view override returns (bool) {
        return farmAddrMap[addr] == 1;
    }
}

