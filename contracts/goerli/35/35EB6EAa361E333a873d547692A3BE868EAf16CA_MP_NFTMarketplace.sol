pragma solidity ^0.8.7;

/////////////////////
//    Imports      //
/////////////////////

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
///@notice need Ownable to restrict functions to owner only
import "@openzeppelin/contracts/access/Ownable.sol";
///@notice need SafeMath for our maths functions (.add, .sub, .div, .mul)
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
/// @notice allows us use console.log in our .sol script
import "hardhat/console.sol";

/////////////////////
//    Interfaces   //
/////////////////////

/**
 * @notice MP_AddressRegistry contract imported functions
 * @dev Contract is a directory containing important contract addresses
 * @dev Functions are GETTERS returning contract addresses
 */
interface IMP_AddressRegistry {
    function s_auction_Contract() external view returns (address);

    function s_721_ContractFactory() external view returns (address);

    function s_feeRecipient() external view returns (address);
}

/**
 * @notice MP_AddressRegistry contract imported functions
 * @dev Contract is a directory containing important contract addresses
 * @dev Functions are GETTERS returning contract addresses
 */
interface IMP_721_ContractFactory {
    function create721Contract(
        string memory _name,
        string memory _symbol,
        uint256 _royaltiesPercentage,
        address payable _royaltiesReceiver,
        address _auction,
        address _marketplace,
        address _feeRecipient
    ) external payable returns (address);
}

/////////////////////
//     Errors      //
/////////////////////

error MP_NFTMarketplace__AddressInvalid(address nftAddress);
error MP_NFTMarketplace__AlreadyListed(address nftAddress, uint256 tokenId);
error MP_NFTMarketplace__InvalidAmount(uint256 amount);
error MP_NFTMarketplace__ItemNotForSale(address nftAddress, uint256 tokenId);
error MP_NFTMarketplace__ItemOwner();
error MP_NFTMarketplace__NoProceeds();
error MP_NFTMarketplace__NotApprovedForMarketplace();
error MP_NFTMarketplace__NotItemOwner();
error MP_NFTMarketplace__NotListed(address nftAddress, uint256 tokenId);
error MP_NFTMarketplace__PriceMustBeAboveZero();
error MP_NFTMarketplace__PriceNotMet(address nftAddress, uint256 tokenId, uint256 price);
error MP_NFTMarketplace__UnauthorisedCaller(address seller);

/////////////////////
//     Contract    //
/////////////////////

contract MP_NFTMarketplace is Ownable, ReentrancyGuard {
    using SafeMath for uint256;

    struct Listing {
        uint256 price;
        address seller;
    }

    /////////////////////
    //     Events      //
    /////////////////////

    event ItemListed(
        address indexed seller,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 price
    );

    event ItemUpdated(
        address indexed seller,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 price
    );

    event ItemCanceled(
        address indexed seller,
        address indexed nftAddress,
        uint256 indexed tokenId
    );

    event ItemBought(
        address indexed buyer,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 price
    );

    event RoyaltiesPaid(uint256 tokenId, uint value);

    event UpdatePlatformFee(uint256 s_platformFee);

    event AddressRegistryUpdated(address s_addressRegistry);

    /////////////////////
    //      State      //
    /////////////////////

    // interface ID
    bytes4 private constant C_INTERFACE_ID_ERC2981 = 0x2a55205a;

    // mapping of mapping stored in private variable "s_listings"
    mapping(address => mapping(uint256 => Listing)) private s_listings;

    mapping(address => uint256) private s_proceeds;

    /// @notice Platform fee
    uint256 public s_platformFee = 25; // 2.5%

    // Initialize interface to addressRegistry contract
    IMP_AddressRegistry public s_addressRegistry;

    /////////////////////
    //    Modifiers    //
    /////////////////////

    /**
     * @notice Modifier checking if tokenId is already listed
     * @dev the listing.price will return "0" if item is not listed
     * @dev in mappings; by default all possible addresses will be stored virtually
     */
    modifier notListed(address nftAddress, uint256 tokenId) {
        Listing memory listing = s_listings[nftAddress][tokenId];
        if (listing.price > 0) {
            revert MP_NFTMarketplace__AlreadyListed(nftAddress, tokenId);
        }
        _;
    }

    /**
     * @notice Modifier checking tokenId is not listed
     * @dev listing.price will return "0" if item is not listed
     * @dev in mappings; by default all possible addresses will be stored virtually
     */
    modifier isListed(address nftAddress, uint256 tokenId) {
        Listing memory listing = s_listings[nftAddress][tokenId];
        if (listing.price <= 0) {
            revert MP_NFTMarketplace__NotListed(nftAddress, tokenId);
        }
        _;
    }

    /**
     * @notice Modifier checking msg.sender owns the tokenId
     */
    modifier whenItemOwner(
        address nftAddress,
        uint256 tokenId,
        address spender
    ) {
        IERC721 nft = IERC721(nftAddress);
        address owner = nft.ownerOf(tokenId);
        if (spender != owner) {
            revert MP_NFTMarketplace__NotItemOwner();
        }
        _;
    }

    /**
     * @notice Modifier checking msg.sender doesn't own the tokenId
     */
    modifier whenNotItemOwner(
        address nftAddress,
        uint256 tokenId,
        address spender
    ) {
        IERC721 nft = IERC721(nftAddress);
        address owner = nft.ownerOf(tokenId);
        if (spender == owner) {
            revert MP_NFTMarketplace__ItemOwner();
        }
        _;
    }

    /**
     * @notice Modifier checking caller address is the Auction contract
     */
    modifier onlyAuction(address seller) {
        if (msg.sender != s_addressRegistry.s_auction_Contract()) {
            revert MP_NFTMarketplace__UnauthorisedCaller(seller);
        }
        _;
    }

    /////////////////////
    //   Constructor   //
    /////////////////////

    // constructor(
    //     address _addressRegistry
    // ) {
    //     s_addressRegistry = _addressRegistry;
    // }

    /////////////////////
    // Main Functions //
    /////////////////////

    /**
     * @notice Method for listing NFT - marketplace is given approval to transfer, but not custody of, the NFT
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     * @param price sale price for each item
     */
    function listItem(
        address nftAddress,
        uint256 tokenId,
        uint256 price
    ) external notListed(nftAddress, tokenId) whenItemOwner(nftAddress, tokenId, msg.sender) {
        if (price <= 0) {
            revert MP_NFTMarketplace__PriceMustBeAboveZero();
        }

        // Check that the marketplace ("address(this)") isApprovedForAll for the nft Address
        // We do the actual approving on the front end and user signs their wallet
        if (IERC721(nftAddress).isApprovedForAll(_msgSender(), address(this)) != true) {
            revert MP_NFTMarketplace__NotApprovedForMarketplace();
        }

        s_listings[nftAddress][tokenId] = Listing(price, msg.sender);
        emit ItemListed(msg.sender, nftAddress, tokenId, price);
    }

    /**
     * @notice Method for cancelling listing
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     */
    function cancelListing(
        address nftAddress,
        uint256 tokenId
    ) external whenItemOwner(nftAddress, tokenId, msg.sender) isListed(nftAddress, tokenId) {
        delete (s_listings[nftAddress][tokenId]);
        emit ItemCanceled(msg.sender, nftAddress, tokenId);
    }

    /**
     * @notice Method for buying a listing
     * @notice function would fail if NFT owner unapproves the marketplace
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     */
    function buyItem(
        address nftAddress,
        uint256 tokenId
    )
        external
        payable
        isListed(nftAddress, tokenId)
        whenNotItemOwner(nftAddress, tokenId, msg.sender)
        nonReentrant
    {
        Listing memory listedItem = s_listings[nftAddress][tokenId];

        if (msg.value < listedItem.price) {
            revert MP_NFTMarketplace__PriceNotMet(nftAddress, tokenId, listedItem.price);
        }

        _buyItem(msg.sender, nftAddress, tokenId);
    }

    function _buyItem(address buyer, address nftAddress, uint256 tokenId) private {
        Listing memory listedItem = s_listings[nftAddress][tokenId];
        address seller = listedItem.seller;
        uint256 price = listedItem.price;

        uint256 payAmount;
        uint256 royaltiesAmount;

        // A Platform Fee is calculated on the Sale Price
        uint256 platformCharge = price.mul(s_platformFee).div(1000);

        // check whether royalties are due and deduct if true
        if (checkRoyalties(nftAddress)) {
            royaltiesAmount = calculateRoyalties(nftAddress, tokenId, price);
        }

        // Define seller proceeds after platformCharge substracted from Winning Bid amount
        payAmount = price.sub(platformCharge).sub(royaltiesAmount);

        s_proceeds[seller] += payAmount;
        delete (s_listings[nftAddress][tokenId]);

        IERC721(nftAddress).safeTransferFrom(seller, buyer, tokenId);
        emit ItemBought(buyer, nftAddress, tokenId, price);
    }

    /**
     * @notice Method for updating listing
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     * @param newPrice Price in Wei of the item
     */
    function updateListing(
        address nftAddress,
        uint256 tokenId,
        uint256 newPrice
    )
        external
        isListed(nftAddress, tokenId)
        whenItemOwner(nftAddress, tokenId, msg.sender)
        nonReentrant
    {
        //We should check the value of `newPrice` and revert if it's below zero (like we also check in `listItem()`)
        if (newPrice <= 0) {
            revert MP_NFTMarketplace__PriceMustBeAboveZero();
        }
        s_listings[nftAddress][tokenId].price = newPrice;
        emit ItemUpdated(msg.sender, nftAddress, tokenId, newPrice);
    }

    /**
     * @notice Checks if NFT contract implements the ERC-2981 interface
     * @return true if ERC-2981 interface is supported, false otherwise
     */
    function checkRoyalties(address nftAddress) internal view returns (bool) {
        // NFT contract complying with 2981 must have supportsInterface implemented
        bool success = IERC2981(nftAddress).supportsInterface(C_INTERFACE_ID_ERC2981);
        return success;
    }

    /**
     * @notice Transfers royalties to the rightsowner if applicable
     * @param tokenId - the NFT token queried for royalties
     * @param saleValue - the price at which the asset will be sold
     * @return _royaltiesAmount - the value that will go to the seller after deducting royalties
     */
    function calculateRoyalties(
        address nftAddress,
        uint256 tokenId,
        uint256 saleValue
    ) private returns (uint256 _royaltiesAmount) {
        // Get amount of royalties to pays and recipient
        (address royaltiesReceiver, uint256 royaltiesAmount) = IERC2981(nftAddress).royaltyInfo(
            tokenId,
            saleValue
        );

        // not using a "success" bool here results in a warning when contract comiled but we can ignore it
        if (royaltiesAmount > 0 && royaltiesReceiver != address(0)) {
            (bool successRoyalty, ) = payable(royaltiesReceiver).call{value: royaltiesAmount}("");
            require(successRoyalty, "failed to send royalty to royalty receiver");

            // Broadcast royalties payment
            emit RoyaltiesPaid(tokenId, royaltiesAmount);

            _royaltiesAmount = royaltiesAmount;
            return _royaltiesAmount;
        } else {
            return 0;
        }
    }

    /**
     *  @notice Methos for getting contract's balance
     */
    function getContractBalance() external onlyOwner nonReentrant returns (uint256) {
        uint256 balance = address(this).balance;
        return balance;
    }

    /**
     * @notice Method for withdrawing proceeds from sales
     */
    function withdrawProceeds() external nonReentrant {
        uint256 proceeds = s_proceeds[msg.sender];
        if (proceeds <= 0) {
            revert MP_NFTMarketplace__NoProceeds();
        }
        s_proceeds[msg.sender] = 0;
        (bool success, ) = payable(msg.sender).call{value: proceeds}("");
        require(success, "Transfer failed");
    }

    /**
     * @notice Method for external contracts to add proceeds to this contract
     */
    function addProceeds(address seller) external payable onlyAuction(seller) {
        s_proceeds[seller] += msg.value;
    }

    /**
     * @notice Method for owner withdrawing contract funds
     */
    function withdrawContractBalance(uint256 amount) external onlyOwner {
        if (amount <= 0 || amount > address(this).balance) {
            revert MP_NFTMarketplace__InvalidAmount(amount);
        }

        (bool success, ) = payable(msg.sender).call{value: amount}("");
        require(success, "Transfer failed");
        // bool withdrawSuccess = payable(msg.sender).send(address(this).balance);
        // require(withdrawSuccess, "Withdraw Failed");
    }

    /**
     * @notice Method for owner creating 721 contracts
     */
    function create721Contract(
        string memory _name,
        string memory _symbol,
        uint256 _royaltiesPercentage,
        address payable _royaltiesReceiver
    ) external {
        address _auctionAddress = s_addressRegistry.s_auction_Contract();
        address _feeRecipient = s_addressRegistry.s_feeRecipient();
        address _marketplaceAddress = address(this);

        // Initialize interface to addressRegistry contract
        IMP_721_ContractFactory contractFactory = IMP_721_ContractFactory(
            s_addressRegistry.s_721_ContractFactory()
        );

        contractFactory.create721Contract(
            _name,
            _symbol,
            _royaltiesPercentage,
            _royaltiesReceiver,
            _auctionAddress,
            _marketplaceAddress,
            _feeRecipient
        );
    }

    //////////////////////
    // Getter Functions //
    //////////////////////

    function getListing(
        address nftAddress,
        uint256 tokenId
    ) external view returns (Listing memory) {
        return s_listings[nftAddress][tokenId];
    }

    function getProceeds(address seller) external view returns (uint256) {
        return s_proceeds[seller];
    }

    //////////////////////
    // Update Functions //
    //////////////////////

    /**
     *  @notice Method for updating platform fee
     *  @dev Only marketplace owner
     *  @param _platformFee uint256 the platform fee to set
     */
    function updatePlatformFee(uint256 _platformFee) external onlyOwner {
        s_platformFee = _platformFee;
        emit UpdatePlatformFee(_platformFee);
    }

    /**
     *  @notice Method for updating address registry
     *  @dev Only auction contract owner
     *  @param _newAddressRegistry is the updated address for the contract
     */
    function updateAddressRegistry(address _newAddressRegistry) external onlyOwner {
        if (_newAddressRegistry == address(0)) {
            revert MP_NFTMarketplace__AddressInvalid(_newAddressRegistry);
        }

        s_addressRegistry = IMP_AddressRegistry(_newAddressRegistry);
        emit AddressRegistryUpdated(_newAddressRegistry);
    }
}

