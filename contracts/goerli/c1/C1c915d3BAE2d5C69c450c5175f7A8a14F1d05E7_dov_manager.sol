pragma solidity 0.8.17;

import "IERC20.sol";
import "Pausable.sol";
import "ERC2771Context.sol";
import "MinimalForwarder.sol";
import "ReentrancyGuard.sol";
import "AccessControlEnumerable.sol";

error Denied();
error tokenTransferError();
error ZeroBalance();
error BalanceLow();

/**
    @title MeetsMeta Staking Contract
    @author ^M
 */

contract dov_manager is
    ReentrancyGuard,
    Pausable,
    AccessControlEnumerable,
    ERC2771Context
{
    /**
    @dev this will tell the top-up function to redirect the fund to out_wallet automatically
    or not */
    bool public auto_withdrawal = true;

    /**
    @dev address of our wallet
     */
    address public our_wallet;

    /**
    @dev trusted signer. this accounts can sign a transaction
     */
    address public trusted_signer;

    /**
    @dev defining PAUSER role
     */
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    /**
    @dev events
     */
    event topped_up(address user, address token, uint256 amount); // user top-up their credit
    event withdraw_admin(address token, uint256 amount, address destination); // admin withdraw funds
    event wallet_updated(address wallet); // fires when updating our wallet address
    event signer_updated(address new_signer); // an address that can sign transactions
    event withdraw_user(address user, address token, uint256 amount);
    event auto_withdrawal_status_changed(bool new_status);

    constructor(MinimalForwarder forwarder, address _trusted_signer)
        ERC2771Context(address(forwarder))
    {
        trusted_signer = _trusted_signer; // setting the trusted signer
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(PAUSER_ROLE, _msgSender());
    }

    /**
    @dev changes the trusted signer
    @param _new_trusted_signer is the address of the new signer to replace previous one
    */
    function changeTrustedSigner(address _new_trusted_signer)
        public
        whenNotPaused
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        trusted_signer = _new_trusted_signer;
        emit signer_updated(_new_trusted_signer);
    }

    /**
    @dev called by user to withdraw balances of an specific option if the option exist for the user. 
    will send the interest to the users address
    @param _user is the user who is the beneficiary of the funds transfer
    @param _token is the token to be transfered to the user
    @param _amount is the amount that should be transfered to the user
    */
    function withdrawUser(
        address _user,
        address _token,
        uint256 _amount
    ) public whenNotPaused nonReentrant {
        if (_msgSender() != trusted_signer) {
            revert Denied();
        }

        if (_token == address(0)) {
            (bool sent, ) = payable(_user).call{value: _amount}("");
            if (!sent) {
                revert tokenTransferError();
            }
        } else {
            if (!IERC20(_token).transfer(_user, _amount)) {
                revert tokenTransferError();
            }
        }
        emit withdraw_user(_user, _token, _amount);
    }

    /**
    @dev by calling this, admin can withdraw token balance to desired destination
    @param _token address which funds need to be transfered
    @param _destination address which funds needs to be sent to
    @notice only admin can call this function */
    function withdrawAdmin(address _token, address _destination)
        public
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        uint256 balance = 0;
        if (_token == address(0)) {
            balance = address(this).balance;
            (bool sent, ) = payable(_destination).call{value: balance}("");
            if (!sent) {
                revert tokenTransferError();
            }
        } else {
            balance = IERC20(_token).balanceOf(address(this));
            if (balance > 0) {
                // Transfering the token from users wallet to the contracts
                if (!IERC20(_token).transfer(_destination, balance)) {
                    revert tokenTransferError();
                }
            }
        }

        emit withdraw_admin(_token, balance, _destination);
    }

    /**
    @dev users adding credit by sending funds using this function
    @param _token is the address of the token which user wants to be credited for
    @param _amount is the amount user wants to add to their credit
    */
    function top_up(address _token, uint256 _amount)
        public
        payable
        whenNotPaused
        nonReentrant
    {
        // making sure we have a wallet registered for the token
        if (our_wallet == address(0)) {
            revert Denied();
        }

        address _user = _msgSender();

        if (_token == address(0)) {
            // check whether the amount sent is exactly as the amount to be credited
            if (msg.value < _amount) {
                revert BalanceLow();
            }
            if (auto_withdrawal) {
                // sending the amount to our wallet
                (bool sent, ) = payable(our_wallet).call{value: _amount}("");
                if (!sent) {
                    revert tokenTransferError();
                }
            }
        } else {
            // Transfering the token from users wallet to the contracts
            if (
                !IERC20(_token).transferFrom(
                    _user,
                    auto_withdrawal ? our_wallet : address(this),
                    _amount
                )
            ) {
                revert tokenTransferError();
            }
        }

        emit topped_up(_user, _token, _amount);
    }

    /**
    @dev this will update a wallet address in our wallet address mapping
    @param _new_wallet is the new wallet address */
    function updateOurWallet(address _new_wallet)
        public
        whenNotPaused
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        our_wallet = _new_wallet;
        emit wallet_updated(_new_wallet);
    }

    /**
    @dev the function will reverse the auto withdrawal status
     */
    function reverseAutoWithdrawal()
        public
        whenNotPaused
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        auto_withdrawal = !auto_withdrawal;
        emit auto_withdrawal_status_changed(auto_withdrawal);
    }

    /**
    @dev Utils
     */

    function _msgSender()
        internal
        view
        override(Context, ERC2771Context)
        returns (address sender)
    {
        if (isTrustedForwarder(msg.sender)) {
            // The assembly code is more direct than the Solidity version using `abi.decode`.
            /// @solidity memory-safe-assembly
            assembly {
                sender := shr(96, calldataload(sub(calldatasize(), 20)))
            }
        } else {
            return super._msgSender();
        }
    }

    function _msgData()
        internal
        view
        override(Context, ERC2771Context)
        returns (bytes calldata)
    {
        if (isTrustedForwarder(msg.sender)) {
            return msg.data[:msg.data.length - 20];
        } else {
            return super._msgData();
        }
    }

    /**
     * @dev Pauses any transaction.
     * Requirements:
     *
     * - the caller must have the `PAUSER_ROLE`.
     */
    function pause() public virtual onlyRole(PAUSER_ROLE) {
        _pause();
    }

    /**
     * @dev Unpauses all transactions.
     * Requirements:
     *
     * - the caller   must have the `PAUSER_ROLE`.
     */
    function unpause() public virtual onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    // Function to receive Ether. msg.data must be empty
    receive() external payable {}

    // Fallback function is called when msg.data is not empty
    fallback() external payable {}
}

