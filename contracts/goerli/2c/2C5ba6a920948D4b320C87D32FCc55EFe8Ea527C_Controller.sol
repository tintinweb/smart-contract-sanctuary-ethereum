pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Address.sol";

import "./abstract/InitContract.sol";

contract ZeroWallet is InitContract {
    using ECDSA for bytes32;
    using SafeERC20 for IERC20;
    using Address for address;

    event TransferETH(address indexed zeroWalletAddress, address indexed dst, uint256 amount);
    event TransferERC20(address indexed zeroWalletAddress, address indexed dst, address indexed tokenAddress, uint256 amount);
    event ReceivedETH(address indexed from, uint256 amount);

    mapping(bytes32 => bool) private txExecuted;
    address public owner;

    function init(address _owner) external initializer {
        require(_owner != address(0), "owner address is empty");
        owner = _owner;
    }

    function balanceOf() external view returns (uint256) {
        return address(this).balance;
    }

    function balanceOf(address _tokenAddress) external view returns (uint256) {
        IERC20 token = IERC20(_tokenAddress);
        require(_tokenAddress.isContract(), "tokenAddress is not contract");

        return token.balanceOf(address(this));
    }

    function transferETH(
        address _to,
        uint256 _amount,
        string memory _nonce,
        bytes memory _signature
    ) external initialized {
        bytes32 hash = keccak256(abi.encode(address(this), _to, _amount, _nonce));
        require(!txExecuted[hash], "tx executed!");
        require(_isOwner(hash, _signature), "only owner!");

        txExecuted[hash] = true;

        Address.sendValue(payable(_to), _amount);
        emit TransferETH(address(this), _to, _amount);
    }

    function transferERC20(
        address _tokenAddress,
        address _to,
        uint256 _amount,
        string memory _nonce,
        bytes memory _signature
    ) external initialized {
        bytes32 hash = keccak256(abi.encode(address(this), _tokenAddress, _to, _amount, _nonce));
        require(!txExecuted[hash], "tx executed!");
        require(_isOwner(hash, _signature), "only owner!");
        require(_tokenAddress.isContract(), "tokenAddress is not contract");

        txExecuted[hash] = true;

        IERC20 token = IERC20(_tokenAddress);
        token.safeTransfer(_to, _amount);
        emit TransferERC20(address(this), _to, _tokenAddress, _amount);
    }

    function _isOwner(bytes32 _hash, bytes memory _sig) internal view returns (bool) {
        return _verify(owner, _hash, _sig);
    }

    function _verify(
        address _signer,
        bytes32 _hash,
        bytes memory _sig
    ) internal pure returns (bool) {
        bytes32 message = _hash.toEthSignedMessageHash();
        return ECDSA.recover(message, _sig) == _signer;
    }

    receive() external payable {
        emit ReceivedETH(msg.sender, msg.value);
    }
}

