pragma solidity ^0.8.0;

import "./utils/BytesLib.sol";

import "./abstract/Create2Clone.sol";
import "./Charge.sol";
import "./ZeroWallet.sol";

contract Controller is Create2Clone {
    using BytesLib for bytes;

    address public chargeImplementation;
    address public zeroWalletImplementation;

    // Base initialization
    constructor(address _zeroWalletImplementation, address _chargeImplementation) {
        require(_chargeImplementation != address(0), "chargeImplementation wrong");
        require(_zeroWalletImplementation != address(0), "zeroWalletImplementation wrong");

        chargeImplementation = _chargeImplementation;
        zeroWalletImplementation = _zeroWalletImplementation;
    }

    function computeChargeAddress(
        address[] memory payees,
        uint256[] memory shares,
        string memory index
    ) external view returns (address) {
        bytes32 hash = keccak256(abi.encode(payees, shares, index));
        return _getCreate2Address(chargeImplementation, hash);
    }

    function computeZeroWalletAddress(address owner, string memory index) external view returns (address) {
        bytes32 hash = keccak256(abi.encode(owner, index));
        return _getCreate2Address(zeroWalletImplementation, hash);
    }

    function deployCharge(
        address[] memory payees,
        uint256[] memory shares,
        string memory index
    ) public returns (address) {
        bytes32 hash = keccak256(abi.encode(payees, shares, index));
        address chargeAddress = _cloneImplementation(chargeImplementation, hash);

        Charge clonedCharge = Charge(payable(chargeAddress));
        clonedCharge.init(payees, shares);

        return chargeAddress;
    }

    // Переделать под ZeroWallet!
    function deployZeroWallet(address owner, string memory index) public returns (address) {
        bytes32 hash = keccak256(abi.encode(owner, index));
        address zeroWalletAddress = _cloneImplementation(zeroWalletImplementation, hash);

        ZeroWallet clonedZeroWallet = ZeroWallet(payable(zeroWalletAddress));
        clonedZeroWallet.init(owner);

        return zeroWalletAddress;
    }
}

