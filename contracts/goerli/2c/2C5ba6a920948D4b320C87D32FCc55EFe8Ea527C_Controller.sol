pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "./abstract/PaymentSplitter.sol";
import "./abstract/InitContract.sol";

contract Charge is PaymentSplitter, InitContract {
    using SafeERC20 for IERC20;

    function init(address[] memory payees, uint256[] memory shares_) external initializer {
        _addPayees(payees, shares_);
    }

    function release() public initialized {
        for (uint256 i = 0; i < _payees.length; i++) {
            releaseAccount(payable(_payees[i]));
        }
    }

    function release(address tokenAddress) public initialized {
        require(Address.isContract(tokenAddress), "token address is not contract");

        for (uint256 i = 0; i < _payees.length; i++) {
            releaseAccount(payable(_payees[i]), tokenAddress);
        }
    }

    function releaseAccount(address payable account) public initialized {
        _release(account);
    }

    function releaseAccount(address payable account, address tokenAddress) public initialized {
        _release(IERC20(tokenAddress), account);
    }
}

