import "Guardable/ERC721AGuardable.sol";
import "solmate/auth/Owned.sol";
import "openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "./lib/MarauderErrors.sol";
import "./lib/MarauderEnums.sol";
import "./lib/MarauderStructs.sol";
import "./interfaces/IBoxContents.sol";
import "./interfaces/IMarauders.sol";

// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.18;

contract MadMarauderBoxOBadGuys is ERC721AGuardable, Owned {
  address public maraudersContract;
  address public AdamArcherMadMarauderAnimationContract;
  address public merchContract;
  address public serumContract;

  bool private contractsSet;
  bool public smashingEnabled;

  mapping(Phase => PhaseDetails) public phases;
  mapping(Item => ItemDetails) public itemDetails;
  mapping(address => MintTracker) public numberMinted;

  constructor(bytes32[3] memory roots, uint64 _startTime) ERC721AGuardable("BoBG", "Box O' Bad Guys") Owned(msg.sender) {
    phases[Phase.NERDS_ONLY] = PhaseDetails(roots[0], _startTime);
    phases[Phase.FRIENDS_AND_FAMILY] = PhaseDetails(roots[1], _startTime + 48 hours);
    phases[Phase.PUBLIC_ALLOWLIST] = PhaseDetails(roots[2], _startTime + 72 hours);
    phases[Phase.PUBLIC] = PhaseDetails(bytes32(0), _startTime + 96 hours);

    itemDetails[Item.BOX_O_BAD_GUYS] = ItemDetails(0, 969, 5, 0.42069 ether, 0.3333 ether);
    itemDetails[Item.ENFORCER] = ItemDetails(0, 2069, 10, 0.1 ether, 0.0666 ether);
    itemDetails[Item.WARLORD] = ItemDetails(0, 3069, 10, 0.169 ether, 0.0999 ether);
    itemDetails[Item.MYSTERY_SERUM] = ItemDetails(0, 2069, 10, 0.269 ether, 0.2 ether);
  }

  function mint(Item item, bytes32[] calldata proof, uint16 amount) external payable {
    if (!contractsSet) revert ContractsUnset();

    Phase phase = currentPhase();
    if (phase == Phase.NOT_STARTED) revert SaleNotActive();

    PhaseDetails memory phaseDetails = phases[phase];

    if (uint(phase) <= 3) _validateSender(phaseDetails.root, proof);

    uint256 cost = priceFor(item, phase, amount);
    if (msg.value != cost) revert WrongValueSent();

    _mintItem(item, amount);
  }

  function smashAndGrab(uint256[] memory tokenIds) external {
    if (!contractsSet) revert ContractsUnset();
    if (!smashingEnabled) revert SmashingNotActive();
    for (uint256 i = 0; i < tokenIds.length;) {
      _burn(tokenIds[i]); // this checks ownership and also prevents duplicate tokenIds
      unchecked { ++i; }
    }

    IBoxContents(maraudersContract).mintFromBox(msg.sender, tokenIds.length);
    IBoxContents(AdamArcherMadMarauderAnimationContract).mintFromBox(msg.sender, tokenIds.length);
    IBoxContents(merchContract).mintFromBox(msg.sender, tokenIds.length);
    IBoxContents(serumContract).mintFromBox(msg.sender, tokenIds.length);
  }

  function priceFor(Item item, Phase phase, uint256 amount) public view returns (uint256) {
    if (phase == Phase.NOT_STARTED) revert SaleNotActive();
    return uint(phase) <= 2 ? itemDetails[item].discountedPrice * amount : itemDetails[item].price * amount;
  }

  /* MAINNET FUNCTION
  function currentPhase() public view returns (Phase) {
    if (block.timestamp < phases[Phase.NERDS_ONLY].startTime) {
      return Phase.NOT_STARTED;
    } else if (block.timestamp < phases[Phase.FRIENDS_AND_FAMILY].startTime) {
      return Phase.NERDS_ONLY;
    } else if (block.timestamp < phases[Phase.PUBLIC_ALLOWLIST].startTime) {
      return Phase.FRIENDS_AND_FAMILY;
    } else if (block.timestamp < phases[Phase.PUBLIC].startTime) {
      return Phase.PUBLIC_ALLOWLIST;
    } else {
      return Phase.PUBLIC;
    }
  }
  */

  /* TESTNET FUNCTION */
  uint256 public mockTimestamp;

  function setTimeStamp(uint256 _timestamp) external {
    mockTimestamp = _timestamp;
  }

  function currentPhase() public view returns (Phase) {
    uint256 timestamp = mockTimestamp > 0 ? mockTimestamp : block.timestamp;

    if (timestamp < phases[Phase.NERDS_ONLY].startTime) {
      return Phase.NOT_STARTED;
    } else if (timestamp < phases[Phase.FRIENDS_AND_FAMILY].startTime) {
      return Phase.NERDS_ONLY;
    } else if (timestamp < phases[Phase.PUBLIC_ALLOWLIST].startTime) {
      return Phase.FRIENDS_AND_FAMILY;
    } else if (timestamp < phases[Phase.PUBLIC].startTime) {
      return Phase.PUBLIC_ALLOWLIST;
    } else {
      return Phase.PUBLIC;
    }
  }
  // REMOVE FOR MAINNET //

  function setContracts(address _marauders, address _AdamArcherMadMarauderAnimation, address _merch, address _serum) external onlyOwner {
    if (contractsSet) revert ContractsAlreadySet();

    maraudersContract = _marauders;
    AdamArcherMadMarauderAnimationContract = _AdamArcherMadMarauderAnimation;
    merchContract = _merch;
    serumContract = _serum;

    contractsSet = true;
  }

  function setRoots(Phase[] calldata _phases, bytes32[] calldata _roots) external onlyOwner {
    if (_phases.length != _roots.length) revert ArrayLengthMismatch();

    for (uint256 i = 0; i < _phases.length; i++) {
      phases[_phases[i]].root = _roots[i];
    }
  }

  function _validateSender(bytes32 root, bytes32[] memory _proof) private view {
    bytes32 leaf = keccak256((abi.encodePacked(msg.sender)));

    if (!MerkleProof.verify(_proof, root, leaf)) {
        revert InvalidProof();
    }
  }

  function _mintItem(Item item, uint16 amount) internal {
    if (totalSupply() + amount > itemDetails[item].maxUnitsAllowed) revert ExceedMaxSupply();

    unchecked { itemDetails[item].numUnitsSold += amount; }

    if (item == Item.BOX_O_BAD_GUYS) {
      if (numberMinted[msg.sender].numBoxesMinted + amount > itemDetails[item].maxUnitsAllowedPerWallet) revert ExceedMaxPerWallet();
      unchecked { numberMinted[msg.sender].numBoxesMinted += amount; }
      _mint(msg.sender, amount);
    } else if (item == Item.ENFORCER) {
      if (numberMinted[msg.sender].numEnforcersMinted + amount > itemDetails[item].maxUnitsAllowedPerWallet) revert ExceedMaxPerWallet();
      unchecked { numberMinted[msg.sender].numEnforcersMinted += amount; }
      IMarauders(maraudersContract).mintEnforcer(msg.sender, amount);
    } else if (item == Item.WARLORD) {
      if (numberMinted[msg.sender].numWarlordsMinted + amount > itemDetails[item].maxUnitsAllowedPerWallet) revert ExceedMaxPerWallet();
      unchecked { numberMinted[msg.sender].numWarlordsMinted += amount; }
      IMarauders(maraudersContract).mintWarlord(msg.sender, amount);
    } else if (item == Item.MYSTERY_SERUM) {
      if (numberMinted[msg.sender].numSerumsMinted + amount > itemDetails[item].maxUnitsAllowedPerWallet) revert ExceedMaxPerWallet();
      unchecked { numberMinted[msg.sender].numSerumsMinted += amount; }
      IBoxContents(serumContract).mintFromBox(msg.sender, amount);
    }
  }

  function setSmashingStatus(bool status) external onlyOwner {
    smashingEnabled = status;
  }

  function withdraw() external onlyOwner {
    (bool success, ) = msg.sender.call{value: address(this).balance}("");
    if (!success) revert WrongValueSent();
  }
}

