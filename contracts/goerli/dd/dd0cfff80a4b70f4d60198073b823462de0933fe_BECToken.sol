        token = IERC20(address(this));
        balanceof = balanceOf(address(this));

    }

    function getLatestPrice() public view returns (int) {
        (
            , // uint80 roundID
            int price, 
            , // uint startedAt
            , // uint timeStamp
              // uint80 answeredInRound
        ) = priceFeed.latestRoundData();
        return price / 10**8;
    }

    function seedSaleBuy()
        payable
        external
    {
        require(msg.value > 0, 'Incorrect Amount');

        uint256 price = uint256(getLatestPrice());
        
        uint256 quantity = msg.value.mul(price).mul(1e18).div(seedSalePrice);

        require(quantity <= seedSaleAvailable, "Not enough tokens left for sale");

        token.transfer(msg.sender, quantity);

        investorsAmount[msg.sender] = investorsAmount[msg.sender].add(quantity);

        if(!investors[msg.sender]) {
            investors[msg.sender] = true;
            totalBuyers = totalBuyers.add(1);
        }

        seedSaleSold = seedSaleSold.add(quantity);
        seedSaleAvailable = seedSaleAvailable.sub(quantity);
    }

