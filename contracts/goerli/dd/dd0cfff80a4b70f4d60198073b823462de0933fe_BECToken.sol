        preSaleTotal = _totalSupply * 15 / 100;
        privateSale1Total = _totalSupply * 75 / 1000;
        privateSale2Total = _totalSupply * 75 / 1000;
        Listing_Nd_LiquidityTotal = _totalSupply * 20/100;
        stakingRewardTotal = _totalSupply * 15 /100;

