        emit SetPaymentToken(newToken);
    }

    function setBaseURI(string memory baseURI_) external onlyOwner {
        baseURI = baseURI_;
        emit SetBaseURI(baseURI);
    }

    function _splitShares(
        address to,
        uint256 sharesToSplit,
        uint256 shareIdsSliceStart
