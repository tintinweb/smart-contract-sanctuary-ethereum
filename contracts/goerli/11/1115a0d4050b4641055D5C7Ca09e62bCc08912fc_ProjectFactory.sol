pragma solidity 0.8.9;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./ShareIdsStorage.sol";

contract ProjectShares is ERC721Enumerable, Ownable, ShareIdsStorage {
    struct ShareIdsSlice {
        uint256 start;
        uint256 limit;
    }
    // block (package) of IP-shares inside one NFT
    struct BlockOfShares {
        uint256 amount;
        uint256 floorPrice;
        uint256 topBid;
        address topBidder;
    }
    struct AuctionData {
        uint256 endTime;
        uint256 totalSharesForSale;
        BlockOfShares[] sharesForSale;
        bool isActive;
    }
    struct InvestRoundData {
        uint256 startsAt;
        uint256 minAmount;
        uint256 sharePrice;
        uint256 maxShares;
        uint256 boughtShares;
        bool ended;
    }

    uint256 private constant INO_TOKEN_ID = 1; // INO = Initial NFT Offering
    uint256 private constant DENOMINATOR = 10_000;
    uint256 public adminShare = 1_000;
    uint256 public maxAuctionEndTime = 180 days;

    address public projectOwner;
    string public baseURI;

    IERC20 public paymentToken;
    InvestRoundData public investRoundData;

    /**
     * NFT ID => Start & End of share ids slice
     * 2317 => {start: 7, limit: 12} means this NFT contains 12 share ids from 7 index in share ids array
     */
    mapping(uint256 => ShareIdsSlice) public shareIdsSliceByTokenId;
    mapping(uint256 => AuctionData) private _auctionsByToken;
    mapping(address => uint256) public claimAmounts;
    mapping(address => bool) public reissueForbidden;

    event InvestedInRound(uint256 tokenId, address user, uint256 amount);
    event NewSharesIssued(uint256 amount);
    event AuctionStarted(uint256 tokenId, address tokenOwner);
    event AuctionEnded(uint256 tokenId, address tokenOwner);
    event PutBlockOfSharesUpForSale(
        uint256 indexed tokenId,
        uint256 indexed blockOfSharesIndex,
        uint256 indexed amount,
        uint256 floorPrice
    );
    event TopBidUpdated(
        uint256 indexed tokenId,
        uint256 indexed blockOfSharesIndex,
        address indexed bidder,
        uint256 bidAmount
    );
    event Withdraw(address user, uint256 amount);
    event ReissueToken(uint256 tokenId, uint256 newTokenId, address newOwner);
    event SetAdminShare(uint256 newShare);
    event SetPaymentToken(address newToken);
    event SetAdmin(address newAdmin);
    event SetBaseURI(string baseURI);
    event InvestRoundEnded();
    event SetInvestRoundData(
        uint256 indexed startsAt,
        uint256 indexed minAmount,
        uint256 indexed sharePrice,
        uint256 maxShares
    );
    event SetMaxAuctionEndTime(uint256 maxAuctionEndTime);
    event SetIsReissueForbidden(bool status);

    error Unauthorized();
    error AuctionNotYetEnded();
    error InvalidBlockOfSharesData(uint256 blockOfSharesIndex);
    error AuctionAlreadyEnded();

    constructor(
        string memory name_,
        string memory symbol_,
        string memory baseURI_,
        address admin_,
        address paymentToken_,
        address projectOwner_,
        uint256 totalShareIds,
        InvestRoundData memory investRoundData_
    ) ERC721(name_, symbol_) {
        baseURI = baseURI_;

        _transferOwnership(admin_);

        projectOwner = projectOwner_;
        paymentToken = IERC20(paymentToken_);

        _mint(projectOwner_, INO_TOKEN_ID);
        shareIdsSliceByTokenId[INO_TOKEN_ID] = ShareIdsSlice({
            start: 0,
            limit: totalShareIds
        });

        investRoundData = investRoundData_;
    }

    modifier onlyTokenOwner(uint256 tokenId) {
        if (msg.sender != ownerOf(tokenId)) revert Unauthorized();
        _;
    }

    modifier onlyProjectOwner() {
        if (msg.sender != projectOwner) revert Unauthorized();
        _;
    }

    modifier onlyAfterInvestRound() {
        require(investRoundData.ended, "invest round not yet ended");
        _;
    }

    function isOwnerOf(address user, bytes32 shareId)
        external
        view
        returns (bool)
    {
        return isOwnerOfPaginated(user, shareId, 0, balanceOf(user) - 1);
    }

    function auctionsByToken(uint256 tokenId)
        external
        view
        returns (AuctionData memory)
    {
        return _auctionsByToken[tokenId];
    }

    function isOwnerOfPaginated(
        address user,
        bytes32 shareId,
        uint256 fromTokenIndex,
        uint256 toTokenIndex
    ) public view returns (bool) {
        uint256 userBalance = balanceOf(user);
        if (userBalance == 0) return false;

        require(toTokenIndex < userBalance, "invalid toTokenIndex");
        require(
            fromTokenIndex <= toTokenIndex,
            "invalid fromTokenIndex or toTokenIndex"
        );

        uint256 index = indexOf(shareId);

        for (uint256 i = fromTokenIndex; i <= toTokenIndex; i++) {
            uint256 tokenId = tokenOfOwnerByIndex(user, i);

            ShareIdsSlice memory shareIdsSlice = shareIdsSliceByTokenId[
                tokenId
            ];

            uint256 start = shareIdsSlice.start;
            uint256 limit = shareIdsSlice.limit;

            if (index < start) continue;
            if (index >= start && index < (start + limit)) return true;
        }

        return false;
    }

    function getShareIds(
        uint256 tokenId,
        uint256 start,
        uint256 limit
    ) external view returns (bytes32[] memory) {
        ShareIdsSlice memory shareIdsSlice = shareIdsSliceByTokenId[tokenId];

        uint256 sliceStart = shareIdsSlice.start;
        uint256 sliceLimit = shareIdsSlice.limit;
        require(
            start >= sliceStart && (start + limit) <= (sliceStart + sliceLimit),
            "invalid start & limit parameters"
        );

        return idsSlice(start, limit);
    }

    function investInRound(uint256 amount) external {
        InvestRoundData memory investRoundData_ = investRoundData;
        ShareIdsSlice memory shareIdsSlice_ = shareIdsSliceByTokenId[
            INO_TOKEN_ID
        ];
        require(
            investRoundData_.startsAt < block.timestamp,
            "invest round not yet started"
        );
        require(!investRoundData_.ended, "invest round already ended");

        require(amount >= investRoundData_.minAmount, "amount < minAmount");
        require(amount <= shareIdsSlice_.limit, "too many shares");
        require(
            (investRoundData_.boughtShares + amount) <=
                investRoundData_.maxShares,
            "too many shares"
        );
        uint256 paymentAmount = amount * investRoundData_.sharePrice;
        paymentToken.transferFrom(msg.sender, address(this), paymentAmount);

        uint256 adminFee = (paymentAmount * adminShare) / DENOMINATOR;
        claimAmounts[owner()] += adminFee;
        claimAmounts[projectOwner] += paymentAmount - adminFee;

        ShareIdsSlice storage shareIdsSlice = shareIdsSliceByTokenId[
            INO_TOKEN_ID
        ];

        uint256 newTokenId = _splitShares(
            msg.sender,
            amount,
            shareIdsSlice_.start
        );

        shareIdsSlice.start += amount;
        shareIdsSlice.limit -= amount;

        investRoundData.boughtShares += amount;
        emit InvestedInRound(newTokenId, msg.sender, amount);
    }

    function issueNewShares(uint256 amount) external onlyOwner {
        shareIdsSliceByTokenId[INO_TOKEN_ID].limit += amount;
        emit NewSharesIssued(amount);
    }

    function setInvestRoundData(
        uint256 startsAt,
        uint256 minAmount,
        uint256 sharePrice,
        uint256 maxShares,
        bool ended
    ) external onlyProjectOwner {
        if (ended) {
            investRoundData.ended = true;
            emit InvestRoundEnded();
            return;
        }

        require(
            maxShares > 0 &&
                maxShares <= shareIdsSliceByTokenId[INO_TOKEN_ID].limit,
            "invalid maxShares"
        );
        require(minAmount <= maxShares, "invalid minAmount");
        require(startsAt > block.timestamp, "invalid startsAt");
        investRoundData = InvestRoundData({
            startsAt: startsAt,
            minAmount: minAmount,
            sharePrice: sharePrice,
            maxShares: maxShares,
            boughtShares: investRoundData.boughtShares,
            ended: false
        });

        emit SetInvestRoundData(startsAt, minAmount, sharePrice, maxShares);
    }

    function startAuction(
        uint256 tokenId,
        uint256 endTime,
        BlockOfShares[] calldata sharesForSale
    ) external onlyTokenOwner(tokenId) onlyAfterInvestRound {
        AuctionData storage auctionData = _auctionsByToken[tokenId];
        if (auctionData.isActive || auctionData.endTime > block.timestamp)
            revert AuctionNotYetEnded();

        require(
            endTime > block.timestamp &&
                endTime < (block.timestamp + maxAuctionEndTime),
            "invalid endTime"
        );
        auctionData.endTime = endTime;

        delete auctionData.sharesForSale;

        uint256 totalSharesForSale;
        for (uint256 index = 0; index < sharesForSale.length; index++) {
            BlockOfShares calldata blockOfShares = sharesForSale[index];

            if (
                blockOfShares.amount == 0 ||
                blockOfShares.topBid != 0 ||
                blockOfShares.topBidder != address(0)
            ) revert InvalidBlockOfSharesData(index);

            totalSharesForSale += blockOfShares.amount;

            auctionData.sharesForSale.push(blockOfShares);
            emit PutBlockOfSharesUpForSale(
                tokenId,
                index,
                blockOfShares.amount,
                blockOfShares.floorPrice
            );
        }

        require(
            totalSharesForSale <= shareIdsSliceByTokenId[tokenId].limit,
            "you don't have enough shares"
        );

        auctionData.totalSharesForSale = totalSharesForSale;
        auctionData.isActive = true;

        emit AuctionStarted(tokenId, msg.sender);
    }

    function endAuction(uint256 tokenId) external {
        AuctionData memory auctionData = _auctionsByToken[tokenId];
        address tokenOwner = ownerOf(tokenId);

        if (msg.sender != tokenOwner && auctionData.endTime > block.timestamp)
            revert AuctionNotYetEnded();
        if (!auctionData.isActive) revert AuctionAlreadyEnded();

        ShareIdsSlice memory shareIdsSlice = shareIdsSliceByTokenId[tokenId];
        uint256 shareIdsSliceStart = shareIdsSlice.start;

        uint256 totalBids;
        uint256 soldShares;
        for (
            uint256 index = 0;
            index < auctionData.sharesForSale.length;
            index++
        ) {
            BlockOfShares memory blockOfShares = auctionData.sharesForSale[
                index
            ];
            if (blockOfShares.topBidder == address(0)) continue;

            totalBids += blockOfShares.topBid;
            soldShares += blockOfShares.amount;
            _splitShares(
                blockOfShares.topBidder,
                blockOfShares.amount,
                shareIdsSliceStart
            );
            shareIdsSliceStart += blockOfShares.amount;
        }

        if (soldShares == shareIdsSlice.limit) {
            _burn(tokenId);
        } else if (soldShares > 0) {
            shareIdsSliceByTokenId[tokenId].start += soldShares;
            shareIdsSliceByTokenId[tokenId].limit -= soldShares;

            delete _auctionsByToken[tokenId];
        }

        uint256 adminFee = (totalBids * adminShare) / DENOMINATOR;

        claimAmounts[owner()] += adminFee;
        claimAmounts[tokenOwner] += totalBids - adminFee;

        emit AuctionEnded(tokenId, tokenOwner);
    }

    function putBlockOfSharesUpForSale(
        uint256 tokenId,
        uint256 amount,
        uint256 floorPrice
