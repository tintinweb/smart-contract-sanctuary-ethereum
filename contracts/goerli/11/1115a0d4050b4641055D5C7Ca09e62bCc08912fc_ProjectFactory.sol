pragma solidity 0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "./ProjectShares.sol";

interface ProjectsRegistry {
    function addProject(address projectAddress) external;
}

contract ProjectFactory is Ownable {
    using ECDSA for bytes32;

    uint256 public projectsCount;
    address public projectsRegistry;

    event ProjectCreated(uint256 id, address projectAddress);

    constructor(address projectsRegistry_) {
        projectsRegistry = projectsRegistry_;
    }

    function setProjectsRegistry(address projectsRegistry_) external onlyOwner {
        projectsRegistry = projectsRegistry_;
    }

    function createProjectWithSignature(
        string memory name,
        string memory symbol,
        string memory baseURI,
        address paymentToken,
        address projectOwner,
        uint256 totalShareIds,
        uint256 startsAt,
        uint256 minAmount,
        uint256 sharePrice,
        uint256 maxShares,
        bytes memory signature
    ) external {
        uint256 projectId = ++projectsCount;

        bytes32 hash = keccak256(
            abi.encodePacked(
                msg.sender,
                projectId,
                name,
                symbol,
                baseURI,
                paymentToken,
                projectOwner,
                totalShareIds,
                startsAt,
                minAmount,
                sharePrice
            )
        );
        require(hash.recover(signature) == owner(), "invalid signature");

        _createProject(
            name,
            symbol,
            baseURI,
            paymentToken,
            projectOwner,
            totalShareIds,
            startsAt,
            minAmount,
            sharePrice,
            maxShares,
            projectId
        );
    }

    function createProject(
        string memory name,
        string memory symbol,
        string memory baseURI,
        address paymentToken,
        address projectOwner,
        uint256 totalShareIds,
        uint256 startsAt,
        uint256 minAmount,
        uint256 sharePrice,
        uint256 maxShares
    ) external onlyOwner {
        uint256 projectId = ++projectsCount;

        _createProject(
            name,
            symbol,
            baseURI,
            paymentToken,
            projectOwner,
            totalShareIds,
            startsAt,
            minAmount,
            sharePrice,
            maxShares,
            projectId
        );
    }

    function _createProject(
        string memory name,
        string memory symbol,
        string memory baseURI,
        address paymentToken,
        address projectOwner,
        uint256 totalShareIds,
        uint256 startsAt,
        uint256 minAmount,
        uint256 sharePrice,
        uint256 maxShares,
        uint256 projectId
    ) internal {
        ProjectShares.InvestRoundData memory investRoundData = ProjectShares
            .InvestRoundData({
                startsAt: startsAt,
                minAmount: minAmount,
                sharePrice: sharePrice,
                maxShares: maxShares,
                boughtShares: 0,
                ended: false
            });
        ProjectShares newProject = new ProjectShares(
            name,
            symbol,
            baseURI,
            owner(),
            paymentToken,
            projectOwner,
            totalShareIds,
            investRoundData
        );

        emit ProjectCreated(projectId, address(newProject));

        ProjectsRegistry(projectsRegistry).addProject(address(newProject));
    }
}

