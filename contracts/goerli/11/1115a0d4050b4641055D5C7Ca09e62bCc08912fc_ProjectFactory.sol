import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

contract ShareIdsStorage is Ownable {
    using EnumerableSet for EnumerableSet.Bytes32Set;
    EnumerableSet.Bytes32Set private _ids;

    event SetIds(uint256 totalIds);
    event IdsAdded(uint256 addedIdsCount, uint256 totalIds);

    function indexOf(bytes32 value) public view returns (uint256) {
        uint256 index = _ids._inner._indexes[value];
        require(index > 0, "no such value");

        return index - 1;
    }

    function contains(bytes32 value) external view returns (bool) {
        return _ids.contains(value);
    }

    function ids(uint256 index) external view returns (bytes32) {
        return _ids.at(index);
    }

    function idsSlice(uint256 start_, uint256 limit_)
        public
        view
        returns (bytes32[] memory)
    {
        require((start_ + limit_) <= _ids.length(), "invalid start & limit");

        bytes32[] memory idsSlice_ = new bytes32[](limit_);

        for (uint256 i = 0; i < limit_; i++) {
            idsSlice_[i] = _ids.at(start_ + i);
        }

        return idsSlice_;
    }

    function idsCount() public view returns (uint256) {
        return _ids.length();
    }

    function setIds(bytes32[] calldata newIds) external onlyOwner {
        for (uint256 i = 0; i < _ids.length(); i++) {
            bytes32 value = _ids.at(i);
            _ids.remove(value);
        }
        for (uint256 i = 0; i < newIds.length; i++) {
            _ids.add(newIds[i]);
        }
        emit SetIds(newIds.length);
    }

    function addIds(bytes32[] calldata newIds) external onlyOwner {
        for (uint256 i = 0; i < newIds.length; i++) {
            _ids.add(newIds[i]);
        }

        emit IdsAdded(newIds.length, _ids.length());
    }
}

