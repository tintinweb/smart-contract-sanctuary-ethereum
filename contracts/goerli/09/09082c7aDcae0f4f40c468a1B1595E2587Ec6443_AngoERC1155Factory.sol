contract AngoNFTERC1155 is ERC1155{

    address public owner;

    address public serviceFeeAddress;

    address public admin;

    using ECDSA for bytes32;


    constructor(string memory name, string memory symbol, address _owner, address _serviceFeeAddress, address _admin) ERC1155("ipfs://", name, symbol){
        owner = _owner;
        serviceFeeAddress = _serviceFeeAddress;
        admin = _admin;
    }

    function setOperator(address _operator) public{
        require(msg.sender == admin,"Not authorized.");
        serviceFeeAddress = _operator;
    }

    function setAdmin(address _admin) public{
        require(msg.sender == admin,"Not authorized.");
        admin = _admin;
    }


    function setURI(string memory newuri) public {
        require(msg.sender == owner,"Not authorized.");
        _setURI(newuri);
    }

    function mint(uint256 id,  string memory suffix, uint256 amount) external {

        require(msg.sender == owner,"Not authorized.");

        _mint(owner, id,amount,suffix ,"0x00");

    }

    function lazyMint(bytes memory signature, uint256 id, address _destination, uint256 value, string memory suffix, uint256 amount, uint256 boughtAmount) external payable {

        bytes32 message = keccak256(abi.encodePacked(owner, id, value, amount, address(this))).toEthSignedMessageHash();

        uint256 supplyOfToken = totalSupplyToken(id);

        require(recoverSigner(message, signature) == owner, "Signature is not valid.");

        require(amount>= boughtAmount + supplyOfToken, "Buying too much.");

        uint256 weiAmount = msg.value;

        require(weiAmount >= value * boughtAmount * 1025 / 1000,"Not enough ether sent.");

        require(amount>=boughtAmount,"Incorrect amount.");

        uint256 ownerWei = value * boughtAmount * 975 / 1000;

        uint256 feeWei = weiAmount - ownerWei;

        payable(owner).transfer(ownerWei);
        payable(serviceFeeAddress).transfer(feeWei);

        _mint(_destination, id,boughtAmount,suffix ,"0x00");

    }

    function splitSignature(bytes memory sig)
        internal
        pure
        returns (uint8 v, bytes32 r, bytes32 s)
    {
        require(sig.length == 65);

        assembly {
            // first 32 bytes, after the length prefix.
            r := mload(add(sig, 32))
            // second 32 bytes.
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 bytes).
            v := byte(0, mload(add(sig, 96)))
        }

        return (v, r, s);
    }

    function recoverSigner(bytes32 message, bytes memory sig)
        internal
        pure
        returns (address)
    {
        (uint8 v, bytes32 r, bytes32 s) = splitSignature(sig);
        return ecrecover(message, v, r, s);
    }

    function getAddress() public view returns (address){
        return address(this);
    }
}


contract AngoERC1155Factory {
    mapping(address => uint256[]) public ownerCollections;

    mapping (uint256=>address) public collectionAddress;

    uint256 public id = 0;

    address public owner;

    address public serviceFee;
    
    address public admin;

    constructor(address _serviceFee, address _admin){
        owner = msg.sender;
        serviceFee = _serviceFee;
        admin = _admin;
    }

    function createNewCollection(string memory name, string memory symbol, address _owner) public returns(address){

        AngoNFTERC1155 newContract = new AngoNFTERC1155(name, symbol,  _owner, serviceFee, admin);

        ownerCollections[msg.sender].push(id);

        collectionAddress[id] = newContract.getAddress();

        id = id + 1;

        return newContract.getAddress();
    }

    function getLengthCollection(address _user) public view returns(uint256){
        return ownerCollections[_user].length;
    }

}