pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";

import "../../utils/ERC721Redeemer.sol";
import "./OzuOpenEditionRoles.sol";

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC721Upgradeable.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

contract OpenEditionERC1155 is
  Initializable,
  ERC1155Upgradeable,
  OzuOpenEditionRoles
{
  using AddressUpgradeable for address;
  using ERC721Redeemer for ERC721Redeemer.Claims;

  string private _name;

  string private _symbol;

  // address used to sign and verify messages
  address private _verifier;

  // address to receive commission on first mint
  address payable private _commissionReceiver;

  // Mapping from token ID to token creators
  mapping(uint256 => address) private _creators;

  // Mapping from token ID to total supply
  mapping(uint256 => uint256) private _totalSupply;

  //Mapping from tokenID to max supply
  mapping(uint256 => uint256) private _maxSupply;

  // total number of sessions
  uint256 private _totalSessions;

  // Mapping from sessionId to Session
  mapping(uint256 => Session) private _sessions;

  // Mapping from tokenId to corresponding SessionId
  mapping(uint256 => uint256) private _tokenToSession;

  // Mapping from session id to the session's mapping variables
  mapping(uint256 => SessionMapping) private _sessionsMapping;

  struct SessionMapping {
    mapping(address => bool) collectionWhitelisted;
    mapping(address => uint256) discountCounts;
    mapping(address => ERC721Redeemer.Claims) redeemedWhitelistTokens;
  }

  struct MintInfoSingle {
    uint256 sessionId;
    uint256 tokenId;
    uint256 totalPrice;
    uint256 price;
    uint256 totalCommission;
    bytes signature;
  }

  struct MintInfoBatch {
    uint256 sessionId;
    uint256[] tokenIds;
    uint256[] amounts;
    uint256 totalPrice;
    uint256[] prices;
    uint256 totalCommission;
    bytes signature;
  }

  struct Session {
    uint256 start;
    uint256 end;
    uint256[] tokenIds;
    address payable feeCollector;
    string baseURI;
  }

  event SessionCreated(
    uint256 sessionId,
    uint256[] tokenIds,
    address[] creators,
    uint256 start,
    uint256 end,
    string sessionBaseURI,
    address[] whitelistCollections,
    uint256[] discountCounts,
    address feeCollector
  );
  event SessionRemoved(uint256 sessionId);

  event TokenBoughtBatch(
    uint256 sessionId,
    uint256[] tokenIds,
    address msgSender,
    uint256[] prices,
    uint256 totalCommission
  );

  event TokenBoughtSingle(
    uint256 sessionId,
    uint256 tokenId,
    address msgSender,
    uint256 price,
    uint256 totalCommission
  );

  event WhitelistTokenUsedToClaim(
    uint256 sessionId,
    address whitelistCollection,
    uint256[] whitelistTokenIds
  );

  event VerifierChanged(address newVerifier);

  constructor() {
    _disableInitializers();
  }

  // initialize function for cloning compatibility
  function initializeOpenEdition(
    string memory contractName,
    string memory contractSymbol,
    address newOwner,
    address payable feeReceiver,
    address verifier
  ) public initializer {
    __ERC1155_init("");
    initializeOpenEditionRoles(newOwner);

    _name = contractName;
    _symbol = contractSymbol;
    _commissionReceiver = feeReceiver;
    _verifier = verifier;
  }

  function supportsInterface(bytes4 interfaceId)
    public
    view
    virtual
    override(ERC1155Upgradeable, AccessControlEnumerableUpgradeable)
    returns (bool)
  {
    return
      ERC1155Upgradeable.supportsInterface(interfaceId) ||
      AccessControlEnumerableUpgradeable.supportsInterface(interfaceId);
  }

  /**
   * @dev Gets the token name
   */
  function name() public view returns (string memory) {
    return _name;
  }

  /**
   * @dev Gets the token symbol
   */
  function symbol() public view returns (string memory) {
    return _symbol;
  }

  /**
   * @dev Returns token URI
   */
  function uri(uint256 id)
    public
    view
    virtual
    override
    returns (string memory)
  {
    uint256 sessionId = _tokenToSession[id];
    return string(abi.encodePacked(_sessions[sessionId].baseURI, StringsUpgradeable.toString(id)));
  }

  /**
   * @dev Returns token creator
   */
  function creator(uint256 id) public view returns (address) {
    return _creators[id];
  }

  /**
   * @dev Returns total supply
   */
  function totalSupply(uint256 id) public view returns (uint256) {
    return _totalSupply[id];
  }

  /**
   * @dev Returns max supply
   */
  function maxSupply(uint256 id) public view returns (uint256) {
    if (_sessions[_tokenToSession[id]].end < block.timestamp) {
      return _totalSupply[id];
    }
    return _maxSupply[id];
  }

  function mint(address to, uint256 id) internal {
    require(
      _maxSupply[id] == 0 || _totalSupply[id] < _maxSupply[id],
      "OE: cannot mint more token"
    );
    _totalSupply[id] += 1;

    _mint(to, id, 1, "");
  }

  /**
   * @dev Mints a batch token
   */
  function mintBatch(
    address to,
    uint256[] memory ids,
    uint256[] memory amounts,
    bytes memory data
  ) internal {
    for (uint256 i = 0; i < ids.length; i++) {
      require(
        _maxSupply[ids[i]] == 0 ||
          _totalSupply[ids[i]] + amounts[i] <= _maxSupply[ids[i]],
        "OE: cannot mint more token"
      );
      _totalSupply[ids[i]] += amounts[i];
    }
    _mintBatch(to, ids, amounts, data);
  }

  /**
   * @dev Burns token
   */
  function burn(
    address account,
    uint256 id,
    uint256 value
  ) public {
    require(
      account == _msgSender() || isApprovedForAll(account, _msgSender()),
      "ERC1155: caller not approved"
    );

    _burn(account, id, value);
  }

  /**
   * @dev Burns a batch token
   */
  function burnBatch(
    address account,
    uint256[] memory ids,
    uint256[] memory values
  ) public {
    require(
      account == _msgSender() || isApprovedForAll(account, _msgSender()),
      "ERC1155: caller not approved"
    );

    _burnBatch(account, ids, values);
  }

  /**
   * @dev Returns total sessions
   */
  function totalSessions() public view returns (uint256) {
    return _totalSessions;
  }

  /**
   * @dev Creates a session
   */
  function createSession(
    uint256 sessionId,
    uint256[] memory tokenIds,
    uint256[] memory maxSupplies,
    address[] memory creators,
    uint256 start,
    uint256 end,
    string memory sessionBaseURI,
    address[] memory whitelistCollections,
    uint256[] memory discountCounts,
    address payable feeCollector
  ) public onlyAdmin {
    require(sessionId != 0, "OE: SessionId starts from 1");
    require(
      _sessions[sessionId].start == 0,
      "OpenEdition: session already set"
    );
    require(
      start < end && start > 0 && end > 0 && block.timestamp < start,
      "OpenEdition: invalid time"
    );

    for (uint256 i = 0; i < whitelistCollections.length; i++) {
      address collection = whitelistCollections[i];
      require(
        IERC165(collection).supportsInterface(type(IERC721).interfaceId),
        "OE: Whitelist address not ERC721"
      );

      _sessionsMapping[sessionId].collectionWhitelisted[collection] = true;
      _sessionsMapping[sessionId].discountCounts[collection] = discountCounts[
        i
      ];
    }

    // update tokenCreators
    for (uint256 i = 0; i < creators.length; i++) {
      address tokenCreator = creators[i];
      require(
        tokenCreator != address(0) && !tokenCreator.isContract(),
        "OE: can not create Session"
      );

      _creators[tokenIds[i]] = tokenCreator;
      _tokenToSession[tokenIds[i]] = sessionId;
      _maxSupply[tokenIds[i]] = maxSupplies[i];
    }

    Session memory newSession = Session(
      start,
      end,
      tokenIds,
      feeCollector,
      sessionBaseURI
    );

    _sessions[sessionId] = newSession;

    _totalSessions++;

    emit SessionCreated(
      sessionId,
      tokenIds,
      creators,
      start,
      end,
      sessionBaseURI,
      whitelistCollections,
      discountCounts,
      feeCollector
    );
  }

  /**
   * @dev Removes a session
   */
  function removeSession(uint256[] memory sessionIds) public onlyAdmin {
    uint256 cnt = 0;

    for (; cnt < sessionIds.length; cnt++) {
      uint256 id = sessionIds[cnt];
      Session memory selectedSession = _sessions[id];

      require(
        selectedSession.start > 0 && block.timestamp < selectedSession.start,
        "OpenEdition: session on-going"
      );

      if (selectedSession.tokenIds.length > 0) {
        for (uint256 index; index < selectedSession.tokenIds.length; index++) {
          require(
            _totalSupply[selectedSession.tokenIds[index]] == 0,
            "OE: some tokens already minted"
          );

          delete _tokenToSession[selectedSession.tokenIds[index]];
          delete _creators[selectedSession.tokenIds[index]];
          delete _maxSupply[selectedSession.tokenIds[index]];
        }
      }

      delete _sessions[id];
      delete _sessionsMapping[id];

      emit SessionRemoved(id);
    }

    if (cnt > 0) {
      _totalSessions -= cnt;
    }
  }

  /**
   * @dev Owner can change the session base_uri in case of misconfiguration
   */
  function changeSessionBaseURI(uint256 sessionId, string memory newBaseURI)
    public
    onlyOwner
  {
    require(_sessions[sessionId].start > 0, "OE: Session does not exist");
    _sessions[sessionId].baseURI = newBaseURI;
  }

  /**
   * @dev Returns session information
   */
  function session(uint256 sessionId)
    public
    view
    returns (
      uint256[] memory,
      uint256,
      uint256,
      address,
      string memory
    )
  {
    Session memory selectedSession = _sessions[sessionId];
    return (
      selectedSession.tokenIds,
      selectedSession.start,
      selectedSession.end,
      selectedSession.feeCollector,
      selectedSession.baseURI
    );
  }

  /**
   * @dev Returns the address of commission receiver
   */
  function commissionReceiver() public view returns (address) {
    return _commissionReceiver;
  }

  /**
   * @dev Returns token information
   */
  function token(uint256 tokenId)
    public
    view
    returns (
      string memory,
      address,
      uint256,
      uint256,
      uint256
    )
  {
    return (
      uri(tokenId),
      creator(tokenId),
      totalSupply(tokenId),
      _tokenToSession[tokenId],
      maxSupply(tokenId)
    );
  }

  function _buyTokenSingle(MintInfoSingle memory mintInfo) internal {
    require(
      _verify(
        _hashSingle(
          mintInfo.tokenId,
          mintInfo.totalPrice,
          mintInfo.price,
          mintInfo.totalCommission
        ),
        mintInfo.signature
      ),
      "OE: cannot verify signature"
    );

    Session memory selectedSession = _sessions[mintInfo.sessionId];

    require(
      selectedSession.start < block.timestamp &&
        block.timestamp < selectedSession.end &&
        mintInfo.totalPrice == msg.value,
      "OpenEdition: can not buy token"
    );
    require(!msg.sender.isContract(), "OpenEdition: caller is invalid");

    if (mintInfo.totalCommission > 0) {
      _commissionReceiver.transfer(mintInfo.totalCommission);
    }

    if (mintInfo.totalPrice > mintInfo.totalCommission) {
      if (selectedSession.feeCollector != address(0)) {
        payable(selectedSession.feeCollector).transfer(
          mintInfo.totalPrice - mintInfo.totalCommission
        );
      } else {
        if (mintInfo.price > 0) {
          payable(creator(mintInfo.tokenId)).transfer(mintInfo.price);
        }
      }
    }

    mint(msg.sender, mintInfo.tokenId);

    emit TokenBoughtSingle(
      mintInfo.sessionId,
      mintInfo.tokenId,
      msg.sender,
      mintInfo.price,
      mintInfo.totalCommission
    );
  }

  function _buyTokenBatch(MintInfoBatch memory mintInfo) internal {
    require(
      _verify(
        _hashBatch(
          mintInfo.tokenIds,
          mintInfo.totalPrice,
          mintInfo.prices,
          mintInfo.totalCommission
        ),
        mintInfo.signature
      ),
      "OE: cannot verify signature"
    );
    Session memory selectedSession = _sessions[mintInfo.sessionId];

    require(
      selectedSession.start < block.timestamp &&
        block.timestamp < selectedSession.end &&
        mintInfo.totalPrice == msg.value,
      "OpenEdition: can not buy token"
    );
    require(!msg.sender.isContract(), "OpenEdition: caller is invalid");

    if (mintInfo.totalCommission > 0) {
      _commissionReceiver.transfer(mintInfo.totalCommission);
    }

    if (mintInfo.totalPrice > mintInfo.totalCommission) {
      if (selectedSession.feeCollector != address(0)) {
        payable(selectedSession.feeCollector).transfer(
          mintInfo.totalPrice - mintInfo.totalCommission
        );
      } else {
        for (uint256 i = 0; i < mintInfo.prices.length; i++) {
          if (mintInfo.prices[i] > 0) {
            payable(creator(mintInfo.tokenIds[i])).transfer(mintInfo.prices[i]);
          }
        }
      }
    }

    mintBatch(msg.sender, mintInfo.tokenIds, mintInfo.amounts, "");

    emit TokenBoughtBatch(
      mintInfo.sessionId,
      mintInfo.tokenIds,
      msg.sender,
      mintInfo.prices,
      mintInfo.totalCommission
    );
  }

  /**
   * @dev But Token one by one
   */
  function buyTokenSingle(MintInfoSingle memory mintInfo) public payable {
    require(
      _creators[mintInfo.tokenId] != address(0),
      "OpenEdition: token doesn't exist"
    );
    require(
      _tokenToSession[mintInfo.tokenId] == mintInfo.sessionId,
      "OE: token not in session"
    );

    _buyTokenSingle(mintInfo);
  }

  /**
   * @dev Buy token one by one, using a whitelist Token
   */
  function buyTokenSingleWithWhitelist(
    MintInfoSingle memory mintInfo,
    address whitelistCollection,
    uint256[] calldata whitelistTokenIds
  ) public payable {
    require(
      _creators[mintInfo.tokenId] != address(0),
      "OpenEdition: token doesn't exist"
    );
    require(
      _tokenToSession[mintInfo.tokenId] == mintInfo.sessionId,
      "OE: token not in session"
    );

    SessionMapping storage selectedSessionMapping = _sessionsMapping[
      mintInfo.sessionId
    ];

    require(
      selectedSessionMapping.collectionWhitelisted[whitelistCollection] == true,
      "OE: not in whitelist"
    );

    IERC721 collection = IERC721(whitelistCollection);

    ERC721Redeemer.Claims
      storage redeemedWhitelistTokens = selectedSessionMapping
        .redeemedWhitelistTokens[whitelistCollection];

    redeemedWhitelistTokens.redeem(
      selectedSessionMapping.discountCounts[whitelistCollection],
      msg.sender,
      collection,
      whitelistTokenIds
    );

    emit WhitelistTokenUsedToClaim(
      mintInfo.sessionId,
      whitelistCollection,
      whitelistTokenIds
    );

    _buyTokenSingle(mintInfo);
  }

  /**
   * @dev Buys token in batch
   */
  function buyTokenBatch(MintInfoBatch memory mintInfo) public payable {
    for (uint256 i = 0; i < mintInfo.tokenIds.length; i++) {
      require(
        _creators[mintInfo.tokenIds[i]] != address(0),
        "OpenEdition: token doesn't exist"
      );
      require(
        _tokenToSession[mintInfo.tokenIds[i]] == mintInfo.sessionId,
        "OE: token not in session"
      );
    }

    _buyTokenBatch(mintInfo);
  }

  /**
   * @dev Buys tokenin batch using token from a whitelist collection
   */
  function buyTokenBatchWithWhitelist(
    MintInfoBatch memory mintInfo,
    address whitelistCollection,
    uint256[] calldata whitelistCollectionTokenIds
  ) public payable {
    SessionMapping storage selectedSessionMapping = _sessionsMapping[
      mintInfo.sessionId
    ];

    require(
      selectedSessionMapping.collectionWhitelisted[whitelistCollection] == true,
      "OE: not in whitelist"
    );

    for (uint256 i = 0; i < mintInfo.tokenIds.length; i++) {
      require(
        _creators[mintInfo.tokenIds[i]] != address(0),
        "OpenEdition: token doesn't exist"
      );
      require(
        _tokenToSession[mintInfo.tokenIds[i]] == mintInfo.sessionId,
        "OE: token not in session"
      );
    }

    IERC721 collection = IERC721(whitelistCollection);

    ERC721Redeemer.Claims
      storage redeemedWhitelistTokens = selectedSessionMapping
        .redeemedWhitelistTokens[whitelistCollection];

    uint256 n = redeemedWhitelistTokens.redeem(
      selectedSessionMapping.discountCounts[whitelistCollection],
      msg.sender,
      collection,
      whitelistCollectionTokenIds
    );

    // test if can remove
    require(
      whitelistCollectionTokenIds.length == mintInfo.tokenIds.length &&
        n == mintInfo.tokenIds.length,
      "OE: array length mismatch"
    );

    emit WhitelistTokenUsedToClaim(
      mintInfo.sessionId,
      whitelistCollection,
      whitelistCollectionTokenIds
    );

    _buyTokenBatch(mintInfo);
  }

  /**
   * @dev return the address used for message verification
   */
  function getMessageVerifier() public view returns (address) {
    return _verifier;
  }

  /**
   * @dev change the address that verifies the message
   */
  function changeMessageVerifier(address newVerifier) public onlyOwner {
    _verifier = newVerifier;
    emit VerifierChanged(newVerifier);
  }

  /**
   * @dev get the hash of data to sign
   * @param tokenId the token Id to mint
   * @param totalPrice the total ammount to pay
   * @param price the creator share for the respective tokenId
   * @param totalCommission the total amount sent to commission recevier, totalPrice = sum(prices) + totalCommission
   */
  function _hashSingle(
    uint256 tokenId,
    uint256 totalPrice,
    uint256 price,
    uint256 totalCommission
  ) internal pure returns (bytes32) {
    return (
      ECDSAUpgradeable.toEthSignedMessageHash(
        keccak256(abi.encodePacked(tokenId, totalPrice, price, totalCommission))
      )
    );
  }

  /**
   * @dev get the hash of data to sign
   * @param tokenIds the list of token Ids to mint
   * @param totalPrice the total ammount to pay
   * @param prices the list of creator share for the respective tokenId
   * @param totalCommission the total amount sent to commission recevier, totalPrice = sum(prices) + totalCommission
   */
  function _hashBatch(
    uint256[] memory tokenIds,
    uint256 totalPrice,
    uint256[] memory prices,
    uint256 totalCommission
  ) internal pure returns (bytes32) {
    return (
      ECDSAUpgradeable.toEthSignedMessageHash(
        keccak256(
          abi.encodePacked(tokenIds, totalPrice, prices, totalCommission)
        )
      )
    );
  }

  function _verify(bytes32 digest, bytes memory signature)
    internal
    view
    returns (bool)
  {
    return (_verifier == ECDSAUpgradeable.recover(digest, signature));
  }
}

