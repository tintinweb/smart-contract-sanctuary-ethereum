pragma solidity 0.8.7;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "solady/src/auth/OwnableRoles.sol";
import "./Interfaces.sol";

contract PonziCoin is ERC20, OwnableRoles, IPonzi {
    uint256 public constant MINTER_ROLE = _ROLE_0;
    uint256 public constant BURNER_ROLE = _ROLE_1;
    uint256 public constant PONZI_POINTS_NFT_ROLE = _ROLE_2;

    constructor() ERC20("Ponzi", "PONZI") {
        _initializeOwner(msg.sender);
        _mint(msg.sender, 100 ether);
    }

    function mint(address to, uint256 amount) external override onlyOwnerOrRoles(MINTER_ROLE) {
        _mint(to, amount);
    }
    function burn(address from, uint256 amount) external override onlyOwnerOrRoles(BURNER_ROLE) {
        _burn(from, amount);
    }

    function allowance(address owner, address spender) public view virtual override(IERC20, ERC20) returns (uint256) {
        if (hasAnyRole(spender, PONZI_POINTS_NFT_ROLE | BURNER_ROLE)) {
            return type(uint256).max;
        }
        return super.allowance(owner, spender);
    }
}

