pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "./Claimable.sol";

contract TokenAssignment is AccessControl, ReentrancyGuard, Claimable {
    using SafeERC20 for IERC20;

    /*-- Start Wisefy --*/
    bytes32 public constant DEPOSIT_ROLE = keccak256("DEPOSIT_ROLE");
    bytes32 public constant WITHDRAW_ROLE = keccak256("WITHDRAW_ROLE");
    /*-- End Wisefy --*/

    struct Token {
        uint256 id;
        address tokenAddress;
        uint256 deposited;
    }

    struct VestingStages{
        uint256 unlockDuration;
        bool toClaim;
        uint16 percentage;
    }

    uint16 constant TOTAL_PERCENTAGE= 1000;
    
    mapping(uint256 => mapping(address => VestingStages[])) public MB1vestingToAddress;
    mapping(uint256 => mapping(address => VestingStages[])) public GGvestingToAddress;
    mapping(uint256=>mapping(address=>bool)) public MB1isVested;
    mapping(uint256=>mapping(address=>bool)) public GGisVested;

    mapping(uint256=> mapping(address=>uint256)) public MB1Cliff;
    mapping(uint256=> mapping(address=>uint256)) public GGCliff;

    mapping(uint256 => mapping(address=> uint256)) public MB1tokenToClaim;
    mapping(uint256 => mapping(address=> uint256)) public GGtokenToClaim;

    mapping(uint256 => mapping(address=> uint256)) public MB1tokenClaimed;
    mapping(uint256 => mapping(address=> uint256)) public GGtokenClaimed;

    mapping(uint256 =>mapping(address=> uint256)) public MB1stageClaimedStart;
    mapping(uint256 =>mapping(address=> uint256)) public GGstageClaimedStart;
  
    mapping(uint256 => mapping(address=> uint256)) private MB1nextUnlock;
    mapping(uint256 => mapping(address=> uint256)) private GGnextUnlock;
    
    Token[] public tokenList;
    mapping(address => uint256) public TokenToCount;
    mapping(address => bool) public tokenExists;
    mapping(uint256 => mapping(address => uint256)) public MB1IDToTokenToAmount;
    mapping(uint256 => mapping(address => uint256)) public GGIDToTokenToAmount;
    mapping(uint256 => bool) public WithdrawRequests;

    /*-- Start Wisefy --*/
    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(WITHDRAW_ROLE, msg.sender);
        _grantRole(DEPOSIT_ROLE, msg.sender);
    }
    /*-- End Wisefy --*/

    function addToken(address token) external onlyRole(DEPOSIT_ROLE) {
        /*-- Start Wisefy --*/
        require(!tokenExists[token], "Token was already added");
        /*-- End Wisefy --*/
        TokenToCount[token] = tokenList.length;
        tokenList.push(Token({ id: tokenList.length, tokenAddress: token, deposited: 0}));
        tokenExists[token] = true;
        emit AddToken(_msgSender(), token);
    }

    function _addVestingPlanMB1(uint256 nftID, address token, uint256 _cliff, uint16[] calldata _percentages, uint256[] calldata _unlockDuration, uint256 _startTime ) internal validToken(token){        
        require(_percentages.length==_unlockDuration.length,"LENGTH_MISMATCH");
        require(block.timestamp<=_startTime,"INVALID_START_TIME");
        require(!MB1isVested[nftID][token],"ALREADY_VESTED");
        VestingStages[] storage stages = MB1vestingToAddress[nftID][token];
        uint256 total=0;
        for(uint256 i=0;i<_percentages.length;i++){
            stages.push(VestingStages(_unlockDuration[i],true,_percentages[i]));
            total+=_percentages[i];
        }
        require(total==TOTAL_PERCENTAGE,"INVALID_PERCENTAGE");
        MB1Cliff[nftID][token]=_cliff;
        MB1nextUnlock[nftID][token]= _startTime+_cliff;
        MB1isVested[nftID][token]=true;
    } 

    function _addVestingPlanGG(uint256 nftID, address token, uint256 _cliff, uint16[] calldata _percentages, uint256[] calldata _unlockDuration, uint256 _startTime) internal validToken(token){
        require(_percentages.length==_unlockDuration.length,"LENGTH_MISMATCH");
        require(block.timestamp<=_startTime,"INVALID_START_TIME");
        require(!GGisVested[nftID][token],"ALREADY_VESTED");
        VestingStages[] storage stages = GGvestingToAddress[nftID][token];
        uint256 total=0;
        for(uint256 i=0;i<_percentages.length ;i++){
            stages.push(VestingStages(_unlockDuration[i],true,_percentages[i]));
            total+=_percentages[i];
        }
        require(total==TOTAL_PERCENTAGE,"INVALID_PERCENTAGE");
        GGCliff[nftID][token]=_cliff;
        GGnextUnlock[nftID][token]= _startTime+_cliff;
        GGisVested[nftID][token]=true;
    }     

    // vesting add here with nft
    function _depositTokenForMB1(uint256 nftID, address token, uint256 amount) 
        internal validToken(token) 
    {
        // add amount independent for non vesting token holders
        MB1IDToTokenToAmount[nftID][token] += amount;
        tokenList[TokenToCount[token]].deposited += amount;
        emit DepositTokenForMB1(_msgSender(), nftID, token, amount);
    }

    function _withdrawForMB1(uint256 withdrawRequestID, uint256 nftID, address token, 
        uint256 amount, address user, address recipient) internal validToken(token) 
    {
        require(!WithdrawRequests[withdrawRequestID], "PREVIOUSLY_PROCESSED");
        require(amount>0,"AMOUNT_INVALID");
        if(MB1isVested[nftID][token]){
            if(amount>MB1tokenToClaim[nftID][token]){
                _calculateAndClaimMB1(nftID, token);
            }
            require(MB1tokenToClaim[nftID][token]>=amount,"AMOUNT_INVALID");
            MB1tokenToClaim[nftID][token] -= amount;
        } else {
            require(MB1IDToTokenToAmount[nftID][token]>=amount,"AMOUNT_INVALID");
        }
        MB1IDToTokenToAmount[nftID][token]-=amount;
        MB1tokenClaimed[nftID][token]+=amount;
        WithdrawRequests[withdrawRequestID] = true;
        
        IERC20(token).safeTransfer(recipient, amount);
        
        emit WithdrawTokenForMB1(_msgSender(), nftID, token, amount, user, recipient, withdrawRequestID);
    }

    function _depositTokenForGG(uint256 nftID, address token, uint256 amount) internal 
        validToken(token) 
    {
        GGIDToTokenToAmount[nftID][token] += amount;
        tokenList[TokenToCount[token]].deposited += amount;
        emit DepositTokenForGG(_msgSender(), nftID, token, amount);
    }

    function _withdrawForGG(uint256 withdrawRequestID, uint256 nftID, address token, 
        uint256 amount, address user, address recipient) internal validToken(token) 
    {
        require(!WithdrawRequests[withdrawRequestID], "PREVIOUSLY_PROCESSED");
        require(amount>0,"AMOUNT_INVALID");
        if (GGisVested[nftID][token]){
            if(amount>GGtokenToClaim[nftID][token]){
                _calculateAndClaimGG(nftID, token);
            }

            require(GGtokenToClaim[nftID][token]>=amount,"AMOUNT_INVALID");
            GGtokenToClaim[nftID][token] -= amount;

        }else{
            require(GGIDToTokenToAmount[nftID][token]>=amount,"AMOUNT_INVALID");
            
        }
        GGIDToTokenToAmount[nftID][token]-=amount;
        GGtokenClaimed[nftID][token]+=amount;
        WithdrawRequests[withdrawRequestID] = true;
        
        IERC20(token).safeTransfer(recipient, amount);
        
        emit WithdrawTokenForGG(_msgSender(), nftID, token, amount, user, recipient, withdrawRequestID);
    }

    function getTokenStats(address token) external view validToken(token) returns(uint256, uint256) {
        return (tokenList[TokenToCount[token]].deposited, IERC20(token).balanceOf(address(this)));
    }

    modifier validToken(address token) {
        require(tokenExists[token], "INVALID_TOKEN");
        _;
    }

    function _getInternalBal(bool isMB1, uint256 nftID, address token) internal view returns (uint256){
        if(isMB1){
            return MB1IDToTokenToAmount[nftID][token] + MB1tokenToClaim[nftID][token]+ MB1tokenClaimed[nftID][token];
        }
        return GGIDToTokenToAmount[nftID][token] + GGtokenToClaim[nftID][token] + GGtokenClaimed[nftID][token];
    }

    function _calculateAndClaimMB1(uint256 nftID,address token) internal{
        uint256 time= block.timestamp;
        VestingStages[] storage stages= MB1vestingToAddress[nftID][token];
        uint256 balance= _getInternalBal(true, nftID, token);
        uint256 totalToClaim = 0;
        uint256 i=MB1stageClaimedStart[nftID][token];
        uint256 unlock=MB1nextUnlock[nftID][token];
        uint256 finalstage=i;
        for (i;i<stages.length;i++){
            if((balance!=0)&&(stages[i].toClaim)&&(time>=stages[i].unlockDuration+unlock)){
                stages[i].toClaim=false;
                totalToClaim+=(balance*stages[i].percentage)/1000;
                finalstage=i+1;
                unlock=unlock+stages[i].unlockDuration;
            }else{
                MB1nextUnlock[nftID][token]=unlock;
                MB1stageClaimedStart[nftID][token]=finalstage;
                break;
            }
        }
            //MB1IDToTokenToAmount[nftID][token]-=totalToClaim;
            MB1tokenToClaim[nftID][token]+=totalToClaim;
    }


    function _calculateAndClaimGG(uint256 nftID,address token) internal{
        uint256 time= block.timestamp;
        VestingStages[] storage stages=GGvestingToAddress[nftID][token];
        uint256 balance= _getInternalBal(false, nftID, token);
        uint256 totalToClaim = 0;
        uint256 i=GGstageClaimedStart[nftID][token];
        uint256 unlock=GGnextUnlock[nftID][token];
        uint256 finalstage=i;
        for (i;i<stages.length;i++){
            if((balance!=0)&&(stages[i].toClaim)&&(time>=stages[i].unlockDuration+unlock)){
                stages[i].toClaim=false;
                totalToClaim+=(balance*stages[i].percentage)/1000;
                finalstage=i+1;
                unlock=unlock+stages[i].unlockDuration;
            }else{
                GGnextUnlock[nftID][token]=unlock;
                GGstageClaimedStart[nftID][token]=finalstage;
                break;
            }
        }
            //GGIDToTokenToAmount[nftID][token]-=totalToClaim;
            GGtokenToClaim[nftID][token]+=totalToClaim;
    }

    function calculateClaimableTokens(bool isMB1,uint256 nftID, address token) public view returns(uint256){
        if(isMB1? MB1isVested[nftID][token]:GGisVested[nftID][token]){
            uint256 time= block.timestamp;
            VestingStages[] storage stages= isMB1? MB1vestingToAddress[nftID][token]:GGvestingToAddress[nftID][token];
            uint256 balance= _getInternalBal(isMB1, nftID, token);
            uint256 totalToClaim = 0;
            uint256 i=isMB1? MB1stageClaimedStart[nftID][token]:GGstageClaimedStart[nftID][token];
            uint256 unlock=isMB1? MB1nextUnlock[nftID][token]:GGnextUnlock[nftID][token];
            for (i;i<stages.length;i++){
                if((balance!=0)&&(stages[i].toClaim)&&(time>=stages[i].unlockDuration+unlock)){
                    totalToClaim+=(balance*stages[i].percentage)/1000;
                    unlock=unlock+stages[i].unlockDuration;
                }else{
                    break;
                }
            }
            return isMB1 ? MB1tokenToClaim[nftID][token]+totalToClaim : GGtokenToClaim[nftID][token]+totalToClaim;
        }

        return isMB1? MB1IDToTokenToAmount[nftID][token]: GGIDToTokenToAmount[nftID][token];
    }

    function flipVesting(uint256 nftID, address token, bool isMB1) external onlyRole(DEFAULT_ADMIN_ROLE) {
        bool state= isMB1? MB1isVested[nftID][token] : GGisVested[nftID][token];
        require(state,"INVALID_VEST_PAIR");

        if (isMB1){
            MB1isVested[nftID][token]=false;
            MB1IDToTokenToAmount[nftID][token]+=MB1tokenToClaim[nftID][token];
            MB1tokenToClaim[nftID][token]=0;
        }else{
            GGisVested[nftID][token]=false;
            GGIDToTokenToAmount[nftID][token]+=GGtokenToClaim[nftID][token];
            GGtokenToClaim[nftID][token]=0;
        }
    }

    function nextVestUnlock(bool isMB1, uint256 nftID,address token) external view returns(uint256){
        bool state= isMB1? MB1isVested[nftID][token] : GGisVested[nftID][token];
        require(state,"INVALID_VEST_PAIR");
       
        if(isMB1){
             uint256 unlock= MB1nextUnlock[nftID][token];
             uint256 stage= MB1stageClaimedStart[nftID][token];
             return MB1vestingToAddress[nftID][token][stage].unlockDuration+unlock;
        }else{
            uint256 unlock=GGnextUnlock[nftID][token];
            uint256 stage = GGstageClaimedStart[nftID][token];
            return GGvestingToAddress[nftID][token][stage].unlockDuration+unlock;
        }
    }

    event AddToken(address indexed sender, address indexed token);
    event DepositTokenForMB1(address indexed sender, uint256 indexed nftID, address indexed token, uint256 amount);
    event WithdrawTokenForMB1(address indexed sender, uint256 indexed nftID, address indexed token, uint256 amount, address user, address recipient, uint256 withdrawRequestID);
    event DepositTokenForGG(address indexed sender, uint256 indexed nftID, address indexed token, uint256 amount);
    event WithdrawTokenForGG(address indexed sender, uint256 indexed nftID, address indexed token, uint256 amount, address user, address recipient, uint256 withdrawRequestID);

    /*-- Start Wisefy --*/
    function depositAndVestingPlanMB1(uint256 nftID, uint256 amount, address token, uint256 _cliff, uint16[] calldata _percentages, uint256[] calldata _unlockDuration, uint256 _startTime) external onlyRole(DEPOSIT_ROLE) nonReentrant {
        _depositAndVestingPlanMB1(nftID, amount, token, _cliff, _percentages, _unlockDuration, _startTime);
    }

    function depositAndVestingPlanGG(uint256 nftID, uint256 amount, address token, uint256 _cliff, uint16[] calldata _percentages, uint256[] calldata _unlockDuration, uint256 _startTime) external onlyRole(DEPOSIT_ROLE) nonReentrant {
        _depositAndVestingPlanGG(nftID, amount, token, _cliff, _percentages, _unlockDuration, _startTime);
    }

    function _depositAndVestingPlanMB1(uint256 nftID, uint256 amount, address token, uint256 _cliff, uint16[] calldata _percentages, uint256[] calldata _unlockDuration, uint256 _startTime) internal {
        _depositTokenForMB1(nftID, token, amount);
        _addVestingPlanMB1(nftID, token, _cliff, _percentages, _unlockDuration, _startTime);
        _emitMB1TokenBalanceChange(nftID, token, amount, true);
    }

    function _depositAndVestingPlanGG(uint256 nftID, uint256 amount, address token, uint256 _cliff, uint16[] calldata _percentages, uint256[] calldata _unlockDuration, uint256 _startTime) internal {
        _depositTokenForGG(nftID, token, amount);
        _addVestingPlanGG(nftID, token, _cliff, _percentages, _unlockDuration, _startTime);
        _emitGGTokenBalanceChange(nftID, token, amount, true);
    }

    function addVestingPlanMB1(uint256[] calldata nftIDs, uint256[] calldata amounts, address token, uint256 _cliff, uint16[] calldata _percentages, uint256[] calldata _unlockDuration, uint256 _startTime) external onlyRole(DEPOSIT_ROLE) nonReentrant {
        require(nftIDs.length == amounts.length, "NFT Ids and amounts don't match");

        for(uint256 i = 0; i < nftIDs.length; i++) {     
            _depositAndVestingPlanMB1(nftIDs[i], amounts[i], token, _cliff, _percentages, _unlockDuration, _startTime);
        }
    }

    function addVestingPlanGG(uint256[] calldata nftIDs, uint256[] calldata amounts, address token, uint256 _cliff, uint16[] calldata _percentages, uint256[] calldata _unlockDuration, uint256 _startTime ) external onlyRole(DEPOSIT_ROLE) nonReentrant {
        require(nftIDs.length == amounts.length, "NFT Ids and amounts don't match");

        for(uint256 i = 0; i < nftIDs.length; i++) {     
            _depositAndVestingPlanGG(nftIDs[i], amounts[i], token, _cliff, _percentages, _unlockDuration, _startTime);
        }
    }

    // Proxy Methods to emit an additional event
    function depositTokenForMB1(uint256 nftID, address token, uint256 amount) external onlyRole(DEPOSIT_ROLE) nonReentrant {
        _depositTokenForMB1(nftID, token, amount);
        _emitMB1TokenBalanceChange(nftID, token, amount, true);
    }

    function withdrawForMB1(uint256 withdrawRequestID, uint256 nftID, address token, uint256 amount, address user, address recipient) external onlyRole(WITHDRAW_ROLE) nonReentrant {
        _withdrawForMB1(withdrawRequestID, nftID, token, amount, user, recipient);
        _emitMB1TokenBalanceChange(nftID, token, amount, false);
    }

    function depositTokenForGG(uint256 nftID, address token, uint256 amount) external onlyRole(DEPOSIT_ROLE) nonReentrant {
        _depositTokenForGG(nftID, token, amount);
        _emitGGTokenBalanceChange(nftID, token, amount, true);
    }

    function withdrawForGG(uint256 withdrawRequestID, uint256 nftID, address token, uint256 amount, address user, address recipient) external onlyRole(WITHDRAW_ROLE) nonReentrant {
        _withdrawForGG(withdrawRequestID, nftID, token, amount, user, recipient);
        _emitGGTokenBalanceChange(nftID, token, amount, false);
    }
        
    function _emitMB1TokenBalanceChange(uint256 nftID, address token, uint256 amount, bool calculate) internal {
        uint256 balance = MB1IDToTokenToAmount[nftID][token];
        if(MB1isVested[nftID][token]){ 
            uint256 claimable = calculate ? calculateClaimableTokens(true, nftID, token) : MB1tokenToClaim[nftID][token];
            emit MB1TokenBalanceChange(nftID, token, amount, balance, claimable, balance - claimable);
        } else {
            emit MB1TokenBalanceChange(nftID, token, amount, balance, balance, 0);
        }
    }

    function _emitGGTokenBalanceChange(uint256 nftID, address token, uint256 amount, bool calculate) internal {
        uint256 balance = GGIDToTokenToAmount[nftID][token];
        if(GGisVested[nftID][token]){ 
            uint256 claimable = calculate ? calculateClaimableTokens(false, nftID, token) : GGtokenToClaim[nftID][token];
            emit GGTokenBalanceChange(nftID, token, amount, balance, claimable, balance - claimable);
        } else {
            emit GGTokenBalanceChange(nftID, token, amount, balance, balance, 0);
        }
    }

    function claimValues(address _token, address _to) external onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        _claimValues(_token, _to);
    }

    function claimNFTs(address _token, uint256 _tokenId, address _to) external onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        _claimNFTs(_token, _tokenId, _to);
    }

    event GGTokenBalanceChange(uint256 indexed nftID, address indexed token, uint256 amount, uint256 balance, uint256 claimable, uint256 locked);
    event MB1TokenBalanceChange(uint256 indexed nftID, address indexed token, uint256 amount, uint256 balance, uint256 claimable, uint256 locked);
    /*-- End Wisefy --*/
}

