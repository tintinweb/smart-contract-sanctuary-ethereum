pragma solidity 0.8.17;

import "forge-std/Script.sol";
import "solmate/tokens/ERC721.sol";

contract MockMk1Solarbots is ERC721 {
    uint256 public totalSupply;

    constructor() ERC721("Solarbots Mk.1", "SBMK1") {}

    function tokenURI(uint256) public pure virtual override returns (string memory) {}

    function mint(uint256 amount) public virtual {
        uint256 id = totalSupply;
        totalSupply += amount;

        for (uint256 i = 0; i < amount; i++) {
            _safeMint(msg.sender, id + i);
        }
    }

    function burn(uint256 id) public virtual {
        address owner = _ownerOf[id];
        require(owner == msg.sender, "NOT_TOKEN_OWNER");

        totalSupply--;

        _burn(id);
    }
}

contract MockMk1SolarbotsScript is Script {
    function run() external {
        uint256 deployerPrivateKey = vm.envUint("PRIVATE_KEY");
        vm.startBroadcast(deployerPrivateKey);

        bytes32 salt = keccak256("MockMk1Solarbots");

        // The contract's creating code and its constructor arguments
        bytes memory creationCode = type(MockMk1Solarbots).creationCode;

        address deployedContract;
        assembly {
            // Deploy the contract using CREATE2
            // Skip the first 32 bytes of the creation code containing its length
            deployedContract := create2(0, add(creationCode, 0x20), mload(creationCode), salt)
        }
        require(deployedContract != address(0), "DEPLOYMENT_FAILED");

        vm.stopBroadcast();
    }
}