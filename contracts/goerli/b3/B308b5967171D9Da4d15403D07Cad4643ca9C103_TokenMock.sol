pragma solidity ^0.8.17;

import {ReentrancyGuard} from "lib/openzeppelin-contracts/contracts/security/ReentrancyGuard.sol";
import {IERC20} from "lib/openzeppelin-contracts/contracts/token/ERC20/ERC20.sol";
import {SafeERC20} from "lib/openzeppelin-contracts/contracts/token/ERC20/utils/SafeERC20.sol";
import {Ownable2StepUpgradeable} from "lib/openzeppelin-contracts-upgradeable/contracts/access/Ownable2StepUpgradeable.sol";
import {PausableUpgradeable} from "lib/openzeppelin-contracts-upgradeable/contracts/security/PausableUpgradeable.sol";

///@title Asset Storage Contract for storing rewards
contract AssetStorage is ReentrancyGuard, Ownable2StepUpgradeable, PausableUpgradeable {
    using SafeERC20 for IERC20;

    /**
     * @notice Token that have been released by this asset storage
     */
    uint256 public totalReleased;

    /**
     * @notice address that can call release
     */
    mapping(address => bool) public releasers; //contract staking, for example

    /**
     * @notice reward token
     */
    IERC20 public reward;

    /// @notice Emitted when new permission is set to the releaser
    event ReleasersUpdated(address releaser, bool releasable);

    /// @notice Emitted when the token has been released
    event Released(address indexed releaser, address user, uint256 amount);
    event Sweep(address token, uint256 amount);

    error UnapprovedReleaser(address caller);
    error AddressZero();
    error RewardZero();
    error InsufficientRewards();

    /**
     * @param _reward address of token, like XP
     */
    function initialize(address _reward) public initializer {
        __Ownable2Step_init();
        reward = IERC20(_reward);
    }

    /**
     * @notice Release token to the user
     * @dev Only callable by releasers (Supposed to be RewardGauge)
     * with rewardAccured as the amount
     * @param _user User to release the token to
     * @param _amount Amount to be released
     * @dev return released amount
     */
    function release(address _user, uint256 _amount) external nonReentrant whenNotPaused returns (uint256 released) {
        if (!releasers[msg.sender]) {
            revert UnapprovedReleaser(msg.sender);
        }

        if (_user == address(0)) {
            revert AddressZero();
        }

        if (_amount == 0) {
            revert RewardZero();
        }

        // release reward amount
        _release(_user, _amount);

        emit Released(msg.sender, _user, _amount);

        return _amount;
    }

    /**
     * @notice Release token to the user
     * @param _user User to release the token to
     * @param _amount Amount to be released
     */
    function _release(address _user, uint256 _amount) internal {
        if (_amount > reward.balanceOf(address(this))) {
            revert InsufficientRewards();
        }

        IERC20(reward).safeTransfer(_user, _amount);

        totalReleased = totalReleased + _amount;
    }

    /**
     * @notice Give permission to the address to be able to release the token
     * @param _newReleaser The new releaser
     * @param _releasable Releaseable or not
     */
    function setReleaser(address _newReleaser, bool _releasable) public onlyOwner whenNotPaused {
        releasers[_newReleaser] = _releasable;

        emit ReleasersUpdated(_newReleaser, _releasable);
    }

    /**
     * @notice pause contract
     *
     */
    function pause() external onlyOwner {
        _pause();
    }

    /**
     * @notice unpause contract
     *
     */
    function unpause() external onlyOwner {
        _unpause();
    }

    /**
     * @notice emergency withdraw
     * @param _token address for token for withdraw
     * @param _recipient address of recipient
     *
     */
    function emergencyWithdraw(IERC20 _token, address _recipient) external onlyOwner whenPaused {
        uint256 amount = _token.balanceOf(address(this));
        _token.safeTransfer(_recipient, amount);

        emit Sweep(address(_token), amount);
    }
}

