//staking contract with programs
contract Staking is AccessControlUpgradeable, ReentrancyGuardUpgradeable, PausableUpgradeable {
    using SafeERC20 for IERC20;

    using CountersUpgradeable for CountersUpgradeable.Counter;

    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

    CountersUpgradeable.Counter private _programIds;

    //program status
    enum PSTATUS {
        QUEUE, //not active
        ACTIVE, //active
        ARCHIVED //archived
    }

    struct Program {
        uint256 rate; //rate, apr, per year
        uint256 maxTotalStaked; //max tokens for staking of all users
        uint256 minTxAmountPerStake; //min tokens for staking per user in tx
        uint256 minTxAmountPerUnstake; //min tokens for unstaking per user in tx
        uint256 maxStakedPerUser; //max tokens for staking per user
        uint256 stakingPeriod; //in seconds
        uint256 totalStaked; //totalStaked
        PSTATUS status; //program status
        bytes data;
    }

    struct Stake {
        uint256 amount;
        uint256 programId;
        uint256 startTime;
        uint256 preStakingPeriod;
    }

    uint256 public constant MULTIPLIER = 1 ether; //for precision

    uint256 private constant TX_SLEEP_PERIOD = 60; //60 sec beetween txs

    /**
     * A four-century period will be missing 3 of its 100 Julian leap years, leaving 97.
     * So the average year has 365 + 97/400 = 365.2425 days
     * ERROR(Julian): -0.0078
     * ERROR(Gregorian): -0.0003
     * A day = 24 * 60 * 60 sec = 86400 sec
     * 365.2425 * 86400 = 31556952.0
     */
    uint256 public constant SECS_PER_YEAR = 31_556_952; //365.2425 days

    IERC20 public token; //token for staking

    AssetStorage public assetStorage; //address of asset storage

    mapping(uint256 => Program) public programs; //programs map, programId => Program, programs start from 1

    mapping(address => Stake) public stakes; // stakes map, user => Stake

    mapping(address => uint256) public latestTxTimestamp;

    uint256 public preStakingPeriod;

    uint256 public txSleepPeriod;

    error ProgramIdIsNotValid();
    error ProgramIsNotActive();
    error ProgramIsNotQueue();
    error ProgramIsArchived();
    error StatusIsNotValid();
    error StakeIsNotExist();
    error PreStakePeriodIsExpired();
    error StakingPeriodIsActive();
    error RewardsAreNotExist();
    error LimitsAreReached();
    error StakeIsVeryLow(uint256 amount);
    error ZeroAmount();
    error TxSleepReached(address user);

    event ProgramAdded(
        uint256 programId,
        uint256 rate,
        uint256 maxTotalStaked,
        uint256 minTxAmountPerStake,
        uint256 minTxAmountPerUnstake,
        uint256 maxStakedPerUser,
        uint256 stakingPeriod,
        uint8 status,
        bytes data
    );

    event ProgramArchived(uint256 programId);
    event ProgramActivated(uint256 programId);
    event Sweep(address token, uint256 amount);
    event RewardsAccrued(address account, uint256 rewards);

    event AmountStaked(address account, uint256 stakedAmount, uint256 programId);
    event ProgramChanged(address account, uint256 stakedAmount);
    event StakeCreated(address account, uint256 startTime, uint256 programId);
    event AmountUnstaked(address account, uint256 unstakedAmount, uint256 programId);

