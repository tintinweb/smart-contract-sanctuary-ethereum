    event MaxTotalStakedUpdated(uint256 programId, uint256 amount);

    event TxSetPeriodUpdated(uint256 txSleepPeriod);

    modifier checkProgramId(uint256 _programId) {
        if (_programId > _programIds.current() || _programId == 0) {
            revert ProgramIdIsNotValid();
        }

        _;
    }

    modifier checkTx() {
        if (block.timestamp < latestTxTimestamp[_msgSender()] + txSleepPeriod) {
            revert TxSleepReached(_msgSender());
        }

        _;

        latestTxTimestamp[_msgSender()] = block.timestamp;
    }

    /**
     * @notice initializer
     * @param _assetStorage address of assetStorage
     * @param _token address of xp token
     * @param _preStakingPeriod preStakingPeriod
     */
    function initialize(AssetStorage _assetStorage, IERC20 _token, uint256 _preStakingPeriod) external initializer {
        assetStorage = _assetStorage;
        token = _token;
        preStakingPeriod = _preStakingPeriod;

        txSleepPeriod = TX_SLEEP_PERIOD;

        __AccessControl_init();
        __ReentrancyGuard_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);

        emit PreStakingPeriodUpdated(_preStakingPeriod);
        emit TxSetPeriodUpdated(txSleepPeriod);
    }

    /**
     * @notice add program
     * @param _maxTotalStaked max tokens per protocol, for wei format
     * @param _maxStakedPerUser max tokens per user, for wei format
     * @param _stakingPeriod, staking period, in seconds
     * @param _status, QUEUE or ACTIVE
     */
    function addProgram(
        uint256 _rate,
        uint256 _maxTotalStaked,
        uint256 _minTxAmountPerStake,
        uint256 _minTxAmountPerUnstake,
        uint256 _maxStakedPerUser,
        uint256 _stakingPeriod,
        PSTATUS _status,
        bytes calldata _data
    ) external onlyRole(MANAGER_ROLE) whenNotPaused {
        if (uint8(_status) >= 2) {
            //archive or higher
            revert StatusIsNotValid();
        }

        _programIds.increment();
        uint256 programId = _programIds.current();

        programs[programId] = Program({
            rate: _rate,
            maxTotalStaked: _maxTotalStaked,
            minTxAmountPerStake: _minTxAmountPerStake,
            minTxAmountPerUnstake: _minTxAmountPerUnstake,
            maxStakedPerUser: _maxStakedPerUser,
            stakingPeriod: _stakingPeriod,
            totalStaked: 0,
            status: _status,
            data: _data
        });

        emit ProgramAdded(
            programId,
            _rate,
            _maxTotalStaked,
            _minTxAmountPerStake,
            _minTxAmountPerUnstake,
            _maxStakedPerUser,
            _stakingPeriod,
            uint8(_status),
            _data
        );
    }

    /**
     * @notice archive program
     * @param _programId program Id
     */
    function archiveProgram(uint256 _programId)
        external
        onlyRole(MANAGER_ROLE)
        whenNotPaused
        checkProgramId(_programId)
    {
        programs[_programId].status = PSTATUS.ARCHIVED;

        emit ProgramArchived(_programId);
    }

    /**
     * @notice activate program
     * @param _programId program Id
     */
    function activateProgram(uint256 _programId)
        external
        onlyRole(MANAGER_ROLE)
        whenNotPaused
        checkProgramId(_programId)
    {
        Program storage p = programs[_programId];

        if (p.status != PSTATUS.QUEUE) {
            revert ProgramIsNotQueue();
        }

        p.status = PSTATUS.ACTIVE;

        emit ProgramActivated(_programId);
    }

    /**
     * @notice stake tokens or change program
     * @param _amount tokens for staking
     * @param _programId program Id
     */
    function stake(uint256 _amount, uint256 _programId)
        external
        nonReentrant
        whenNotPaused
        checkTx
        checkProgramId(_programId)
    {
        Program storage p = programs[_programId];

        if (p.status != PSTATUS.ACTIVE) {
            revert ProgramIsNotActive();
        }

        Stake memory s = stakes[_msgSender()];

        //check if stake is exists or no
        if (s.programId == 0) {
            //create new stake
            s.programId = _programId;
            s.startTime = block.timestamp;
            s.preStakingPeriod = preStakingPeriod;

            emit StakeCreated(_msgSender(), s.startTime, _programId);
        }

        if (block.timestamp > s.startTime + s.preStakingPeriod) {
            revert StakingPeriodIsActive();
        }

        if (s.programId != _programId) {
            //clear totalStaked previous program
            Program storage prevP = programs[s.programId];
            prevP.totalStaked -= s.amount;

            // add amount to totalStaked for new program
            p.totalStaked += s.amount;

            //change programId
            s.programId = _programId;

            emit ProgramChanged(_msgSender(), s.programId);
        } else if (_amount == 0) {
            revert ZeroAmount();
        }

        uint256 depositAmount = _adjustAmountByLimits(s, _amount);

        if (depositAmount > 0) {
            s.amount += depositAmount;
            p.totalStaked += depositAmount;
            token.transferFrom(_msgSender(), address(this), depositAmount);

            emit AmountStaked(_msgSender(), _amount, s.programId);
        }

        stakes[_msgSender()] = s;
    }

    /**
     * @notice unstake tokens
     * @param _amount amount for staking
     * @dev available on prestaking period
     */
    function unstake(uint256 _amount) external nonReentrant whenNotPaused checkTx {
        Stake memory s = _getStakeByAccount(_msgSender());
        Program storage p = programs[s.programId];

        uint256 startStakingTimestamp = s.startTime + s.preStakingPeriod;

        if (block.timestamp > startStakingTimestamp) {
            revert PreStakePeriodIsExpired();
        }

        if (_amount > s.amount || s.amount - _amount < p.minTxAmountPerUnstake) {
            // unstake all amount
            _amount = s.amount;
        } else if (_amount < p.minTxAmountPerUnstake) {
            revert LimitsAreReached();
        }

        _unstake(_msgSender(), _amount, p, s.programId);

        if (s.amount == _amount) {
            delete stakes[_msgSender()];
        } else {
            stakes[_msgSender()].amount -= _amount;
        }
    }

    /**
     * @notice unstake and claim rewards
     * @dev available after staking period
     */
    function unstake() external nonReentrant whenNotPaused checkTx {
        Stake memory s = _getStakeByAccount(_msgSender());
        Program storage p = programs[s.programId];

        if (!isStakeExpired(_msgSender())) {
            revert StakingPeriodIsActive();
        }

        _unstakeAndClaimRewards(_msgSender(), s, p);
    }

    /**
     * @notice calculate rewards by user id
     * @param _account address of staker
     */
    function calculateRewards(address _account) external view returns (uint256 rewards) {
        Stake memory s = _getStakeByAccount(_account);
        Program memory p = programs[s.programId];

        uint256 startStakingTimestamp = s.startTime + s.preStakingPeriod;
        uint256 endStakingTimestamp = startStakingTimestamp + p.stakingPeriod;

        if (block.timestamp >= startStakingTimestamp && block.timestamp < endStakingTimestamp) {
            rewards = _calculateRewards(p.rate, s.amount, startStakingTimestamp, block.timestamp);
        } else if (block.timestamp >= endStakingTimestamp) {
            rewards = _calculateRewards(p.rate, s.amount, startStakingTimestamp, endStakingTimestamp);
        }
    }

    /**
     * @notice calculate bonus by user id
     * @param _account address of staker
     * @param _rate bonus percentage
     */
    function calculateBonus(address _account, uint256 _rate) external view returns (uint256 rewards) {
        Stake memory s = _getStakeByAccount(_account);
        Program memory p = programs[s.programId];

        uint256 startStakingTimestamp = s.startTime + s.preStakingPeriod;
        uint256 endStakingTimestamp = startStakingTimestamp + p.stakingPeriod;

        if (block.timestamp >= startStakingTimestamp && block.timestamp < endStakingTimestamp) {
            rewards = _calculateRewards(_rate, s.amount, startStakingTimestamp, block.timestamp);
        } else if (block.timestamp >= endStakingTimestamp) {
            rewards = _calculateRewards(_rate, s.amount, startStakingTimestamp, endStakingTimestamp);
        }
    }

    /**
     * @notice check expiration stake
     * @param _account address of staker
     */
    function isStakeExpired(address _account) public view returns (bool expired) {
        Stake memory s = _getStakeByAccount(_account);
        Program storage p = programs[s.programId];

        uint256 endStakingTimestamp = s.startTime + s.preStakingPeriod + p.stakingPeriod;

        if (block.timestamp >= endStakingTimestamp) {
            expired = true;
        }
    }

    function _unstake(address _account, uint256 _amount, Program storage _p, uint256 _programId) internal {
        _p.totalStaked -= _amount;

        //0. send tokens to wallet
        token.safeTransfer(_account, _amount);

        emit AmountUnstaked(_account, _amount, _programId);
    }

    function _unstakeAndClaimRewards(address _account, Stake memory _s, Program storage _p) internal {
        uint256 startStakingTimestamp = _s.startTime + _s.preStakingPeriod;
        uint256 endStakingTimestamp = startStakingTimestamp + _p.stakingPeriod;

        _unstake(_account, _s.amount, _p, _s.programId);

        //2. calculate rewards
        uint256 rewards = _calculateRewards(_p.rate, _s.amount, startStakingTimestamp, endStakingTimestamp);

        try assetStorage.release(_account, rewards) {
            //3. delete stakes
            delete stakes[_account];
            emit RewardsAccrued(_account, rewards);
        } catch {
            revert RewardsAreNotExist();
        }
    }

    function _calculateRewards(uint256 _rate, uint256 _stakedAmount, uint256 _startTime, uint256 _endTime)
        internal
        pure
        returns (uint256 reward)
    {
        uint256 rewardPerYear = (_stakedAmount * _rate) / MULTIPLIER; //10% from staked amount, for example

        reward = (rewardPerYear * (_endTime - _startTime)) / SECS_PER_YEAR; //calculate reward for period;
    }

    function _adjustAmountByLimits(Stake memory _s, uint256 _amount) internal returns (uint256 amount) {
        Program storage p = programs[_s.programId];

        unchecked {
            uint256 totalLimit = p.maxTotalStaked - p.totalStaked;
            uint256 userLimit;

            // program can changed and maxStakedPerUser can be less than _s.amount
            if (p.maxStakedPerUser > _s.amount) {
                userLimit = p.maxStakedPerUser - _s.amount;
            } else {
                userLimit = 0;

                // refund
                uint256 refundAmount = _s.amount - p.maxStakedPerUser;

                _s.amount -= refundAmount;
                stakes[_msgSender()] = _s;

                _unstake(_msgSender(), refundAmount, p, _s.programId);
            }

            //program can be changed with _amount 0 (and we check that amount enough for stake and with _amount, but we need check that _amount > min stake)
            if ((_amount > 0 && _amount < p.minTxAmountPerStake) || (_amount == 0 && _s.amount < p.minTxAmountPerStake)) {
                revert StakeIsVeryLow(_amount);
            }

            //check limit per total stake and limit per user
            if (totalLimit > 0) {
                if (userLimit > 0) {
                    if (_amount > totalLimit) {
                        amount = totalLimit;
                    } else {
                        amount = _amount;
                    }

                    if (amount > userLimit) {
                        amount = userLimit;
                    }
                }
            } else {
                revert LimitsAreReached();
            }
        }
    }

    function _getStakeByAccount(address _account) internal view returns (Stake memory s) {
        s = stakes[_account];

        if (s.programId == 0) {
            revert StakeIsNotExist();
        }
    }

    // region - Service functions -

    /**
     * @notice pause contract
     *
     */
    function pause() external onlyRole(MANAGER_ROLE) {
        _pause();
    }

    /**
     * @notice unpause contract
     */
    function unpause() external onlyRole(MANAGER_ROLE) {
        _unpause();
    }

    /**
     * @notice emergency withdraw
     * @param _token address of token
     * @param _recipient address of recipient
     */
    function emergencyWithdraw(IERC20 _token, address _recipient) external onlyRole(DEFAULT_ADMIN_ROLE) whenPaused {
        uint256 amount = _token.balanceOf(address(this));
        _token.safeTransfer(_recipient, amount);

        emit Sweep(address(_token), amount);
    }

    /**
     * @notice update prestaking period
     * @param _preStakingPeriod period time
     */
    function updatePrestakingPeriod(uint256 _preStakingPeriod) external onlyRole(MANAGER_ROLE) whenPaused {
        preStakingPeriod = _preStakingPeriod;
        emit PreStakingPeriodUpdated(_preStakingPeriod);
    }

    /**
     * @notice update tx sleep period
     * @param _txSleepPeriod sleep period beetwin txs
     */
    function updateTxSleepPeriod(uint256 _txSleepPeriod) external onlyRole(MANAGER_ROLE) whenPaused {
        txSleepPeriod = _txSleepPeriod;
        emit TxSetPeriodUpdated(_txSleepPeriod);
    }

    /**
     * @notice update maxTotalStaked for the program
     * @param _programId program Id
     * @param _maxTotalStaked max total staked
     */
    function updateMaxTotalStaked(uint256 _programId, uint256 _maxTotalStaked)
        external
        nonReentrant
        onlyRole(MANAGER_ROLE)
        whenNotPaused
        checkProgramId(_programId)
    {
        Program storage p = programs[_programId];

        if (p.status == PSTATUS.ARCHIVED) {
            revert ProgramIsArchived();
        }

        programs[_programId].maxTotalStaked = _maxTotalStaked;

        emit MaxTotalStakedUpdated(_programId, _maxTotalStaked);
    }

    // endregion
}

