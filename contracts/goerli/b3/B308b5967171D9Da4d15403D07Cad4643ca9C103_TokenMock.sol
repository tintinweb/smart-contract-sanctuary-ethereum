pragma solidity ^0.8.17;

import {IERC20} from "lib/openzeppelin-contracts/contracts/token/ERC20/ERC20.sol";
import {SafeERC20} from "lib/openzeppelin-contracts/contracts/token/ERC20/utils/SafeERC20.sol";
import {ReentrancyGuardUpgradeable} from "lib/openzeppelin-contracts-upgradeable/contracts/security/ReentrancyGuardUpgradeable.sol";
import {PausableUpgradeable} from "lib/openzeppelin-contracts-upgradeable/contracts/security/PausableUpgradeable.sol";
import {AccessControlUpgradeable} from "lib/openzeppelin-contracts-upgradeable/contracts/access/AccessControlUpgradeable.sol";
import {CountersUpgradeable} from "lib/openzeppelin-contracts-upgradeable/contracts/utils/CountersUpgradeable.sol";
import {MerkleProofUpgradeable} from "lib/openzeppelin-contracts-upgradeable/contracts/utils/cryptography/MerkleProofUpgradeable.sol";

import {AssetStorage} from "./AssetStorage.sol";
import {IStaking} from "./interfaces/IStaking.sol";

contract Whitelist is AccessControlUpgradeable, ReentrancyGuardUpgradeable, PausableUpgradeable {
    using SafeERC20 for IERC20;

    using CountersUpgradeable for CountersUpgradeable.Counter;

    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

    CountersUpgradeable.Counter private _rootIds;

    AssetStorage public assetStorage; //address of asset storage

    IStaking public staking;

    enum WSTATUS {
        ACTIVE, //active
        ARCHIVED //archived
    }

    struct List {
        bytes32 merkleRoot;
        WSTATUS status;
    }

    mapping(uint256 => List) public roots;

    mapping(address => mapping(uint256 => bool)) public userClaims;

    mapping(bytes32 => uint256) public rootIds;

    event MerkleRootSet(uint256 rootId, bytes32 merkleRoot);
    event RootArchived(uint256 rootId);
    event RootActivated(uint256 rootId);
    event BonusClaimed(uint256 rootId, address account, uint256 bonus);
    event Sweep(address token, uint256 amount);

    error BonusIsNotExist();
    error BonusAlreadyClaimed();
    error RootIsArchived();
    error MerkleProofIsNotValid();
    error RootIdIsNotValid();
    error StakingIsNotExpired();

    modifier checkRootId(uint256 _rootId) {
        if (_rootId > _rootIds.current() || _rootId == 0) {
            revert RootIdIsNotValid();
        }

        _;
    }

    /**
     * @notice initializer
     * @param _assetStorage address of assetStorage
     * @param _staking address of staking
     */
    function initialize(AssetStorage _assetStorage, IStaking _staking) external initializer {
        assetStorage = _assetStorage;
        staking = _staking;
        __AccessControl_init();
        __ReentrancyGuard_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    /**
     * @notice set merkle root
     * @param _merkleRoot root
     */
    function setMerkleRoot(bytes32 _merkleRoot) external onlyRole(MANAGER_ROLE) {
        _rootIds.increment();

        uint256 current = _rootIds.current();

        roots[current] = List({merkleRoot: _merkleRoot, status: WSTATUS.ACTIVE});

        rootIds[_merkleRoot] = current;

        emit MerkleRootSet(current, _merkleRoot);
    }

    /**
     * @notice archive merkle root
     * @param _rootId root Id
     */
    function archiveMerkleRoot(uint256 _rootId) external onlyRole(MANAGER_ROLE) checkRootId(_rootId) {
        roots[_rootId].status = WSTATUS.ARCHIVED;

        emit RootArchived(_rootId);
    }

    /**
     * @notice activate merkle root
     * @param _rootId root Id
     */
    function activateMerkleRoot(uint256 _rootId) external onlyRole(MANAGER_ROLE) checkRootId(_rootId) {
        roots[_rootId].status = WSTATUS.ACTIVE;

        emit RootActivated(_rootId);
    }

    /**
     * @notice claim bonus based on whitelist
     * @param _rootId root Id
     * @param _index index of user in whitelist
     * @param _account address of user
     * @param _bonusRate bonus rate based on whitelist
     * @param _merkleProof merkle proof for user whitelist verify
     */
    function claimBonus(
        uint256 _rootId,
        uint256 _index,
        address _account,
        uint256 _bonusRate,
        bytes32[] calldata _merkleProof
    ) external nonReentrant whenNotPaused checkRootId(_rootId) {
        if (!IStaking(staking).isStakeExpired(_account)) {
            revert StakingIsNotExpired();
        }

        if (userClaims[_account][_rootId]) {
            revert BonusAlreadyClaimed();
        }

        if (roots[_rootId].status != WSTATUS.ACTIVE) {
            revert RootIsArchived();
        }

        // Verify the merkle proof.
        bytes32 node = keccak256(abi.encode(_index, _account, _bonusRate));

        if (MerkleProofUpgradeable.verify(_merkleProof, roots[_rootId].merkleRoot, node)) {
            uint256 bonus = staking.calculateBonus(_account, _bonusRate);

            try assetStorage.release(_account, bonus) {
                userClaims[_account][_rootId] = true;

                emit BonusClaimed(_rootId, _account, bonus);
            } catch {
                revert BonusIsNotExist();
            }
        } else {
            revert MerkleProofIsNotValid();
        }
    }

    // region - Service functions -

    /**
     * @notice pause contract
     */
    function pause() external onlyRole(MANAGER_ROLE) {
        _pause();
    }

    /**
     * @notice unpause contract
     */
    function unpause() external onlyRole(MANAGER_ROLE) {
        _unpause();
    }

    /**
     * @notice sweep of any token
     * @param _token address of token
     * @param _recipient address of recipient
     */
    function sweep(IERC20 _token, address _recipient) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 amount = _token.balanceOf(address(this));
        _token.safeTransfer(_recipient, amount);

        emit Sweep(address(_token), amount);
    }

    // endregion
}