pragma solidity ^0.8.16;

import "lib/openzeppelin-contracts/contracts/token/ERC20/IERC20.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC20/utils/SafeERC20.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC721/ERC721.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "lib/openzeppelin-contracts/contracts/access/Ownable.sol";
import "lib/openzeppelin-contracts/contracts/utils/cryptography/draft-EIP712.sol";
import "lib/openzeppelin-contracts/contracts/utils/Counters.sol";
import "lib/openzeppelin-contracts/contracts/governance/utils/Votes.sol";

using SafeERC20 for IERC20;

contract DonateERC721 is ERC721, ERC721Enumerable, Ownable, EIP712, Votes {
    using Counters for Counters.Counter;

    mapping(uint256 => uint256) private _tokenIdToDonatedAmountsMap;

    struct DonationTier {
        uint256 startCap;
        string url;
    }

    string internal baseURI;

    DonationTier[] public tiers;
    
    Counters.Counter private _tokenIdCounter;

    function _validateTiers(DonationTier[] memory _tiers) pure internal {
        require(_tiers.length > 0, "tiers must be non-empty");
        require(_tiers[0].startCap == 0, "first tier start cap must always be zero");
        uint256 currentCap = 0;
        for (uint256 i = 1; i < _tiers.length; ++i) {
            require(currentCap < _tiers[i].startCap, "next tier's startcap must always be greater than the previous one");
            currentCap = _tiers[i].startCap;
        }
    }

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    constructor(string memory name, string memory symbol, string memory _base, DonationTier[] memory _tiers) ERC721(name, symbol) EIP712(name, "1") Ownable() {
        _validateTiers(_tiers);
        for (uint256 i = 0; i < _tiers.length; ++i) tiers.push(_tiers[i]);
        baseURI = _base;
    }

    function safeMint(address to, uint256 donatedAmount) external onlyOwner {
        require(balanceOf(to) == 0, "only one mint per collection is allowed");
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _tokenIdToDonatedAmountsMap[tokenId] = donatedAmount;
        _safeMint(to, tokenId);
    }

    function increaseDonatedValue(uint256 tokenId, uint256 donatedAmount) external onlyOwner {
        _tokenIdToDonatedAmountsMap[tokenId] += donatedAmount;
    }

    function releaseFunds(IERC20 token, address to, uint256 amount) external onlyOwner {
        token.safeTransfer(to, amount);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721)
        returns (string memory)
    {
        _requireMinted(tokenId);

        DonationTier[] memory _tiers = tiers;

        uint256 donatedAmount = _tokenIdToDonatedAmountsMap[tokenId];
        string memory base = _baseURI();

        for (uint256 i = 1; i < _tiers.length; ++i) {
            if (_tiers[i].startCap > donatedAmount) return string(abi.encodePacked(base, _tiers[i - 1].url));
        }

        return string(abi.encodePacked(base, _tiers[_tiers.length - 1].url));
    }

    // The following functions are overrides required by Solidity.

    function _burn(uint256 tokenId) internal override(ERC721) {
        super._burn(tokenId);
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721) {
        _transferVotingUnits(from, to, _tokenIdToDonatedAmountsMap[tokenId]);
        super._afterTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
    /**
     * @dev Returns the balance of `account`.
     */
    function _getVotingUnits(address account) internal view virtual override returns (uint256) {
        if (balanceOf(account) == 0) return 0;
        return _tokenIdToDonatedAmountsMap[tokenOfOwnerByIndex(account, 0)];
    }
}

