pragma solidity ^0.8.4;

import './InsomniaCollectible.sol';
import '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';

contract InsomniaFactory is Initializable, OwnableUpgradeable {
  // an array that contains different ERC1155 tokens deployed
  InsomniaCollectible[] public tokens;

  // openzeppelin defender address
  address public defender;

  // emitted when ERC1155 token is deployed
  event ERC1155Created(address owner, address tokenContract);

  // emmitted when ERC1155 token is minted
  event ERC1155Minted(address owner, address tokenContract, uint256 amount);

  // emitted when deployContract is called
  event PaidETHForDeployment(address indexed payer, uint256 amount, string collectionId);
  event PaidTokenForDeployment(address indexed payer, address indexed token, uint256 amount, string collectionId);

  // emitted when withdrawal
  event WithdrawalETH(address indexed sender, address indexed recipient, uint256 amount);
  event WithdrawalToken(address indexed sender, address indexed recipient, address indexed token, uint256 amount);

  function initialize(address _defender) public initializer {
    OwnableUpgradeable.__Ownable_init();

    defender = _defender;
  }

  /// @notice deployERC1155 - deploys a ERC1155 token with given parameters - returns deployed address
  /// @dev msg.sender has its ownership
  /// @param _name name of our ERC1155 token
  /// @param _symbol symbol of ERC1155 token
  /// @param _tokenURIPrefix URI resolving to our hosted metadata
  /// @param _royalty royality percent of our ERC1155 token
  /// @param _tokenCount number of tokens
  /// @param _mintPrices prices of tokens
  /// @param _maxSupplies supplies of tokens
  function deployERC1155(
    string memory _name,
    string memory _symbol,
    string memory _tokenURIPrefix,
    address _royalty,
    uint256 _tokenCount,
    uint256[] calldata _mintPrices,
    uint256[] calldata _maxSupplies,
    uint256 _royaltyPercent,
    bool _isRandomMinting
  ) external onlyOwner returns (address) {
    // deploy new NFT collection created
    InsomniaCollectible nft = new InsomniaCollectible(_name, _symbol, _tokenURIPrefix, _royalty, address(this), _isRandomMinting);

    nft.setInfo(_tokenCount, _mintPrices, _maxSupplies, _royaltyPercent);

    // set caller admin role
    nft.addAdminRole(msg.sender);

    // transfer ownership of new NFT collection created to owner
    // nft.transferOwnership(msg.sender);

    tokens.push(nft);

    emit ERC1155Created(msg.sender, address(nft));
    return address(nft);
  }

  function payETHForDeployment(string memory _collectionId) external payable {
    require(msg.value > 0, 'Gas Fee should be valid');

    (bool withdrawSuccess, ) = payable(defender).call{value: address(this).balance}('');
    require(withdrawSuccess, 'Failed to pay funds');

    emit PaidETHForDeployment(msg.sender, msg.value, _collectionId);
  }

  function payTokenForDeployment(
    string memory _collectionId,
    IERC20 _token,
    uint256 _tokenAmount
  ) external {
    require(_tokenAmount > 0, 'Token amount should be valid');

    require(_token.balanceOf(msg.sender) >= _tokenAmount, 'not enough token to pay gas fee');

    _token.transferFrom(msg.sender, address(this), _tokenAmount);

    emit PaidTokenForDeployment(msg.sender, address(_token), _tokenAmount, _collectionId);
  }

  /// @notice mintERC1155 - mints a ERC1155 token with given parameters
  /// @dev mint a new token on already deployed NFT collection
  /// @param _nft index position in our tokens array - represents which ERC1155 you want to interact with
  /// @param _id token Id desired to mint
  /// @param _amount amount of tokens you wish to mint
  function mintERC1155(
    address _nft,
    uint256 _id,
    uint256 _amount
  ) external {
    InsomniaCollectible(_nft).mint(InsomniaCollectible(_nft).owner(), _id, _amount);
    emit ERC1155Minted(InsomniaCollectible(_nft).owner(), address(InsomniaCollectible(_nft)), _amount);
  }

  function setDefender(address _newDefender) external onlyOwner {
    require(_newDefender != address(0), 'New defender must not be zero address');
    defender = _newDefender;
  }

  /////////
  // Helper functions below retrieve contract data given an ID
  /////////
  function getCountERC1155byId(address _nft, uint256 _id) public view returns (uint256 amount) {
    return InsomniaCollectible(_nft).balanceOf(InsomniaCollectible(_nft).owner(), _id);
  }

  function getERC1155byIdAndId(address _nft, uint256 _id)
    public
    view
    returns (
      address _contract,
      address _owner,
      string memory _uri,
      uint256 supply
    )
  {
    return (
      address(InsomniaCollectible(_nft)),
      InsomniaCollectible(_nft).owner(),
      InsomniaCollectible(_nft).uri(_id),
      InsomniaCollectible(_nft).balanceOf(InsomniaCollectible(_nft).owner(), _id)
    );
  }
}

