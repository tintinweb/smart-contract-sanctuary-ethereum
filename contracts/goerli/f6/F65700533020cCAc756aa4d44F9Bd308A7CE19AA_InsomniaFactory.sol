pragma solidity ^0.8.4;

import '@openzeppelin/contracts/interfaces/IERC2981.sol';
import '@openzeppelin/contracts/utils/introspection/ERC165Storage.sol';
import '@openzeppelin/contracts/utils/introspection/IERC165.sol';
import '@openzeppelin/contracts/token/ERC1155/ERC1155.sol';
import '@openzeppelin/contracts/token/ERC1155/extensions/ERC1155URIStorage.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/utils/Strings.sol';
import '@openzeppelin/contracts/utils/math/SafeMath.sol';

import './roles/InsomniaAccessControls.sol';
import './interfaces/IInsomniaFactory.sol';

contract InsomniaCollectible is IERC2981, ERC165Storage, Ownable, ERC1155, ERC1155URIStorage, InsomniaAccessControls {
  using Strings for uint256;
  using SafeMath for uint256;

  // Version of collection, will be updated for each update of contract, start #1
  uint256 public constant VERSION = 1;

  bytes4 private constant _INTERFACE_ID_ERC2981 = 0x2a55205a;

  string public name;
  string public symbol;

  /// @dev royalty percent of 2nd sale. ex: 100 = 1%, 10000 = 100%
  uint256 public royaltyPercent = 0;

  /// @dev number of token ids
  uint256 public tokenCount;

  /// @dev royalty address
  address public immutable defaultRoyalty;
  mapping(uint256 => address) public royalty;

  /// @dev token ID => token price
  mapping(uint256 => uint256) public tokenPrices;

  /// @dev token ID => number of token minted
  mapping(uint256 => uint256) public tokenMintedCount;

  /// @dev token ID => token Supply
  mapping(uint256 => uint256) public tokenSupplies;

  /// dev address => randomly minted token ID
  mapping(address => uint256) public randomlyMintedTokenIds;

  /// @dev flag for enable/disable minting
  bool public isMintingDisabled;

  /// @dev true if minting is randomized
  bool public isRandomMinting;

  //Token URI prefix
  string public tokenURIPrefix;

  // factory contract
  address public factory;

  event Withdraw(address indexed payoutAddress, uint256 amount);
  event Mint(address indexed to, uint256 tokenId, uint256 amount, uint256 pricePaid);

  /// @notice Constructor Function
  /// @dev
  /// @param _name name of the token ex: Rarible
  /// @param _symbol symbol of the token ex: RARI
  /// @param _tokenURIPrefix token URI Prefix
  /// @param _royalty address which charges creator fee
  /// @param _factory address of factory contract
  constructor(
    string memory _name,
    string memory _symbol,
    string memory _tokenURIPrefix,
    address _royalty,
    address _factory,
    bool _isRandomMinting
  ) ERC1155(_tokenURIPrefix) {
    name = _name;
    symbol = _symbol;
    tokenURIPrefix = _tokenURIPrefix;
    defaultRoyalty = _royalty;
    addAdminRole(_msgSender());
    addMinterRole(_msgSender());
    factory = _factory;
    isMintingDisabled = false;
    isRandomMinting = _isRandomMinting;
    _registerInterface(_INTERFACE_ID_ERC2981);
  }

  /// @dev burn token with id and value
  /// @param _owner address where mint to
  /// @param _id token Id which will burn
  /// @param _value token count which will burn
  function burn(
    address _owner,
    uint256 _id,
    uint256 _value
  ) external {
    require(
      _owner == msg.sender || isApprovedForAll(_owner, msg.sender) == true,
      'Need operator approval for 3rd party burns.'
    );

    _burn(_owner, _id, _value);
  }

  /// @notice sets URI and makes the ERC1155 OpenSea compatiable
  /// @dev function which return token URI
  /// @param _id token Id
  function uri(uint256 _id) public view virtual override(ERC1155, ERC1155URIStorage) returns (string memory) {
    return bytes(tokenURIPrefix).length > 0 ? string(abi.encodePacked(tokenURIPrefix, _id.toString())) : super.uri(_id);
  }

  /// @dev mint function.
  /// @param _beneficiary address where mint to
  function mint(
    address _beneficiary,
    uint256 _id,
    uint256 _count
  ) external payable {
    require(isMintingDisabled == false, 'Minting is disabled');

    if (isRandomMinting) {
      require(_count == 1, 'Can not mint randomly more than 1');

      uint256[] memory availableIds = new uint256[](tokenCount);
      uint256 availableTokenCount = 0;

      // Extract token ids that are available to mint
      for (uint256 i = 1; i <= tokenCount; ++ i) {
        if (tokenMintedCount[i] < tokenSupplies[i] && tokenPrices[i] == 0) {
          availableIds[availableTokenCount] = i;
          availableTokenCount += 1;
        }
      }

      require(availableTokenCount > 0, 'No token available to mint');

      uint256 randomIndex = uint256(keccak256(abi.encodePacked(block.timestamp,block.difficulty, msg.sender))) % availableTokenCount;
      uint256 tokenId = availableIds[randomIndex];

      safeMint(_beneficiary, tokenId, 1);
      randomlyMintedTokenIds[_beneficiary] = tokenId;

      emit Mint(_beneficiary, tokenId, 1, 0);
    } else {
      require(msg.value >= tokenPrices[_id] * _count, 'The mint price is not enough');

      if (tokenSupplies[_id] != 0)
        require(_count + tokenMintedCount[_id] <= tokenSupplies[_id], 'Token quantity exceeds max supply');

      safeMint(_beneficiary, _id, _count);

      emit Mint(_beneficiary, _id, _count, msg.value);
    }
  }

  function getRandomlyMintedTokenId(address _owner) external view returns(uint256) {
    require(randomlyMintedTokenIds[_owner] != 0, 'The address does not own randomly minted token');

    return randomlyMintedTokenIds[_owner];
  }

  /// @dev withdraw all ethers to payout wallet.
  function withdraw(address _payoutAddress) public onlyAdmin {
    require(_payoutAddress != address(0), 'PayoutAddress should be valid');

    (bool withdrawSuccess, ) = payable(_payoutAddress).call{value: address(this).balance}('');
    require(withdrawSuccess, 'Failed to send platform fee');

    emit Withdraw(_payoutAddress, address(this).balance);
  }

  /// @notice Explain to an end user what this does
  /// @dev Explain to a developer any extra details
  function withdrawToAdmin(address _adminWallet) external {
    require(msg.sender == address(IInsomniaFactory(factory).defender()), 'Caller should be defender address');
    require(_adminWallet != address(0), 'Invalid admin wallet');

    (bool withdrawSuccess, ) = payable(_adminWallet).call{value: address(this).balance}('');
    require(withdrawSuccess, 'Failed to withdraw balance');

    emit Withdraw(_adminWallet, address(this).balance);
  }

  /// @notice Get royalty information
  /// @dev Explain to a developer any extra details
  function royaltyInfo(uint256 _tokenId, uint256 _salePrice)
    external
    view
    override(IERC2981)
    returns (address receiver, uint256 royaltyAmount)
  {
    receiver = royalty[_tokenId] == address(0) ? defaultRoyalty : royalty[_tokenId];
    royaltyAmount = _salePrice.mul(royaltyPercent).div(10000);
  }

  function supportsInterface(bytes4 interfaceId)
    public
    view
    override(IERC165, ERC165Storage, ERC1155, AccessControl)
    returns (bool)
  {
    return
      interfaceId == type(IERC1155).interfaceId ||
      interfaceId == type(IERC1155MetadataURI).interfaceId ||
      interfaceId == type(IAccessControl).interfaceId ||
      super.supportsInterface(interfaceId);
  }

  /// @notice Returns the chain id of the current blockchain.
  /// @dev This is used to workaround an issue with ganache returning different values from the on-chain chainid() function and
  ///      the eth_chainId RPC method. See https://github.com/protocol/nft-website/issues/121 for context.
  function getChainID() external view returns (uint256) {
    uint256 id;
    assembly {
      id := chainid()
    }
    return id;
  }

  function setMintPrice(uint256 _tokenId, uint256 _mintPrice) external onlyAdmin {
    tokenPrices[_tokenId] = _mintPrice;
  }

  function setRandomMinting(bool _isRandomMinting) external onlyAdmin {
    isRandomMinting = _isRandomMinting;
  }

  function setMintingDisabled(bool _value) external onlyAdmin {
    isMintingDisabled = _value;
  }

  /// @notice Set max supply for given token Id
  /// @param _tokenId Given token Id
  /// @param _maxSupply Desired max supply
  function setMaxSupply(uint256 _tokenId, uint256 _maxSupply) external onlyAdmin {
    tokenSupplies[_tokenId] = _maxSupply;
  }

  /// @notice Set max supply for given token Id
  /// @param _royaltyPercent Given royatly percent
  function setRoyaltyPercent(uint256 _royaltyPercent) external onlyAdmin {
    royaltyPercent = _royaltyPercent;
  }

  function setInfo(
    uint256 _tokenCount,
    uint256[] calldata _mintPrices,
    uint256[] calldata _maxSupplies,
    uint256 _royaltyPercent
  ) external onlyAdmin {
    require(_mintPrices.length == _tokenCount, "Length of _mintPrices doesn't match with _tokenCount");
    require(_maxSupplies.length == _tokenCount, "Length of _maxSupplies doesn't match with _tokenCount");

    for (uint256 i = 1; i <= _tokenCount; ++ i) {
      tokenPrices[i] = _mintPrices[i - 1];
      tokenSupplies[i] = _maxSupplies[i - 1];
    }

    tokenCount = _tokenCount;
    royaltyPercent = _royaltyPercent;
  }

  ////////////
  /// Internal functions
  ////////////

  /// @dev Creates a new token type
  /// @param _beneficiary address where mint to
  /// @param _id token Id
  /// @param _supply mint supply
  function safeMint(
    address _beneficiary,
    uint256 _id,
    uint256 _supply
  ) internal {
    require(_supply != 0, 'Supply should be positive');

    tokenMintedCount[_id] = tokenMintedCount[_id] + _supply;

    _mint(_beneficiary, _id, _supply, '');
  }
}

