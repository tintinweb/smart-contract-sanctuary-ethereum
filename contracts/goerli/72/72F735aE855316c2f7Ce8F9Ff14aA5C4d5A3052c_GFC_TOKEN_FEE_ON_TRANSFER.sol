import "Address.sol";
import "ERC20.sol";
import "ERC20Burnable.sol"; //needed?
import "AccessControl.sol";
import "ReentrancyGuard.sol";
import "ChainlinkClient.sol";
import "ConfirmedOwner.sol";
import "helpers.sol";

/**
 * Request testnet LINK and ETH here: https://faucets.chain.link/
 * Find information on LINK Token Contracts and get the latest ETH and LINK faucets here: https://docs.chain.link/docs/link-token-contracts/
 */

/** @dev function: to handle user claims of GFC token based on app data. carries out taxing 
need: delay to protect against claims that occur before database updates. approaches: time lock, or just a bool that doesnt allow a second claim until fulfill() reopens the claim window, or just one claim per *time interval* or something
need: claim any amount instead of all at once. will require more talk between app and contract
can proxy this contract for future improvements too
instead of PUT requests from chainlink to update app, could use API that reads contract events such as https://bitquery.io/blog/ethereum-events-api

How often do tax rates need to be updates, and by what source, i.e. should chainlink handle the rate modifications too every so often? another potential keeper application in addition to the claim receipt

Additions needed to proxy this contract, too...

What is function of ReentrancyGuard vs the claimSlot method I'm using? claimSlot aim is to avoid two simultaneously active claims to keep requests as serial as possible

Other useful events for App??

FOR UNISWAP V3: Only Claim Tax, no buy/sell tax. Uniswap V2 works with modified transfer functions...

*/

contract GFC_TOKEN_FEE_ON_TRANSFER is ERC20, ERC20Burnable, AccessControl, ReentrancyGuard, ChainlinkClient, ConfirmedOwner, GFC_Helpers {

    //-----------------------------------------------------------------------------
    //-------------SETUP-----------------------------------------------------------
    //-----------------------------------------------------------------------------
    using Chainlink for Chainlink.Request;
    
    address private chainlinkToken = 0x326C977E6efc84E512bB9C30f76E30c160eD06FB;
    address private chainlinkOracle = 0xCC79157eb46F5624204f47AB42b3906cAA40eaB7;
    address public treasuryAddress;

    bytes32 private jobId = '7d80a6386ef543a3abb52817f6707e3b'; //get string jobId
    bytes32 public constant ADMIN_ROLE = keccak256('ADMIN_ROLE');
    bytes32 public constant TAX_MANAGER_ROLE = keccak256('TAX_MANAGER_ROLE');
    bytes32 public constant SNAPSHOT_ROLE = keccak256('SNAPSHOT_ROLE');
    bytes32 public constant EXEMPT_ROLE = keccak256('EXEMPT_ROLE');
    
    string public content; //response content
    string public getURL = 'https://arcane-sands-66304.herokuapp.com/test';   // have a / at the end??? returns: {'update':'0xcee4D01BFb1c75cc8EF255946a996B15e3A2731E,3000000000000000000'}
    
    uint256 private taxDenominator = 10000;    
    uint256 public taxRate;
    uint256 public fee = (1 * LINK_DIVISIBILITY) / 10; // 0,1 * 10**18 (Varies by network and job)
    uint256 public mintedSupply = 0;
    uint256 public tokenDecimals = 10**18;
    uint256 public maxSupply = 21000000 * tokenDecimals;

    mapping(address => uint256) public userClaimableBalance;
    mapping(address => bool) public claimSlotIsClosed; //add mapping for claimed rewards?
    mapping(address => uint256) public claimedRewards;
    
    event HelloGFC();
    event RewardClaimedTaxPaid(address indexed user, address indexed taxdest, uint256 reward, uint256 taxpaid);
    event Request(bytes32 indexed requestId, string content);
    event TokenMint(address indexed user, uint256 amount);
    event ClaimSlotReopened(address indexed user);

    error ClaimSlotAlreadyUsed();
    error OnlyContractCanDispenseClaim();
    error ClaimAttemptForMoreThanBalance();
    error ClaimWillExceedMaxSupply();

    constructor(
        uint256 _defaultTaxRate, 
        address _treasuryAddr
        ) 
        ERC20('Global Fit Club Token', 'GFCT') 
        ConfirmedOwner(msg.sender) 
        GFC_Helpers() 
    {
        address creator = msg.sender;
        _grantRole(DEFAULT_ADMIN_ROLE, creator);
        _grantRole(ADMIN_ROLE, creator);
        _grantRole(SNAPSHOT_ROLE, creator);
        _grantRole(EXEMPT_ROLE, _treasuryAddr); //transfers into/out of treasury are not taxed
        _grantRole(TAX_MANAGER_ROLE, creator);
        
        setChainlinkToken(chainlinkToken);
        setChainlinkOracle(chainlinkOracle);

        setTaxRate(_defaultTaxRate);
        setTreasuryAddress(_treasuryAddr);

        emit HelloGFC();
    }

    receive() external payable {}

    //-----------------------------------------------------------------------------
    //-------------CHAINLINK FUNCTIONS---------------------------------------------
    //-----------------------------------------------------------------------------

    function sendRequest() private returns (bytes32 requestId) {
        Chainlink.Request memory req = buildChainlinkRequest(jobId, address(this), this.fulfill.selector);
        req.add('get', getURL);
        req.add('path', 'update');
        return sendChainlinkRequest(req, fee);
    }

    /// @dev fulfills API call request, then updates balance, and re-opens the user's claim slot. protected by recordChainlinkFulfillment.    
    function fulfill(bytes32 _requestId, string memory _content) public recordChainlinkFulfillment(_requestId) {
        emit Request(_requestId, _content);
        content = _content;
        // insert checks for validity  of rewards here, i.e. within bounds of issuance, etc.
        address userAddress;
        uint256 userBalanceAddition;
        (userAddress, userBalanceAddition) = splitInput(content,',');
        userClaimableBalance[userAddress] += userBalanceAddition;
        claimSlotIsClosed[userAddress] = false; //reopen claim slot   
        emit ClaimSlotReopened(userAddress); 
    }
    
    //-----------------------------------------------------------------------------
    //-----------USER INTERACTIONS-------------------------------------------------
    //-----------------------------------------------------------------------------

    /// @dev user initiates claim
    function updateClaimableBalance() public {
        address sender = msg.sender;
        if ( claimSlotIsClosed[sender] == false ) {
            claimSlotIsClosed[sender] = true; //close claim slot until fulfilled
            sendRequest();
        } else {
            revert ClaimSlotAlreadyUsed(); //reverts until claim is fulfilled
        }       
    }   

    /// @dev initially user-called, called by keeper later (change sendTo in this case?)
    function claimTokens(uint256 _claimAmount) public nonReentrant {
        address sendTo = msg.sender;
        address taxDestinationAddress = getTreasuryAddress();
        uint256 taxAmount = (_claimAmount * getTaxRate())/taxDenominator;
        
        // can't claim until pending requests fulfill, to keep the flow under control
        if(claimSlotIsClosed[sendTo] == true){ revert ClaimSlotAlreadyUsed(); }
        if(_claimAmount > userClaimableBalance[sendTo]){ revert ClaimAttemptForMoreThanBalance(); }
        if(( mintedSupply + _claimAmount) > maxSupply){ revert ClaimWillExceedMaxSupply(); }

        userClaimableBalance[sendTo] -= _claimAmount;
        claimedRewards[sendTo] += _claimAmount;
        mintedSupply += _claimAmount;

        _mint(sendTo, _claimAmount-taxAmount);
        _mint(taxDestinationAddress, taxAmount);

        emit RewardClaimedTaxPaid(sendTo, taxDestinationAddress, _claimAmount, taxAmount);
    }


    //-----------------------------------------------------------------------------
    // ------ ROLE SPECIFIC FUNCTIONS----------------------------------------------
    //-----------------------------------------------------------------------------
    function setTaxRate(uint256 newRate) public onlyRole(ADMIN_ROLE) {
        taxRate = newRate;
    }

    function getTaxRate() public view returns (uint256) {
        return taxRate;
    }

    function setTreasuryAddress(address _newTreasuryAddress) public onlyRole(ADMIN_ROLE) {
        treasuryAddress = _newTreasuryAddress;
    }

    function getTreasuryAddress() public view returns (address) {
        return treasuryAddress;
    }

    /// @dev sets details relating to the chainlink get request made to the fitness-tracking app.
    function setChainlinkDetails(address _token, address _oracle, bytes32 _jobid, string memory _geturl) external onlyRole(ADMIN_ROLE) {
        setChainlinkToken(_token);
        setChainlinkOracle(_oracle);
        jobId = _jobid;
        getURL = _geturl;
    }

    function addExemptAddress(address _newexempt) public onlyRole(ADMIN_ROLE) {
        _grantRole(EXEMPT_ROLE, _newexempt);
    }

    function removeExemptAddress(address _oldexempt) public onlyRole(ADMIN_ROLE) {
        _revokeRole(EXEMPT_ROLE, _oldexempt);
    }

    function mintERC20(address _to, uint256 _amount) public onlyRole(ADMIN_ROLE) {
        _mint(_to, _amount);
    }
    
    function withdrawLink() public onlyRole(ADMIN_ROLE) {
        LinkTokenInterface link = LinkTokenInterface(chainlinkTokenAddress());
        require(link.transfer(msg.sender, link.balanceOf(address(this))));
    }

    function withdrawETH() public onlyRole(ADMIN_ROLE)  {
        (bool os, ) = payable(msg.sender).call{ value: address(this).balance }('');
        require(os);
    }

    //-----------------------------------------------------------------------------
    // ------ OTHER ---------------------------------------------------------------
    //-----------------------------------------------------------------------------

    /// @dev Override the _transfer() function to perform the necessary tax functions:
    function _transfer(address from, address to, uint256 amount) internal virtual override(ERC20) nonReentrant  {
        if(hasRole(EXEMPT_ROLE, from) || hasRole(EXEMPT_ROLE, to) || !Address.isContract(to)) { // If to another EOA, no tax
            super._transfer(from, to, amount); // Transfers 100% of amount to recipient.
        } else { // If not to/from a tax excluded address & tax is on...
            require(balanceOf(from) >= amount, "ERC20: transfer amount exceeds balance"); // Makes sure sender has the required token amount for the total.
            // If the above requirement is not met, then it is possible that the sender could pay the tax but not the recipient, which is bad...
            super._transfer(from, getTreasuryAddress(), amount*getTaxRate()/10000); // Transfers tax to the tax destination address.
            super._transfer(from, to, amount*(10000-getTaxRate())/10000); // Transfers the remainder to the recipient.
        }
    }  

    /// @dev Override _beforeTokenTransfer() as required by certain contracts. 
    function _beforeTokenTransfer(address from, address to, uint256 amount) internal override(ERC20) {
        super._beforeTokenTransfer(from, to, amount);
    }
}

