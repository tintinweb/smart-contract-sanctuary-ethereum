pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
//import "@openzeppelin/contracts/interfaces/IERC4906.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./Error.sol";
import "./PaymentSplitterEnumerable.sol";
import "./Redeemable.sol";
import "./IERC4906.sol"; // TODO: replace by the OpenZeppelin version once released

contract MyToken is 
	ERC721, 
	ERC721Enumerable, 
	Redeemable, 
	Pausable, 
	AccessControlEnumerable, 
	IERC2981, 
	IERC4906, 
	PaymentSplitterEnumerable 
{
	using Counters for Counters.Counter;
	using Strings for uint256;

	Counters.Counter private _tokenIdCounter;

	uint256 public immutable MAX_SUPPLY;
	uint256 public immutable ID_OFFSET;
	bytes32 public immutable ADMIN_ROLE;
	uint256 public immutable ROYALTY_AMOUNT;
	uint256 public immutable ROYALTY_DENOMINATOR;

	string public notRevealedURI; 
	string public baseURI; 
	bool public revealed;

	uint256 public redeemCost = 0.01 ether;

	error RoleRenounceNotSupported();
	error TokenDoesNotExist(uint256 tokenId);
	error OutOfStock(uint256 requested, uint256 available);

	constructor(
		string memory name,
		string memory symbol,
		uint256 maxSupply,
		uint256 idOffset,
		string memory _notRevealedURI,
		uint256 royalty,
		address[] memory payees, 
		uint256[] memory shares
	) 
		ERC721(name, symbol)
		PaymentSplitterEnumerable(payees, shares)
	{
		if (maxSupply == 0) 
			revert Error.ArgumentOutOfRange("maxSupply", maxSupply);
		if (bytes(_notRevealedURI).length == 0) 
			revert Error.ArgumentEmpty("_notRevealedURI");

		ADMIN_ROLE = keccak256("ADMIN_ROLE"); // create unique identifier for role

		_setRoleAdmin(ADMIN_ROLE, ADMIN_ROLE); // users with admin role can manage admin role
		_setupRole(ADMIN_ROLE, _msgSender()); // grant admin role to deployer 

		MAX_SUPPLY = maxSupply;
		ID_OFFSET = idOffset;
		notRevealedURI = _notRevealedURI;

		ROYALTY_AMOUNT = royalty;
		ROYALTY_DENOMINATOR = 10000; // two decimal cases percentage

		pause(); // start paused
	}

	// don't allow account to revoke role to self
	function revokeRole(bytes32 role, address account) 
	public virtual override(IAccessControl, AccessControl) 
	onlyRole(getRoleAdmin(role)) 
	{
		if(account == _msgSender()) revert RoleRenounceNotSupported();
		
		super.revokeRole(role, account);
	}

	// don't allow role renouncing
	function renounceRole(bytes32, address) 
	public virtual override(IAccessControl, AccessControl) 
	{
		revert RoleRenounceNotSupported();
	}

	function pause() public onlyRole(ADMIN_ROLE) {
		_pause();
	}

	function unpause() public onlyRole(ADMIN_ROLE) {
		_unpause();
	}

	function safeMint(address to) 
		public 
		onlyRole(ADMIN_ROLE) 
		returns (uint256) 
	{
		uint256 tokenId = _tokenIdCounter.current();
		_tokenIdCounter.increment();
		_safeMint(to, tokenId);
		return tokenId;
	}

	function setRedeemRoot(bytes32 root) external onlyRole(ADMIN_ROLE) {
		_setRedeemRoot(_msgSender(), root);
	}

	function redeem(
		uint256 ticketId,
		bytes32[] calldata proof
	) 
		external 
		payable 
		onlyOnPayment(redeemCost) 
		returns (uint256) 
	{
		address sender = _msgSender();		
		_redeem(sender, ticketId, proof);

		uint256 tokenId = _tokenIdCounter.current();
		_tokenIdCounter.increment();
		_mint(sender, tokenId);
		return tokenId;
	}

	function reveal(string memory _baseURI) public onlyRole(ADMIN_ROLE) 
	{
		require(bytes(_baseURI).length > 0);
		baseURI = _baseURI;
		revealed = true;

		emit BatchMetadataUpdate(0, _tokenIdCounter.current() - 1);
	}

	function _beforeTokenTransfer(address from, address to, uint256 tokenId, uint256 batchSize)
		internal
		whenNotPaused
		override(ERC721, ERC721Enumerable)
	{
		if (totalSupply() + 1 > MAX_SUPPLY)
			revert OutOfStock(1, MAX_SUPPLY - totalSupply());

		super._beforeTokenTransfer(from, to, tokenId, batchSize);
	}

	function tokenURI(uint256 tokenId)
		public
		view
		virtual
		override
		tokenExists(tokenId)
		returns (string memory)
	{
		if (!revealed)
			return notRevealedURI;

		return bytes(baseURI).length > 0 
			? string(abi.encodePacked(baseURI, (tokenId + ID_OFFSET).toString(), ".json")) 
			: "";
	}

	function release(
		address payable account
	) public override onlyPayeeOrRole(ADMIN_ROLE) {
		super.release(account);
	}

	function release(
		IERC20 token,
		address account
	) public override onlyPayeeOrRole(ADMIN_ROLE) {
		super.release(token, account);
	}

	function royaltyInfo(uint256 /* tokenId */, uint256 salePrice)
		external
		view
		returns (address receiver, uint256 royaltyAmount)
	{
		return (
			address(this),
			(salePrice * ROYALTY_AMOUNT) / ROYALTY_DENOMINATOR
		);
	}

	// custom modifiers

	modifier tokenExists(uint256 tokenId) {
		if (!_exists(tokenId)) revert TokenDoesNotExist(tokenId);
		_;
	}

	modifier onlyPayeeOrRole(bytes32 role) {
		address sender = _msgSender();
		if (!(shares(sender) != 0 || hasRole(role, sender)))
			revert Error.NoPermission(sender);
		_;
	}

	modifier onlyOnPayment(uint256 value) {
		if (msg.value != value)
			revert Error.InvalidPayment(value, msg.value);
		_;
	}

	// The following functions are overrides required by Solidity.

	function supportsInterface(bytes4 interfaceId)
		public
		view
		override(ERC721, ERC721Enumerable, AccessControlEnumerable, IERC165)
		returns (bool)
	{
		return super.supportsInterface(interfaceId);
	}
}

