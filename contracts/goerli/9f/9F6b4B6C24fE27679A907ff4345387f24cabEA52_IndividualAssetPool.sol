interface ICurvePoolEth {
    function exchange(
        uint256 i,
        uint256 j,
        uint256 dx,
        uint256 min_dy,
        bool use_eth
    ) external payable;
}

interface ICurvePoolErc20 {
    function exchange(
        int128 i,
        int128 j,
        uint256 dx,
        uint256 min_dy
    ) external;
}

interface ICompoundComet {
    function supply(address asset, uint  amount) external;
    function withdraw(address asset, uint256 amount) external;
    function balanceOf(address account) external view returns(uint256);
    function borrowBalanceOf(address account) external view returns(uint256);
    function collateralBalanceOf(address account, address asset) external view returns(uint256);
}

abstract contract BaseAssetPool is
    OwnableUpgradeable,
    UUPSUpgradeable,
    AssetPoolToken
{
    AssetPool internal _assetPool;

    IWitnetPriceRouter public router;

    uint256 usdVal;

    uint256 amt;

    uint256 price;

    uint256 usdTotal;

    // address public constant UNI_ADDR = 0x1f9840a85d5aF5bf1D1762F925BDADdC4201F984;
    // address public constant WETH_ADDR = 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6;
    // address public constant MY_UNI = 0xaD261f63266244dCE1Aa89ec1039735153B1D926;//Goerli
    address public constant MY_AAVE = 0xE0Ab489636453Eafb586F1237A27e46858d13827;//Goerli
    address public constant POOL_PROXY_AAVE = 0x368EedF3f56ad10b9bC57eed4Dac65B26Bb667f6;//Goerli
    // address public constant COMPOUND_COMET = 0xc3d688B66703497DAA19211EEdff47f25384cdc3;//Mainnet
    address public constant COMPOUND_COMET = 0x3EE77595A8459e93C2888b13aDB354017B198188;//Goerli

    // IUniswapRouter public constant uniswapRouter = IUniswapRouter(0xE592427A0AEce92De3Edee1F18E0157C05861564);
    // IMyUni public constant MY_UNI_CONTRACT = IMyUni(MY_UNI);
    // IMyAave public constant MY_AAVE_CONTRACT = IMyAave(MY_AAVE);
    IPool public constant POOL_PROXY_AAVE_CONTRACT = IPool(POOL_PROXY_AAVE);
    ICompoundComet public constant COMPOUND_COMET_CONTRACT = ICompoundComet(COMPOUND_COMET);

    function _BaseAssetPool_init(
        AssetPoolCreationParameters memory _parameters,
        FeeOptions memory _feeOptions,
        WithdrawOptions memory _withdrawOptions,
        DepositLimits memory _depositLimits,
        address _allowedDepositor,
        AssetPoolType _assetPoolType,
        string memory _tokenName,
        string memory _tokenSymbol
    ) internal initializer {
        _AssetPoolToken_initialize(_tokenName, _tokenSymbol);
        _assetPool.poolType = _assetPoolType;
        _assetPool.name = _parameters.name;
        _assetPool.symbol = _parameters.symbol;
        _assetPool.owner = msg.sender;
        _assetPool.denominationCurrency = IERC20Upgradeable(
            _parameters.denominationCurrency
        );
        setFeeOptions(_feeOptions);
        setDepositLimits(_depositLimits);
        allowDepositor(_allowedDepositor);
        setWithdrawOptions(_withdrawOptions);

        /**
         * IMPORTANT: replace the address below with the WitnetPriceRouter address
         * of the network you are using! Please find the address here:
         * https://docs.witnet.io/smart-contracts/price-feeds/contract-addresses
         */
        router = IWitnetPriceRouter(0x1cF3Aa9DBF4880d797945726B94B9d29164211BE);
    }

    function setFeeOptions(FeeOptions memory _feeOptions)
        public
        virtual
        onlyOwner
    {
        _assetPool.feeOptions.chargeManagementFee = _feeOptions
            .chargeManagementFee;
        _assetPool.feeOptions.managementFeeRate = _feeOptions.managementFeeRate;
        _assetPool.feeOptions.chargePerformanceFee = _feeOptions
            .chargePerformanceFee;
        _assetPool.feeOptions.performanceFeeRate = _feeOptions
            .performanceFeeRate;
        _assetPool.feeOptions.chargeEntranceFee = _feeOptions.chargeEntranceFee;
        _assetPool.feeOptions.entranceFeeRate = _feeOptions.entranceFeeRate;
        _assetPool.feeOptions.chargeExitFee = _feeOptions.chargeExitFee;
        _assetPool.feeOptions.exitFeeRate = _feeOptions.exitFeeRate;
    }

    function setDepositLimits(DepositLimits memory _depositLimits)
        public
        virtual
        onlyOwner
    {
        _assetPool.depositLimits.enableLimits = _depositLimits.enableLimits;
        _assetPool.depositLimits.minDeposit = _depositLimits.minDeposit;
        _assetPool.depositLimits.maxDeposit = _depositLimits.maxDeposit;
        _assetPool.depositLimits.rejectAll = _depositLimits.rejectAll;
    }

    function allowDepositor(address _depositor) public virtual onlyOwner {
        _assetPool.allowedDepositor = _depositor;
    }

    function setWithdrawOptions(WithdrawOptions memory _withdrawOptions)
        public
        virtual
        onlyOwner
    {
        _assetPool.withdrawOptions.minWithdrawPeriod = _withdrawOptions
            .minWithdrawPeriod;
        _assetPool.withdrawOptions.enableAssetThreshold = _withdrawOptions
            .enableAssetThreshold;
        for (
            uint256 i = 0;
            i < _withdrawOptions.assetsWithThreshold.length;
            i++
        ) {
            _assetPool.withdrawOptions.assetsWithThreshold.push(
                _withdrawOptions.assetsWithThreshold[i]
            );
        }
    }

    // caller gives denomination token to pool, gets pool token from pool
    function depositToken(uint256 _amount) public {
        // asy ver pt1 al
        //require(!_assetPool.depositLimits.rejectAll, "1");
        //require(msg.sender == _assetPool.allowedDepositor, "2");
        if (
            !_assetPool.depositLimits.rejectAll &&
            _assetPool.depositLimits.enableLimits
        ) {
            require(_amount >= _assetPool.depositLimits.minDeposit, "minn");
            require(_amount <= _assetPool.depositLimits.maxDeposit, "maxx");
        }
        require(
            _assetPool.denominationCurrency.balanceOf(msg.sender) >= _amount,
            "3"
        );

        require(
            (_assetPool.denominationCurrency.balanceOf(address(this)) +
                _amount) >
                _assetPool.denominationCurrency.balanceOf(address(this)),
            "4"
        );

        if (
            _assetPool.denominationCurrency.transferFrom(
                msg.sender,
                address(this),
                _amount
            )
        ) {
            if (_assetPool.feeOptions.chargeEntranceFee) {
                _amount =
                    (_amount * (100 - _assetPool.feeOptions.entranceFeeRate)) /
                    100;
            }

            uint256 totalUsd = _amount * uint256(getEthUsdPrice());
            this.mint(
                msg.sender,
                (
                    totalUsd /** (10**12)*/
                ) / (10**6)
            );
        }
    }

    function calculatePoolTokenPrice() public virtual returns (uint256) {
        if (totalSupply() == 0) return 1;

        price = calculateTotalUsdValue() / totalSupply();
        return price;
    }

    function getPoolTokenPriceView() public view returns (uint256) {
        if (totalSupply() == 0) return 1;

        return getTotalUsdValueView() / totalSupply();
    }

    function calculateTotalUsdValue() public virtual returns (uint256) {
        if (_assetPool.denominationCurrency.balanceOf(address(this)) == 0)
            return 0;

        uint256 usdPrice = uint256(getEthUsdPrice()); // get denomination currency usd price from price oracles

        // add pool invesment values in future

        usdTotal =
            _assetPool.denominationCurrency.balanceOf(address(this)) *
            usdPrice;
        return usdTotal;
    }

    function getTotalUsdValueView() public view returns (uint256) {
        if (_assetPool.denominationCurrency.balanceOf(address(this)) == 0)
            return 1;

        // add pool invesment values in future

        return (_assetPool.denominationCurrency.balanceOf(address(this)) * uint256(getEthUsdPrice())) / (10**6);
    }

    function getDenominationCurrencyAddress() public view returns (address) {
        return address(_assetPool.denominationCurrency);
    }

    /// Returns the ETH / USD price (6 decimals), ultimately provided by the Witnet oracle.
    function getEthUsdPrice() public view returns (int256 _price) {
        (_price, , ) = router.valueFor(bytes4(0x3d15f701));
    }

    function withdraw(uint256 _amount, uint256 _period) public {
        require(_period >= _assetPool.withdrawOptions.minWithdrawPeriod, "1");

        require(this.balanceOf(msg.sender) >= _amount, "2");

        uint256 usdValue = _amount * calculatePoolTokenPrice();
        uint256 amount = usdValue / uint256(getEthUsdPrice()); // denom amount

        if (_assetPool.feeOptions.chargeExitFee) {
            amount = (amount * (100 - _assetPool.feeOptions.exitFeeRate)) / 100;
        }

        //require(_assetPool.denominationCurrency.balanceOf(address(this)) >= amount, "3");
        if (this.transferFrom(msg.sender, address(this), _amount)) {
            _assetPool.denominationCurrency.transfer(msg.sender, amount);
            this.burn(_amount);
        }
    }

    //TODO: Bu fonksiyonu ownable olarak cagirilmasi lazim !!!!!!!!!!!!!!
    function calculateUsdVal(uint256 _amount) public {
        usdVal = _amount * calculatePoolTokenPrice();
    }

    function getUsdVal() public view returns (uint256) {
        return usdVal;
    }

    function calculateAmt(uint256 _amount) public {
        uint256 usdValue = _amount * calculatePoolTokenPrice();
        amt = usdValue / uint256(getEthUsdPrice());
    }

    function getAmt() public view returns (uint256) {
        return amt;
    }

    function getPrice() public view returns (uint256) {
        return price;
    }

    function getTotalUsdValue() public view returns (uint256) {
        return usdTotal;
    }

    ////////////////////////////////////////////////////////////////////////////////////
    modifier onlyManager() {
        require(
            msg.sender == _assetPool.allowedDepositor,
            "Only pool manager has access!"
        );
        _;
    }

    //   ============= APPROVEMENT =============
    function callApprove(
        address tokenIn,
        address spenderForApprove,
        uint256 amount
    ) internal {
        // check the amount token of pool contract?
        // uint256 tokenAmountOfPool = IERC20(tokenIn).balanceOf(address(this));
        // require(
        //     amount <= tokenAmountOfPool,
        //     "Not enough token to approve in the Pool!"
        // );

        bool approveSuccess = IERC20(tokenIn).approve(
            spenderForApprove,
            amount
        );
        require(approveSuccess, "Approvement failed!");
    }

    //   ============= UNISWAP Functions =============
    // function uniDynamicSwap(
    //     uint256 amountIn,
    //     address tokenIn,
    //     address tokenOut
    // ) public onlyManager {
    //     callApprove(tokenIn, MY_UNI, amountIn);

    //     MY_UNI_CONTRACT.dynamicSwapExactInputSingle(
    //         tokenIn,
    //         tokenOut,
    //         amountIn
    //     );
    //     //olasÄ± bir swap fail'de revert ?????
    // }

    //   ============= AAVE Functions =============
    // AAVE - SUPPLY -----------
    function aaveSupply(address token, uint256 amount) public onlyManager {
        callApprove(token, POOL_PROXY_AAVE, amount);

        POOL_PROXY_AAVE_CONTRACT.supply(token, amount, address(this), 0);
    }

    // AAVE - WITHDRAW ------------
    function aaveWithdraw(
        bool isMaxAmount,
        address asset,
        uint256 amount
    ) public onlyManager {
        POOL_PROXY_AAVE_CONTRACT.withdraw(
            asset,
            isMaxAmount ? type(uint).max : amount,
            address(this)
        );
    }

    // AAVE - BORROW ------------
    function aaveBorrow(
        address token,
        uint256 amount,
        uint256 interestRateMode
    ) public onlyManager {
        POOL_PROXY_AAVE_CONTRACT.borrow(
            token, //address asset,
            amount, //uint256 amount,
            interestRateMode, //uint256 interestRateMode, //Stable: 1, Variable: 2
            0, //uint16 referralCode,
            address(this) //address onBehalfOf
        );
    }

    // AAVE - REPAY ------------
    function aaveRepayDirectly(
        address token,
        uint256 amount,
        uint256 interestRateMode
    ) public onlyManager {
        callApprove(token, POOL_PROXY_AAVE, amount);

        POOL_PROXY_AAVE_CONTRACT.repay(
            token, //address asset,
            amount, //uint256 amount,
            interestRateMode, //uint256 interestRateMode,
            address(this) //address onBehalfOf
        );
    }

    // function aaveRepayDirectlyMAX(address token, uint256 interestRateMode)
    //     public
    //     onlyManager
    // {
    //     callApprove(token, POOL_PROXY_AAVE, type(uint).max);

    //     POOL_PROXY_AAVE_CONTRACT.repay(
    //         token, //address asset,
    //         type(uint).max, //uint256 amount,
    //         interestRateMode, //uint256 interestRateMode,
    //         address(this) //address onBehalfOf
    //     );
    // }

    // AAVE - SET AS COLLATERAL ------------
    function aaveSetAsCollateral(address token, bool useAsCollateral)
        public
        onlyManager
    {
        POOL_PROXY_AAVE_CONTRACT.setUserUseReserveAsCollateral(
            token,
            useAsCollateral
        );
    }

    // AAVE - SWAP BORROW RATE MODE ------------
    function aaveSwapBorrowRateMode(address token, uint256 rateModeFrom)
        public
        onlyManager
    {
        POOL_PROXY_AAVE_CONTRACT.swapBorrowRateMode(token, rateModeFrom);
    }

    //   ============= CURVE Functions =============
    // CURVE - EXCHANGE -----------
    function curveExchangeForEth(
        //example: ETH (index 1) -> USDC (index 2)
        address curvePool,
        uint256 tokenInIndex,
        uint256 tokenOutIndex,
        uint256 amount,
        uint256 minAmount
    ) public {
        ICurvePoolEth(curvePool).exchange{value: amount}(
            tokenInIndex,
            tokenOutIndex,
            amount,
            minAmount,
            true
        );
        // ICurvePoolEth(curvePool).exchange.value(amount)(
        //     tokenInIndex,
        //     tokenOutIndex,
        //     amount,
        //     minAmount,
        //     true
        // );
    }

    function curveExchangeForErc20(
        //example: DAI (index 1) with Approve -> USDC (index 2)
        address tokenInToApprove,
        address curvePool,
        int128 tokenInIndex,
        int128 tokenOutIndex,
        uint256 amount,
        uint256 minAmount
    ) public {
        callApprove(tokenInToApprove, curvePool, amount);

        ICurvePoolErc20(curvePool).exchange(
            tokenInIndex,
            tokenOutIndex,
            amount,
            minAmount
        );
    }

    //   ============= COMPOUND Functions =============
    // COMPOUND - SUPPLY -----------
    function compoundSupply(address asset, uint256 amount) public {
        callApprove(asset, COMPOUND_COMET, amount);

        COMPOUND_COMET_CONTRACT.supply(asset, amount);
    }

    function compoundRepay(address asset, uint256 amount, bool isMaxAmount) public {
        compoundSupply(
            asset,
            isMaxAmount ? type(uint).max : amount
        );
    }

    // COMPOUND - WITHDRAW -----------
    function compoundWithdraw(address asset, uint256 amount) public {
        COMPOUND_COMET_CONTRACT.withdraw(asset, amount);
    }

    // COMPOUND - BALANCEOF -----------
    function compoundBalanceOf() public view returns(uint256){
        uint256 balance = COMPOUND_COMET_CONTRACT.balanceOf(address(this));
        return balance;
    }

    // COMPOUND - BORROWBALANCEOF -----------
    function compoundBorrowBalanceOf() public view returns(uint256){
        uint256 borrowBalance = COMPOUND_COMET_CONTRACT.borrowBalanceOf(address(this));
        return borrowBalance;
    }

    // COMPOUND - COLLATERALBALANCEOF -----------
    function compoundCollateralBalanceOf(address asset) public view returns(uint256){
        uint256 collateralBalance = COMPOUND_COMET_CONTRACT.collateralBalanceOf(address(this), asset);
        return collateralBalance;
    }

    


    
    /// TEST FUNCTION
    function myTestFunction() public pure returns(uint256) {
        return 1881;
    }

    uint256 private myTestNumber;

    function myTestReadFunction() public view returns(uint256) {
        return myTestNumber;
    }

    function myTestSetFunction(uint256 _number) public {
        myTestNumber = _number;
    }
    
}

