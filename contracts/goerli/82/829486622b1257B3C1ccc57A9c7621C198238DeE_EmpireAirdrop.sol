// File: contracts/38_EmpireAirdrop.sol


pragma solidity 0.8.19;









contract EmpireAirdrop is Context, Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;
    IERC20 public EMPIRE_TOKEN;

    bytes32 public root;

    mapping(address => bool) public hasClaimed;

    constructor(address empire_token_address, bytes32 _root) {
        EMPIRE_TOKEN = ERC20(empire_token_address);
        root = _root;
    }

    /**
     * @dev If msg.sender() has been whitelisted, he can call this method to claim the amount of EMPIRE 
       set by admin for this msg.sender() to claim
     */
    function claim(bytes32[] memory proof, uint _amount)public nonReentrant {
        require(!hasClaimed[msg.sender], "User has already claimed!");
        require(isWhitelisted(proof, keccak256(abi.encodePacked(msg.sender, _amount))), "User not whitelisted OR Incorrect amount!");
        hasClaimed[msg.sender] = true;
        EMPIRE_TOKEN.transfer(msg.sender, _amount);
    }

    /**
     * @dev The owner can call this method to withdraw the EMPIRE tokens depoisted in this contract. Only the owner can call this method
     */
    function emergencyWithdraw(uint256 amount) external onlyOwner {
        uint256 token_balance = EMPIRE_TOKEN.balanceOf(address(this));
        require (amount <= token_balance, "Amount exceeds balance");

        EMPIRE_TOKEN.transfer(msg.sender, amount);
    }

    function isWhitelisted(bytes32[] memory proof, bytes32 leaf) public view returns (bool) {
        return MerkleProof.verify(proof, root, leaf);
    }

    function setRoot(bytes32 _root) external onlyOwner {
        root = _root;
    }

    function setHasClaimed(address _user, bool _hasClaimed) external onlyOwner {
        hasClaimed[_user] = _hasClaimed;
    }
}