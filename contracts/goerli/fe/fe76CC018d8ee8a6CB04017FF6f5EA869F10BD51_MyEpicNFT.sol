pragma solidity ^0.8.9;

import '@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol';
import '@openzeppelin/contracts/utils/Counters.sol';
import 'hardhat/console.sol';

import {Base64} from './libraries/Base64.sol';

contract MyEpicNFT is ERC721URIStorage {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    uint256 public maxCollectionSize = 5;

    string private svgPrefix =
        "<svg xmlns='http://www.w3.org/2000/svg' preserveAspectRatio='xMinYMin meet' viewBox='0 0 350 350'><style>.base { fill: white; font-family: serif; font-size: 24px; }</style><rect width='100%' height='100%' fill='#";
    string private svgMiddle =
        "' /><text x='50%' y='50%' class='base' dominant-baseline='middle' text-anchor='middle'>";

    string[20] private firstWords = [
        'gaudy',
        'ill-informed',
        'tranquil',
        'voracious',
        'same',
        'dusty',
        'violent',
        'existing',
        'protective',
        'mammoth',
        'acceptable',
        'common',
        'wet',
        'electric',
        'seemly',
        'assorted',
        'purring',
        'fanatical',
        'psychotic',
        'flippant'
    ];

    string[20] private secondWords = [
        'customer',
        'media',
        'emotion',
        'decision',
        'week',
        'gene',
        'guitar',
        'fortune',
        'expression',
        'bread',
        'quality',
        'charity',
        'speaker',
        'lady',
        'celebration',
        'football',
        'nature',
        'oven',
        'police',
        'ear'
    ];

    string[20] private thirdWords = [
        'storage',
        'revolution',
        'thanks',
        'week',
        'injury',
        'surgery',
        'consequence',
        'basket',
        'menu',
        'organization',
        'article',
        'clothes',
        'artisan',
        'membership',
        'fact',
        'buyer',
        'preparation',
        'disaster',
        'camera',
        'stranger'
    ];

    event NewEpicNFTMinted(address sender, uint256 tokenId);

    constructor() ERC721('SquareNFT', 'Square') {
        console.log('Hello, world!');
    }

    function random(string memory input) internal pure returns (uint256) {
        return uint256(keccak256(abi.encodePacked(input)));
    }

    function pickRandomWord(
        uint256 tokenId,
        string memory seed,
        string[20] storage words
    ) private view returns (string memory) {
        seed = string(
            abi.encodePacked(Strings.toHexString(msg.sender), Strings.toString(tokenId), seed)
        );
        uint256 rand = random(seed) % words.length;
        return words[rand];
    }

    function getTotalMinted() public view returns (uint256) {
        return _tokenIds.current();
    }

    function colorFromAddress(string memory str) public pure returns (string memory) {
        bytes memory strBytes = bytes(str);
        bytes memory result = new bytes(6);
        for (uint256 i = 0; i < 6; i++) {
            result[i] = strBytes[strBytes.length - 6 + i];
        }
        return string(result);
    }

    function makeAnEpicNFT() public {
        require(getTotalMinted() < maxCollectionSize, 'Max number of Epic NFTs already minted');

        uint256 newItemId = _tokenIds.current();
        string memory first = pickRandomWord(newItemId, 'FIRST', firstWords);
        string memory second = pickRandomWord(newItemId, 'SECOND', firstWords);
        string memory third = pickRandomWord(newItemId, 'THIRD', thirdWords);
        string memory combinedWord = string(abi.encodePacked(first, second, third));

        string memory color = colorFromAddress(Strings.toHexString(msg.sender));
        string memory finalSvg = string(
            abi.encodePacked(svgPrefix, color, svgMiddle, combinedWord, '</text></svg>')
        );

        console.log(finalSvg);

        string memory encodedSvg = Base64.encode(bytes(finalSvg));

        string
            memory animationUrl = 'data:text/html;base64,';

        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{ "name": "',
                        combinedWord,
                        '", ',
                        '"description": "Foo", ',
                        '"image": "data:image/svg+xml;base64,',
                        encodedSvg,
                        '", ',
                        '"animation_url": "',
                        animationUrl,
                        '" }'
                    )
                )
            )
        );

        string memory finalTokenUri = string(
            abi.encodePacked('data:application/json;base64,', json)
        );

        console.log('\n--------------------');
        console.log(
            string(abi.encodePacked('https://nftpreview.0xdev.codes/?code=', finalTokenUri))
        );
        console.log('--------------------\n');

        _safeMint(msg.sender, newItemId);

        _setTokenURI(newItemId, finalTokenUri);
        console.log('New NFT w/ ID %s has been minted to %s', newItemId, msg.sender);

        _tokenIds.increment();

        emit NewEpicNFTMinted(msg.sender, newItemId);
    }
}

