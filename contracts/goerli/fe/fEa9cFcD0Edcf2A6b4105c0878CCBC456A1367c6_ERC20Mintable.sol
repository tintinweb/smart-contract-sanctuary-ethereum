import {ERC20PresetMinterPauser} from "@openzeppelin/contracts/token/ERC20/presets/ERC20PresetMinterPauser.sol";
import {ERC721} from "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {ERC721Burnable} from "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import {ERC2981} from "@openzeppelin/contracts/token/common/ERC2981.sol";
import {Counters} from "@openzeppelin/contracts/utils/Counters.sol";
import {SafeTransferLib} from "solmate/src/utils/SafeTransferLib.sol";
import {IERC721Receiver} from "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";

/**
 * @notice This is the contract for COLT
 */
contract ERC20Mintable is ERC20PresetMinterPauser, Ownable {
    using SafeTransferLib for ERC20PresetMinterPauser;

    constructor(string memory _name, string memory _symbol) ERC20PresetMinterPauser(_name, _symbol) {}

    /**
     * Only the owner (PackageAirdrop) can call this.
     */
    function freeMint(address to, uint256 amount) external onlyOwner {
        _mint(to, amount);
    }

    function decimals() public view virtual override returns (uint8) {
        return 0;
    }
}

/**
 * @notice This is the contract for Zodiac NFT
 */
contract ERC721Mintable is ERC721, Ownable, ERC721Burnable, ERC2981 {
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIdCounter;
    string standardURI;

    constructor(string memory _name, string memory _symbol, string memory _standardURI) ERC721(_name, _symbol) {
        // 1%
        _setDefaultRoyalty(msg.sender, 100);
        standardURI = _standardURI;
    }

    function setBaseURI(string memory _uri) external onlyOwner {
        standardURI = _uri;
    }

    function _baseURI() internal view override returns (string memory) {
        return standardURI;
    }

    /**
     * Only the owner (PackageAirdrop) can call this.
     */
    function freeMint(address to, uint256 amount) external onlyOwner {
        uint256[] memory idsMinted = new uint256[](amount);
        uint256 tokenId = _tokenIdCounter.current();
        for (uint256 i; i < amount; ++i) {
            _safeMint(to, tokenId);
            idsMinted[i] = tokenId;
            tokenId++;
            _tokenIdCounter.increment();
        }
    }

    // The following functions are overrides required by Solidity.

    function _burn(uint256 tokenId) internal override (ERC721) {
        super._burn(tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view override (ERC721, ERC2981) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}

/**
 * @notice This contract does a one off minting of the total ERC20 and ERC721
 * supplies. The deployer can then airdrop at most 1 package to every wallet they
 * so desire.
 */
contract PackageAirdrop is Ownable, IERC721Receiver {
    using Counters for Counters.Counter;
    using SafeTransferLib for address payable;

    ERC20Mintable public immutable erc20Faucet;
    ERC721Mintable public immutable erc721Faucet;
    uint256 public immutable MAX_AIRDROPS;
    uint256 public immutable ERC20_SUPPLY;
    uint256 public immutable ERC721_SUPPLY;

    uint256 public constant ERC20_AIRDROP_AMOUNT = 100;
    uint256 public constant ERC721_AIRDROP_AMOUNT = 12;
    uint256 public constant ETH_AIRDROP_AMOUNT = 0.005 ether;

    Counters.Counter private nextTokenId;

    uint256 public remainingAirdrops;

    bool public airdropsPrepared;

    mapping(address => bool) public hasBeenAirdropped;

    /**
     * Events
     */
    event toBeMinted(uint256 amount);
    event receivedETH(uint256 amount);
    event insufficientETH(address to);
    event dispensedETH(address to);
    event dispensedERC20(address to);
    event dispensedERC721(uint256[] nftIds, address to);

    constructor(
        string memory erc20Name,
        string memory erc20Symbol,
        string memory erc721Name,
        string memory erc721Symbol,
        string memory erc721StandardURI,
        uint256 maxAirdrops
    ) payable {
        erc20Faucet = new ERC20Mintable(erc20Name, erc20Symbol);
        erc721Faucet = new ERC721Mintable(erc721Name, erc721Symbol, erc721StandardURI);
        
        MAX_AIRDROPS = maxAirdrops;
        remainingAirdrops = maxAirdrops;

        ERC20_SUPPLY = ERC20_AIRDROP_AMOUNT * MAX_AIRDROPS;
        ERC721_SUPPLY = ERC721_AIRDROP_AMOUNT * MAX_AIRDROPS;

        /// @dev NFTs must be minted separately due to gas costs
        erc20Faucet.freeMint(address(this), ERC20_SUPPLY);
        emit toBeMinted(MAX_AIRDROPS);
    }

    /**
     * @dev Call this function repeatedly to mint the total supply. 240k 
     * uint256's is too much to mint in 1 external call sadly.
     */
    function prepareAirdrops(uint256 numPackages) external onlyOwner {
        require(!airdropsPrepared, "Already ready to airdrop");

        /// @dev Don't mint more than total supply
        uint256 amount = ERC721_AIRDROP_AMOUNT * numPackages;
        uint256 remainderToMint = ERC721_SUPPLY - erc721Faucet.balanceOf(address(this));
        if (amount >= remainderToMint) {
            amount = remainderToMint;
            airdropsPrepared = true;
            emit toBeMinted(0);
        }

        erc721Faucet.freeMint(address(this), amount);
        emit toBeMinted(remainderToMint - amount);
    }

    /**
     * @notice Dispense ETH, ERC20 tokens, and ERC721 tokens to the specified
     * address
     */
    function airdrop(address[] calldata recipients) external onlyOwner {
        require(airdropsPrepared, "Total supply must be minted");
        uint256 length = recipients.length;
        require(remainingAirdrops >= length, "Too many recipients");
        uint256 numFailedAirdrops = 0;
        uint256[] memory idsAirdropped = new uint256[](ERC721_AIRDROP_AMOUNT);
        for (uint256 i = 0; i < length;) {
            address payable recipient = payable(recipients[i]);
            if (!hasBeenAirdropped[recipient]) {
                hasBeenAirdropped[recipient] = true;

                /// @dev airdrop ETH if there is ETH
                bool ethDispensed = address(this).balance >= ETH_AIRDROP_AMOUNT;

                /// @dev airdrop ERC20
                erc20Faucet.transfer(recipient, ERC20_AIRDROP_AMOUNT);

                /// @dev airdrop NFTs
                uint256 tokenId = nextTokenId.current();
                for (uint256 j; j < ERC721_AIRDROP_AMOUNT; ++j) {
                    erc721Faucet.safeTransferFrom(address(this), recipient, tokenId);
                    idsAirdropped[j] = tokenId;
                    tokenId++;
                    nextTokenId.increment();
                }

                /// @dev emit events
                if (ethDispensed) {
                    emit dispensedETH(recipient);
                    recipient.safeTransferETH(ETH_AIRDROP_AMOUNT);
                } else {
                    emit insufficientETH(recipient);
                }
                emit dispensedERC20(recipient);
                emit dispensedERC721(idsAirdropped, recipient);
            } else {
                ++numFailedAirdrops;
            }

            unchecked {
                ++i;
            }
        }

        remainingAirdrops -= length - numFailedAirdrops;
    }

    /**
     * @notice Set the base token URI for the NFT collection this faucet dispenses
     */
    function setBaseURI(string memory _uri) external onlyOwner {
        erc721Faucet.setBaseURI(_uri);
    }

    /**
     * @notice Returns the tokenURI for the tokenId given of the NFT collection
     * this faucet dispenses
     */
    function tokenURI(uint256 tokenId) external view returns (string memory) {
        return erc721Faucet.tokenURI(tokenId);
    }

    /**
     * Proxy for calling the same function on the NFT collection this faucet
     * dispenses
     */
    function supportsInterface(bytes4 interfaceId) external view returns (bool) {
        return erc721Faucet.supportsInterface(interfaceId);
    }

    /**
     * @notice Recharge the pool's ETH balance. Refunds ETH that this contract
     * will not be able to airdrop
     */
    function rechargeETH() external payable {
        uint256 maxRechargeAmount = remainingAirdrops * ETH_AIRDROP_AMOUNT;
        uint256 balance = address(this).balance;
        uint256 refundAmount = balance > maxRechargeAmount ? balance - maxRechargeAmount : 0;
        if (refundAmount > 0) {
            payable(msg.sender).safeTransferETH(refundAmount);

            emit receivedETH(msg.value - refundAmount);
        } else {
            emit receivedETH(msg.value);
        }
    }

    /**
     * @dev All ETH transfers into the faucet are accepted. This is functionally
     * equivalent to recharging the pool.
     */
    receive() external payable {
        emit receivedETH(msg.value);
    }

    /**
     * @dev All ETH transfers into the faucet are accepted. This is functionally
     * equivalent to recharging the pool.
     */
    fallback() external payable {
        emit receivedETH(msg.value);
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }
}

