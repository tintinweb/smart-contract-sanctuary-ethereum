// File contracts/platziPunks.sol



pragma solidity ^0.8.0;
//OpenZeppelin
//import BASE64 library
contract PlatziPunks is ERC721, ERC721Enumerable, PlatziPunksDNA {
    //Agregando un contador para el idToken del Mint
    using Counters for Counters.Counter;
    using Strings for uint256;

    Counters.Counter private _idCounter;

    VRFv2Consumer public contractRandomize;

    uint256 public maxSupply;

    address _owner;

    mapping(uint256 => uint256) public tokenDNA;

    //El max suply se pone en el constructor por que es con el que iniciamos en el contrato
    constructor(uint256 _maxSupply) ERC721("PlatziPunks", "PLPKS") {
        maxSupply = _maxSupply;
        _owner = msg.sender;
    }

    //Set Randomize Contract in a function
    function setContractRandomize(address _vrfConsumerContract) private {
        contractRandomize = VRFv2Consumer(_vrfConsumerContract);
    }

    //RANDOM FUNCTIONS
    function getRandom() private {
        contractRandomize.requestRandomWords();
    }

    //GET REQUESTID for random
    function getRequestId() public view returns (uint256) {
        return contractRandomize.lastRequestId();
    }

    function getRandomValues(uint256 _requestId)
        external
        view
        returns (bool fulfilled, uint256[] memory randomWords)
    {
        (fulfilled, randomWords) = contractRandomize.getRequestStatus(
            _requestId
        );
    }

    // Funcion que contiene la base del URI de la API del avatar
    function _baseURI() internal pure override returns (string memory) {
        return "https://avataaars.io/";
    }

    //Funcion que construye la parte parametrica de la URL
    function _paramsURI(uint256 _dna) internal view returns (string memory) {
        string memory params;
        //concatetando los parametros usando las funciones de get en el ADN
        {
            params = string(
                abi.encodePacked(
                    "accesoriesType=",
                    getAccesoriesType(uint8(_dna)),
                    "&clotheColor=",
                    getClotheColor(uint8(_dna)),
                    "&clotheType=",
                    getClotheType(uint8(_dna)),
                    "&eyeType=",
                    getEyeType(uint8(_dna)),
                    "&eyebrowType=",
                    getEyeBrowType(uint8(_dna)),
                    "&facialHairColor=",
                    getFacialHairColor(uint8(_dna)),
                    "&facialHairType=",
                    getFacialHairType(uint8(_dna)),
                    "&hairColor=",
                    getHairColor(uint8(_dna)),
                    "&hatColor=",
                    getHatColor(uint8(_dna)),
                    "&graphicType=",
                    getGraphicType(uint8(_dna)),
                    "&mouthType=",
                    getMouthType(uint8(_dna)),
                    "&skinColor=",
                    getSkinColor(uint8(_dna))
                )
            );
        }
        return
            string(
                abi.encodePacked(params, "&topType=", getTopType(uint8(_dna)))
            );
    }

    //RETURN DE IMAGE BY DNA
    function returnImageByDNA(uint256 _dna)
        public
        view
        returns (string memory)
    {
        string memory baseURI = _baseURI();
        string memory paramsURI = _paramsURI(_dna);
        return string(abi.encodePacked(baseURI, "?", paramsURI));
    }

    //MINT Funtion
    function mint(address _contractConsumerAdd) public payable {
        // pregunto si esta enviando 0.00001 eth
        // require(msg.value >= 50000000000000000, " you neet 0.05 ETH to Mint");

        uint256 current = _idCounter.current();

        require(current < maxSupply, "NO PLTAZIPUNKS LEFT!!");
        // Guardamos en el mapping el tokenDNA que se genera de la funcion determinista
        // tokenDNA[current] = _deterministicPesudoRandomDNA(current, msg.sender);
        setContractRandomize(_contractConsumerAdd);
        getRandom();
        tokenDNA[current] = getRequestId();
        // Transfiero la cantidad de Erthers
        payable(_owner).transfer(msg.value);
        //Minteo y envio el NFT
        _safeMint(msg.sender, current);
        //Incremento el contador del ID
        _idCounter.increment();
    }

    /*
     *@tokenURI
     *@param(uint256 _tokenId) => el id es secuencial y viene de la metadata del NFT
     */
    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        //Exist es una funcion de OpenZeppelin que verifica si existe o no un token
        require(
            _exists(_tokenId),
            "ERC721 Metadata: URI queru for nonexistend token"
        );
        uint256 _dna = tokenDNA[_tokenId];
        string memory image = returnImageByDNA(_dna);
        //Concatenando en Solididty
        string memory jsonURI = Base64.encode(
            abi.encodePacked(
                '{ "name": "PlatziPunks #',
                _tokenId.toString(),
                '", "description": "Platzi Punks are randomized Avataaars stored on chain to teach DApp development on Platzi", "image": "',
                image,
                '"}'
            )
        );

        return
            string(abi.encodePacked("data:application/json;base64,", jsonURI));
    }

    // Override Required
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}