contract SwapERC20 {
    using SafeERC20 for IERC20;
    using BytesAddress for bytes;

    address private constant SWAP_ROUTER =
        0xE592427A0AEce92De3Edee1F18E0157C05861564;
    address private constant BRIDGE_ADDRESS =
        0x6EA0C45bAC29a2A2769ec14f4BCD959b07780106;
    address private constant PROXY_ADDRESS =
        0x9F499B3D5030fa926985E517132a80e28bEECEb0;

    ISwapRouter public immutable swapRouter = ISwapRouter(SWAP_ROUTER);
    IBridge public immutable bridge = IBridge(BRIDGE_ADDRESS);

    function safeTransferWithApprove(
        uint256 amountIn,
        address routerAddress,
        IERC20 tokenIn
    ) internal {
        console.log(tokenIn.balanceOf(msg.sender));
        console.log(amountIn);
        tokenIn.safeTransferFrom(msg.sender, address(this), amountIn);
        tokenIn.safeApprove(routerAddress, amountIn);
    }

    function swapExactInputSingle(
        uint256 amountIn,
        uint256 amountOutMinimum,
        IERC20 tokenIn,
        IERC20 tokenOut
    ) private returns (uint256 amountOut) {
        safeTransferWithApprove(amountIn, address(swapRouter), tokenIn);
        ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
            .ExactInputSingleParams({
                tokenIn: address(tokenIn),
                tokenOut: address(tokenOut),
                fee: 3000,
                recipient: msg.sender,
                deadline: block.timestamp,
                amountIn: amountIn,
                amountOutMinimum: amountOutMinimum,
                sqrtPriceLimitX96: 0
            });

        amountOut = swapRouter.exactInputSingle(params);
    }

    function swapExactOutputSingle(
        uint256 amountOut,
        uint256 amountInMaximum,
        IERC20 tokenIn,
        IERC20 tokenOut
    ) private returns (uint256 amountIn) {
        safeTransferWithApprove(amountInMaximum, address(swapRouter), tokenIn);

        ISwapRouter.ExactOutputSingleParams memory params = ISwapRouter
            .ExactOutputSingleParams({
                tokenIn: address(tokenIn),
                tokenOut: address(tokenOut),
                fee: 3000,
                recipient: msg.sender,
                deadline: block.timestamp,
                amountOut: amountOut,
                amountInMaximum: amountInMaximum,
                sqrtPriceLimitX96: 0
            });

        amountIn = swapRouter.exactOutputSingle(params);
        if (amountIn < amountInMaximum) {
            // Reset approval on router
            tokenIn.safeApprove(address(swapRouter), 0);
            // Refund WETH to user
            tokenIn.safeTransfer(msg.sender, amountInMaximum - amountIn);
        }
    }

    function swapExactInputSingleThenBridge(
        uint256 amountIn,
        uint256 amountOutMinimum,
        IERC20 tokenIn,
        IERC20 tokenOut,
        string memory receiver,
        string memory network,
        bool isWrapped,
        IBundler.Bundle calldata bundle_
    ) external returns (uint256 amountOut) {
        amountOut = swapExactInputSingle(
            amountIn,
            amountOutMinimum,
            tokenIn,
            tokenOut
        );
        console.log(amountOut);

        safeTransferWithApprove(amountOut, BRIDGE_ADDRESS, tokenOut);
        bridge.depositERC20(
            address(tokenOut),
            amountOut,
            bundle_,
            network,
            receiver,
            isWrapped
        );
    }

    function swapExactOutputSingleThenBridge(
        uint256 amountOut,
        uint256 amountInMaximum,
        IERC20 tokenIn,
        IERC20 tokenOut,
        string memory receiver,
        string memory network,
        bool isWrapped,
        IBundler.Bundle calldata bundle_
    ) external returns (uint256 amountIn) {
        amountIn = swapExactOutputSingle(
            amountOut,
            amountInMaximum,
            tokenIn,
            tokenOut
        );

        safeTransferWithApprove(amountOut, BRIDGE_ADDRESS, tokenOut);
        bridge.depositERC20(
            address(tokenOut),
            amountOut,
            bundle_,
            network,
            receiver,
            isWrapped
        );
    }


    function swapExactOutputMultihop(
        uint256 amountOut,
        uint256 amountInMaximum,
        bytes calldata path
    ) private returns (uint256 amountIn) {
        (address tokenIn, address tokenOut) = path.getInOutTokens();
        console.log(tokenOut);
        safeTransferWithApprove(amountInMaximum, address(swapRouter), IERC20(tokenIn));

        ISwapRouter.ExactOutputParams memory params = ISwapRouter
            .ExactOutputParams({
                path: path,
                recipient: msg.sender,
                deadline: block.timestamp,
                amountOut: amountOut,
                amountInMaximum: amountInMaximum
            });

        amountIn = swapRouter.exactOutput(params);
        if (amountIn < amountInMaximum) {
            // Reset approval on router
            IERC20(tokenIn).safeApprove(address(swapRouter), 0);

            console.log("hrre I sa");
            // Refund WETH to user
            IERC20(tokenIn).safeTransfer(msg.sender, amountInMaximum - amountIn);
        }
    }

     function swapExactOutputMultiHopThenBridge(
        uint256 amountOut,
        uint256 amountInMaximum,
        bytes calldata path,
        string memory receiver,
        string memory network,
        bool isWrapped,
        IBundler.Bundle calldata bundle_
    ) external returns (uint256 amountIn) {
        amountIn = swapExactOutputMultihop(
            amountOut,
            amountInMaximum,
            path
        );
        (, address tokenOut) = path.getInOutTokens();


        safeTransferWithApprove(amountOut, BRIDGE_ADDRESS, IERC20(tokenOut));
        bridge.depositERC20(
            address(tokenOut),
            amountOut,
            bundle_,
            network,
            receiver,
            isWrapped
        );
    }
}

//SPDX-License-Identifier: Unlicense
pragma solidity ^0.8.9;

library BytesAddress {
    function getInOutTokens(bytes calldata path) internal pure returns(address,address) {
        require(path.length > 42, "BytesAddress: invalid path length");
        return (_toAddress(path, path.length-20), _toAddress(path, 0));
    }

    function _toAddress(bytes memory _bytes, uint256 _start) internal pure returns (address parsedAddress) {
        assembly {
            parsedAddress := div(mload(add(add(_bytes, 0x20), _start)), 0x1000000000000000000000000)
        }
    }
}

