interface ERC1155 {
    function price() external view returns (uint256);

    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) external
      returns (bool);

    // function mintBatch(
    //     address to,
    //     uint256[] memory ids,
    //     uint256[] memory amounts,
    //     bytes memory data
    // ) external;

    function getSupplies() external view returns (uint256[] memory);
    function getMintsRemaining() external view returns (uint256);
    function getTotalNFTs() external view returns (uint256);
}

contract MintLogic is Initializable, AccessControlUpgradeable {
    ERC1155 Jer;
    ERC1155 Pat;
    uint256 jPrice;
    uint256 pPrice;
    uint256 seed;
    uint256 allowListRequired;
    uint256 freePatches;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    // allow list mint
    // 

    bytes32 merkleRoot;

    function initialize(
        address j,
        address p,
        uint256 rSeed
    ) public initializer {
        __AccessControl_init();
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        Jer = ERC1155(j);
        Pat = ERC1155(p);
        seed = rSeed;
        jPrice = 0.001 ether;
        pPrice = 0.001 ether;
        allowListRequired = 0;
        freePatches = 3;
    }

    // @notice Is a given address allowlisted based on proof provided
    // @param _proof Merkle proof
    // @param _claimer address to check
    // @param _contract NFT contract address
    // @return Is allowlisted
    function isOnAllowlist(
        bytes32[] memory _proof,
        address _claimer
    ) public view returns (bool) {
        bytes32 leaf = keccak256(abi.encodePacked(_claimer));
        return MerkleProof.verify(_proof, merkleRoot, leaf);
    }

    function setAllowListRequired(uint256 _num) public onlyRole(DEFAULT_ADMIN_ROLE) {
        allowListRequired = _num;
    }

    function setSeed(uint256 _newSeed) public onlyRole(DEFAULT_ADMIN_ROLE) {
        seed = _newSeed;
    }

    function buySets(address to, uint256 amt, bytes32[] memory proof) public payable {
        if (allowListRequired > 0) {
            require(isOnAllowlist(proof, to), "Not on allowlist");
        }
        require(msg.value == jPrice * amt, "Not the right amount of ether");
        require(Jer.getMintsRemaining() >= amt, "Not enough jerseys remaining");
        require(Pat.getMintsRemaining() >= (amt * 3), "Not enough jerseys remaining");
        bool success = mintItems(to, amt, Jer);
        require(success, "Minting jerseys failed");
        success = mintItems(to, amt * 3, Pat);
        require(success, "Minting patches failed");
    }

    function buyPatches(address to, uint256 amt, bytes32[] memory proof) public payable {
        if (allowListRequired > 0) {
            require(isOnAllowlist(proof, to), "Not on allowlist");
        }
        require(msg.value == pPrice * amt, "Not the right amount of ether");
        require(Pat.getMintsRemaining() >= ((Jer.getMintsRemaining() * 3) + amt), "Not enough patches remaining");
        bool success = mintItems(to, amt, Pat);
        require(success, "Minting patches failed");
    }

    function mintItems(address to, uint256 amt, ERC1155 Contr) internal returns (bool) {
        uint256[] memory list = Contr.getSupplies();
        for (uint i = 0; i < amt; i++) {
            uint256 id = _random(list.length);
            uint256 toMint = id;
            bool success = false;
            while (success == false) {
                success = Contr.mint(to, list[toMint], 1, "");
                if (success == false) {
                    toMint++;
                    if (toMint >= list.length) {
                        toMint = 0;
                    }
                    if (toMint == id) {
                        return false;
                    }
                }
            } 
        }
        return true;
    }

    function setjprice(uint256 newPrice) public onlyRole(DEFAULT_ADMIN_ROLE) {
        jPrice = newPrice;
    }

    function setpprice(uint256 newPrice) public onlyRole(DEFAULT_ADMIN_ROLE) {
        pPrice = newPrice;
    }

    function setPrices(uint256 newjPrice, uint256 newpPrice) public onlyRole(DEFAULT_ADMIN_ROLE) {
        jPrice = newjPrice;
        pPrice = newpPrice;
    }

    function getBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function setMerkleRoot(bytes32 _merkleRoot)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        merkleRoot = _merkleRoot;
    }

    // @notice Generates a random number within a range
    // @param inRange - max number from 0 to generate
    // @return a single number
    function _random(uint256 inRange) internal returns (uint256) {
        uint256 randomNumber = uint256(
            keccak256(
                abi.encodePacked(blockhash(block.number - 1), msg.sender, seed)
            )
        );
        seed = randomNumber;
        return randomNumber % inRange;
    }

    function withdraw() public onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }
}

