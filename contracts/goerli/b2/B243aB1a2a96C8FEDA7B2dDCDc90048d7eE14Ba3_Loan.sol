import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "./Loan.sol";

/* Errors */
error BioKoinNFT__notEnoughToReserve();
error BioKoinNFT__notEtherToWithdraw();
error BioKoinNFT__transferFailed();
error BioKoinNFT__contractPaused();
error BioKoinNFT__notEnoughNFT();
error BioKoinNFT__notEnoughEther();
error BioKoinNFT__canNotTransferBorrowedERC721();

/** @title a BioKoin NFT Smart Contract
 * @author Alvaro Teran Rodriguez
 * @notice This contract is for creating an NFT Collection for BioKoin
 * @dev This implements ERC721Enumerable and Ownable by Openzeppelin
 */

contract BioKoinNFT is ERC721Enumerable, Ownable {
    using SafeMath for uint256;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    uint256 private constant MAX_SUPPLY = 100;
    uint256 private constant PRICE = 0.001 ether;
    address loanAddress;
    bool private paused = false;
    string private baseTokenURI;

    /* Functions */
    constructor(string memory baseURI) ERC721("NFT BioKoin", "NFTBK") {
        setBaseURI(baseURI);
    }

    function mintNFTs(uint256 _count) public payable {
        uint256 totalMinted = _tokenIds.current();

        if (totalMinted.add(_count) > MAX_SUPPLY) {
            revert BioKoinNFT__notEnoughToReserve();
        }
        if (msg.value < PRICE.mul(_count)) {
            revert BioKoinNFT__notEnoughEther();
        }

        for (uint256 i = 0; i < _count; i++) {
            _mintSingleNFT();
        }
    }

    function tokensOfOwner(address _owner)
        external
        view
        returns (uint256[] memory)
    {
        uint256 tokenCount = balanceOf(_owner);
        uint256[] memory tokensId = new uint256[](tokenCount);

        for (uint256 i = 0; i < tokenCount; i++) {
            tokensId[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokensId;
    }

    function setBaseURI(string memory _baseTokenURI) public onlyOwner {
        baseTokenURI = _baseTokenURI;
    }

    function _mintSingleNFT() private {
        if (paused) {
            revert BioKoinNFT__contractPaused();
        }

        uint256 newTokenID = _tokenIds.current();
        _safeMint(msg.sender, newTokenID);
        _tokenIds.increment();
    }

    function mintNFT() public payable {
        uint256 totalMinted = _tokenIds.current();

        if (totalMinted.add(1) > MAX_SUPPLY) {
            revert BioKoinNFT__notEnoughNFT();
        }
        if (msg.value < PRICE.mul(1)) {
            revert BioKoinNFT__notEnoughEther();
        }

        _mintSingleNFT();
    }

    function withdraw() public payable onlyOwner {
        uint256 balance = address(this).balance;
        if (balance <= 0) {
            revert BioKoinNFT__notEtherToWithdraw();
        }

        (bool success, ) = (msg.sender).call{value: balance}("");
        if (!success) {
            revert BioKoinNFT__transferFailed();
        }
    }

    function pause(bool _state) public onlyOwner {
        paused = _state;
    }

    function setLoanAddress(address _loan) external {
        loanAddress = _loan;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
        interfaceLoan loan = interfaceLoan(loanAddress);
        if (loan.checkBlackList(tokenId) == true) {
            revert BioKoinNFT__canNotTransferBorrowedERC721();
        }
    }

    /* View / Pure Functions */

    function getContractState() public view returns (bool) {
        return paused;
    }

    function getTokenCounter() public view returns (Counters.Counter memory) {
        return _tokenIds;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenURI;
    }

    function getMaxSupply() public pure returns (uint256) {
        return MAX_SUPPLY;
    }

    function getPricePerNFT() public pure returns (uint256) {
        return PRICE;
    }
}

