pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@uniswap/lib/contracts/libraries/TransferHelper.sol";
import "./BioKoinNFT.sol";

abstract contract interfaceLoan {
    function checkBlackList(uint256 _id) external view virtual returns (bool);
}

/* Errors */
error Loan__notEnoughERC721s();
error Loan__notEnoughTokenInSender(string description);
error Loan__notEnoughTokenSent(string description);
error Loan__notEnoughERC721sInContract();
error Loan__notEnoughETHInContract();
error Loan__transferFailed();
error Loan__timestampExceeded();
error Loan__incorrectTxIndex();
error Loan__notCorrectTimestampToWithdraw();
error Loan__ethPayableExceeded();
error Loan__canExceedPayOffAmount();

contract Loan {
    BioKoinNFT public BioKoinContract;

    using Counters for Counters.Counter;
    Counters.Counter private _loanIds;

    mapping(uint256 => bool) public ERC721Borrow;
    loanStruct[] public loanList;

    uint256 private constant THREE_MONTHS = 7889229;
    uint256 private constant INICIAL_EPOCH = 1664575200;

    address public constant routerAddress =
        0xE592427A0AEce92De3Edee1F18E0157C05861564;
    ISwapRouter public immutable swapRouter = ISwapRouter(routerAddress);

    uint24 public constant poolFee = 3000;
    address public constant LINK = 0x326C977E6efc84E512bB9C30f76E30c160eD06FB;
    address public constant WETH = 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6;

    struct loanStruct {
        uint256 index;
        uint256 loanType;
        address owner;
        uint256 ownerAsset;
        uint256 collateredAsset;
        uint256 payAmount;
        uint256 payedAmount;
        uint256 payOffAmount;
        uint256 payOffTimestamp;
    }

    constructor(BioKoinNFT _tokenContract) {
        BioKoinContract = _tokenContract;
    }

    function addLiquidity(address _token, uint256 _amount) public {
        if (ERC20(_token).balanceOf(msg.sender) < _amount) {
            revert Loan__notEnoughTokenInSender({
                description: "No tokens in sender"
            });
        }
        if (_amount <= 0) {
            revert Loan__notEnoughTokenSent({
                description: "The amount needs to be more than 0"
            });
        }

        ERC20(_token).transferFrom(msg.sender, address(this), _amount);

        uint256 newTokenID = _loanIds.current();

        loanList.push(
            loanStruct(
                newTokenID,
                0,
                msg.sender,
                _amount,
                0,
                0,
                0,
                0,
                block.timestamp + THREE_MONTHS
            )
        );
        _loanIds.increment();
    }

    function withdraw(address _token, uint256 _index) public {
        // if (block.timestamp < loanList[_index].payOffTimestamp) {
        //     revert Loan__notCorrectTimestampToWithdraw();
        // }
        if (loanList[_index].loanType != 0) {
            revert Loan__incorrectTxIndex();
        }
        if (
            ERC20(_token).balanceOf(address(this)) < loanList[_index].ownerAsset
        ) {
            revert Loan__notEnoughETHInContract();
        }

        ERC20(_token).transferFrom(
            address(this),
            msg.sender,
            loanList[_index].ownerAsset
        );
    }

    function collateredERC721(
        address _tokenA,
        // address _tokenB,
        uint256 _amount,
        uint256 _tokenId
    ) public {
        uint256 i_payOffAmount;

        if (BioKoinContract.balanceOf(address(this)) == 0) {
            i_payOffAmount = ERC20(_tokenA).balanceOf(address(this)) / 1;
        } else {
            i_payOffAmount =
                ERC20(_tokenA).balanceOf(address(this)) /
                BioKoinContract.balanceOf(address(this));
        }

        if (i_payOffAmount < _amount) {
            revert Loan__notEnoughETHInContract();
        }

        BioKoinContract.transferFrom(msg.sender, address(this), _tokenId);
        ERC721Borrow[_tokenId] = true;

        loanList.push(
            loanStruct(
                _loanIds.current(),
                1,
                msg.sender,
                _amount,
                _tokenId,
                _amount,
                0,
                i_payOffAmount,
                block.timestamp + THREE_MONTHS
            )
        );
        _loanIds.increment();

        ERC20(_tokenA).transfer(msg.sender, _amount);

        // ERC20(_tokenA).approve(ROUTER, _amount);
        // ERC20(_tokenB).approve(ROUTER, _amount);

        // flashSwap(_tokenA, _tokenB, _amount / 2);

        // IUniswapV2Router01(ROUTER).addLiquidity(
        //     _tokenA,
        //     _tokenB,
        //     _amount / 2,
        //     _amount / 2,
        //     1,
        //     1,
        //     address(this),
        //     block.timestamp
        // );
    }

    function approve(uint256 amountIn) public {
        ERC20(LINK).approve(address(swapRouter), amountIn);
    }

    function swapExactInputSingle(uint256 amountIn)
        external
        returns (uint256 amountOut)
    {
        ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
            .ExactInputSingleParams({
                tokenIn: LINK,
                tokenOut: WETH,
                fee: poolFee,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: amountIn,
                amountOutMinimum: 0,
                sqrtPriceLimitX96: 0
            });

        amountOut = swapRouter.exactInputSingle(params);
    }

    function borrow(
        address _token,
        uint256 _index,
        uint256 _amount
    ) public {
        if (
            (loanList[_index].payOffAmount - loanList[_index].ownerAsset) >
            _amount
        ) {
            revert Loan__canExceedPayOffAmount();
        }

        loanStruct storage loan = loanList[_index];

        loan.payAmount = loanList[_index].payAmount + _amount;
        loan.ownerAsset = loanList[_index].ownerAsset + _amount;

        ERC20(_token).transfer(msg.sender, _amount);
    }

    function pay(
        address _token,
        uint256 _index,
        uint256 _amount
    ) public {
        if (block.timestamp > block.timestamp + THREE_MONTHS) {
            revert Loan__timestampExceeded();
        }
        if (loanList[_index].loanType != 1) {
            revert Loan__incorrectTxIndex();
        }
        if (loanList[_index].payAmount < _amount) {
            revert Loan__ethPayableExceeded();
        }

        ERC20(_token).transferFrom(msg.sender, address(this), _amount);

        loanStruct storage loan = loanList[_index];

        if (
            _amount == loanList[_index].payAmount - loanList[_index].payedAmount
        ) {
            ERC721Borrow[loanList[_index].collateredAsset] = false;
            BioKoinContract.transferFrom(
                address(this),
                msg.sender,
                loanList[_index].collateredAsset
            );
            delete loanList[_index];
        }
        if (_amount < loanList[_index].payAmount) {
            loan.payedAmount = loanList[_index].payedAmount + _amount;
        }
    }

    function getLoansOfOwner(address _owner)
        public
        view
        returns (loanStruct[] memory)
    {
        uint256 resultCount;
        uint256 j;
        for (uint256 i = 0; i < loanList.length; i++) {
            if (loanList[i].owner == _owner) {
                resultCount++;
            }
        }
        loanStruct[] memory result = new loanStruct[](resultCount);
        for (uint256 i = 0; i < loanList.length; i++) {
            if (loanList[i].owner == _owner) {
                result[j] = loanList[i];
                j++;
            }
        }
        return result;
    }

    function checkBlackList(uint256 _tokenId) external view returns (bool) {
        return ERC721Borrow[_tokenId];
    }

    function balanceOf(address _token) external view returns (uint256) {
        return ERC20(_token).balanceOf(address(this));
    }

    function getFinalEpochDate() public pure returns (uint256) {
        return INICIAL_EPOCH + THREE_MONTHS;
    }

    function getInicialEpochDate() public pure returns (uint256) {
        return INICIAL_EPOCH;
    }
}

