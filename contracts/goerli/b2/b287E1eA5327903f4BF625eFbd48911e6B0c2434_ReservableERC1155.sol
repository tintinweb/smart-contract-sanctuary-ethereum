        if (reservations[index].uriSettings.prereveal) {
            return reservations[index].uriSettings.prerevealUri;
        } else {
            if (reservations[index].uriSettings.appendTokenId) {
                return string(abi.encodePacked(reservations[index].uriSettings.baseUri, _convertTokenId(index, _tokenId).toString(), reservations[index].uriSettings.suffix));
            } else {
                return reservations[index].uriSettings.baseUri;
            }
        }
    }

    function _royaltyInfo(uint256 _tokenId) internal view returns (ERC2981.RoyaltyInfo memory) {
        bool found;
        uint256 index;

        (found, index) = _findReservation(_tokenId);
        if (!found) return ERC2981.RoyaltyInfo(address(0), 0);

        return reservations[index].royaltyInfo;
    }

    function _findReservation(uint256 _tokenId) private view returns (bool, uint256) {
        for (uint256 i = 0; i < reservations.length; i++) {
            uint256 lastId = reservations[i].startId;
            if (reservations[i].uniqueIds) {
                lastId += reservations[i].qty.sub(1);
            }

            if (_tokenId >= reservations[i].startId && _tokenId <= lastId) {
                return (true, i);
            }
        }

        return (false, 0);
    }

    function _convertTokenId(uint256 _index, uint256 _tokenId) private view returns (uint256 tokenId) {
        tokenId = (_tokenId.sub(reservations[_index].startId)).add(1);
    }
}

contract ReservableERC1155 is Reservable, ERC1155, ERC1155Supply, ERC2981, AccessControl, Pausable, ReentrancyGuard {
    bytes32 public constant POLICY_ROLE = keccak256("POLICY_ROLE");

    address public treasury;
    RoyaltyInfo public defaultRoyaltyInfo;

    constructor(address _treasury) ERC1155("") {
        treasury = _treasury;
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(POLICY_ROLE, msg.sender);
    }

    function pause() public onlyRole(POLICY_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(POLICY_ROLE) {
        _unpause();
    }

    function setTreasury(address _treasury) external onlyRole(POLICY_ROLE) {
        treasury = _treasury;
    }

    function addReservation(bool _uniqueIds, uint256 _qty) external onlyRole(POLICY_ROLE) returns (uint256) {
        return _addReservation(_uniqueIds, _qty);
    }

    function setUri(uint256 _index, string calldata _baseUri, bool _appendTokenId, string calldata _suffix) external onlyRole(POLICY_ROLE) {
        _setUri(_index, _baseUri, _appendTokenId, _suffix);
    }

    function setPrerevealUri(uint256 _index, string calldata _prerevealUri) external onlyRole(POLICY_ROLE) {
        _setPrerevealUri(_index, _prerevealUri);
    }

    function setRoyalty(uint256 _index, address _receiver, uint96 _feeNumerator) external onlyRole(POLICY_ROLE) {
        _setRoyalty(_index, _receiver, _feeNumerator);
    }

    function setDefaultRoyalty(address _receiver, uint96 _feeNumerator) external onlyRole(POLICY_ROLE) {
        require(_receiver != address(0), "INVALID_RECEIVER");
        require(_feeNumerator <= _feeDenominator(), "INVALID_FEE");

        defaultRoyaltyInfo = RoyaltyInfo(_receiver, _feeNumerator);
    }

    function deleteDefaultRoyalty() external onlyRole(POLICY_ROLE) {
        delete defaultRoyaltyInfo;
    }

    function setMerkleRoot(uint256 _index, bytes32 _merkleRoot) external onlyRole(POLICY_ROLE) {
        _setMerkleRoot(_index, _merkleRoot);
    }

    function setPayment(uint256 _index, address _paymentToken, uint256 _paymentAmount) external onlyRole(POLICY_ROLE) {
        _setPayment(_index, _paymentToken, _paymentAmount);
    }

    function setMaxMintPerWallet(uint256 _index, uint256 _maxMintPerWallet) external onlyRole(POLICY_ROLE) {
        _setMaxMintPerWallet(_index, _maxMintPerWallet);
    }

    function toggleMint(uint256 _index) external onlyRole(POLICY_ROLE) {
        _toggleMint(_index);
    }

    function togglePrereveal(uint256 _index) external onlyRole(POLICY_ROLE) {
        _togglePrereveal(_index);
    }

    function deleteReservation(uint256 _index) external onlyRole(POLICY_ROLE) {
        _deleteReservation(_index);
    }

    function airdrop(uint256 _index, address[] calldata _to, uint256[] calldata _amounts) external onlyRole(POLICY_ROLE) {
        require(_to.length == _amounts.length, "INVALID_ARR");
        for (uint256 i = 0; i < _to.length; i++) {
            uint256[] memory tokenIds = _reservationRecordMint(_index, _to[i], _amounts[i]);
            for (uint256 j = 0; j < tokenIds.length; j++) {
                _mint(_to[i], tokenIds[j], 1, "");
            }
        }
    }

    function mint(uint256 _index, uint256 _amount, bytes32[] calldata _merkleProof) external whenNotPaused nonReentrant {
        uint256[] memory tokenIds = _reservationMint(_index, msg.sender, _amount, _merkleProof, treasury);
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _mint(msg.sender, tokenIds[i], 1, "");
        }
    }

    function uri(uint256 _tokenId) public view override returns (string memory) {
        require(exists(_tokenId), "NO_TOKEN");
        return _tokenUri(_tokenId);
    }

    function royaltyInfo(uint256 _tokenId, uint256 _salePrice) public view override returns (address, uint256) {
        RoyaltyInfo memory royalty = _royaltyInfo(_tokenId);

        if (royalty.receiver == address(0)) {
            royalty = defaultRoyaltyInfo;
        }

        uint256 royaltyAmount = (_salePrice * royalty.royaltyFraction) / _feeDenominator();

        return (royalty.receiver, royaltyAmount);
    }

    function _feeDenominator() internal pure override returns (uint96) {
        return FEE_DENOM;
    }

    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
        internal
        whenNotPaused
        override(ERC1155, ERC1155Supply)
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    // Required overrides

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155, ERC2981, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}