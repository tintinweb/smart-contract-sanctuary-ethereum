        return MerkleProof.verify(_merkleProof, reservations[_index].merkleRoot, leaf);
    }

    function reservationIsMintable(uint256 _index) public view returns (bool) {
        require(_index < reservations.length, "INVALID_INDEX");
        return reservations[_index].mintable;
    }

    function _tokenUri(uint256 _tokenId) internal view returns (string memory) {
