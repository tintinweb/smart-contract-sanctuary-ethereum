    using Strings for uint256;

    uint96 internal constant FEE_DENOM = 10000;

    Reservation[] public reservations;
    uint256 private nextTokenId = 1;
    mapping(uint256 => mapping(address => uint256)) private userMints;

    function _addReservation(bool _uniqueIds, uint256 _qty) internal returns (uint256) {
        require(_qty > 0, "ZERO_QTY");
        Reservation memory reservation = Reservation({
            uniqueIds: _uniqueIds,
            mintable: false,
            locked: false,
            qty: _qty,
            totalMinted: 0,
            startId: nextTokenId,
            merkleRoot: 0,
            paymentToken: address(0),
            paymentAmount: 0,
            maxMintPerWallet: 1,
            uriSettings: URISettings({
                baseUri: "",
                appendTokenId: true,
                suffix: ".json",
                prerevealUri: "",
                prereveal: true
            }),
            royaltyInfo: ERC2981.RoyaltyInfo({
                receiver: address(0),
                royaltyFraction: 0
            })
        });
        reservations.push(reservation);

        if (_uniqueIds) {
            nextTokenId += _qty;
        } else {
            nextTokenId += 1;
        }

        if (reservations.length > 1) {
            // Lock previous reservation
            reservations[reservations.length - 2].locked = true;
        }

        return reservations.length - 1;
    }

    function _setUri(uint256 _index, string calldata _baseUri, bool _appendTokenId, string calldata _suffix) internal {
        require(_index < reservations.length, "INVALID_INDEX");
        reservations[_index].uriSettings.baseUri = _baseUri;
        reservations[_index].uriSettings.appendTokenId = _appendTokenId;
        reservations[_index].uriSettings.suffix = _suffix;
    }

    function _setPrerevealUri(uint256 _index, string calldata _prerevealUri) internal {
        require(_index < reservations.length, "INVALID_INDEX");
        reservations[_index].uriSettings.prerevealUri = _prerevealUri;
    }

    function _setRoyalty(uint256 _index, address _receiver, uint96 _feeNumerator) internal {
        require(_index < reservations.length, "INVALID_INDEX");
        require(_receiver != address(0), "INVALID_RECEIVER");
        require(_feeNumerator <= FEE_DENOM, "INVALID_FEE");
        reservations[_index].royaltyInfo = ERC2981.RoyaltyInfo(_receiver, _feeNumerator);
    }

    function _setMerkleRoot(uint256 _index, bytes32 _merkleRoot) internal {
        require(_index < reservations.length, "INVALID_INDEX");
        reservations[_index].merkleRoot = _merkleRoot;
    }

    function _setPayment(uint256 _index, address _paymentToken, uint256 _paymentAmount) internal {
        require(_index < reservations.length, "INVALID_INDEX");
        reservations[_index].paymentToken = _paymentToken;
        reservations[_index].paymentAmount = _paymentAmount;
    }

    function _setMaxMintPerWallet(uint256 _index, uint256 _maxMintPerWallet) internal {
        require(_index < reservations.length, "INVALID_INDEX");
        require(_maxMintPerWallet > 0, "ZERO_MAX_MINT");
        reservations[_index].maxMintPerWallet = _maxMintPerWallet;
    }

    function _toggleMint(uint256 _index) internal {
        require(_index < reservations.length, "INVALID_INDEX");
        reservations[_index].mintable = !reservations[_index].mintable;
    }

    function _togglePrereveal(uint256 _index) internal {
        require(_index < reservations.length, "INVALID_INDEX");
        reservations[_index].uriSettings.prereveal = !reservations[_index].uriSettings.prereveal;
    }

    function _deleteReservation(uint256 _index) internal {
        require(_index < reservations.length, "INVALID_INDEX");
        require(!reservations[_index].locked, "RESERVATION_LOCKED");

        if (reservations[_index].uniqueIds) {
            nextTokenId -= reservations[_index].qty;
        } else {
            nextTokenId -= 1;
        }

        reservations[_index] = reservations[reservations.length - 1];
        reservations.pop();
    }

    function _reservationRecordMint(uint256 _index, address _to, uint256 _amount) internal returns (uint256[] memory tokenIds) {
        require(_index < reservations.length, "INVALID_INDEX");
        require(_amount > 0, "INVALID_AMOUNT");
        require(reservations[_index].totalMinted.add(_amount) <= reservations[_index].qty, "ALL_MINTED");

        tokenIds = new uint256[](_amount);
        if (reservations[_index].uniqueIds) {
            for (uint256 i = 0; i < _amount; i++) {
                tokenIds[i] = reservations[_index].startId.add(reservations[_index].totalMinted).add(i);
            }
        } else {
            for (uint256 i = 0; i < _amount; i++) {
                tokenIds[i] = reservations[_index].startId;
            }
        }

        reservations[_index].locked = true;
        reservations[_index].totalMinted += _amount;
        userMints[_index][_to] += _amount;
    }

    function _reservationMint(uint256 _index, address _to, uint256 _amount, bytes32[] calldata _merkleProof, address _treasury) internal returns (uint256[] memory tokenIds) {
        require(_index < reservations.length, "INVALID_INDEX");
        require(_amount > 0, "INVALID_AMOUNT");
        require(reservations[_index].mintable, "NOT_MINTABLE");
        require(_isWhitelisted(_index, _to, _merkleProof), "NOT_WHITELISTED");
        require(userMints[_index][_to].add(_amount) <= reservations[_index].maxMintPerWallet, "MAX_MINT_REACHED");

        if (reservations[_index].paymentToken != address(0) && reservations[_index].paymentAmount > 0) {
