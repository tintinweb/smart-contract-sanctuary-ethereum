pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../FetcchBridge.sol";
import "../LayerZero/LayerZeroImpl.sol";
import "../Dex/UniV2Provider.sol";

import "../Governance/FetcchGovernance.sol";

contract FetcchPool is Ownable {
    using SafeERC20 for IERC20;

    uint256 constant FLOAT_HANDLER_TEN_4 = 10000;

    address private constant NATIVE_TOKEN_ADDRESS =
        address(0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE);

    struct TokenValidity {
        bool usable;
        bool approved;
    }

    FetcchBridge public bridge;
    FetcchGovernance public gov;
    LayerZeroImpl public lz;
    UniV2Provider public dex;

    constructor(address[] memory _supportedTokens, address _dex) {
        dex = UniV2Provider(_dex);

        uint256 leng = _supportedTokens.length;
        for (uint256 i = 0; i < leng; ) {
            supportedTokens[_supportedTokens[i]].usable = false;
            supportedTokens[_supportedTokens[i]].approved = true;
            emit AddedNewToken(_supportedTokens[i], false, true);
            unchecked {
                i++;
            }
        }
    }

    event LiquidityAdded(address _token, uint256 _amount, address _provider);
    event LiquidityRemoved(
        address _token,
        uint256 _amount,
        uint256 _rewards,
        address _provider
    );
    event AddedNewToken(address _token, bool usable, bool approved);
    event ValidateToken(address _token);
    event InvalidateToken(address _token);
    event RemovedToken(address _token);

    mapping(address => TokenValidity) public supportedTokens;

    modifier onlyCommLayer() {
        require(msg.sender == address(lz));
        _;
    }

    modifier onlyBridge() {
        require(msg.sender == address(bridge));
        _;
    }

    function changeBridge(address _bridge) external onlyOwner {
        bridge = FetcchBridge(_bridge);
    }

    function changeCommLayer(address _lz) external onlyOwner {
        lz = LayerZeroImpl(_lz);
    }

    function changeGovernance(address _gov) external onlyOwner {
        gov = FetcchGovernance(_gov);
    }

    function changeDex(address _dex) external onlyOwner {
        dex = UniV2Provider(_dex);
    }

    function addToken(address _token) public onlyOwner {
        supportedTokens[_token].approved = true;
        supportedTokens[_token].usable = false;
        emit AddedNewToken(_token, false, true);
    }

    function invalidateToken(address _token) public onlyOwner {
        supportedTokens[_token].usable = false;
        emit InvalidateToken(_token);
    }

    function removeToken(address _token) public onlyOwner {
        delete supportedTokens[_token];
        emit RemovedToken(_token);
    }

    function isValidToken(address _token) public view returns (bool) {
        require(supportedTokens[_token].usable);
        return true;
    }

    function addLiquidity(address _token, uint256 _amount)
        external
        returns (uint256)
    {
        IERC20 token = IERC20(_token);

        require(
            supportedTokens[_token].approved == true,
            "Fetcch: Token not supported"
        );
        require(_amount > 0, "Fetcch: Amount less than 0");

        token.safeIncreaseAllowance(address(this), _amount);
        token.safeTransferFrom(msg.sender, address(this), _amount);

        gov.addLP(msg.sender, _token, _amount);

        emit LiquidityAdded(_token, _amount, msg.sender);
        return _amount;
    }

    function removeLiquidity(address _token, uint256 _amount)
        external
        returns (uint256)
    {
        IERC20 token = IERC20(_token);
        uint256 balance = gov.lpBalance(msg.sender, _token);
        uint256 rewards = gov.lpRewards(msg.sender, _token);

        require(balance > 0, "Fetcch: Liquidity is less than zero");
        require(
            _amount <= balance + rewards,
            "Fetcch: Can't withdraw more amount"
        );
        require(
            token.balanceOf(address(this)) >= _amount,
            "Fetcch: Liquidity is too low"
        );

        token.safeTransferFrom(address(this), msg.sender, _amount);

        gov.updateLP(msg.sender, _token, _amount, rewards);

        if (balance > _amount) {
            emit LiquidityRemoved(
                _token,
                balance - _amount,
                rewards,
                msg.sender
            );
        } else {
            emit LiquidityRemoved(
                _token,
                _amount - balance,
                rewards,
                msg.sender
            );
        }
        return _amount;
    }

    function deposit(
        address _tokenIn,
        address _fromToken,
        address _toToken,
        address _receiver,
        uint256 _amount,
        uint16 _chainId,
        bytes memory _destination,
        bool _dexRequired,
        address refundAd
    ) external payable onlyBridge {
        //require(isValidToken(_fromToken), "Token not supported");
        IERC20(_tokenIn).safeTransferFrom(msg.sender, address(this), _amount);
        // bytes memory _dst = abi.encode(_destination);

        uint256 _amountOut = _amount - ((_amount * 10) / FLOAT_HANDLER_TEN_4);
        bytes memory _payload = abi.encode(
            _fromToken,
            _toToken,
            _amountOut,
            _receiver,
            _dexRequired
        );

        lz.sendMsg{value: msg.value}(
            _chainId,
            _destination,
            _payload,
            refundAd
        );
    }

    function release(bytes memory _swapData) external onlyCommLayer {
        (
            address _fromToken,
            address _toToken,
            uint256 _amount,
            address _receiver,
            bool _dexRequired
        ) = abi.decode(_swapData, (address, address, uint256, address, bool));

        if (_dexRequired) {
            IERC20(_fromToken).safeIncreaseAllowance(address(dex), _amount);
            dex.swapERC20(_fromToken, _toToken, _amount, _receiver);
        } else {
            IERC20(_toToken).safeTransfer(_receiver, _amount);
        }
    }

    /**
	// @notice function responsible to rescue funds if any
	// @param  tokenAddr address of token
	 */
    function rescueFunds(address tokenAddr) external onlyOwner {
        if (tokenAddr == NATIVE_TOKEN_ADDRESS) {
            uint256 balance = address(this).balance;
            payable(msg.sender).transfer(balance);
        } else {
            uint256 balance = IERC20(tokenAddr).balanceOf(address(this));
            IERC20(tokenAddr).safeTransfer(msg.sender, balance);
        }
    }
}

