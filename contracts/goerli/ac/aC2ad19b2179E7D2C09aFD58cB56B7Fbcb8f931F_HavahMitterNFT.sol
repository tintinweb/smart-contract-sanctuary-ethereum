pragma solidity ^0.8.0;

// import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

contract WERC721 is ERC721Upgradeable, OwnableUpgradeable {

    // Mapping from token ID to owner address
    mapping(uint256 => string) private _tokenURI;
    address public admin;

    function initialize(string memory name, string memory symbol) public initializer {
        __ERC721_init(name, symbol);
        __Ownable_init();
    }

    modifier onlyAdmin() {
        require(admin == _msgSender(), "Adminable: caller is not the admin");
        _;
    }
    function tokenURI(uint256 tokenID) public view virtual override returns (string memory) {
        _requireMinted(tokenID);

        return _tokenURI[tokenID];
    }

    function mint(address account, uint256 tokenID, string memory tokenURI) public virtual onlyAdmin {
        _mint(account, tokenID);
        _tokenURI[tokenID] = tokenURI;
    }

    function burn(uint256 tokenID) public virtual onlyAdmin {
        _burn(tokenID);
    }

    function setAdmin(address account) public {
        admin = account;
    }

}

