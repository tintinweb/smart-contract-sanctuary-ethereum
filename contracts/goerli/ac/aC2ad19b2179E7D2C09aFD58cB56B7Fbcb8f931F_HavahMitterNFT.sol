import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

contract WERC1155 is ERC1155Upgradeable, OwnableUpgradeable {
    // Contract name
    string public name;
    // Contract symbol
    string public symbol;
    string public baseMetadataURI;
    address public admin;
    mapping (uint256 => address) public creators;
    mapping (uint256 => uint256) public tokenSupply;

    modifier onlyAdmin() {
        require(admin == _msgSender(), "Adminable: caller is not the admin");
        _;
    }

    function initialize(string memory tokenName, string memory tokenSymbol, string memory metaUri) public initializer {
        name = tokenName;
        symbol = tokenSymbol;
        __ERC1155_init(metaUri);
        __Ownable_init();
    }

    function createToken(string memory tokenURI) public {
        _setURI(tokenURI);
    }

    function mint(address account, uint256 tokenID, uint256 amount, bytes memory data, string memory tokenURI) public virtual onlyAdmin {
        _mint(account, tokenID, amount, data);
        _setURI(tokenURI);
    }

    function burn(address account, uint256 tokenID, uint256 amount) public virtual onlyAdmin {
        _burn(account, tokenID, amount);
    }

    function setAdmin(address account) public {
        admin = account;
    }

}

