// File: contracts/HavahMitter.sol
contract HavahMitterNFT is OwnableUpgradeable, UUPSUpgradeable {
    using Strings for uint256;
    using Strings for address;

    uint256 public chainId;
    mapping(string => bool) public completed;
    bool public activated;
    address public validator;
    address public treasury;
    address public deployer;
    
    struct WrappedToken {
        address wrappedTokenAddress;
        string originTokenAddress;
        uint256 originChainID;
        uint256 tokenType;
    }
    
    mapping(address=>WrappedToken) public deployedToken;
    mapping(bytes=>WrappedToken) public hashToToken;

    function initialize(uint256 _chainId) public initializer {
        chainId = _chainId;
        activated = false;
        __Ownable_init();
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    receive() external payable {
    }
    event TransferNFT(string messageId, uint256 originChain, string originToken, uint256 fromChain, string fromToken, uint256 toChain, 
                    uint256 originTokenId, uint256 tokenType, uint256 commission, uint256 targetChainfee, string sender, string recipient);
    event ReceiveNFT(string messageId, uint256 originChain, string originToken, uint256 fromChain, string fromToken,
                    address toToken, uint256 tokenId, string sender, string recipient);
    event DeployWrappedNFT(address wrappedToken,  uint256 originChain, string originToken,
                              string name, string symbol, uint256 tokenType);

    function transferNFT(string memory messageId, uint256 expires,
                      uint256 fromChain, string memory fromToken, uint256 toChain, uint256 tokenId, 
                      uint256 tokenType, uint256 commission, uint256 targetChainFee, 
                      string memory recipient, bytes memory signature) external payable {
        require(activated, "smart contract is not activated");
        require(validator != address(0), "validator has not been set");
        require(treasury != address(0), "treasury has not been set");
        require(block.number <= expires, "transaction time exceeded.");
        require(!completed[messageId], "already completed");
        require(verifyTransferData(messageId, expires, fromChain, fromToken, toChain, tokenId, tokenType, commission, targetChainFee, recipient, signature), "failed to verify data");
        
        address WrappedToken = parseAddr(fromToken);
        
        
        console.log("token Type", tokenType);
        //내가 배포한 토큰인 경우
        if (fromChain == chainId && deployedToken[WrappedToken].wrappedTokenAddress != address(0)) {
            require(msg.value == commission + targetChainFee, "check ether amount");
            require(payable(treasury).send(commission + targetChainFee), "failed to send Ether");

            if (tokenType == 1) {
                require(WERC721(WrappedToken).ownerOf(tokenId) == address(msg.sender), "not enough balance");
                WERC721(WrappedToken).burn(tokenId);
            } 
            else if (tokenType == 2) {
                require(WERC1155(WrappedToken).balanceOf(address(msg.sender), tokenId) != 0, "not enough balance");
                WERC1155(WrappedToken).burn(address(msg.sender), tokenId, 1);
            }
            emit TransferNFT(messageId, chainId, WrappedToken.toHexString(), fromChain, fromToken, toChain, tokenId, tokenType, 
                    commission, targetChainFee, msg.sender.toHexString(), recipient);
        } 
        else {
            require(msg.value == commission + targetChainFee, "check ether amount");
            require(payable(treasury).send(commission + targetChainFee), "Failed to send Ether"); // send(commission + targetChainFee);

            if (tokenType == 1) {
                require(WERC721(WrappedToken).ownerOf(tokenId) == address(msg.sender), "not enough balance");
                WERC721(WrappedToken).safeTransferFrom(msg.sender, address(this), tokenId);
            } 
            else if (tokenType == 2) {
                console.log("balanceOf = ", WERC1155(WrappedToken).balanceOf(address(msg.sender), tokenId));
                require(WERC1155(WrappedToken).balanceOf(address(msg.sender), tokenId) != 0, "not enough balance");
                WERC1155(WrappedToken).safeTransferFrom(msg.sender, address(this), tokenId, 1, "");
            }
            emit TransferNFT(messageId, chainId, fromToken, fromChain, fromToken, toChain, tokenId, tokenType, 
                    commission, targetChainFee, msg.sender.toHexString(), recipient);
        }
        completed[messageId] = true;
    }

    function receiveNFT(string memory messageId, uint256 expires, uint256 originChain, string memory originToken,
                      uint256 fromChain, string memory fromToken, 
                      uint256 toChain, uint256 tokenId, string memory metadataUri,
                      string memory sender, string memory recipient, bytes memory signature) external {
        require(activated, "smart contract is not activated");
        require(validator != address(0), "validator is empty");
        require(treasury != address(0), "treasury is empty");
        require(block.number <= expires, "transaction time exceeded.");
        require(!completed[messageId], "already completed");
        require(toChain == chainId, "invalid chain id");
        require(verifyReceiveData(messageId, expires, originChain, originToken, fromChain, fromToken, toChain, tokenId, metadataUri, sender, recipient, signature), "failed to verify data");
        
        address WrappedToken = parseAddr(originToken);

        if(originChain == chainId) {
            bytes memory tokenHash = abi.encodePacked(originChain, originToken);
            require(hashToToken[tokenHash].wrappedTokenAddress != address(0), "wrappedToken does not exist.");
            if (hashToToken[tokenHash].tokenType == 1) {
                WERC721(hashToToken[tokenHash].wrappedTokenAddress).safeTransferFrom(address(this), parseAddr(recipient), tokenId);
            } 
            else if (hashToToken[tokenHash].tokenType == 1) {
                WERC1155(hashToToken[tokenHash].wrappedTokenAddress).safeTransferFrom(address(this), parseAddr(recipient), tokenId, 1, "");
            }
            emit ReceiveNFT(messageId, originChain, originToken, fromChain, fromToken, hashToToken[tokenHash].wrappedTokenAddress, tokenId, sender, recipient);
        } else {
            bytes memory tokenHash = abi.encodePacked(originChain, originToken);
            require(hashToToken[tokenHash].wrappedTokenAddress != address(0), "wrappedToken does not exist.");
            
            if (hashToToken[tokenHash].tokenType == 1) {
                WERC721(hashToToken[tokenHash].wrappedTokenAddress).mint(parseAddr(recipient), tokenId, metadataUri);
            } 
            // 최초가 아닐떄 create mint 관련 확인 진행 필요
            else if (hashToToken[tokenHash].tokenType == 2) {
                WERC1155(hashToToken[tokenHash].wrappedTokenAddress).mint(parseAddr(recipient), tokenId, 1, "", metadataUri);
            }
            emit ReceiveNFT(messageId, originChain, originToken, fromChain, fromToken, hashToToken[tokenHash].wrappedTokenAddress, tokenId, sender, recipient);
        }
        completed[messageId] = true;
    }

    function deployWrappedNFT(uint256 originChain, string memory originToken, uint256 tokenType, string memory name, string memory symbol) external {
        require(msg.sender == validator, "not granted account");
        require(deployer != address(0), "set up the deployer first");
        
        bytes memory tokenHash = abi.encodePacked(originChain, originToken);
        address wTokenAddr;
  
        require(hashToToken[tokenHash].wrappedTokenAddress == address(0), "already deployed.");
        wTokenAddr = IDeployer(deployer).deployWrappedNFT(originChain, originToken, tokenType, name, symbol);
        WrappedToken memory data = WrappedToken(wTokenAddr, originToken, originChain, tokenType);
        hashToToken[tokenHash] = data;
        deployedToken[wTokenAddr] = data;

        emit DeployWrappedNFT(wTokenAddr, originChain, originToken, name, symbol, tokenType);
    }

    function isActive() external view returns (bool)  {
        return activated;
    }

    function setActive(bool _active) external onlyOwner {
        activated = _active;
    }

    function setValidator(address _validator) external onlyOwner {
        require(_validator != address(0), "validator should not be zero address");
        validator = _validator;
    }

    function setTreasury(address _treasury) external onlyOwner {
        require(_treasury != address(0), "treasury should not be zero address");
        treasury = _treasury;
    }

    function setDeployer(address _deployer) external onlyOwner {
        require(_deployer != address(0), "deployer should not be zero address");
        deployer = _deployer;
    }

    function hasWrappedNFT(uint256 originChain, string memory originToken) external view returns (bool) {
        bytes memory tokenHash = abi.encodePacked(originChain, originToken);
        return (hashToToken[tokenHash].wrappedTokenAddress != address(0));
    }

    function parseAddr (string memory _a) internal pure returns (address _parsedAddress) {
        bytes memory tmp = bytes(_a);
        uint160 iaddr = 0;
        uint160 b1;
        uint160 b2;
        for (uint i = 2; i < 2 + 2 * 20; i += 2) {
            iaddr *= 256;
            b1 = uint160(uint8(tmp[i]));
            b2 = uint160(uint8(tmp[i + 1]));
            if ((b1 >= 97) && (b1 <= 102)) {
                b1 -= 87;
            } else if ((b1 >= 65) && (b1 <= 70)) {
                b1 -= 55;
            } else if ((b1 >= 48) && (b1 <= 57)) {
                b1 -= 48;
            }
            if ((b2 >= 97) && (b2 <= 102)) {
                b2 -= 87;
            } else if ((b2 >= 65) && (b2 <= 70)) {
                b2 -= 55;
            } else if ((b2 >= 48) && (b2 <= 57)) {
                b2 -= 48;
            }
            iaddr += (b1 * 16 + b2);
        }
        return address(iaddr);
    }

    function verifyReceiveData(string memory messageId, uint256 expires, uint256 originChain, string memory originToken, 
                      uint256 fromChain, string memory fromToken, 
                      uint256 toChain, uint256 tokenId, string memory metadataUri,
                      string memory sender, string memory recipient, 
                      bytes memory signature) internal returns (bool) {
        string memory data = string.concat(messageId, "|", expires.toString(), "|", originChain.toString(), "|", originToken, "|", fromChain.toString(), "|", fromToken, "|");
        data = string.concat(data, toChain.toString(), "|", sender, "|", tokenId.toString(), "|", recipient);
        bytes32 hash = keccak256(abi.encodePacked(data));

        return (ECDSA.recover(hash, signature) == validator);
    }


    function verifyTransferData(string memory messageId, uint256 expires, uint256 fromChain, string memory fromToken, 
                    uint256 toChain, uint256 tokenId, uint256 tokenType, uint256 commission, uint256 targetChainfee, string memory recipient,
                    bytes memory signature) internal returns (bool) {
        string memory data = string.concat(messageId, "|", expires.toString(), "|", fromChain.toString(), "|", fromToken, "|", toChain.toString());
        data = string.concat(data, "|", tokenId.toString(), "|", commission.toString(), "|", targetChainfee.toString(), "|", recipient);
        bytes32 hash = keccak256(abi.encodePacked(data));

        return (ECDSA.recover(hash, signature) == validator);
    }

    function onERC1155Received(address _operator, address _from, uint256 _id, uint256 _amount, bytes calldata _data) external returns(bytes4) {
        return this.onERC1155Received.selector;
    }

    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) external returns (bytes4) {
        return this.onERC721Received.selector;
    }
}

