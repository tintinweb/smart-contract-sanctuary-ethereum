pragma solidity 0.8.17;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";

abstract contract TokenVesting {
    using SafeERC20 for IERC20;
    using SafeCast for uint256;

    event RoundAdd(uint256 indexed roundId);
    event RoundPriceUpdate(uint256 indexed roundId, address indexed paymentToken, uint256 price);
    event Release(uint256 indexed roundId, address indexed beneficiary, uint256 amount);

    struct Round {
        uint64 cliff; // cliff period in seconds
        uint64 start; // start time of the vesting period
        uint64 duration; // duration of the vesting period in seconds
        uint64 slice; // duration of a slice period for the vesting in seconds
        uint256 totalTokens; // total amount of tokens to be released at the end of the vesting
        uint256 initialTokens; // initial amount of tokens to be released immediately
        uint256 releasedTokens; // amount of tokens released
        bool free; // whether or not the vesting is payable
        bool initialized;
        mapping(address => uint256) prices;
        mapping(address => uint256) earnings;
    }

    IERC20 private immutable _token;

    uint256 private _scheduledTokens;
    mapping(uint256 => Round) private _rounds;

    constructor(address token_) {
        require(token_ != address(0), "TokenVesting: the token is the zero address");
        _token = IERC20(token_);
    }

    function token() external view returns (IERC20) {
        return _token;
    }

    function getScheduledTokens() external view returns (uint256) {
        return _scheduledTokens;
    }

    function getAvailableTokens() external view returns (uint256) {
        return _token.balanceOf(address(this));
    }

    function getRound(uint256 roundId)
        external
        view
        returns (
            uint256, /* cliff */
            uint256, /* start */
            uint256, /* duration */
            uint256, /* slice */
            uint256, /* totalTokens */
            uint256, /* initialTokens */
            uint256, /* releasedTokens */
            bool /* free */
        )
    {
        Round storage round = _getRound(roundId);
        return (round.cliff, round.start, round.duration, round.slice, round.totalTokens, round.initialTokens, round.releasedTokens, round.free);
    }

    function getRoundPrice(uint256 roundId, address paymentToken) external view returns (uint256) {
        Round storage round = _getRound(roundId);
        return _getRoundPrice(round, paymentToken);
    }

    function getRoundEarnings(uint256 roundId, address paymentToken) external view returns (uint256) {
        Round storage round = _getRound(roundId);
        return _getRoundEarnings(round, paymentToken);
    }

    function getReleasableTokens(uint256 roundId) external view returns (uint256) {
        Round storage round = _getRound(roundId);
        return _getReleasableTokens(round);
    }

    function getWithdrawableEarnings(address paymentToken) public view returns (uint256) {
        return IERC20(paymentToken).balanceOf(address(this));
    }

    function _addRound(
        uint256 roundId,
        uint256 cliff,
        uint256 start,
        uint256 duration,
        uint256 slice,
        uint256 totalTokens,
        uint256 initialTokens,
        bool free
    ) internal {
        Round storage round = _rounds[roundId];
        require(round.initialized == false, "TokenVesting: the round already exists");

        round.cliff = cliff.toUint64();
        round.start = start.toUint64();
        round.duration = duration.toUint64();
        round.slice = slice.toUint64();
        round.totalTokens = totalTokens;
        round.initialTokens = initialTokens;
        round.free = free;
        round.initialized = true;

        _scheduledTokens += totalTokens;

        emit RoundAdd(roundId);
    }

    function _updateRoundPrice(
        uint256 roundId,
        address paymentToken,
        uint256 price
    ) internal {
        Round storage round = _getRound(roundId);
        require(round.free == false, "TokenVesting: the round is free");

        round.prices[paymentToken] = price;

        emit RoundPriceUpdate(roundId, paymentToken, price);
    }

    function _release(
        uint256 roundId,
        uint256 amount,
        address beneficiary,
        address paymentToken
    ) internal {
        Round storage round = _getRound(roundId);

        if (paymentToken == address(0)) {
            require(round.free == true, "TokenVesting: the round is paid");
        } else {
            require(round.free == false, "TokenVesting: the round is free");
        }

        uint256 releasableTokens = _getReleasableTokens(round);
        require(amount <= releasableTokens, "TokenVesting: not enough tokens");

        round.releasedTokens += amount;

        _scheduledTokens -= amount;
        _token.safeTransfer(beneficiary, amount);

        if (round.free == false) {
            uint256 price = _getRoundPrice(round, paymentToken);
            require(price > 0, "TokenVesting: unallowed payment token");

            uint256 fundsToPay = amount * price;

            round.earnings[paymentToken] += fundsToPay;
            IERC20(paymentToken).safeTransferFrom(beneficiary, address(this), fundsToPay);
        }

        emit Release(roundId, beneficiary, amount);
    }

    function _withdrawEarnings(
        address to,
        uint256 amount,
        address paymentToken
    ) internal {
        require(amount <= getWithdrawableEarnings(paymentToken), "TokenVesting: not enough earnings");
        IERC20(paymentToken).safeTransfer(to, amount);
    }

    function _getRoundPrice(Round storage round, address paymentToken) internal view returns (uint256) {
        return round.prices[paymentToken];
    }

    function _getRoundEarnings(Round storage round, address paymentToken) internal view returns (uint256) {
        return round.earnings[paymentToken];
    }

    function _getReleasableTokens(Round storage round) internal view returns (uint256) {
        uint256 currentTime = block.timestamp;

        if (currentTime < round.start) {
            return 0;
        } else if (currentTime < round.start + round.cliff) {
            return round.initialTokens - round.releasedTokens;
        } else if (currentTime >= round.start + round.duration) {
            return round.totalTokens - round.releasedTokens;
        } else {
            uint256 timeFromStart = currentTime - round.start - round.cliff;
            uint256 vestedSlicePeriods = timeFromStart / round.slice;
            uint256 vestedSeconds = vestedSlicePeriods * round.slice;
            uint256 vestedAmount = ((round.totalTokens - round.initialTokens) * vestedSeconds) / (round.duration - round.cliff);

            vestedAmount = vestedAmount + round.initialTokens - round.releasedTokens;

            return vestedAmount;
        }
    }

    function _getRound(uint256 roundId) internal view returns (Round storage round) {
        round = _rounds[roundId];
        require(round.initialized, "TokenVesting: the round does not exist");
    }
}

