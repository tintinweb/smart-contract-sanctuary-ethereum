pragma solidity 0.8.17;

import "./vesting/TokenVesting.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract AllStarsCoinVesting is Ownable, ReentrancyGuard, TokenVesting {
    event WhitelistAdd(uint256 indexed roundId, address indexed account);
    event WhitelistRemove(uint256 indexed roundId, address indexed account);

    uint256 private immutable _tgeTime;
    mapping(uint256 => mapping(address => bool)) private _whitelist;

    // 1666137600 - 19 Oct 2022 00:00:00 UTC
    // 1667401200 - 02 Nov 2022 15:00:00 UTC
    constructor(uint256 tgeTime_, address token_) TokenVesting(token_) {
        _tgeTime = tgeTime_;

        _addRound(
            uint256(keccak256("Cornerstone Investor Round")),
            365 days, /* cliff */
            tgeTime_, /* start */
            1096 days, /* duration */
            1, /* slice */
            27_000_000 * (10**18), /* totalTokens */
            0, /* initialTokens */
            false /* free */
        );

        _addRound(
            uint256(keccak256("Seed Round")),
            365 days, /* cliff */
            tgeTime_, /* start */
            731 days, /* duration */
            1, /* slice */
            72_000_000 * (10**18), /* totalTokens */
            0, /* initialTokens */
            false /* free */
        );

        _addRound(
            uint256(keccak256("Public Round")),
            31 days, /* cliff */
            tgeTime_, /* start */
            365 days, /* duration */
            1, /* slice */
            9_000_000 * (10**18), /* totalTokens */
            450_000 * (10**18), /* initialTokens */
            false /* free */
        );

        _addRound(
            uint256(keccak256("Marketing & Partnerships")),
            0, /* cliff */
            tgeTime_, /* start */
            1096 days, /* duration */
            1, /* slice */
            225_000_000 * (10**18), /* totalTokens */
            0, /* initialTokens */
            true /* free */
        );

        _addRound(
            uint256(keccak256("Treasury & Staking")),
            0, /* cliff */
            tgeTime_, /* start */
            731 days, /* duration */
            1, /* slice */
            270_000_000 * (10**18), /* totalTokens */
            0, /* initialTokens */
            true /* free */
        );

        _addRound(
            uint256(keccak256("Liquidity")),
            0, /* cliff */
            tgeTime_, /* start */
            0, /* duration */
            1, /* slice */
            90_000_000 * (10**18), /* totalTokens */
            90_000_000 * (10**18), /* initialTokens */
            true /* free */
        );

        _addRound(
            uint256(keccak256("Advisors")),
            365 days, /* cliff */
            tgeTime_, /* start */
            731 days, /* duration */
            1, /* slice */
            36_000_000 * (10**18), /* totalTokens */
            0, /* initialTokens */
            true /* free */
        );

        _addRound(
            uint256(keccak256("Team & Operations")),
            365 days, /* cliff */
            tgeTime_, /* start */
            1096 days, /* duration */
            1, /* slice */
            171_000_000 * (10**18), /* totalTokens */
            0, /* initialTokens */
            true /* free */
        );
    }

    function startTime() external view returns (uint256) {
        return _tgeTime;
    }

    function whitelistAdd(uint256 roundId, address account) external onlyOwner {
        require(account != address(0), "AllStarsCoinVesting: the account is the zero address");
        require(_whitelist[roundId][account] == false, "AllStarsCoinVesting: the address already added");

        _whitelist[roundId][account] = true;

        emit WhitelistAdd(roundId, account);
    }

    function whitelistRemove(uint256 roundId, address account) external onlyOwner {
        require(account != address(0), "AllStarsCoinVesting: the account is the zero address");
        require(_whitelist[roundId][account] == true, "AllStarsCoinVesting: the address not found");

        delete _whitelist[roundId][account];

        emit WhitelistRemove(roundId, account);
    }

    function updateRoundPrice(
        uint256 roundId,
        address paymentToken,
        uint256 price
    ) external onlyOwner {
        require(paymentToken != address(0), "AllStarsCoinVesting: payment token is the zero address");
        require(price > 0, "AllStarsCoinVesting: price is 0");

        _updateRoundPrice(roundId, paymentToken, price);
    }

    function withdrawEarnings(uint256 amount, address paymentToken) external onlyOwner {
        require(paymentToken != address(0), "AllStarsCoinVesting: payment token is the zero address");
        require(amount > 0, "AllStarsCoinVesting: amount is 0");

        _withdrawEarnings(owner(), amount, paymentToken);
    }

    function release(uint256 roundId, uint256 amount) external nonReentrant {
        require(amount > 0, "AllStarsCoinVesting: amount is 0");

        address caller = _msgSender();

        _whitelistCheck(roundId, caller);
        _release(roundId, amount, caller, address(0));
    }

    function release(
        uint256 roundId,
        uint256 amount,
        address paymentToken
    ) external nonReentrant {
        require(amount > 0, "AllStarsCoinVesting: amount is 0");
        require(paymentToken != address(0), "AllStarsCoinVesting: payment token is the zero address");

        address caller = _msgSender();

        _whitelistCheck(roundId, caller);
        _release(roundId, amount, caller, paymentToken);
    }

    function _whitelistCheck(uint256 roundId, address account) internal view {
        require(_whitelist[roundId][account] == true, "AllStarsCoinVesting: no permission");
    }
}

