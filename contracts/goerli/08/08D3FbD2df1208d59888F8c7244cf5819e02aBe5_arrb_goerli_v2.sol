import "IERC20.sol";
import "Ownable.sol";
import "EnumerableSet.sol";
import "IUniswapV2Pair.sol";
import "IUniswapV3Pool.sol";
import "IWETH.sol";
import "IUniswapV2Callee.sol";
import "IUniswapV2Factory.sol";

contract arrb_goerli_v2 is IUniswapV2Callee, Ownable {
    //address private constant UNISWAP_V2_FACTORY =
    //    0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f;

    address private constant UNI = 0x1f9840a85d5aF5bf1D1762F925BDADdC4201F984;
    address private constant WETH = 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6;

    //IUniswapV2Factory private constant factory =
    //    IUniswapV2Factory(UNISWAP_V2_FACTORY);

    IERC20 private constant weth = IERC20(WETH);

    IUniswapV2Pair private pair;

    // For this example, store the amount to repay
    uint256 public amountToRepay;

    constructor() {
        //pair = IUniswapV2Pair(factory.getPair(DAI, WETH));
    }

    function setPool(address poolAddress) external onlyOwner {
        pair = IUniswapV2Pair(poolAddress);
    }

    function flashSwap(uint256 wethAmount) external {
        // Need to pass some data to trigger uniswapV2Call
        bytes memory data = abi.encode(weth, msg.sender);

        // amount0Out is UNI, amount1Out is WETH
        pair.swap(0, wethAmount, address(this), data);
    }

    // This function is called by the UNI/WETH pair contract
    function uniswapV2Call(
        address sender,
        uint256 amount0,
        uint256 amount1,
        bytes calldata data
    ) external {
        require(msg.sender == address(pair), "not pair");
        require(sender == address(this), "not sender");

        (address tokenBorrow, address caller) = abi.decode(
            data,
            (address, address)
        );

        // Your custom code would go here. For example, code to arbitrage.
        require(tokenBorrow == WETH, "token borrow != WETH");

        // about 0.3% fee, +1 to round up
        uint256 fee = (amount1 * 3) / 997 + 1;
        amountToRepay = amount1 + fee;

        // Transfer flash swap fee from caller
        weth.transferFrom(caller, address(this), fee);

        // Repay
        weth.transfer(address(pair), amountToRepay);
    }
}

