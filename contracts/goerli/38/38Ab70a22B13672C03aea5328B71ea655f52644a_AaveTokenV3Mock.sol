}

contract AaveTokenV3Mock is AaveTokenV3 {
  uint256 TRANSFER_AMOUNT = 1000 ether;

  function initialize() external override initializer {
    _name = 'Aave token';
    uint256 chainId;

    //solium-disable-next-line
    assembly {
      chainId := chainid()
    }

    DOMAIN_SEPARATOR = keccak256(
      abi.encode(
        EIP712_DOMAIN,
        keccak256(bytes(name())),
        keccak256(EIP712_REVISION),
        chainId,
        address(this)
      )
    );
  }

  function mintToWallet(address account) external {
    _totalSupply += TRANSFER_AMOUNT;

    emit Transfer(address(0), account, TRANSFER_AMOUNT);

    _transferWithDelegation(address(0), account, TRANSFER_AMOUNT);
  }
}

contract DeployAaveTokenV3 {
  function _deploy(string memory path, Vm vm) public {
    // ----------------- Persist addresses -----------------------------------------------------------------------------
    DeployerHelpers.Addresses memory addresses = DeployerHelpers.decodeJson(
      path,
      vm
    );
    // -----------------------------------------------------------------------------------------------------------------

    // deploy token v2
    AaveTokenV2 aaveTokenV2Impl = new AaveTokenV2();

    InitializableAdminUpgradeabilityProxy aaveTokenProxy = new InitializableAdminUpgradeabilityProxy();
    aaveTokenProxy.initialize(
      address(aaveTokenV2Impl),
      addresses.proxyAdmin,
      abi.encodeWithSelector(AaveTokenV2.initialize.selector)
    );

    AaveTokenV3 aaveTokenV3Impl = new AaveTokenV3Mock();
    aaveTokenProxy.upgradeToAndCall(
      address(aaveTokenV3Impl),
      abi.encodeWithSelector(AaveTokenV2.initialize.selector)
    );

    // ----------------- Persist addresses -----------------------------------------------------------------------------
    addresses.aaveTokenV3 = address(aaveTokenProxy);
    DeployerHelpers.encodeJson(path, addresses, vm);
    // -----------------------------------------------------------------------------------------------------------------
  }
}

// last deployed revision is 5. remember to add 1 if new implementation is needed
contract UpdateAaveTokenV3 {
  function _update(string memory path, Vm vm) public {
    // ----------------- Persist addresses -----------------------------------------------------------------------------
    DeployerHelpers.Addresses memory addresses = DeployerHelpers.decodeJson(
      path,
      vm
    );
    // -----------------------------------------------------------------------------------------------------------------

    AaveTokenV3Mock aaveTokenV3Impl = new AaveTokenV3Mock();
    InitializableAdminUpgradeabilityProxy(payable(addresses.aaveTokenV3))
      .upgradeToAndCall(
        address(aaveTokenV3Impl),
        abi.encodeWithSelector(AaveTokenV3Mock.initialize.selector)
      );
  }
}

